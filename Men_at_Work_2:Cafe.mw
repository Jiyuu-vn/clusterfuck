  <Line StartPos="15922" Length="5" Skip="false">
    <Translated>Cliff</Translated>
  </Line>
  <Line StartPos="15949" Length="12" Skip="false">
    <Translated>&quot;Oh, I see.&quot;</Translated>
  </Line>
  <Line StartPos="15975" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="15992" Length="35" Skip="false">
    <Translated>&quot;Did you find us a job as Hunters?&quot;</Translated>
  </Line>
  <Line StartPos="16032" Length="5" Skip="false">
    <Translated>Cliff</Translated>
  </Line>
  <Line StartPos="16059" Length="7" Skip="false">
    <Translated>&quot;No...&quot;</Translated>
  </Line>
  <Line StartPos="16082" Length="28" Skip="false">
