  <Line StartPos="11316" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="11333" Length="15" Skip="false">
    <Translated>&quot;You&apos;re right.&quot;</Translated>
  </Line>
  <Line StartPos="11355" Length="31" Skip="false">
