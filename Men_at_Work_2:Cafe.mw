  </Line>
  <Line StartPos="16128" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="16145" Length="10" Skip="false">
    <Translated>&quot;Welcome.&quot;</Translated>
  </Line>
  <Line StartPos="16160" Length="5" Skip="false">
    <Translated>Cliff</Translated>
  </Line>
  <Line StartPos="16187" Length="12" Skip="false">
    <Translated>&quot;Huh? Mena?&quot;</Translated>
  </Line>
  <Line StartPos="16213" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="16230" Length="44" Skip="false">
