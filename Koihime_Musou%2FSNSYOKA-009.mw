  <String index="216">
    <ToTranslate>Shuri$&quot;Hau...&quot;@</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>sei_2396</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>Sei$&quot;Although the public safety within our neighboring cities has improved, these are still turbulent times we&apos;re living in.&quot;@</ToTranslate>
