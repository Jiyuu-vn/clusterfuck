  <String index="281">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="282">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="283">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="284">
    <ToTranslate>rinr0266</ToTranslate>
  </String>
  <String index="285">
    <ToTranslate>Rinrin$&quot;I see the enemy&apos;s rear guard!&quot;@</ToTranslate>
  </String>
  <String index="286">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="287">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="288">
    <ToTranslate>aisy0469</ToTranslate>
  </String>
  <String index="289">
    <ToTranslate>Aisha$&quot;Okay! Everyone, follow me!&quot;@</ToTranslate>
  </String>
  <String index="290">
    <ToTranslate>tcg_bac32fa</ToTranslate>
  </String>
  <String index="291">
    <ToTranslate>tcg_bac32m</ToTranslate>
  </String>
  <String index="292">
    <ToTranslate>sui_0001</ToTranslate>
  </String>
  <String index="293">
    <ToTranslate>Sui$&quot;All units, charge! We&apos;re going to blow Enshou&apos;s army away!&quot;@</ToTranslate>
  </String>
