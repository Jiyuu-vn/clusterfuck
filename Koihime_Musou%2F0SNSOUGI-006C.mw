  <String index="754">
    <ToTranslate>bgm20</ToTranslate>
  </String>
  <String index="755">
    <ToTranslate>$After confirming my nod of acceptance, Sousou resolved herself and began taking her underwear off, blushing.@</ToTranslate>
  </String>
  <String index="756">
    <ToTranslate>sous0342</ToTranslate>
  </String>
  <String index="757">
    <ToTranslate>Sousou$&quot;Huff...... huff......&quot;@</ToTranslate>
  </String>
  <String index="758">
    <ToTranslate>$Sousou took several deep breathes to try and relax her tense body.@</ToTranslate>
  </String>
  <String index="759">
    <ToTranslate>sous0343</ToTranslate>
  </String>
  <String index="760">
    <ToTranslate>Sousou$&quot;......Bind me.&quot;@</ToTranslate>
  </String>
  <String index="761">
    <ToTranslate>$She turned her back towards me and held her hands out.@</ToTranslate>
  </String>
  <String index="762">
    <ToTranslate>$I tied her wrists carefully so it wouldn&apos;t hurt, and laid her on my bed.@</ToTranslate>
  </String>
  <String index="763">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
