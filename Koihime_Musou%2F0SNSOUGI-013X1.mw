[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_sou22ea</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>sonk0382</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>Sonken$&quot;Brave Son-Go soldiers! Display your ferocious power! Don&apos;t let Sougi outperform you!&quot;@</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>sous0429</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>Sousou$&quot;Faithful Gi soldiers! Don&apos;t fall behind the Son-Go soldiers! Defeat the enemy with Gi&apos;s pride in your hearts!&quot;@</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>sonk0383</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Sonken$&quot;Hmph. Are you challenging the might of Son-Go, Sousou?&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_sou22ma</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>sous0430</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>Sousou$&quot;Challenge you? Heh heh, I think you have a misunderstanding. I&apos;m letting you practice with us.&quot;@</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_son12ia</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>sonk0384</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Sonken$&quot;......Oh, that&apos;s quite the boast.&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_sou22ba</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>sous0431</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Sousou$&quot;Oh? Same to you.&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>sonk0385</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>Sonken$&quot;......Okay then. Shall we compete to see who is more helpful to Kazuto?&quot;@</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_sou22ma</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>sous0432</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Sousou$&quot;Fine. ......I don&apos;t care about being helpful to that guy, but the ruler of Sougi can&apos;t be defeated by Son-Go.&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>sonk0386</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>Sonken$&quot;......You&apos;re not honest, are you.&quot;@</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_sou22ga</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>sous0433</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Sousou$&quot;......Hmph. Could you keep your chatter to yourself?&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>sonk0387</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>Sonken$&quot;......Fine. Come on, Sousou, it&apos;s a match.&quot;@</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_sou22ea</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>sous0434</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>Sousou$&quot;Very well. It&apos;s a match, Sonken.&quot;@</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="50">
