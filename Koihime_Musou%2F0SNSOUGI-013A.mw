[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>rinr0540</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Rinrin$&quot;Ugahhh! No matter how many of them we knock down, there are always more!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>sui_0253</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Sui$&quot;There&apos;s just too many of them! Dammit, this is endless!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>aisy1025</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Aisha$&quot;Stop complaining! Even if there number 10,000 more than us, all we have to do is take down 1,000 each ourselves!&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_cho21da</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>rinr0541</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>Rinrin$&quot;But a battle depends on the number of soldiers!&quot;@</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_cun21ha</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>sei_0338</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Sei$&quot;But we just don&apos;t have any more reserve soldiers.&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_koc32ia</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>shio0119</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Shion$&quot;I don&apos;t think there will be reinforcements either.&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>syur0520</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>Shuri$&quot;Sousou-san and Sonken-san are both busy governing Gi and Go and they don&apos;t even have time to organize an army.....&quot;@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>Kazuto$&quot;That&apos;s why I declined their offer to help. ......Those two have things to do hereafter.&quot;@</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_syo41ea</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>syur0521</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Shuri$&quot;That&apos;s true. That&apos;s why we must defeat the enemy and claim the future of this world.&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>aisy1026</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Aisha$&quot;Shuri is absolutely right! Rinrin! Sui! If you have time to complain, take down the enemy!&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>tcg_bac12ga</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>sui_0254</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>Sui$&quot;Uggh, that&apos;s absurd!&quot;@</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>rinr0542</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>Rinrin$&quot;Aisha is a demon.&quot;@</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>aisy1027</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>Aisha$&quot;Who&apos;s a demon?! Before you move your impudent mouth, move those hands of yours!&quot;@</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>rinr0543</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Rinrin$&quot;Okay.&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>battle_kan</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>SE033</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>yure_y</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>battle_cho</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>SE034</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>yure_r</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>battle_bac</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>SE035</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>yure_t</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>Soldier$&quot;Ghah.&quot;@</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>SE038</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>shio0120</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>Shion$&quot;But...... they really do continue to pop up one after another. If they have any reserve troops, they&apos;ll break the balance.&quot;@</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>tcg_syo11ga</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>syur0522</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>Shuri$&quot;I pray that they don&apos;t...... but I bet......&quot;@</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>SE047a</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>Soldier$&quot;A-a new enemy army appeared ahead of us! It&apos;s an enemy ambush!&quot;@</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>syur0523</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>Shuri$&quot;......I knew it wouldn&apos;t be that easy.&quot;@</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>tcg_koc32ia</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>shio0121</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>Shion$&quot;Indeed......&quot;@</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>bgm0000</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>sei_0339</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>Sei$&quot;But we can&apos;t afford to lose here. ......Chouun Squad, we&apos;re going to counterattack the ambush! Follow my flag!&quot;@</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>Kazuto$&quot;Sei?!&quot;@</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>sei_0340</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>Sei$&quot;Hmph...... we&apos;ll take these troops. My Lord, this is the roughest spot. I&apos;ll inspire my soldiers and get through it somehow.&quot;@</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>Kazuto$&quot;......Sure! But don&apos;t die, okay Sei?&quot;@</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>sei_0341</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>Sei$&quot;Of course not. ......I&apos;ll see you later!&quot;@</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>aisy1028</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>Aisha$&quot;I&apos;m counting on you, Sei...... We&apos;ll protect Our Lord here and repel the enemy attack! Let&apos;s go!&quot;@</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>tcg_bac11m</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>sui_0255</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>Sui$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>bg_black</ToTranslate>
  </String>
</DBS>

</poem>
