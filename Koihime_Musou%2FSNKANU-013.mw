  <String index="268">
    <ToTranslate>aisy2492</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>Aisha$&quot;My Lord is such a bother. These don&apos;t...look good on me. How many times must I tell him for him to understand?&quot;@</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>$She was holding the hair accessory that I bought for her this afternoon.@</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>$She was also holding the clothes that she bought in the city with Shion and Sei against her chest.@</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>aisy2493</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>Aisha$&quot;Sei told me to show my womanly side to My Lord......How irresponsible.&quot;@</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>aisy2494</ToTranslate>
  </String>
  <String index="275">
    <ToTranslate>Aisha$&quot;But is that what he wants? Is that why he gave me this today?&quot;@</ToTranslate>
  </String>
  <String index="276">
    <ToTranslate>aisy2495</ToTranslate>
  </String>
  <String index="277">
    <ToTranslate>Aisha$&quot;Does he want to see me like this......?&quot;@</ToTranslate>
  </String>
  <String index="278">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="279">
    <ToTranslate>kyara_kesi</ToTranslate>
