    <ToTranslate>tcg_kan61aa</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>tcg_kan61m</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>aisy0829</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>Aisha$&quot;Hmh! Hmh! Hmh! Hmh!&quot;@</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>aisy0830</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>Aisha$&quot;That&apos;s too slow, Chouryou! Your spear looks like it&apos;s not moving at all!&quot;@</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>tcg_cry41aa</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>tcg_cry41m</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>chor0201</ToTranslate>
