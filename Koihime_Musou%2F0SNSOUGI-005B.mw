  </String>
  <String index="179">
    <ToTranslate>aisy0827</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>Aisha$&quot;Guh...... you parried every strike?!&quot;@</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>tcg_cry41aa</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>tcg_cry41m</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>chor0198</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>Chouryou$&quot;Hmph! Are you surprised by that? Maybe you aren&apos;t so strong after all, Kan&apos;u-chan!&quot;@</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>aisy0828</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>Aisha$&quot;You&apos;re bragging just because you blocked a few mere stikes, Chouryou?!&quot;@</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>tcg_cry21ha</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>chor0199</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>Chouryou$&quot;Find out if I&apos;m bragging or not for yourself! It&apos;s my turn now!&quot;@</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>tcg_cry91aa</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>tcg_cry91m</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>chor0200</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>Chouryou$&quot;Uraaaaaah!&quot;@</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>battle_harai2</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>s_yure_y</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>SE016</ToTranslate>
  </String>
