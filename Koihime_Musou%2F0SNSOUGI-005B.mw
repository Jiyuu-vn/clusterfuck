  <String index="379">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="380">
    <ToTranslate>chor0218</ToTranslate>
  </String>
  <String index="381">
    <ToTranslate>Chouryou$&quot;I won&apos;t be able to fight against Mou-chan, though. Is that okay?&quot;@</ToTranslate>
