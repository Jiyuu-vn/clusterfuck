    <ToTranslate>tcg_cry11ca</ToTranslate>
  </String>
  <String index="387">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="388">
    <ToTranslate>chor0219</ToTranslate>
  </String>
  <String index="389">
    <ToTranslate>Chouryou$&quot;Thanks. That makes me feel a bit better.&quot;@</ToTranslate>
