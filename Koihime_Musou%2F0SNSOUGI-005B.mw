[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_cry11fa</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>chor0176</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Chouryou$&quot;Ahh! Shit, we lost again!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_zgi11aa</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_zgi11m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>Soldier$&quot;General Chouryou! It&apos;s becoming difficult to rejoin the rear! We must withdraw quickly!&quot;@</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>tcg_cry21ha</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>chor0177</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>Chouryou$&quot;I know that! All units, withdraw!&quot;@</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Soldier$&quot;Yes, Ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>SE047a</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>aisy0813</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>Aisha$&quot;Wait!&quot;@</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>tcg_cry21ha</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>chor0178</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>Chouryou$&quot;Geh! Kan&apos;u?!&quot;@</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>aisy0814</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>Aisha$&quot;I can not allow you to escape! Come and face me, Chouryou!&quot;@</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_cry31ja</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_cry31m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>chor0179</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Chouryou$&quot;Ooo...... ck...... w-what to do......Ohh, I want to fight, I want to fight, I want to fight, I want to fight......&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_zgi11aa</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>tcg_zgi11m</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Soldier$&quot;N-no, General! We must withdraw immediately and merge with General Kakouen......!&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_cry21ha</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>chor0180</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Chouryou$&quot;Ooooooahhh! No! I want to fight! You guys go ahead!&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Soldier$&quot;B-but!&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>chor0181</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>Chouryou$&quot;Just go! I&apos;m going to stop Kan&apos;u here! You take everyone else and go!&quot;@</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>Soldier$&quot;But you already saved our lives at Hulao Pass! We&apos;ll never leave you and run again!&quot;@</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_cry11fa</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>chor0182</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>Chouryou$&quot;Fools! A commander shouldn&apos;t get emotional! It&apos;s your job to take the rest of the soldiers and run!&quot;@</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>chor0183</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>Chouryou$&quot;Hurry up and go! That&apos;s an order!&quot;@</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>Soldier$&quot;But......!&quot;@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>tcg_cry21ha</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>chor0184</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>Chouryou$&quot;Go!&quot;@</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Soldier$&quot;Y-yes ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>aisy0815</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>Aisha$&quot;Letting your soldiers escape? ......What a kind general you are.&quot;@</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>tcg_cry21ga</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>chor0185</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>Chouryou$&quot;Hmph...... Don&apos;t say the same thing as Gen-chan.&quot;@</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>ecg_cry01a</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>SE014</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>chor0186</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>Chouryou$&quot;It&apos;s not a matter of being kind or not. I just don&apos;t want anyone else to interfere with my fight against you, Kan&apos;u.&quot;@</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>aisy0816</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>Aisha$&quot;......So I heard from Rinrin. It seems you&apos;re only after me, for some reason.&quot;@</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>chor0187</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>Chouryou$&quot;I&apos;m not really after you as such...... it&apos;s more like an admiration.&quot;@</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>aisy0817</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>Aisha$&quot;An admiration?&quot;@</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>chor0188</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>Chouryou$&quot;Yes. When I saw you fight against Ryofu-chan at Hulao Pass...... I fell in love with you.&quot;@</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>aisy0818</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>Aisha$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>chor0189</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>Chouryou$&quot;I truly fell in love with you from the bottom of my heart. Isn&apos;t it normal for a warrior to desire the life of the person they fall for?&quot;@</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>aisy0819</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>Aisha$&quot;......As a warrior, huh?&quot;@</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>chor0190</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>Chouryou$&quot;As a warrior. ......Well, I think I desire you a bit as a woman, too.&quot;@</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>aisy0820</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>Aisha$&quot;Are you saying...... you want me in the same way Sousou wants me?&quot;@</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>chor0191</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>Chouryou$&quot;I want you. Your body, your soul...... and your life, too.&quot;@</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>aisy0821</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>Aisha$&quot;......Hmph. Then try to take them by force if you can!&quot;@</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>chor0192</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>Chouryou$&quot;Very good! If I win, everything about you is mine!&quot;@</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>aisy0822</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>Aisha$&quot;Whatever. Here I come!&quot;@</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_cry41aa</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_cry41m</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>chor0193</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>Chouryou$&quot;Come on!&quot;@</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>tcg_kan91aa</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>tcg_kan91m</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>aisy0823</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>Aisha$&quot;Yahhhh!&quot;@</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>battle_kesa</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>$With a brave shout, Kan&apos;u&apos;s strike left a shining arc in the air as it fell towards Chouryou&apos;s head.@</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>s_yure_r</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>tcg_cry41aa</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>tcg_cry41m</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>chor0194</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>Chouryou$&quot;Fool! Did you think such a straightforward attack was going to work on me?&quot;@</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>aisy0824</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>Aisha$&quot;Hmph, I guess I&apos;ll need more than one strike, huh?&quot;@</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>ecg_cry01a</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>chor0195</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>Chouryou$&quot;Of course! You&apos;ll lose if you underestimate me and hold back, Kan&apos;u!&quot;@</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>aisy0825</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>Aisha$&quot;All right. Then I won&apos;t hold back! Take my Green Dragon Blade!&quot;@</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>chor0196</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>Chouryou$&quot;Come on!&quot;@</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>tcg_kan91aa</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>tcg_kan91m</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>aisy0826</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>Aisha$&quot;Yahh!&quot;@</ToTranslate>
  </String>
