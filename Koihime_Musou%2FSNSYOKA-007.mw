  <String index="256">
    <ToTranslate>syur1301</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>Shuri$&quot;Ahh... so embarrassing that he caught me in that.&quot;@</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>tcg_syo21fa</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>syur1302</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>Shuri$&quot;Does he think I&apos;m a dirty girl...? Ooh, I&apos;m embarrassed.&quot;@</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>syur1303</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>Shuri$&quot;But... this is for him. So I shouldn&apos;t be embarrassed.&quot;@</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>syur1304</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>Shuri$&quot;If I study this Fang Zhong Shu for My Lord, I&apos;ll be able to please him.&quot;@</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>tcg_syo11la</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>tenten</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>syur1305</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>Shuri $&quot;......Tee hee.&quot;@</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>tcg_syo11ka</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>syur1306</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>Shuri$&quot;I have to work hard. Okay, I should go back to my room and review it.&quot;@</ToTranslate>
