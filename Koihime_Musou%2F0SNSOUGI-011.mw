    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="495">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="496">
    <ToTranslate>tcg_hg11aa</ToTranslate>
  </String>
  <String index="497">
    <ToTranslate>tcg_hg11m</ToTranslate>
  </String>
  <String index="498">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="499">
    <ToTranslate>chos0126</ToTranslate>
  </String>
  <String index="500">
    <ToTranslate>Chousen$&quot;Good luck. Even if you have been scripted to do so......&quot;@</ToTranslate>
  </String>
  <String index="501">
    <ToTranslate>tcg_hg11ga</ToTranslate>
  </String>
  <String index="502">
    <ToTranslate>tcg_hg11m</ToTranslate>
  </String>
  <String index="503">
    <ToTranslate>chos0127</ToTranslate>
  </String>
  <String index="504">
    <ToTranslate>Chousen$&quot;It will connect to the next story...&quot;@</ToTranslate>
  </String>
  <String index="505">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
