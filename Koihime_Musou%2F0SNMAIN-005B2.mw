    <ToTranslate>ren_0003</ToTranslate>
  </String>
  <String index="1132">
    <ToTranslate>Ren$&quot;......Sekito.&quot;@</ToTranslate>
  </String>
  <String index="1133">
    <ToTranslate>Sekito$&quot;Woof!&quot;@</ToTranslate>
  </String>
