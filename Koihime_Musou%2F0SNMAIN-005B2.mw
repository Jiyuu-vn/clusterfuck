  <String index="1434">
    <ToTranslate>aisy0406</ToTranslate>
  </String>
  <String index="1435">
    <ToTranslate>Aisha$&quot;Rinrin? Thanks for the help!&quot;@</ToTranslate>
  </String>
  <String index="1436">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="1437">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="1438">
    <ToTranslate>rinr0236</ToTranslate>
  </String>
  <String index="1439">
    <ToTranslate>Rinrin$&quot;Don&apos;t mention it! Hurry up and gather your men! We&apos;re going to counterattack!&quot;@</ToTranslate>
  </String>
  <String index="1440">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="1441">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="1442">
    <ToTranslate>aisy0407</ToTranslate>
  </String>
  <String index="1443">
    <ToTranslate>Aisha$&quot;But they just pop up one after another!&quot;@</ToTranslate>
  </String>
  <String index="1444">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="1445">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="1446">
    <ToTranslate>rinr0237</ToTranslate>
  </String>
  <String index="1447">
    <ToTranslate>Rinrin$&quot;And we&apos;ll just take them down one at a time! That&apos;s all we can do!&quot;@</ToTranslate>
  </String>
  <String index="1448">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="1449">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="1450">
    <ToTranslate>aisy0408</ToTranslate>
  </String>
  <String index="1451">
    <ToTranslate>Aisha$&quot;I know, but......&quot;@</ToTranslate>
