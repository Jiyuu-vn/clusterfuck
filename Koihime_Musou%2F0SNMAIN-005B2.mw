  <String index="1189">
    <ToTranslate>tcg_ryo11m</ToTranslate>
  </String>
  <String index="1190">
    <ToTranslate>ren_0014</ToTranslate>
  </String>
  <String index="1191">
    <ToTranslate>Ren$&quot;......Enemy&apos;s coming.&quot;@</ToTranslate>
  </String>
  <String index="1192">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="1193">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="1194">
    <ToTranslate>aisy0395</ToTranslate>
  </String>
  <String index="1195">
    <ToTranslate>Aisha$&quot;What? Where?&quot;@</ToTranslate>
  </String>
  <String index="1196">
    <ToTranslate>$No sooner did Aisha speak than the soldiers who were guarding the gate start screaming.@</ToTranslate>
  </String>
  <String index="1197">
    <ToTranslate>Kazuto$&quot;......?! What?! What happened?!&quot;@</ToTranslate>
  </String>
  <String index="1198">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="1199">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="1200">
    <ToTranslate>Soldier$&quot;We sheltered a coach with a girl who appears to be an aristocrat, but a group of men in white clothing chased after and attacked us.&quot;@</ToTranslate>
  </String>
  <String index="1201">
    <ToTranslate>Soldier$&quot;Furthermore, a mysterious giant appeared and entered the fray. The area in front of the gate is in chaos!&quot;@</ToTranslate>
  </String>
  <String index="1202">
    <ToTranslate>tcg_syo21ca</ToTranslate>
  </String>
  <String index="1203">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="1204">
    <ToTranslate>syur0217</ToTranslate>
  </String>
  <String index="1205">
    <ToTranslate>Shuri$&quot;White clothes...... are they the ones?&quot;@</ToTranslate>
  </String>
  <String index="1206">
    <ToTranslate>tcg_ryo41ka</ToTranslate>
