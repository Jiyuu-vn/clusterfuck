    <ToTranslate>ren_0012</ToTranslate>
  </String>
  <String index="1177">
    <ToTranslate>Ren$&quot;......The halberd.&quot;@</ToTranslate>
  </String>
  <String index="1178">
    <ToTranslate>Kazuto$&quot;Huh?&quot;@</ToTranslate>
  </String>
