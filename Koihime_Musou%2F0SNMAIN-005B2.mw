    <ToTranslate>ren_0002</ToTranslate>
  </String>
  <String index="1127">
    <ToTranslate>Ren$&quot;......(Nod)&quot;@</ToTranslate>
  </String>
  <String index="1128">
    <ToTranslate>Dog$&quot;Woof, woof! Woof!&quot;@</ToTranslate>
  </String>
