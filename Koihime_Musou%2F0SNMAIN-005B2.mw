    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1474">
    <ToTranslate>chos0001</ToTranslate>
  </String>
  <String index="1475">
    <ToTranslate>???$&quot;Whahhtt!&quot;@</ToTranslate>
