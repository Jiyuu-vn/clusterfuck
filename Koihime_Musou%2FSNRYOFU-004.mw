  <String index="234">
    <ToTranslate>ren_0208</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>Ren$&quot;... what is this awkward stirring in my heart. When is it going to stop?&quot;@</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>ren_0209</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>Ren$&quot;When is this thought going away? I can&apos;t stop thinking of him.&quot;@</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>ren_0210</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>Ren$&quot;...... it is so strange.&quot;@</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>Sekito$&quot;Ruff?&quot;@</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>ren_0211</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>Ren$&quot;Me... and My Lord... I don&apos;t understand any of it.&quot; @</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>ren_0212</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>Ren$&quot;I&apos;m strong. I don&apos;t obey anyone&apos;s orders...&quot; @</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>ren_0213</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>Ren$&quot;Everyone is scared of me. I don&apos;t mind that.&quot;@</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>ren_0214</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>Ren$&quot;...... but.&quot;@</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>ren_0215</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>Ren$&quot;He was laughing.&quot;@</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>ren_0216</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>Ren$&quot;He is always laughing. He&apos;s not afraid of me.&quot;@</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>ren_0217</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>Ren$&quot;...... maybe I&apos;m the one afraid of him.&quot;@</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>ren_0218</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>Ren$&quot;I&apos;ve never met anyone like him. He comes so close to me, he calls my name like a friend...&quot;@</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>ren_0219</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>Ren$&quot;...... it&apos;s strange.&quot;@</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>ren_0220</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>Ren$&quot;I don&apos;t want him in my heart... but he won&apos;t go away.&quot;@</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>ren_0221</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>Ren$&quot;My heart is hurting.&quot;@</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>ren_0222</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>Ren$&quot;...... this is so strange.&quot;@</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>ren_0223</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>Ren$&quot;...... this is really really......&quot;@</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>ren_0224</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>Ren$&quot;Such a strange...... way to be.&quot; @</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>ren_0225</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>Ren$&quot;Such a strange.......... feeling.&quot;@</ToTranslate>
