    <ToTranslate>SE010c</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>ef_kiriage01b_m</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>BSE08b</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>ef_kiriage03b</ToTranslate>
  </String>
  <String index="82">
