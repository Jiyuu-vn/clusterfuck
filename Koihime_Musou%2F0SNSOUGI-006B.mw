  </String>
  <String index="83">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>bg_white</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>bg_black</ToTranslate>
  </String>
