    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>sonk0099</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>Sonken$&quot;Charge!!&quot;@</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>SE124</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>Kazuto$&quot;That&apos;s the Son family flag! Sonken is here!&quot;@</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>aisy0896</ToTranslate>
  </String>
