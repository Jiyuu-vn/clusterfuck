  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>aisy0885</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Aisha$&quot;Kkh...... There are just too many of them......!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_cun21ha</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>sei_0235</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Sei$&quot;This is bad......! They are about to reach headquarters!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>sui_0176</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Sui$&quot;My Lord, this is getting too dangerous, fall back!&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>Kazuto$&quot;O-okay.&quot;@</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>$I didn&apos;t want to hinder their battle.@</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>$I followed Sui&apos;s instructions, and tried to step back...@</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>tcg_zak11aa</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_zak11m</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>Soldier$&quot;Evil shall perish!&quot;@</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>SE010</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>$Enemy soldiers surged in, brandishing their swords and trying to attack me.@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>aisy0886</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Aisha$&quot;My Lord?!&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>Kazuto$&quot;Ugh......!&quot;@</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>yure_r</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>$As I heard Aisha&apos;s scream, I quickly drew my sword and parried the enemy weapon that arced towards my neck.@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>SE024</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>sei_0236</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Sei$&quot;My Lord, are you okay?!&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>Kazuto$&quot;Y-yeah, somehow...!&quot;@</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>aisy0887</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Aisha$&quot;I&apos;m glad......!&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>$As if they were mocking Aisha&apos;s sigh of relief, more White Clothes stormed in and surrounded us so tightly, an ant couldn&apos;t crawl through.@</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>sui_0177</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>Sui$&quot;Dammit! They just keep crawling up like insects!&quot;@</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>tcg_cun21ha</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>sei_0237</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>Sei$&quot;There&apos;s no end to them, and the battle has only just begun.&quot;@</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>$I looked around, and amongst Sei and the others I saw...@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>tcg_kan11ua</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>aisy0888</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Aisha$&quot;......Pay.&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>$Aisha muttered, with a shaky voice.@</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>tcg_kan11wa</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>aisy0889</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>Aisha$&quot;Thou shalt pay!!&quot;@</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>tcg_bac22da</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>tcg_bac22m</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>sui_0178</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>Sui$&quot;Wah?! You scared me!&quot;@</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_kan11ua</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>aisy0890</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>Aisha$&quot;How preposterous of thou to point thine blades at Our Lord! Do not think that any one of thou shalt go home alive!&quot;@</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_kan91aa</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_kan91m</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>aisy0891</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>Aisha$&quot;Hoooooooooo!&quot;@</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>ef_kesa03a</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>ef_kesa01a_m</ToTranslate>
  </String>
