  </String>
  <String index="186">
    <ToTranslate>sonk0094</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>Sonken$&quot;Brave people of Son-Go! Now is the time to let the world know our power!&quot;@</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>sonk0095</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>Sonken$&quot;Show your bravery! Get rid of your weakness! Carry out the will of Son-Go!&quot;@</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>ecg_son01b</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>sonk0096</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>Sonken$&quot;If your friend falls, step over their corpse! And for every corpse you climb, stomp on an enemy&apos;s corpse!&quot;@</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>sonk0097</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>Sonken$&quot;Shoot the enemy! Shoot the ally! Annihilate all who oppose Son-Go&apos;s flag!&quot;@</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>ecg_son01d</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>sonk0098</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>Sonken$&quot;Ready your weapons!&quot;@</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>SE014</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>ecg_son01c</ToTranslate>
  </String>
