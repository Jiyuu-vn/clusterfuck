    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="625">
    <ToTranslate>syur0991</ToTranslate>
  </String>
  <String index="626">
    <ToTranslate>Shuri $&quot;But your kissing is......Mmm?! Daring.&quot;@</ToTranslate>
  </String>
  <String index="627">
    <ToTranslate>Kazuto $&quot;Is it too much?&quot;@</ToTranslate>
  </String>
