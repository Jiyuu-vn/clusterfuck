  <String index="578">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="579">
    <ToTranslate>syur0984</ToTranslate>
  </String>
  <String index="580">
    <ToTranslate>Shuri$&quot;No...... mwah... you love me, that&apos;s why we&apos;re doing this...&quot;@</ToTranslate>
  </String>
  <String index="581">
    <ToTranslate>$I held her tight and close, our lips touching and parting. I couldn&apos;t get enough of her supple lips.@</ToTranslate>
  </String>
  <String index="582">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="583">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="584">
    <ToTranslate>ecg_syo03e</ToTranslate>
  </String>
  <String index="585">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="586">
    <ToTranslate>syur0985</ToTranslate>
  </String>
  <String index="587">
    <ToTranslate>Shuri$&quot;I love you because you&apos;re so kind... I love the way you smile when you&apos;re perplexed... and your strong, courageous profile.&quot;@</ToTranslate>
  </String>
  <String index="588">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="589">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="590">
    <ToTranslate>ecg_syo03f</ToTranslate>
  </String>
  <String index="591">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="592">
    <ToTranslate>syur0986</ToTranslate>
  </String>
  <String index="593">
    <ToTranslate>Shuri$&quot;I love everything... I want you to look at me all the time.&quot;@</ToTranslate>
  </String>
  <String index="594">
    <ToTranslate>Kazuto$&quot;I don&apos;t know if I can do that all the time, but... I&apos;ll try to as much as I can.&quot;@</ToTranslate>
  </String>
  <String index="595">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="596">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="597">
    <ToTranslate>ecg_syo03d</ToTranslate>
