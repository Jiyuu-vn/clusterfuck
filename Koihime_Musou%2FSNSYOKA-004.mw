  <String index="598">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="599">
    <ToTranslate>syur0987</ToTranslate>
  </String>
  <String index="600">
    <ToTranslate>Shuri$&quot;How much is that......?&quot;@</ToTranslate>
  </String>
  <String index="601">
    <ToTranslate>Kazuto$&quot;I don&apos;t know. I don&apos;t know the future. Our time has just begun.&quot;@</ToTranslate>
  </String>
  <String index="602">
    <ToTranslate>$Everything starts with this kiss. Shuri suddenly gasped.@</ToTranslate>
  </String>
  <String index="603">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="604">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="605">
    <ToTranslate>ecg_syo03a</ToTranslate>
  </String>
  <String index="606">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="607">
    <ToTranslate>syur0988</ToTranslate>
  </String>
  <String index="608">
    <ToTranslate>Shuri$&quot;...Hawawa... I feel like I&apos;m collapsing... I suddenly feel dizzy...&quot;@</ToTranslate>
  </String>
  <String index="609">
    <ToTranslate>Kazuto$&quot;You&apos;ve been dreaming about kissing me?&quot;@</ToTranslate>
  </String>
  <String index="610">
    <ToTranslate>syur0989</ToTranslate>
  </String>
  <String index="611">
    <ToTranslate>Shuri$&quot;Kiss......?&quot;@</ToTranslate>
  </String>
  <String index="612">
    <ToTranslate>Kazuto$&quot;Putting our lips together......&quot;@</ToTranslate>
  </String>
