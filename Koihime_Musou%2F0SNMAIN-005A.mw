  <String index="1102">
    <ToTranslate>syuy0007</ToTranslate>
  </String>
  <String index="1103">
    <ToTranslate>Shuuyu$&quot;......You&apos;re not going, Renfa-sama?&quot;@</ToTranslate>
  </String>
  <String index="1104">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="1105">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="1106">
    <ToTranslate>sonk0019</ToTranslate>
  </String>
  <String index="1107">
    <ToTranslate>Sonken$&quot;Do you need my command to sweep up ordinary soldiers?&quot;@</ToTranslate>
  </String>
  <String index="1108">
    <ToTranslate>tcg_syu11ba</ToTranslate>
  </String>
  <String index="1109">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="1110">
    <ToTranslate>syuy0008</ToTranslate>
  </String>
  <String index="1111">
    <ToTranslate>Syuuyu$&quot;No... Then I&apos;ll take command for the remainder.&quot;@</ToTranslate>
  </String>
  <String index="1112">
    <ToTranslate>tcg_son12aa</ToTranslate>
  </String>
  <String index="1113">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="1114">
    <ToTranslate>sonk0020</ToTranslate>
  </String>
  <String index="1115">
    <ToTranslate>Sonken$&quot;Good. Hakugen, I&apos;m leaving. Accompany me.&quot;@</ToTranslate>
  </String>
  <String index="1116">
    <ToTranslate>tcg_rik11ba</ToTranslate>
  </String>
  <String index="1117">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
