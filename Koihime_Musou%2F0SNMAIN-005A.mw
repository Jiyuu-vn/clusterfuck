  </String>
  <String index="827">
    <ToTranslate>tcg_kay31m</ToTranslate>
  </String>
  <String index="828">
    <ToTranslate>kayu0017</ToTranslate>
  </String>
  <String index="829">
    <ToTranslate>Kayuu$&quot;Kan&apos;u! Your blood will be rust on my battle axe!&quot;@</ToTranslate>
  </String>
  <String index="830">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="831">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="832">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="833">
    <ToTranslate>aisy0228</ToTranslate>
  </String>
  <String index="834">
