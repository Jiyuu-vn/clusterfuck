  <String index="788">
    <ToTranslate>tcg_zto11m</ToTranslate>
  </String>
  <String index="789">
    <ToTranslate>Soildier$&quot;General! We&apos;ve spotted several soldiers on horses! Their flag carries a cross and the character &apos;Kan&apos;!&quot;@</ToTranslate>
  </String>
  <String index="790">
    <ToTranslate>tcg_kay11aa</ToTranslate>
  </String>
  <String index="791">
    <ToTranslate>tcg_kay11m</ToTranslate>
  </String>
  <String index="792">
    <ToTranslate>kayu0015</ToTranslate>
  </String>
  <String index="793">
    <ToTranslate>Kayuu$&quot;So Hongou&apos;s rear guard is Kan&apos;u... Then it&apos;s time to pay back the debt we made!&quot;@</ToTranslate>
  </String>
  <String index="794">
    <ToTranslate>tcg_kay21ca</ToTranslate>
  </String>
  <String index="795">
    <ToTranslate>tcg_kay21m</ToTranslate>
  </String>
  <String index="796">
    <ToTranslate>kayu0016</ToTranslate>
  </String>
  <String index="797">
    <ToTranslate>Kayuu$&quot;All units, speed up! Trample them under hoof like the small, impudent force they are!&quot;@</ToTranslate>
  </String>
  <String index="798">
    <ToTranslate>SE124</ToTranslate>
  </String>
