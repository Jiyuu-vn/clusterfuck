  <String index="1125">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="1126">
    <ToTranslate>sonk0021</ToTranslate>
  </String>
  <String index="1127">
    <ToTranslate>Sonken$&quot;...&quot;@</ToTranslate>
  </String>
  <String index="1128">
    <ToTranslate>kyara_kesi</ToTranslate>
