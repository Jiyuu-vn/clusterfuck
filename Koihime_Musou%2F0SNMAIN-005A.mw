  <String index="1129">
    <ToTranslate>tcg_syu11ba</ToTranslate>
  </String>
  <String index="1130">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="1131">
    <ToTranslate>syuy0010</ToTranslate>
  </String>
  <String index="1132">
    <ToTranslate>Shuuyu$&quot;Hmph. ......She has dignity, but not as much ambition as Sousaku.&quot;@</ToTranslate>
