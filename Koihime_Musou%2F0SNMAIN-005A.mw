    <ToTranslate>riku0003</ToTranslate>
  </String>
  <String index="1119">
    <ToTranslate>Rikuson$&quot;Sure thing!&quot;@</ToTranslate>
  </String>
  <String index="1120">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="1121">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="1122">
    <ToTranslate>syuy0009</ToTranslate>
  </String>
