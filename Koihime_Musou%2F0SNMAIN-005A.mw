  <String index="807">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="808">
    <ToTranslate>Soldier$&quot;General Kan&apos;u! The enemy has sped up!&quot;@</ToTranslate>
  </String>
  <String index="809">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="810">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="811">
    <ToTranslate>aisy0227</ToTranslate>
  </String>
  <String index="812">
    <ToTranslate>Aisha$&quot;Fine. We&apos;ll speed up too.&quot;@</ToTranslate>
  </String>
  <String index="813">
    <ToTranslate>Soldier$&quot;Yes, sir!&quot;@</ToTranslate>
  </String>
  <String index="814">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
