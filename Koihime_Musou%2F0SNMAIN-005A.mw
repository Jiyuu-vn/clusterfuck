[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>tcg_kay11da</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>tcg_kay11m</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>kayu0009</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>Kayuu$&quot;Shit! We couldn&apos;t overcome them. ......All units, about face! Return to Sishui Pass!&quot;@</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>tcg_kan11fa</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>aisy0196</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>Aisha$&quot;I won&apos;t let you escape!&quot;@</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>tcg_kay91aa</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_kay91m</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>kayu0010</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>Kayuu$&quot;Hngh!&quot;@</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>battle_kiriage</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>m_yure_r</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_kan61aa</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_kan61m</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>aisy0197</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>Aisha$&quot;Ngh!&quot;@</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>tcg_kay11fa</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_kay11m</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>kayu0011</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>Kayuu$&quot;Hmm? The Green Dragon Blade... So you&apos;re the Kan&apos;u they speak of, huh?&quot;@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>aisy0198</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Aisha$&quot;So you know my name? That makes things simple. General Kayuu, face me fair and square!&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_kay11ba</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>tcg_kay11m</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>kayu0012</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Kayuu$&quot;Hmph! You may be making a name for yourself, but there&apos;s no point in defeating the general of a small army. ......Make a swift retreat!&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>aisy0199</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>Aisha$&quot;Hold it!&quot;@</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>tcg_kay31aa</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_kay31m</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>kayu0013</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>Kayuu$&quot;Be gone!&quot;@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>battle_kay</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>m_yure_r</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>tcg_kan61aa</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_kan61m</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>aisy0200</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>Aisha$&quot;Ngh!&quot;@</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_kay31aa</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>tcg_kay31m</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>kayu0014</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>Kayuu$&quot;Hmph. If fortune wills it, we shall meet again.&quot;@</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>tcg_kan11fa</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>aisy0201</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>Aisha$&quot;Ngh...She got away.&quot;@</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>SE024</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>rinr0125</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>Rinrin$&quot;Aneja!&quot;@</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>Kazuto$&quot;Aisha! Are you okay?&quot;@</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>bgm03</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>aisy0202</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>Aisha$&quot;My Lord, Rinrin...it&apos;s unfortunate. I failed to capture Kayuu.&quot;@</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>Kazuto$&quot;Yeah.  It&apos;s okay.  As long as you&apos;re safe, that&apos;s okay.&quot;@</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_kan21na</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>aisy0203</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>Aisha$&quot;My Lord...&quot;@</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>rinr0126</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>Rinrin$&quot;Hey, Aisha, was Kayuu strong?&quot;@</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>aisy0204</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>Aisha$&quot;Yes. She predicted how the Anti-Toutaku Alliance would act and laid an ambush in our rear at the base of the mountains. She&apos;s a good general.&quot;@</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>aisy0205</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>Aisha$&quot;And judging from her attack...She is likely one of great prowess herself.&quot;@</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>tcg_cho21ia</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>rinr0127</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>Rinrin$&quot;I see... Ahh, Rinrin&apos;s getting excited!&quot;@</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>Kazuto$&quot;Excited? This isn&apos;t a game, Rinrin.&quot;@</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>$As I was rebuking Rinrin...@</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>SE024</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>tcg_syo21da</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>syur0093</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>Shuri$&quot;My Lord!!&quot;@</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>$Shuri ran towards me in a hurry.@</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>syur0094</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>Shuri$&quot;General Kayuu&apos;s detached force has broken through our blockade and entered Sishui Pass!&quot;@</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>tcg_kan31ka</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>aisy0206</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>Aisha$&quot;Hahh... what are the allied armies doing? How could they let them rejoin the main force?&quot;@</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_cho31ga</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>rinr0128</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>Rinrin$&quot;That&apos;s because Enshou is the supreme commander...&quot;@</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>Kazuto$&quot;Hey, be nice. Anyway, I guess it&apos;ll come down to a battle to break through the fortress&apos;s gates...&quot;@</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>aisy0207</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>Aisha$&quot;Yes. However the enemy is in an impregnable fortress with high spirits and their general is superb. ......It will take time to break through.&quot;@</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>rinr0129</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>Rinrin$&quot;If you let Rinrin, I&apos;d break through with a kablam and a kaboom!&quot;@</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>Kazuto$&quot;Well, let&apos;s observe for a little bit. If the front lines come to a stalemate, we may need both of your abilities.&quot;@</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>tcg_syo41aa</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>syur0095</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>Shuri$&quot;That&apos;s right. We&apos;ll let the soldiers rest for a while.&quot;@</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>Kazuto$&quot;Yeah, please do that.&quot;@</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>$I nodded to Shuri. We decided to watch over the battle and reorganize our formation.@</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>bgm11</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>tcg_ens22ia</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>tcg_ens22m</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>ensy0043</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>Enshou$&quot;Hey, Ganryou-san! Haven&apos;t we broken through the gate yet?&quot;@</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>tcg_gan22ga</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>ganr0000</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>Ganryou$&quot;Gi&apos;s army is leading an offensive as we speak, so we should break through very soon...&quot;@</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>tcg_bun22fa</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>buns0000</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>Bunshuu$&quot;Princess, calm down a little.&quot;@</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>tcg_ens12fa</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>tcg_ens12m</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>ensy0044</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>Enshou$&quot;I can&apos;t be as carefree as you, Bunshuu-san! How many hours do you think have passed since our Anti-Toutaku Alliance started the attack?!&quot;@</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>buns0001</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>Bunshuu$&quot;It has been only three hours or so, right? If that&apos;s all it took, then it wouldn&apos;t be much of a fortress.&quot;@</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>tcg_gan12aa</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>ganr0001</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>Ganryou$&quot;Yes, yes, she&apos;s right.&quot;@</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>tcg_ens22ia</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>tcg_ens22m</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>ensy0045</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>Enshou$&quot;Ahhh!! You are figureheads of the prestigious En family! How can you be so calm?&quot;@</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>ensy0046</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>Enshou$&quot;Can&apos;t you say something more valiant, like, &apos;I&apos;ll go break through that gate for you!&apos;?&quot;@</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>tcg_bun22ha</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>buns0002</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>Bunshuu$&quot;If we were the vanguard, then sure, I would&apos;ve said that, but we&apos;re the rear guard, right? So what&apos;s the rush?&quot;@</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>tcg_gan22ba</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>ganr0002</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>Ganryou$&quot;She&apos;s right.  Let&apos;s just wait, Princess.&quot;@</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>tcg_ens12ja</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>tcg_ens12m</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>ensy0047</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>Enshou$&quot;Nngh... if you both say so, I&apos;ll wait. But I can only wait for another hour!&quot;@</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>tcg_gan22ea</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>ganr0003</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>Ganryou$&quot;Ahh... Princess... you&apos;re being unreasonable...&quot;@</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>buns0003</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>Bunshuu$&quot;Hmm, then how about this? Why don&apos;t we put Hongou&apos;s army in front? It&apos;s better to have more soldiers in the assault.&quot;@</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>tcg_gan22da</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>ganr0004</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>Ganryou$&quot;Wait...Bun-chan, that&apos;s dangerous. The battle is already hectic. If we send another army into the fray, the battle will just become more chaotic...&quot;@</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>tcg_bun12ia</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>buns0004</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>Bunshuu$&quot;It&apos;ll be okay. Neither Sousou&apos;s army nor Sonken&apos;s army are the type who would get confused because of something like that.&quot;@</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>tcg_gan22ca</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>ganr0005</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>Ganryou$&quot;You may be right, but...&quot;@</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>tcg_bun22ba</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>buns0005</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>Bunshuu$&quot;Even if it gets confusing, it&apos;ll all work out somehow! A battle is like a gamble! Somehow it&apos;ll work out.&quot;@</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>tcg_gan12ha</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>ganr0006</ToTranslate>
  </String>
  <String index="219">
    <ToTranslate>Ganryou$&quot;Ohh...You&apos;re as haphazard as ever.&quot;@</ToTranslate>
  </String>
  <String index="220">
    <ToTranslate>tcg_bun12da</ToTranslate>
  </String>
  <String index="221">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="222">
    <ToTranslate>buns0006</ToTranslate>
  </String>
  <String index="223">
    <ToTranslate>Bunshuu$&quot;I wish you&apos;d say I&apos;m leaving our fate up to Heaven, rather than being haphazard. ......Princess, what do you think of my plan?&quot;@</ToTranslate>
  </String>
  <String index="224">
    <ToTranslate>tcg_ens22ba</ToTranslate>
  </String>
  <String index="225">
    <ToTranslate>tcg_ens22m</ToTranslate>
  </String>
  <String index="226">
    <ToTranslate>ensy0048</ToTranslate>
  </String>
  <String index="227">
    <ToTranslate>Enshou$&quot;Well, it&apos;s a good idea coming from you, Bunshuu. Perhaps I should slightly revise my opinion of you.&quot;@</ToTranslate>
  </String>
  <String index="228">
    <ToTranslate>tcg_bun22fa</ToTranslate>
  </String>
  <String index="229">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="230">
    <ToTranslate>buns0007</ToTranslate>
  </String>
  <String index="231">
    <ToTranslate>Bunshuu$&quot;Yay, I&apos;m happy. So, since the supreme commander of the allied armies agreed, that&apos;s the plan. Toshi, you&apos;ll be a messenger.&quot;@</ToTranslate>
  </String>
  <String index="232">
    <ToTranslate>tcg_gan22ea</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>ganr0007</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>Ganryou$&quot;Uwah...Are you really okay with this?&quot;@</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>tcg_bun22ha</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>buns0008</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>Bunshuu$&quot;Of course. You worry too much, Toshi.&quot;@</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>tcg_gan12ca</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>ganr0008</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>Ganryou$&quot;You and the Princess are too laid back...&quot;@</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>buns0009</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>Bunshuu$&quot;Hmm? Did you say something?&quot;@</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>tcg_gan22la</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>ganr0009</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>Ganryou$&quot;No, I didn&apos;t. Anyway, I&apos;ll go over to Hongou&apos;s army. Bun-chan, you guard the Princess.&quot;@</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>tcg_bun22ba</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>buns0010</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>Bunshuu$&quot;Okay.&quot;@</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>tcg_ens12fa</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>tcg_ens12m</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>ensy0049</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>Enshou$&quot;Hey, Ganryou-san! Why are you dilly dallying? Hurry up and give Hongou&apos;s army their orders!&quot;@</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>tcg_gan12ca</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>ganr0010</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>Ganryou$&quot;Okay, okay. I&apos;m going.&quot;@</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>bgm05</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>tcg_gan12aa</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>ganr0011</ToTranslate>
  </String>
  <String index="275">
    <ToTranslate>Ganryou$&quot;Excuse me.&quot;@</ToTranslate>
  </String>
  <String index="276">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="277">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="278">
    <ToTranslate>syur0096</ToTranslate>
  </String>
  <String index="279">
    <ToTranslate>Shuri$&quot;Yes?&quot;@</ToTranslate>
  </String>
  <String index="280">
    <ToTranslate>tcg_gan22ba</ToTranslate>
  </String>
  <String index="281">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="282">
    <ToTranslate>ganr0012</ToTranslate>
  </String>
  <String index="283">
    <ToTranslate>Ganryou$&quot;Are you from Hongou&apos;s army? I&apos;m here to give you a message. Is there someone in charge here?&quot;@</ToTranslate>
  </String>
  <String index="284">
    <ToTranslate>tcg_syo11ba</ToTranslate>
  </String>
  <String index="285">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="286">
    <ToTranslate>syur0097</ToTranslate>
  </String>
  <String index="287">
    <ToTranslate>Shuri$&quot;Ah, yes. If it&apos;s an order, I&apos;ll take it.&quot;@</ToTranslate>
  </String>
  <String index="288">
    <ToTranslate>tcg_gan22ia</ToTranslate>
  </String>
  <String index="289">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="290">
    <ToTranslate>ganr0013</ToTranslate>
  </String>
  <String index="291">
    <ToTranslate>Ganryou$&quot;Um... You?&quot;@</ToTranslate>
  </String>
  <String index="292">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="293">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="294">
    <ToTranslate>syur0098</ToTranslate>
  </String>
  <String index="295">
    <ToTranslate>Shuri$&quot;Yes!  I&apos;m Shokatsuryou.  I&apos;m the Hongou army&apos;s strategist.  Nice to meet you!&quot;@</ToTranslate>
  </String>
  <String index="296">
    <ToTranslate>$Bow.@</ToTranslate>
  </String>
  <String index="297">
    <ToTranslate>tcg_gan12aa</ToTranslate>
  </String>
  <String index="298">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="299">
    <ToTranslate>ganr0014</ToTranslate>
  </String>
  <String index="300">
    <ToTranslate>Ganryou$&quot;Ha, ha, ha, nice to meet you too. Okay Shokatsuryou-chan, can you take a message to Hongou-san?&quot;@</ToTranslate>
  </String>
  <String index="301">
    <ToTranslate>Ganryou$&quot;Please tell him your army is to advance and assist the Gi and Go in breaking through the gates.&quot;@</ToTranslate>
  </String>
  <String index="302">
    <ToTranslate>tcg_syo21da</ToTranslate>
  </String>
  <String index="303">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="304">
    <ToTranslate>syur0099</ToTranslate>
  </String>
  <String index="305">
    <ToTranslate>Shuri$&quot;Right now, in the middle of this hectic battle? I think that&apos;s dangerous...&quot;@</ToTranslate>
  </String>
  <String index="306">
    <ToTranslate>tcg_gan22ca</ToTranslate>
  </String>
  <String index="307">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="308">
    <ToTranslate>ganr0015</ToTranslate>
  </String>
  <String index="309">
    <ToTranslate>Ganryou$&quot;Well, I think so too, but the Princess... I mean Enshou-sama says so.&quot;@</ToTranslate>
  </String>
  <String index="310">
    <ToTranslate>tcg_syo41ga</ToTranslate>
  </String>
  <String index="311">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="312">
    <ToTranslate>tenten</ToTranslate>
  </String>
  <String index="313">
    <ToTranslate>syur0100</ToTranslate>
  </String>
  <String index="314">
    <ToTranslate>Shuri$&quot;This is an order, isn&apos;t it?&quot;@</ToTranslate>
  </String>
  <String index="315">
    <ToTranslate>tcg_gan12ea</ToTranslate>
  </String>
  <String index="316">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="317">
    <ToTranslate>ganr0016</ToTranslate>
  </String>
  <String index="318">
    <ToTranslate>Ganryou$&quot;Yes, it&apos;s an order. Sorry...&quot;@</ToTranslate>
  </String>
  <String index="319">
    <ToTranslate>tcg_syo41ea</ToTranslate>
  </String>
  <String index="320">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="321">
    <ToTranslate>syur0101</ToTranslate>
  </String>
  <String index="322">
    <ToTranslate>Shuri$&quot;I understand. We&apos;ll do our best. Uhh...&quot;@</ToTranslate>
  </String>
  <String index="323">
    <ToTranslate>tcg_gan12ba</ToTranslate>
  </String>
  <String index="324">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="325">
    <ToTranslate>ganr0017</ToTranslate>
  </String>
  <String index="326">
    <ToTranslate>Ganryou$&quot;Oh, my name is Ganryou. Nice to meet you.&quot;@</ToTranslate>
  </String>
  <String index="327">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="328">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="329">
    <ToTranslate>syur0102</ToTranslate>
  </String>
  <String index="330">
    <ToTranslate>Shuri$&quot;Yes! Nice to meet you too! Ganryou-san, please tell Enshou-san that Hongou&apos;s army will move forward as ordered.&quot;@</ToTranslate>
  </String>
  <String index="331">
    <ToTranslate>tcg_gan12ga</ToTranslate>
  </String>
  <String index="332">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="333">
    <ToTranslate>ganr0018</ToTranslate>
  </String>
  <String index="334">
    <ToTranslate>Ganryou$&quot;Roger. Good luck!&quot;@</ToTranslate>
  </String>
  <String index="335">
    <ToTranslate>tcg_syo11ba</ToTranslate>
  </String>
  <String index="336">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="337">
    <ToTranslate>syur0103</ToTranslate>
  </String>
  <String index="338">
    <ToTranslate>Shuri$&quot;Thank you!&quot;@</ToTranslate>
  </String>
  <String index="339">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="340">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="341">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="342">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="343">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="344">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="345">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="346">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="347">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="348">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="349">
    <ToTranslate>syur0104</ToTranslate>
  </String>
  <String index="350">
    <ToTranslate>Shuri$&quot;My Lord!&quot;@</ToTranslate>
  </String>
  <String index="351">
    <ToTranslate>Kazuto$&quot;Yes?  What is it?&quot;@</ToTranslate>
  </String>
  <String index="352">
    <ToTranslate>tcg_syo11ga</ToTranslate>
  </String>
  <String index="353">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="354">
    <ToTranslate>syur0105</ToTranslate>
  </String>
  <String index="355">
    <ToTranslate>Shuri$&quot;We&apos;ve received a message from the main army. They said, &apos;The Hongou Army is to advance and aid Gi and Go in breaking through the gates.&apos;&quot;@</ToTranslate>
  </String>
  <String index="356">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="357">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="358">
    <ToTranslate>aisy0208</ToTranslate>
  </String>
  <String index="359">
    <ToTranslate>Aisha$&quot;Wait... Advance into that hectic battle? If we do that, things will become even more chaotic! What is Enshou thinking?!&quot;@</ToTranslate>
  </String>
  <String index="360">
    <ToTranslate>tcg_cho31ia</ToTranslate>
  </String>
  <String index="361">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="362">
    <ToTranslate>rinr0130</ToTranslate>
  </String>
  <String index="363">
    <ToTranslate>Rinrin$&quot;She isn&apos;t thinking anything, and pass two meat buns!&quot;@</ToTranslate>
  </String>
  <String index="364">
    <ToTranslate>tcg_syo41ga</ToTranslate>
  </String>
  <String index="365">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="366">
    <ToTranslate>syur0106</ToTranslate>
  </String>
  <String index="367">
    <ToTranslate>Shuri$&quot;Rinrin-chan is right. But this is an order from the supreme commander...&quot;@</ToTranslate>
  </String>
  <String index="368">
    <ToTranslate>Kazuto$&quot;It&apos;s not like we can disobey.&quot;@</ToTranslate>
  </String>
  <String index="369">
    <ToTranslate>tcg_syo41ea</ToTranslate>
  </String>
  <String index="370">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="371">
    <ToTranslate>syur0107</ToTranslate>
  </String>
  <String index="372">
    <ToTranslate>Shuri$&quot;Yes. Anyway, to avoid the confusion our advance is likely to cause, let&apos;s avoid the areas where they&apos;re already engaged.&quot;@</ToTranslate>
  </String>
  <String index="373">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="374">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="375">
    <ToTranslate>aisy0209</ToTranslate>
  </String>
  <String index="376">
    <ToTranslate>Aisha$&quot;What about aiding them?&quot;@</ToTranslate>
  </String>
  <String index="377">
    <ToTranslate>tcg_syo41aa</ToTranslate>
  </String>
  <String index="378">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="379">
    <ToTranslate>syur0108</ToTranslate>
  </String>
  <String index="380">
    <ToTranslate>Shuri$&quot;I don&apos;t think Sousou-san and Sonken-san&apos;s armies need our aid. We&apos;ll obey the command, but at the same time, we&apos;ll try to keep our losses to a minimum.&quot;@</ToTranslate>
  </String>
  <String index="381">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="382">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="383">
    <ToTranslate>rinr0131</ToTranslate>
  </String>
  <String index="384">
    <ToTranslate>Rinrin$&quot;We don&apos;t want everyone to get injured in some idiot&apos;s battle.&quot;@</ToTranslate>
  </String>
  <String index="385">
    <ToTranslate>tcg_syo11ba</ToTranslate>
  </String>
  <String index="386">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="387">
    <ToTranslate>syur0109</ToTranslate>
  </String>
  <String index="388">
    <ToTranslate>Shuri$&quot;That&apos;s right. Then, once Sousou-san and Sonken-san break through the gates, let&apos;s go in with them.&quot;@</ToTranslate>
  </String>
  <String index="389">
    <ToTranslate>tcg_kan31ka</ToTranslate>
  </String>
  <String index="390">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="391">
    <ToTranslate>aisy0210</ToTranslate>
  </String>
  <String index="392">
    <ToTranslate>Aisha$&quot;Hmm... I don&apos;t like that plan...&quot;@</ToTranslate>
  </String>
  <String index="393">
    <ToTranslate>Kazuto$&quot;Because it&apos;s cowardly?&quot;@</ToTranslate>
  </String>
  <String index="394">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="395">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="396">
    <ToTranslate>aisy0211</ToTranslate>
  </String>
  <String index="397">
    <ToTranslate>Aisha$&quot;Cowardly... no, but it&apos;s like we are trying to avoid battle...&quot;@</ToTranslate>
  </String>
  <String index="398">
    <ToTranslate>Kazuto$&quot;That&apos;s because we are trying to avoid battle. ......However, it&apos;s just like Rinrin said.  If we follow this thoughtless strategy, then we&apos;ll just be putting our troops in danger.&quot;@</ToTranslate>
  </String>
  <String index="399">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="400">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="401">
    <ToTranslate>aisy0212</ToTranslate>
  </String>
  <String index="402">
    <ToTranslate>Aisha$&quot;......I guess you&apos;re right. I suppose this is a matter of strategy as well?&quot;@</ToTranslate>
  </String>
  <String index="403">
    <ToTranslate>Kazuto$&quot;There you go. Anyway, let&apos;s go with Shuri&apos;s plan and advance.&quot;@</ToTranslate>
  </String>
  <String index="404">
    <ToTranslate>Kazuto$&quot;Aisha, Rinrin, Shuri. Give our troops the command.&quot;@</ToTranslate>
  </String>
  <String index="405">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="406">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="407">
    <ToTranslate>aisy0213</ToTranslate>
  </String>
  <String index="408">
    <ToTranslate>Aisha$&quot;Yes, sir!&quot;@</ToTranslate>
  </String>
  <String index="409">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="410">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="411">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="412">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="413">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="414">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="415">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="416">
    <ToTranslate>bgm09</ToTranslate>
  </String>
  <String index="417">
    <ToTranslate>tcg_sou12ha</ToTranslate>
  </String>
  <String index="418">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="419">
    <ToTranslate>sous0032</ToTranslate>
  </String>
  <String index="420">
    <ToTranslate>Sousou$&quot;Can&apos;t you break through yet?&quot;@</ToTranslate>
  </String>
  <String index="421">
    <ToTranslate>tcg_ton12ga</ToTranslate>
  </String>
  <String index="422">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="423">
    <ToTranslate>kato0010</ToTranslate>
  </String>
  <String index="424">
    <ToTranslate>Kakouton$&quot;No. Shuuran&apos;s unit is engaged at the gates, but the enemy resistance is fierce and they can&apos;t attack very well.&quot;@</ToTranslate>
  </String>
  <String index="425">
    <ToTranslate>tcg_sou12ca</ToTranslate>
  </String>
  <String index="426">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="427">
    <ToTranslate>sous0033</ToTranslate>
  </String>
  <String index="428">
    <ToTranslate>Sousou$&quot;I see... What about Sonken&apos;s unit?&quot;@</ToTranslate>
  </String>
  <String index="429">
    <ToTranslate>tcg_ton12aa</ToTranslate>
  </String>
  <String index="430">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="431">
    <ToTranslate>kato0011</ToTranslate>
  </String>
  <String index="432">
    <ToTranslate>Kakouton$&quot;They seem to be having trouble as well.&quot;@</ToTranslate>
  </String>
  <String index="433">
    <ToTranslate>tcg_sou22ga</ToTranslate>
  </String>
  <String index="434">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="435">
    <ToTranslate>sous0034</ToTranslate>
  </String>
  <String index="436">
    <ToTranslate>Sousou$&quot;Even Son Chuubou is having trouble, huh? Damn. We&apos;re wasting soldiers thanks to that stupid Enshou.&quot;@</ToTranslate>
  </String>
  <String index="437">
    <ToTranslate>tcg_ton12ca</ToTranslate>
  </String>
  <String index="438">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="439">
    <ToTranslate>kato0012</ToTranslate>
  </String>
  <String index="440">
    <ToTranslate>Kakouton$&quot;You&apos;re absolutely right. Charging the citadel straight on......There&apos;s a limit to how reckless one can be.&quot;@</ToTranslate>
  </String>
  <String index="441">
    <ToTranslate>tcg_sou12ca</ToTranslate>
  </String>
  <String index="442">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="443">
    <ToTranslate>sous0035</ToTranslate>
  </String>
  <String index="444">
    <ToTranslate>Sousou$&quot;Yes. Now...Jun&apos;iku, do you have a plan?&quot;@</ToTranslate>
  </String>
  <String index="445">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="446">
    <ToTranslate>tcg_ton12ca</ToTranslate>
  </String>
  <String index="447">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="448">
    <ToTranslate>tcg_sou12ca</ToTranslate>
  </String>
  <String index="449">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="450">
    <ToTranslate>tcg_jun11aa</ToTranslate>
  </String>
  <String index="451">
    <ToTranslate>tcg_jun11m</ToTranslate>
  </String>
  <String index="452">
    <ToTranslate>jyun0000</ToTranslate>
  </String>
  <String index="453">
    <ToTranslate>Jun&apos;iku$&quot;There is no method to kill a turtle that&apos;s retreated into its shell. We must get it to stick out its head.&quot;@</ToTranslate>
  </String>
  <String index="454">
    <ToTranslate>tcg_jun21ia</ToTranslate>
  </String>
  <String index="455">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="456">
    <ToTranslate>jyun0001</ToTranslate>
  </String>
  <String index="457">
    <ToTranslate>Jun&apos;iku$&quot;However, to lure it&apos;s head out, we need bait. ......Luckily the Hongou army has begun to move. I believe we should use them.&quot;@</ToTranslate>
  </String>
  <String index="458">
    <ToTranslate>tcg_sou22ba</ToTranslate>
  </String>
  <String index="459">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="460">
    <ToTranslate>sous0036</ToTranslate>
  </String>
  <String index="461">
    <ToTranslate>Sousou$&quot;Use that ugly man as bait? ......Hu-hu, that sounds fun.&quot;@</ToTranslate>
  </String>
  <String index="462">
    <ToTranslate>tcg_jun11aa</ToTranslate>
  </String>
  <String index="463">
    <ToTranslate>tcg_jun11m</ToTranslate>
  </String>
  <String index="464">
    <ToTranslate>jyun0002</ToTranslate>
  </String>
  <String index="465">
    <ToTranslate>Jun&apos;iku$&quot;Yes. As Hongou&apos;s army advances, we shall split our army in two and make a swift retreat.&quot;@</ToTranslate>
  </String>
  <String index="466">
    <ToTranslate>tcg_ton12ga</ToTranslate>
  </String>
  <String index="467">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="468">
    <ToTranslate>kato0013</ToTranslate>
  </String>
  <String index="469">
    <ToTranslate>Kakouton$&quot;What?! You want us to hand over the vanguard?!&quot;@</ToTranslate>
  </String>
  <String index="470">
    <ToTranslate>tcg_jun21ha</ToTranslate>
  </String>
  <String index="471">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="472">
    <ToTranslate>jyun0003</ToTranslate>
  </String>
  <String index="473">
    <ToTranslate>Jun&apos;iku$&quot;Yes. Getting results is more important than intangible honor now. Once we&apos;ve let Hongou&apos;s army eliminate Kayuu, we&apos;ll seize the opportunity and push forward to break through the gate.&quot;@</ToTranslate>
  </String>
  <String index="474">
    <ToTranslate>tcg_ton12ca</ToTranslate>
  </String>
  <String index="475">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="476">
    <ToTranslate>kato0014</ToTranslate>
  </String>
  <String index="477">
    <ToTranslate>Kakouton$&quot;Why use such a cowardly tactic? Shuuran will break through the castle gate soon enough!&quot;@</ToTranslate>
  </String>
  <String index="478">
    <ToTranslate>tcg_jun21fa</ToTranslate>
  </String>
  <String index="479">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="480">
    <ToTranslate>jyun0004</ToTranslate>
  </String>
  <String index="481">
    <ToTranslate>Jun&apos;iku$&quot;Aren&apos;t we using this plan because we can&apos;t wait for that?&quot;@</ToTranslate>
  </String>
  <String index="482">
    <ToTranslate>tcg_sou22da</ToTranslate>
  </String>
  <String index="483">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="484">
    <ToTranslate>sous0037</ToTranslate>
  </String>
  <String index="485">
    <ToTranslate>Sousou$&quot;Shunran, obey Keifa for now.&quot;@</ToTranslate>
  </String>
  <String index="486">
    <ToTranslate>tcg_ton12ea</ToTranslate>
  </String>
  <String index="487">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="488">
    <ToTranslate>kato0015</ToTranslate>
  </String>
  <String index="489">
    <ToTranslate>Kakouton$&quot;But, Karin-sama!&quot;@</ToTranslate>
  </String>
  <String index="490">
    <ToTranslate>tcg_sou22ga</ToTranslate>
  </String>
  <String index="491">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="492">
    <ToTranslate>sous0038</ToTranslate>
  </String>
  <String index="493">
    <ToTranslate>Sousou$&quot;Shunran. ......do you mean to disobey me?&quot;@</ToTranslate>
  </String>
  <String index="494">
    <ToTranslate>tcg_ton12ja</ToTranslate>
  </String>
  <String index="495">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="496">
    <ToTranslate>tenten</ToTranslate>
  </String>
  <String index="497">
    <ToTranslate>kato0016</ToTranslate>
  </String>
  <String index="498">
    <ToTranslate>Kakouton$&quot;......No, I understand. I will follow Jun&apos;iku&apos;s command...&quot;@</ToTranslate>
  </String>
  <String index="499">
    <ToTranslate>tcg_sou12aa</ToTranslate>
  </String>
  <String index="500">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="501">
    <ToTranslate>sous0039</ToTranslate>
  </String>
  <String index="502">
    <ToTranslate>Sousou$&quot;Hu-hu, good girl. That&apos;s why I like you, Shunran.&quot;@</ToTranslate>
  </String>
  <String index="503">
    <ToTranslate>tcg_ton22fa</ToTranslate>
  </String>
  <String index="504">
    <ToTranslate>tcg_ton22m</ToTranslate>
  </String>
  <String index="505">
    <ToTranslate>kato0017</ToTranslate>
  </String>
  <String index="506">
    <ToTranslate>Kakouton$&quot;Karin-sama...&quot;@</ToTranslate>
  </String>
  <String index="507">
    <ToTranslate>tcg_jun11da</ToTranslate>
  </String>
  <String index="508">
    <ToTranslate>tcg_jun11m</ToTranslate>
  </String>
  <String index="509">
    <ToTranslate>jyun0005</ToTranslate>
  </String>
  <String index="510">
    <ToTranslate>Jun&apos;iku$&quot;Shunran! Before you become fascinated by Karin-sama, send a messenger to Shuuran!&quot;@</ToTranslate>
  </String>
  <String index="511">
    <ToTranslate>tcg_ton22ia</ToTranslate>
  </String>
  <String index="512">
    <ToTranslate>tcg_ton22m</ToTranslate>
  </String>
  <String index="513">
    <ToTranslate>kato0018</ToTranslate>
  </String>
  <String index="514">
    <ToTranslate>Kakouton$&quot;......Hmph. I know that.&quot;@</ToTranslate>
  </String>
  <String index="515">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="516">
    <ToTranslate>tcg_jun21ba</ToTranslate>
  </String>
  <String index="517">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="518">
    <ToTranslate>jyun0006</ToTranslate>
  </String>
  <String index="519">
    <ToTranslate>Jun&apos;iku$&quot;Geez. When it comes to Karin-sama, she completely forgets about her position as a general.&quot;@</ToTranslate>
  </String>
  <String index="520">
    <ToTranslate>tcg_sou22ea</ToTranslate>
  </String>
  <String index="521">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="522">
    <ToTranslate>sous0040</ToTranslate>
  </String>
  <String index="523">
    <ToTranslate>Sousou$&quot;Hu-hu, that&apos;s just shows how much she loves me.&quot;@</ToTranslate>
  </String>
  <String index="524">
    <ToTranslate>tcg_jun21ea</ToTranslate>
  </String>
  <String index="525">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="526">
    <ToTranslate>jyun0007</ToTranslate>
  </String>
  <String index="527">
    <ToTranslate>Jun&apos;iku$&quot;Karin-sama...Um, I-I also, uh...&quot;@</ToTranslate>
  </String>
  <String index="528">
    <ToTranslate>tcg_sou12aa</ToTranslate>
  </String>
  <String index="529">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="530">
    <ToTranslate>sous0041</ToTranslate>
  </String>
  <String index="531">
    <ToTranslate>Sousou$&quot;Do you love me too?&quot;@</ToTranslate>
  </String>
  <String index="532">
    <ToTranslate>tcg_jun11aa</ToTranslate>
  </String>
  <String index="533">
    <ToTranslate>tcg_jun11m</ToTranslate>
  </String>
  <String index="534">
    <ToTranslate>jyun0008</ToTranslate>
  </String>
  <String index="535">
    <ToTranslate>Jun&apos;iku$&quot;Yes!&quot;@</ToTranslate>
  </String>
  <String index="536">
    <ToTranslate>tcg_sou12la</ToTranslate>
  </String>
  <String index="537">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="538">
    <ToTranslate>sous0042</ToTranslate>
  </String>
  <String index="539">
    <ToTranslate>Sousou$&quot;Thank you, I love you too, Keika. ......I&apos;ll love you a lot today.&quot;@</ToTranslate>
  </String>
  <String index="540">
    <ToTranslate>tcg_jun11ca</ToTranslate>
  </String>
  <String index="541">
    <ToTranslate>tcg_jun11m</ToTranslate>
  </String>
  <String index="542">
    <ToTranslate>jyun0009</ToTranslate>
  </String>
  <String index="543">
    <ToTranslate>Jun&apos;iku$&quot;Ahh... Karin-sama...&quot;@</ToTranslate>
  </String>
  <String index="544">
    <ToTranslate>tcg_sou22ea</ToTranslate>
  </String>
  <String index="545">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="546">
    <ToTranslate>sous0043</ToTranslate>
  </String>
  <String index="547">
    <ToTranslate>Sousou$&quot;Hu-hu...how cute...&quot;@</ToTranslate>
  </String>
  <String index="548">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="549">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="550">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="551">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="552">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="553">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="554">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="555">
    <ToTranslate>bgm10</ToTranslate>
  </String>
  <String index="556">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="557">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="558">
    <ToTranslate>kann0000</ToTranslate>
  </String>
  <String index="559">
    <ToTranslate>Kannei$&quot;Sonken-sama! Sousou&apos;s troops are retreating!&quot;@</ToTranslate>
  </String>
  <String index="560">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="561">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="562">
    <ToTranslate>sonk0008</ToTranslate>
  </String>
  <String index="563">
    <ToTranslate>Sonken$&quot;What...? What&apos;s going on?&quot;@</ToTranslate>
  </String>
  <String index="564">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="565">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="566">
    <ToTranslate>syuy0000</ToTranslate>
  </String>
  <String index="567">
    <ToTranslate>Shuuyu$&quot;I see... How despicable of them.&quot;@</ToTranslate>
  </String>
  <String index="568">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="569">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="570">
    <ToTranslate>sonk0009</ToTranslate>
  </String>
  <String index="571">
    <ToTranslate>Sonken$&quot;Despicable?&quot;@</ToTranslate>
  </String>
  <String index="572">
    <ToTranslate>tcg_rik11aa</ToTranslate>
  </String>
  <String index="573">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="574">
    <ToTranslate>riku0000</ToTranslate>
  </String>
  <String index="575">
    <ToTranslate>Rikuson$&quot;That&apos;s right! Sonken-sama, please look over there.&quot;@</ToTranslate>
  </String>
  <String index="576">
    <ToTranslate>tcg_son22da</ToTranslate>
  </String>
  <String index="577">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="578">
    <ToTranslate>sonk0010</ToTranslate>
  </String>
  <String index="579">
    <ToTranslate>Sonken$&quot;Hmm? Oh, Hongou&apos;s army from the rear guard is moving forward, huh?&quot;@</ToTranslate>
  </String>
  <String index="580">
    <ToTranslate>tcg_rik11ba</ToTranslate>
  </String>
  <String index="581">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="582">
    <ToTranslate>riku0001</ToTranslate>
  </String>
  <String index="583">
    <ToTranslate>Rikuson$&quot;It appears that way. I bet Enshou-san told them to advance.&quot;@</ToTranslate>
  </String>
  <String index="584">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="585">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="586">
    <ToTranslate>sonk0011</ToTranslate>
  </String>
  <String index="587">
    <ToTranslate>Sonken$&quot;Probably. But, why is Sousou&apos;s army retreating?&quot;@</ToTranslate>
  </String>
  <String index="588">
    <ToTranslate>tcg_syu21fa</ToTranslate>
  </String>
  <String index="589">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="590">
    <ToTranslate>syuy0001</ToTranslate>
  </String>
  <String index="591">
    <ToTranslate>Shuuyu$&quot;I bet she&apos;s using Hongou&apos;s army as bait to lure Kayuu out.&quot;@</ToTranslate>
  </String>
  <String index="592">
    <ToTranslate>tcg_son12ia</ToTranslate>
  </String>
  <String index="593">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="594">
    <ToTranslate>sonk0012</ToTranslate>
  </String>
  <String index="595">
    <ToTranslate>Sonken$&quot;I see. Letting a rabbit play in front of a hibernating bear&apos;s den, huh?&quot;@</ToTranslate>
  </String>
  <String index="596">
    <ToTranslate>tcg_rik11ba</ToTranslate>
  </String>
  <String index="597">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="598">
    <ToTranslate>riku0002</ToTranslate>
  </String>
  <String index="599">
    <ToTranslate>Rikuson$&quot;That&apos;s right! Good job, Renfa-sama!&quot;@</ToTranslate>
  </String>
  <String index="600">
    <ToTranslate>tcg_syu11ba</ToTranslate>
  </String>
  <String index="601">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="602">
    <ToTranslate>syuy0002</ToTranslate>
  </String>
  <String index="603">
    <ToTranslate>Shuuyu$&quot;What will our army do?&quot;@</ToTranslate>
  </String>
  <String index="604">
    <ToTranslate>tcg_son12aa</ToTranslate>
  </String>
  <String index="605">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="606">
    <ToTranslate>sonk0013</ToTranslate>
  </String>
  <String index="607">
    <ToTranslate>Sonken$&quot;Kouha, tell the army to retreat.&quot;@</ToTranslate>
  </String>
  <String index="608">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="609">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="610">
    <ToTranslate>kann0001</ToTranslate>
  </String>
  <String index="611">
    <ToTranslate>Kannei$&quot;As you wish!&quot;@</ToTranslate>
  </String>
  <String index="612">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="613">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="614">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="615">
    <ToTranslate>sonk0014</ToTranslate>
  </String>
  <String index="616">
    <ToTranslate>Sonken$&quot;Is that okay with you, Koukin?&quot;@</ToTranslate>
  </String>
  <String index="617">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="618">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="619">
    <ToTranslate>syuy0003</ToTranslate>
  </String>
  <String index="620">
    <ToTranslate>Shuuyu$&quot;Hmph......You pass this time.&quot;@</ToTranslate>
  </String>
  <String index="621">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="622">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="623">
    <ToTranslate>sonk0015</ToTranslate>
  </String>
  <String index="624">
    <ToTranslate>Sonken$&quot;Then I&apos;ll let you take care of the rest. Show me what you can do.&quot;@</ToTranslate>
  </String>
  <String index="625">
