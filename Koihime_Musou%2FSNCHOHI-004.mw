    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>rinr0754</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>Rinrin$&quot;Come on, charge!&quot;@</ToTranslate>
  </String>
