  <String index="232">
    <ToTranslate>bcg_heroroom_a</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>bgm02</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>tcg_kan21na</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>aisy2205</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>Aisha$&quot;Heh, heh...... I need to get back to my work now. You must get back to your job too, My Lord.&quot;@</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>&quot;Kazuto$&quot;Yeah...Good luck with your job, Aisha......&quot;@</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>tcg_kan21oa</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>aisy2206</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>Aisha$&quot;Yes. See you later!&quot;@</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>tcg_kan21ia</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>aisy2207</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>Aisha$&quot;Hum hum hum......&quot;@</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>Kazuto$&quot;She&apos;s humming so joyfully......&quot;@</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>$Once she left my room...@</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>Kazuto$&quot;Hahhh...... ugh...... I think I&apos;m going to puke......&quot;Ѓ@@</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>$I had to fight my nausea and the urge to vomit.@</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>Kazuto$&quot;Bahhh......Aisha&apos;s cooking is scarier than Sousou or Sonken. I can&apos;t fight that.&quot;@</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>$I really did appreciate her feelings, though.@</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>$......First I needed water. Then I needed to fish for something to get rid of this taste in my mouth.@</ToTranslate>
  </String>
