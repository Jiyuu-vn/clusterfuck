    <ToTranslate>tcg_cun31ia</ToTranslate>
  </String>
  <String index="800">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="801">
    <ToTranslate>sei_0121</ToTranslate>
  </String>
  <String index="802">
    <ToTranslate>Sei$&quot;Hmph. If you want to make me stop, then come get me!&quot;@</ToTranslate>
