  <String index="725">
    <ToTranslate>Kakouton$&quot;All units, halt! Kii, fan out our troops!&quot;@</ToTranslate>
  </String>
  <String index="726">
    <ToTranslate>tcg_kyo22fa</ToTranslate>
  </String>
