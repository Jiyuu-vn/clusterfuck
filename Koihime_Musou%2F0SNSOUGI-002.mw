  <String index="223">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="224">
    <ToTranslate>aisy0718</ToTranslate>
  </String>
  <String index="225">
    <ToTranslate>Aisha$&quot;Ngh......&quot;@</ToTranslate>
  </String>
  <String index="226">
    <ToTranslate>Kazuto$&quot;Hahh... If you all want to lead the second formation, why don&apos;t you play some kind of a game to decide, rather than trying to out-scream one another?&quot;@</ToTranslate>
  </String>
  <String index="227">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="228">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="229">
    <ToTranslate>rinr0364</ToTranslate>
  </String>
  <String index="230">
    <ToTranslate>Rinrin$&quot;What kind of game are you talking about?&quot;@</ToTranslate>
  </String>
  <String index="231">
    <ToTranslate>Kazuto$&quot;Let&apos;s see... How about a game of strike and cover rock paper scissors?&quot;@</ToTranslate>
  </String>
  <String index="232">
    <ToTranslate>tcg_bac12aa</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>sui_0057</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>Sui$&quot;What the heck is that?&quot;@</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>Kazuto$&quot;You do rock paper scissors, and the winner picks up a weapon and hits their opponent. The loser tries to defend herself with a shield. If the winner strikes successfully, she gets a point.&quot;@</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>tcg_bac12ba</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>bgm28</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>sui_0058</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>Sui$&quot;Hmmm... that sounds like fun.&quot;@</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>aisy0719</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>Aisha$&quot;Yes. That&apos;s a suitable game for warriors.&quot;@</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>rinr0365</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>Rinrin$&quot;Okay, we&apos;ll decide with a game of strike and defend rock paper scissors!&quot;@</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>Kazuto$&quot;Not defend, cover. But do we have any helmets?&quot;@</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>$The three of them interrupted my question.@</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>sui_0059</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>Sui$&quot;Strike and defend rock paper scissors! Shoot!&quot;@</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>$The three started with a serious look on their faces.@</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>tcg_kan11wa</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>aisy0720</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>Aisha$&quot;Draw!&quot;@</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>rinr0366</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>Rinrin$&quot;Draw! Draw! Draw!&quot;@</ToTranslate>
