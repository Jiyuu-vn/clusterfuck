  <String index="728">
    <ToTranslate>kyoc0021</ToTranslate>
  </String>
  <String index="729">
    <ToTranslate>Kyocho$&quot;Okay! Everyone, fan out!&quot;@</ToTranslate>
  </String>
  <String index="730">
    <ToTranslate>kato0208</ToTranslate>
  </String>
  <String index="731">
    <ToTranslate>Kakouton$&quot;After you fan out, stand by!&quot;@</ToTranslate>
