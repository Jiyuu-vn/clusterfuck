    <ToTranslate>tcg_cho41ha</ToTranslate>
  </String>
  <String index="279">
    <ToTranslate>tcg_cho41m</ToTranslate>
  </String>
  <String index="280">
    <ToTranslate>rinr0367</ToTranslate>
  </String>
  <String index="281">
    <ToTranslate>Rinrin$&quot;Wahh!&quot;@</ToTranslate>
  </String>
  <String index="282">
    <ToTranslate>tcg_bac41aa</ToTranslate>
  </String>
  <String index="283">
    <ToTranslate>tcg_bac41m</ToTranslate>
  </String>
  <String index="284">
    <ToTranslate>sui_0062</ToTranslate>
  </String>
  <String index="285">
    <ToTranslate>Sui$&quot;Tsk, I missed!&quot;@</ToTranslate>
  </String>
  <String index="286">
    <ToTranslate>Kazuto$&quot;Uwaaah! Hold on, hold on, hold on! Whoever said to use real weapons in this game?!&quot;@</ToTranslate>
  </String>
  <String index="287">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="288">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="289">
    <ToTranslate>aisy0722</ToTranslate>
  </String>
  <String index="290">
    <ToTranslate>Aisha$&quot;Be quiet, My Lord!&quot;@</ToTranslate>
  </String>
  <String index="291">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="292">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="293">
    <ToTranslate>rinr0368</ToTranslate>
  </String>
  <String index="294">
    <ToTranslate>Rinrin$&quot;This is a serious match of strike and defend rock paper scissors with our lives at stake!&quot;@</ToTranslate>
  </String>
  <String index="295">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="296">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="297">
    <ToTranslate>sui_0063</ToTranslate>
  </String>
  <String index="298">
    <ToTranslate>Sui$&quot;Fine with me! Continue!&quot;@</ToTranslate>
  </String>
  <String index="299">
    <ToTranslate>tcg_kan11wa</ToTranslate>
  </String>
  <String index="300">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="301">
    <ToTranslate>aisy0723</ToTranslate>
  </String>
  <String index="302">
    <ToTranslate>Aisha$&quot;Strike and cover rock paper scissors! Shoot!&quot;@</ToTranslate>
  </String>
  <String index="303">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="304">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="305">
    <ToTranslate>rinr0369</ToTranslate>
  </String>
  <String index="306">
    <ToTranslate>Rinrin$&quot;Draw! Draw!...&quot;@</ToTranslate>
  </String>
  <String index="307">
    <ToTranslate>tcg_syo11ba</ToTranslate>
  </String>
  <String index="308">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="309">
    <ToTranslate>syur0344</ToTranslate>
  </String>
  <String index="310">
    <ToTranslate>Shuri$&quot;Hahaha, they&apos;re full of energy, aren&apos;t they?&quot;@</ToTranslate>
  </String>
  <String index="311">
    <ToTranslate>Kazuto$&quot;Can&apos;t you stop them?&quot;@</ToTranslate>
  </String>
  <String index="312">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="313">
    <ToTranslate>cc</ToTranslate>
  </String>
  <String index="314">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="315">
    <ToTranslate>cc</ToTranslate>
  </String>
  <String index="316">
    <ToTranslate>bcg_sky_a</ToTranslate>
  </String>
  <String index="317">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="318">
    <ToTranslate>$Is our battle about to begin? &apos;Is it really?&apos; I wonder as I feel both concern and encouragment at the lack of nervousness in my generals.@</ToTranslate>
  </String>
  <String index="319">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
