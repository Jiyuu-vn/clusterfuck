    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>aisy0716</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>Aisha$&quot;But they started it!&quot;@</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>tcg_bac12ga</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>sui_0056</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>Sui$&quot;Wow, now she&apos;s tattling...&quot;@</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>rinr0363</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>Rinrin$&quot;Aisha is more cowardly than I thought.&quot;@</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="219">
    <ToTranslate>aisy0717</ToTranslate>
  </String>
  <String index="220">
    <ToTranslate>Aisha$&quot;...How dare you!&quot;@</ToTranslate>
  </String>
  <String index="221">
    <ToTranslate>Kazuto$&quot;Come on! Stop fighting!&quot;@</ToTranslate>
  </String>
  <String index="222">
    <ToTranslate>tcg_kan11sa</ToTranslate>
