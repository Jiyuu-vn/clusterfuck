  <String index="1534">
    <ToTranslate>tcg_ens22ca</ToTranslate>
  </String>
  <String index="1535">
    <ToTranslate>tcg_ens22m</ToTranslate>
  </String>
  <String index="1536">
    <ToTranslate>ensy0096</ToTranslate>
  </String>
  <String index="1537">
    <ToTranslate>Enshou$&quot;Oh, that little girl&apos;s army nearly crumbled for just a moment, huh?&quot;@</ToTranslate>
  </String>
  <String index="1538">
    <ToTranslate>tcg_bun22ia</ToTranslate>
  </String>
  <String index="1539">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="1540">
    <ToTranslate>buns0014</ToTranslate>
  </String>
  <String index="1541">
    <ToTranslate>Bunshuu$&quot;Wow, you&apos;re sharp, Princess. You&apos;re absolutely right. Sousou&apos;s army&apos;s advance guard panicked just for a moment.&quot;@</ToTranslate>
  </String>
  <String index="1542">
    <ToTranslate>tcg_ens22ba</ToTranslate>
  </String>
  <String index="1543">
    <ToTranslate>tcg_ens22m</ToTranslate>
  </String>
  <String index="1544">
    <ToTranslate>ensy0097</ToTranslate>
  </String>
  <String index="1545">
    <ToTranslate>Enshou$&quot;O-ho-ho-ho-ho! That much was clear to me!&quot;@</ToTranslate>
  </String>
  <String index="1546">
    <ToTranslate>tcg_gan22ia</ToTranslate>
  </String>
  <String index="1547">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="1548">
    <ToTranslate>ganr0023</ToTranslate>
  </String>
  <String index="1549">
    <ToTranslate>Ganryou$&quot;But...... I wonder what happened?&quot;@</ToTranslate>
  </String>
  <String index="1550">
    <ToTranslate>tcg_bun12ha</ToTranslate>
  </String>
  <String index="1551">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="1552">
    <ToTranslate>buns0015</ToTranslate>
  </String>
  <String index="1553">
    <ToTranslate>Bunshuu$&quot;I don&apos;t know. Maybe something happened to the general.&quot;@</ToTranslate>
  </String>
  <String index="1554">
    <ToTranslate>tcg_gan22ga</ToTranslate>
  </String>
  <String index="1555">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="1556">
    <ToTranslate>ganr0024</ToTranslate>
  </String>
  <String index="1557">
    <ToTranslate>Ganryou$&quot;But they recovered pretty quickly.&quot;@</ToTranslate>
  </String>
  <String index="1558">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="1559">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="1560">
    <ToTranslate>buns0016</ToTranslate>
  </String>
  <String index="1561">
    <ToTranslate>Bunshuu$&quot;Either she didn&apos;t die and encouraged the soldiers, or someone else took command.&quot;@</ToTranslate>
  </String>
  <String index="1562">
    <ToTranslate>tcg_gan22ia</ToTranslate>
  </String>
  <String index="1563">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="1564">
    <ToTranslate>ganr0025</ToTranslate>
  </String>
  <String index="1565">
    <ToTranslate>Ganryou$&quot;Maybe...... Oh, Sousou&apos;s army has breached the gate.&quot;@</ToTranslate>
  </String>
  <String index="1566">
    <ToTranslate>tcg_bun22ha</ToTranslate>
  </String>
  <String index="1567">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="1568">
    <ToTranslate>buns0017</ToTranslate>
  </String>
  <String index="1569">
    <ToTranslate>Bunshuu$&quot;Yeah, you&apos;re right. Princess, do you think we should go out there again?&quot;@</ToTranslate>
  </String>
  <String index="1570">
    <ToTranslate>tcg_ens12aa</ToTranslate>
  </String>
  <String index="1571">
    <ToTranslate>tcg_ens12m</ToTranslate>
  </String>
  <String index="1572">
    <ToTranslate>ensy0098</ToTranslate>
  </String>
  <String index="1573">
    <ToTranslate>Enshou$&quot;Maybe so. Toshi-san, Iishe-san, why don&apos;t you go with your soldiers?&quot;@</ToTranslate>
  </String>
  <String index="1574">
    <ToTranslate>tcg_bun12ia</ToTranslate>
  </String>
  <String index="1575">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="1576">
    <ToTranslate>buns0018</ToTranslate>
  </String>
  <String index="1577">
    <ToTranslate>Bunshuu$&quot;Got it, got it. Well, let&apos;s go loot the burning building.&quot;@</ToTranslate>
  </String>
  <String index="1578">
    <ToTranslate>tcg_gan22ca</ToTranslate>
  </String>
  <String index="1579">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="1580">
    <ToTranslate>ganr0026</ToTranslate>
  </String>
  <String index="1581">
    <ToTranslate>Ganryou$&quot;I don&apos;t want to take away credit from other people though......&quot;@</ToTranslate>
  </String>
  <String index="1582">
    <ToTranslate>tcg_bun22ba</ToTranslate>
  </String>
  <String index="1583">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="1584">
    <ToTranslate>buns0019</ToTranslate>
  </String>
  <String index="1585">
    <ToTranslate>Bunshuu$&quot;Oh, don&apos;t say that. Come on, Toshi.&quot;@</ToTranslate>
  </String>
  <String index="1586">
    <ToTranslate>tcg_gan12ha</ToTranslate>
  </String>
  <String index="1587">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="1588">
    <ToTranslate>ganr0027</ToTranslate>
  </String>
  <String index="1589">
    <ToTranslate>Ganryou$&quot;Okay......&quot;@</ToTranslate>
  </String>
