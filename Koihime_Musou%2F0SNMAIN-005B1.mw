  <String index="1393">
    <ToTranslate>tcg_zgi11m</ToTranslate>
  </String>
  <String index="1394">
    <ToTranslate>Soldier$&quot;General Kakouton! Hongou&apos;s army broke the gate of Hulao Pass!&quot;@</ToTranslate>
  </String>
  <String index="1395">
    <ToTranslate>tcg_ton12ga</ToTranslate>
  </String>
  <String index="1396">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="1397">
    <ToTranslate>kato0105</ToTranslate>
  </String>
  <String index="1398">
    <ToTranslate>Kakouton$&quot;What!? They did it before us?&quot;@</ToTranslate>
  </String>
  <String index="1399">
    <ToTranslate>Soldier$&quot;Yes ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="1400">
    <ToTranslate>tcg_ton12ca</ToTranslate>
  </String>
  <String index="1401">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="1402">
    <ToTranslate>kato0106</ToTranslate>
  </String>
  <String index="1403">
    <ToTranslate>Kakouton$&quot;Damn it! We fought so hard, only to let Hongou take the credit?&quot;@</ToTranslate>
  </String>
  <String index="1404">
    <ToTranslate>tcg_en12ja</ToTranslate>
  </String>
  <String index="1405">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="1406">
    <ToTranslate>kaen0018</ToTranslate>
  </String>
  <String index="1407">
    <ToTranslate>Kakouen$&quot;What do we do, sister?&quot;@</ToTranslate>
  </String>
  <String index="1408">
    <ToTranslate>tcg_ton12ga</ToTranslate>
  </String>
  <String index="1409">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="1410">
    <ToTranslate>kato0107</ToTranslate>
  </String>
  <String index="1411">
    <ToTranslate>Kakouton$&quot;......Forget about arriving first, then. We&apos;ll take credit for securing Hulao Pass completely.&quot;@</ToTranslate>
  </String>
  <String index="1412">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="1413">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="1414">
    <ToTranslate>kaen0019</ToTranslate>
  </String>
  <String index="1415">
    <ToTranslate>Kakouen$&quot;But our soldiers are worn out from the lengthy attack, and morale has fallen.&quot;@</ToTranslate>
  </String>
  <String index="1416">
    <ToTranslate>tcg_ton12ca</ToTranslate>
  </String>
  <String index="1417">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="1418">
    <ToTranslate>kato0108</ToTranslate>
  </String>
  <String index="1419">
    <ToTranslate>Kakouton$&quot;But we can&apos;t let Hongou take credit alone! Everyone, line up! We&apos;ll pass Hougou&apos;s army and secure Hulao Pass!&quot;@</ToTranslate>
  </String>
  <String index="1420">
    <ToTranslate>tcg_en12ja</ToTranslate>
  </String>
  <String index="1421">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="1422">
    <ToTranslate>kaen0020</ToTranslate>
  </String>
  <String index="1423">
    <ToTranslate>Kakouen$&quot;Wait, Shunran! We don&apos;t have a plan!&quot;@</ToTranslate>
  </String>
  <String index="1424">
    <ToTranslate>kato0109</ToTranslate>
  </String>
  <String index="1425">
    <ToTranslate>Kakouton$&quot;Then what do you want me to do? We must take Hulao Pass for Karin-sama......&quot;@</ToTranslate>
  </String>
  <String index="1426">
    <ToTranslate>SE018</ToTranslate>
  </String>
