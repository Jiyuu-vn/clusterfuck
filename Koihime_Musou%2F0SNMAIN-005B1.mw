  <String index="1620">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="1621">
    <ToTranslate>kato0127</ToTranslate>
  </String>
  <String index="1622">
    <ToTranslate>Kakouton$&quot;Yeah......&quot;@</ToTranslate>
  </String>
