  <String index="4">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>rinr0166</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>Rinrin$&quot;That&apos;s the signal from the main force!&quot;@</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>aisy0286</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>Aisha$&quot;Okay! All troops, defensive retreat!&quot;@</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>tcg_ryo11ca</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_ryo11m</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>ryof0017</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>Ryofu$&quot;......Tsk.&quot;@</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>tcg_cry21ea</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>chor0035</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>Chouryou$&quot;Aww, we just needed one more push. ......Ryofu-chin, what should we do?&quot;@</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_ryo11aa</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_ryo11m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>ryof0018</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Ryofu$&quot;......Pursue.&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_cry11ia</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>chor0036</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>Chouryou$&quot;Pursue? Isn&apos;t a bad idea to pursue them now? Sure, we can handle the Hongou army in their vanguard, but aren&apos;t Sousou and Sonken pulling up behind them?&quot;@</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_ryo11ca</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>tcg_ryo11m</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>ryof0019</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Ryofu$&quot;......Pursue.&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_cry21ea</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>chor0037</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>Chouryou$&quot;Hahh......well, I can&apos;t say I&apos;m not happy to fight against those generals of Hongou&apos;s army...They&apos;re both pretty strong. They&apos;ll make good game.&quot;@</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_ryo11aa</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>tcg_ryo11m</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>ryof0020</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>Ryofu$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_cry21ga</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>chor0038</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>Chouryou$&quot;Shall we go?&quot;@</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>tcg_ryo41ka</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>tcg_ryo41m</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>ryof0021</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>Ryofu$&quot;......(Nod)&quot;@</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>Messenger$&quot;The enemy has started to fight with our rear guard!&quot;@</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>rinr0167</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>Rinrin$&quot;Good! Okay, everyone, we&apos;ll continue to pull back!&quot;@</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_ton12ca</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>kato0023</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>Kakouton$&quot;Karin-sama! Hongou&apos;s army is--! At this rate, our own vanguard will get involved!&quot;@</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>tcg_sou22da</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>sous0062</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>Sousou$&quot;......Not bad for an ugly man. ......Shunran, reorganize our formation.&quot;@</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>tcg_ton12ga</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_ton12m</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>kato0024</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>Kakouton$&quot;Yes ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_syu21fa</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>syuy0011</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>Shuuyu$&quot;Well, well...... Hongou&apos;s army has good tacticians, doesn&apos;t it?&quot;@</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>sonk0029</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>Sonken$&quot;......Kouha.&quot;@</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>tcg_kne11da</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>kann0004</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>Kannei$&quot;Yes ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>sonk0030</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>Sonken$&quot;Arrange our troops in a spearhead formation. We&apos;re flanking the enemy.&quot;@</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>kann0005</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>Kannei$&quot;As you wish!&quot;@</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>tcg_syu21fa</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>syuy0012</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>Shuuyu$&quot;Are we going to attack?&quot;@</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>tcg_son12aa</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>sonk0031</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>Sonken$&quot;If we twiddle our thumbs, we&apos;ll suffer more casualties. After we flank them, we&apos;ll use that momentum as an impetus to break through the gate.&quot;@</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>syuy0013</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>Shuuyu$&quot;I see......&quot;@</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>sonk0032</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>Sonken$&quot;Do you have any objections?&quot;@</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>tcg_syu11ba</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>syuy0014</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>Shuuyu$&quot;No. I think that&apos;s an effective plan, given the situation.&quot;@</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>sonk0033</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>Sonken$&quot;Then we&apos;ll execute it.&quot;@</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>tcg_syu21fa</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>syuy0015</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>Shuuyu$&quot;As you wish......&quot;@</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>syur0153</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>Shuri$&quot;My Lord! Sousou&apos;s army and Sonken&apos;s army are moving! Our strategy worked!&quot;@</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>Kazuto$&quot;Great!&quot;@</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>aisy0287</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>Aisha$&quot;We&apos;ll begin the counterattack!&quot;@</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>rinr0168</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>Rinrin$&quot;All units, follow me!&quot;@</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>SE124</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>aa2</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>tcg_ens12aa</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>tcg_ens12m</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>ensy0092</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>Enshou$&quot;Oh my? My, my, my? Is our Anti-Toutaku Alliance doing surprisingly well?&quot;@</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>tcg_gan22aa</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>ganr0019</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>Ganryou$&quot;Yes! Hongou&apos;s army did a great job luring out the enemy troops. Sousou and Sonken&apos;s armies are taking advantage of the situation and beating them to a pulp.&quot;@</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>tcg_bun22fa</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>buns0011</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>Bunshuu$&quot;That was cruel of Sousou and Sonken.&quot;@</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>tcg_ens12ja</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>tcg_ens12m</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>ensy0093</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>Enshou$&quot;I know, that was heartless......&quot;@</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>tcg_gan22ca</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>ganr0020</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>Ganryou$&quot;(I think the Princess is far more heartless though......)&quot;@</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>tcg_ens22ca</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>tcg_ens22m</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>ensy0094</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>Enshou$&quot;What is it, Ganryou-san? Is there something on my face?&quot;@</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>tcg_gan12ba</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>ganr0021</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>Ganryou$&quot;No, nothing. By the way, Princess, don&apos;t you think now is the time to throw in the reserve troops?&quot;@</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>tcg_ens22ba</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>tcg_ens22m</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>ensy0095</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>Enshou$&quot;Oh, you&apos;re right. Bunshuu-san, hurry up and go beat up Ryofu.&quot;@</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>tcg_bun22ha</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>buns0012</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>Bunshuu$&quot;Okay, okay. Toshi, I&apos;m going.&quot;@</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>tcg_gan22aa</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>ganr0022</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>Ganryou$&quot;Okay. Be careful, Bun-chan.&quot;@</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="219">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="220">
    <ToTranslate>buns0013</ToTranslate>
  </String>
  <String index="221">
    <ToTranslate>Bunshuu$&quot;Thanks. See you later.&quot;@</ToTranslate>
  </String>
