  <String index="1345">
    <ToTranslate>aisy0301</ToTranslate>
  </String>
  <String index="1346">
    <ToTranslate>Aisha$&quot;The gate will come down any moment! Attack!&quot;@</ToTranslate>
  </String>
  <String index="1347">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="1348">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="1349">
    <ToTranslate>rinr0176</ToTranslate>
  </String>
  <String index="1350">
    <ToTranslate>Rinrin$&quot;Come on everyone! We can do it!&quot;@</ToTranslate>
  </String>
  <String index="1351">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1352">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="1353">
    <ToTranslate>$The soldiers roar in response to Aisha and Rinrin&apos;s encouragements.@</ToTranslate>
  </String>
  <String index="1354">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="1355">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="1356">
    <ToTranslate>syur0165</ToTranslate>
  </String>
  <String index="1357">
    <ToTranslate>Shuri$&quot;A little more...... a little more......!&quot;@</ToTranslate>
  </String>
  <String index="1358">
    <ToTranslate>$Shuri was clenching her fists and glaring at the gates, chanting away.@</ToTranslate>
  </String>
  <String index="1359">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1360">
    <ToTranslate>$The soldiers aimed for the gate, protecting themselves from the rain of arrows above with their shields, and slamming the giant, wooden battering ram against the iron doors.@</ToTranslate>
  </String>
  <String index="1361">
    <ToTranslate>SE081</ToTranslate>
  </String>
  <String index="1362">
    <ToTranslate>yure_r</ToTranslate>
  </String>
  <String index="1363">
    <ToTranslate>$Baaaam!@</ToTranslate>
  </String>
  <String index="1364">
    <ToTranslate>SE081</ToTranslate>
  </String>
  <String index="1365">
    <ToTranslate>yure_r</ToTranslate>
  </String>
  <String index="1366">
    <ToTranslate>$Baaaam!@</ToTranslate>
  </String>
  <String index="1367">
    <ToTranslate>$The deep sound that shook our stomachs slowly changed with each blow.@</ToTranslate>
  </String>
  <String index="1368">
    <ToTranslate>$Finally...@</ToTranslate>
  </String>
  <String index="1369">
    <ToTranslate>SE081</ToTranslate>
  </String>
  <String index="1370">
    <ToTranslate>SE060</ToTranslate>
  </String>
  <String index="1371">
    <ToTranslate>yure_t</ToTranslate>
  </String>
  <String index="1372">
    <ToTranslate>$The iron door shook the ground as it fell.@</ToTranslate>
  </String>
  <String index="1373">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="1374">
    <ToTranslate>SE124</ToTranslate>
  </String>
  <String index="1375">
    <ToTranslate>$The soldiers cheered, and started pouring into Hulao Pass.@</ToTranslate>
  </String>
  <String index="1376">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="1377">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="1378">
    <ToTranslate>aisy0302</ToTranslate>
  </String>
  <String index="1379">
    <ToTranslate>Aisha$&quot;Good! Rinrin, we are going too!&quot;@</ToTranslate>
  </String>
