  <String index="1613">
    <ToTranslate>kato0126</ToTranslate>
  </String>
  <String index="1614">
    <ToTranslate>Kakouton$&quot;Whew......&quot;@</ToTranslate>
  </String>
  <String index="1615">
    <ToTranslate>tcg_en22ca</ToTranslate>
  </String>
  <String index="1616">
    <ToTranslate>tcg_en22m</ToTranslate>
  </String>
  <String index="1617">
    <ToTranslate>kaen0028</ToTranslate>
  </String>
  <String index="1618">
    <ToTranslate>Kakouen$&quot;That was great, Aneja. You revived our soldiers&apos; plummeting morale and gave them courage.&quot;@</ToTranslate>
  </String>
  <String index="1619">
    <ToTranslate>tcg_ton14ea</ToTranslate>
