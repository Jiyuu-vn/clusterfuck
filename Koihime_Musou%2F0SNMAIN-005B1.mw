  <String index="1601">
    <ToTranslate>Kakouen$&quot;Aneja, take this......&quot;@</ToTranslate>
  </String>
  <String index="1602">
    <ToTranslate>kato0125</ToTranslate>
  </String>
  <String index="1603">
    <ToTranslate>Kakouton$&quot;Thank you, Shuuran......&quot;@</ToTranslate>
