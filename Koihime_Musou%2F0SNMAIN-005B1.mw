  <String index="439">
    <ToTranslate>chor0056</ToTranslate>
  </String>
  <String index="440">
    <ToTranslate>Chouryou$&quot;Hmm? Oh, who&apos;s that? They&apos;re pretty brave to come face us alone!&quot;@</ToTranslate>
  </String>
  <String index="441">
    <ToTranslate>Soldier$&quot;What should we do?&quot;@</ToTranslate>
  </String>
  <String index="442">
    <ToTranslate>tcg_cry21ga</ToTranslate>
  </String>
  <String index="443">
