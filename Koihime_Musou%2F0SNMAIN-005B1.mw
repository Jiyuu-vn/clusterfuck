    <ToTranslate>tcg_en12ha</ToTranslate>
  </String>
  <String index="1624">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="1625">
    <ToTranslate>kaen0029</ToTranslate>
  </String>
  <String index="1626">
    <ToTranslate>Kakouen$&quot;Are you okay......?&quot;@</ToTranslate>
  </String>
  <String index="1627">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="1628">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="1629">
    <ToTranslate>kato0128</ToTranslate>
  </String>
  <String index="1630">
    <ToTranslate>Kakouton$&quot;I&apos;m okay. This wound is nothing. But......&quot;@</ToTranslate>
  </String>
  <String index="1631">
    <ToTranslate>tcg_en12ba</ToTranslate>
  </String>
  <String index="1632">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="1633">
    <ToTranslate>kaen0030</ToTranslate>
  </String>
  <String index="1634">
    <ToTranslate>Kakouen$&quot;Karin-sama is fine.&quot;@</ToTranslate>
  </String>
  <String index="1635">
    <ToTranslate>tcg_ton24fa</ToTranslate>
  </String>
  <String index="1636">
    <ToTranslate>tcg_ton24m</ToTranslate>
  </String>
  <String index="1637">
    <ToTranslate>kato0129</ToTranslate>
  </String>
  <String index="1638">
    <ToTranslate>Kakouton$&quot;I didn&apos;t say anything......&quot;@</ToTranslate>
  </String>
  <String index="1639">
    <ToTranslate>tcg_en22ea</ToTranslate>
  </String>
  <String index="1640">
    <ToTranslate>tcg_en22m</ToTranslate>
  </String>
  <String index="1641">
    <ToTranslate>kaen0031</ToTranslate>
  </String>
  <String index="1642">
    <ToTranslate>Kakouen$&quot;I know what you want to say, Shunran. You&apos;re worried because your face was injured, right?&quot;@</ToTranslate>
  </String>
  <String index="1643">
    <ToTranslate>tcg_ton24ia</ToTranslate>
  </String>
  <String index="1644">
    <ToTranslate>tcg_ton24m</ToTranslate>
  </String>
  <String index="1645">
    <ToTranslate>kato0130</ToTranslate>
  </String>
  <String index="1646">
    <ToTranslate>Kakouton$&quot;......Hmph.&quot;@</ToTranslate>
  </String>
  <String index="1647">
    <ToTranslate>tcg_en22ca</ToTranslate>
  </String>
  <String index="1648">
    <ToTranslate>tcg_en22m</ToTranslate>
  </String>
  <String index="1649">
    <ToTranslate>kaen0032</ToTranslate>
  </String>
  <String index="1650">
    <ToTranslate>Kakouen$&quot;She will love you, even with that injury.&quot;@</ToTranslate>
  </String>
  <String index="1651">
    <ToTranslate>tcg_ton24fa</ToTranslate>
  </String>
  <String index="1652">
    <ToTranslate>tcg_ton24m</ToTranslate>
  </String>
  <String index="1653">
    <ToTranslate>kato0131</ToTranslate>
  </String>
  <String index="1654">
    <ToTranslate>Kakouton$&quot;I hope so......&quot;@</ToTranslate>
  </String>
  <String index="1655">
    <ToTranslate>tcg_en12ba</ToTranslate>
  </String>
  <String index="1656">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="1657">
    <ToTranslate>kaen0033</ToTranslate>
  </String>
  <String index="1658">
    <ToTranslate>Kakouen$&quot;Of course she will. Can you move?&quot;@</ToTranslate>
  </String>
  <String index="1659">
    <ToTranslate>tcg_ton14ga</ToTranslate>
  </String>
  <String index="1660">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="1661">
    <ToTranslate>kato0132</ToTranslate>
  </String>
  <String index="1662">
    <ToTranslate>Kakouton$&quot;Yeah, I&apos;m fine. I&apos;ll go back to the front line and command the soldiers. It&apos;d help if you could stay with me.&quot;@</ToTranslate>
  </String>
  <String index="1663">
    <ToTranslate>tcg_en22ca</ToTranslate>
  </String>
  <String index="1664">
    <ToTranslate>tcg_en22m</ToTranslate>
  </String>
  <String index="1665">
    <ToTranslate>kaen0034</ToTranslate>
  </String>
  <String index="1666">
    <ToTranslate>Kakouen$&quot;Of course. You and I are always together. I&apos;d never leave you.&quot;@</ToTranslate>
  </String>
  <String index="1667">
    <ToTranslate>tcg_ton24ba</ToTranslate>
  </String>
  <String index="1668">
    <ToTranslate>tcg_ton24m</ToTranslate>
  </String>
  <String index="1669">
    <ToTranslate>kato0133</ToTranslate>
  </String>
  <String index="1670">
    <ToTranslate>Kakouton$&quot;Thank you. Shall we go?&quot;@</ToTranslate>
  </String>
  <String index="1671">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="1672">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="1673">
    <ToTranslate>kaen0035</ToTranslate>
  </String>
  <String index="1674">
    <ToTranslate>Kakouen$&quot;Yeah.&quot;@</ToTranslate>
  </String>
