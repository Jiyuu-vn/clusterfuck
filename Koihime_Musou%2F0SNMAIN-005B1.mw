  <String index="459">
    <ToTranslate>chor0060</ToTranslate>
  </String>
  <String index="460">
    <ToTranslate>Chouryou$&quot;Stop sniveling like that. Come on, go.&quot;@</ToTranslate>
  </String>
  <String index="461">
    <ToTranslate>Soldier$&quot;Yes!&quot;@</ToTranslate>
