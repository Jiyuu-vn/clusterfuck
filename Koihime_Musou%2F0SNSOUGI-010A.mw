[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_camp_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_zgo11aa</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_zgo11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>Soldier$&quot;The enemy army has broken through our front lines! They are heading towards us at full strength!&quot;@</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>tcg_syu21ia</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>syuy0169</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>Shuuyu$&quot;......I guess I&apos;m not powerful enough to reach heaven...... Is that it?&quot;@</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Soldier$&quot;Wh-what should we do?&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_syu11ca</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>syuy0170</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Shuuyu$&quot;......Assemble the entire army. Protect the headquarters!&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>Soldier$&quot;Yes ma&apos;am. B-But I don&apos;t think our loss is...&quot;@</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_syu11ba</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>syuy0171</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>Shuuyu$&quot;Are you telling me there&apos;s no use trying to protect the headquarters?&quot;@</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>Soldier$&quot;Yes. Maybe we should all surrender......&quot;@</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>tcg_syu21da</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>syuy0172</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>Shuuyu$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>battle_kiriage</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>SE033</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>Soldier$&quot;Gyah!&quot;@</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>SE038</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_syu31ea</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_syu31m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>syuy0173</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Shuuyu$&quot;Thy churl, only thinking of thine own safety. Repent spewing such cowardly words with thine death!&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>syuy0174</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>Shuuyu$&quot;If there&apos;s any coward who&apos;s afraid to die, I will cut them down for betrayal! Everyone, protect the headquarters at all costs!&quot;@</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>syuy0175</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>Shuuyu$&quot;I have a plan to recover! Stall the enemy attack while I prepare it! Got that?!&quot;@</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_zgo11aa</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>tcg_zgo11m</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Soldier$&quot;Y-yes!&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_syo21ca</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>syur0494</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>Shuri$&quot;......Oh? They were just disturbed for a moment.&quot;@</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>Kazuto$&quot;Huh? How can you tell?&quot;@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>tcg_syo41ea</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>syur0495</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>Shuri$&quot;Well, the organized resistance from the remaining enemies has weakened a bit. ......I wonder what happened?&quot;@</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>aisy0975</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>Aisha$&quot;Did an internal conflict take place?&quot;@</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>sei_0314</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>Sei$&quot;That&apos;s possible. She seized the entirety of Son-Go in a short time, but that doesn&apos;t mean she seized the people&apos;s hearts.&quot;@</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>sui_0236</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>Sui$&quot;I wonder if the people who were on the fence about following Shuuyu tried to run, or surrender, or something like that?&quot;@</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>tcg_cho21da</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>rinr0521</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>Rinrin$&quot;How cowardly of them.&quot;@</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>shio0107</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>Shion$&quot;Either way, this is a good chance to destroy the enemy. ...Let&apos;s finish them off.&quot;@</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>syur0496</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>Shuri$&quot;That&apos;s right. Okay everyone, give them one final blow. Pay attention to the situation, and mop up the rest of the enemy.&quot;@</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>sei_0315</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>Sei$&quot;Understood. ......What do we do with those who surrender?&quot;@</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>aisy0976</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>Aisha$&quot;Accept them. Sonken must judge her officers herself.&quot;@</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>sei_0316</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>Sei$&quot;That&apos;s how it should be. Okay everyone, let&apos;s go.&quot;@</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>aisy0977</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>Aisha$&quot;Yes. Now, My Lord, we&apos;ll be going.&quot;@</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>Kazuto$&quot;Okay. Be careful, everyone.&quot;@</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>rinr0522</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>Rinrin$&quot;Leave it to us!&quot;@</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>sui_0237</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>Sui$&quot;Okay, let&apos;s go, Rinrin!&quot;@</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>rinr0523</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>Rinrin$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>sei_0317</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>Sei$&quot;Shall we go too?&quot;@</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>aisy0978</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>Aisha$&quot;Yeah.&quot;@</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>shio0108</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>Shion$&quot;Okay then, shall we solidify our main army as we advance?&quot;@</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>syur0497</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>Shuri$&quot;Yes. Everyone, advance!&quot;@</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>$Everyone grabbed their weapons and bravely marched onto the battlefield, slowly pushing the enemy back before eventually causing them to collapse.@</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>$We pushed on with our advance and assaulted the enemy&apos;s headquarters.@</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>$However, our greatest enemy, Shuuyu, the ringleader of this insurrection, was nowhere to be found.@</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>$Once we seized the enemy headquarters and asked one of the prisoners, we learned Shuuyu had retreated to Go&apos;s main palace.@</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>$We reorganized our troops and immediately pursued Shuuyu.@</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>bcg_go_outcast_a</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>$Although we suffered major casualties in the last battle, Shuri advised that this was the final hurdle, and we pressed onward to Go&apos;s stronghold.@</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>$However, what awaited us was an army just as large as the one we just faced.@</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>$To top it off, there were a great amount of White Clothes mixed in amongst the Son-Go soldiers.@</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>tcg_cun21ga</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>sei_0318</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>Sei$&quot;I knew they had something to do with this......&quot;@</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>aisy0979</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>Aisha$&quot;Who are they anyway? They controlled Sougi, instigated Son-Go, and do nothing but interfere with us.&quot;@</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>Kazuto$&quot;They already tried to take my life several times before, so I guess their goal is to kill me.&quot;@</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>syur0498</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>Shuri$&quot;But their behavior is too exaggerated. They must have another goal.&quot;@</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>shio0109</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>Shion$&quot;A purpose other than taking Our Lord&apos;s life...... I wonder what it is.&quot;@</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>sui_0238</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>Sui$&quot;Who cares? Right now, we need to beat down that bitch Shuuyu! That&apos;s all.&quot;@</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>rinr0524</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>Rinrin$&quot;That&apos;s right!&quot;@</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>aisy0980</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>Aisha$&quot;Hahh...... you two. How can you be so carefree?&quot;@</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>tcg_bac12ga</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>sui_0239</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>Sui$&quot;Speak for yourself, Aisha. You can&apos;t afford to think about something else right before a battle.&quot;@</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>rinr0525</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>Rinrin$&quot;You should only think about fighting when you&apos;re in a battle!&quot;@</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>sei_0319</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>Sei$&quot;You have a point there. ...Hmm?&quot;@</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>Kazuto$&quot;Sei, what is it?&quot;@</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>sei_0320</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>Sei$&quot;It seems their supreme commander is making her appearance. Someone is riding forth alone on horseback.&quot;@</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>Kazuto$&quot;Really? ......Is that Shuuyu?&quot;@</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>aisy0981</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>Aisha$&quot;Oh, she wants a war of words before the battle, huh? But...... I know what she&apos;s after.&quot;@</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>sonk0342</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>Sonken$&quot;Yes, her goal is me. ......She&apos;s planning to denounce me with her speech and raise her army&apos;s morale. I don&apos;t, however, intend to deal with her.&quot;@</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>Kazuto$&quot;Good. Whether you win or lose, you won&apos;t gain anything from a squabble.&quot;@</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>sei_0321</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>Sei$&quot;A war of words is a squabble to you, My Lord? ......But perhaps that&apos;s the truth.&quot;@</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>&quot;Kazuto$&quot;That&apos;s right. ......Well, let&apos;s listen to what Shuuyu has to say.&quot;@</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>tcg_syu21da</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>bgm35</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>syuy0176</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>Shuuyu$&quot;Son Chuubou! You, who betrayed Go and sold out our country, will never be forgiven by us!&quot;@</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>syuy0177</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>Shuuyu$&quot;Neither will we forgive you, Hongou Kazuto, Demon King who aided our betrayer and holds the ambition of conquering our lands!&quot;@</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>syuy0178</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>Shuuyu$&quot;So long as I, Shuu Koukin, exist, Go will never bow before you! If you wish to steal our land, then try and take it by force!&quot;@</ToTranslate>
  </String>
  <String index="219">
    <ToTranslate>tcg_syu31ea</ToTranslate>
  </String>
  <String index="220">
    <ToTranslate>tcg_syu31m</ToTranslate>
  </String>
  <String index="221">
    <ToTranslate>syuy0179</ToTranslate>
  </String>
  <String index="222">
    <ToTranslate>Shuuyu$&quot;But do not forget! We will risk our lives to prevent your tyrannous ways! Come taste the ferocity of Go&apos;s brave soldiers!&quot;@</ToTranslate>
  </String>
  <String index="223">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="224">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="225">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="226">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="227">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="228">
    <ToTranslate>bcg_go_outcast_a</ToTranslate>
  </String>
  <String index="229">
    <ToTranslate>tcg_son12ia</ToTranslate>
  </String>
  <String index="230">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="231">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="232">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>sonk0343</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>Sonken$&quot;Heh...... I see. She made me into a traitor to rally the radical factions.&quot;@</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>Kazuto$&quot;Looks like it. And apparently I&apos;m a demon king. ......I&apos;m starting to see what ideas those White Clothes planted in her head.&quot;@</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>tcg_cun21ha</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>sei_0322</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>Sei$&quot;Indeed. But with that accusation, the enemy&apos;s spirit has skyrocketed. This isn&apos;t good.&quot;@</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>aisy0982</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>Aisha$&quot;It&apos;s not good, but there&apos;s only one thing for us to do.&quot;@</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>tcg_cho31ia</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>rinr0526</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>Rinrin$&quot;We&apos;re going to crush everyone!&quot;@</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>sui_0240</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>Sui$&quot;You said it! Let&apos;s go wild!&quot;@</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>shio0110</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>Shion$&quot;Oh my. You two are so full of energy.&quot;@</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>syur0499</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>Shury$&quot;But they&apos;re right. At this point, all we have to do is defeat the enemy ahead. Let&apos;s do our best and take them out!&quot;@</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>Kazuto$&quot;Understood. Let&apos;s watch out for Shuuyu&apos;s famous fire plans, and for ambushes while we deploy our troops.&quot;@</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>aisy0983</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>Aisha$&quot;As you wish!  Now, all units...&quot;@</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>Soldier$&quot;Charge!&quot;@</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>sei_0323</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>Sei$&quot;Tsk...... I fell behind!&quot;@</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>tcg_kan11wa</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>aisy0984</ToTranslate>
  </String>
  <String index="275">
    <ToTranslate>Aisha$&quot;Kan&apos;u squad, we&apos;re going to intercept! Follow me!&quot;@</ToTranslate>
  </String>
  <String index="276">
    <ToTranslate>tcg_cun21ga</ToTranslate>
  </String>
  <String index="277">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="278">
    <ToTranslate>sei_0324</ToTranslate>
  </String>
  <String index="279">
    <ToTranslate>Sei$&quot;Chouun squad, we&apos;re following Kan&apos;u too! We&apos;ll strike them down as they come forth! Be ready, everyone!&quot;@</ToTranslate>
  </String>
  <String index="280">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="281">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="282">
    <ToTranslate>Soldier$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="283">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="284">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="285">
    <ToTranslate>sui_0241</ToTranslate>
  </String>
  <String index="286">
    <ToTranslate>Sui$&quot;Don&apos;t fall behind, Bachou squad! We&apos;re going to charge in! Let&apos;s go!&quot;@</ToTranslate>
  </String>
  <String index="287">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="288">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="289">
    <ToTranslate>rinr0527</ToTranslate>
  </String>
  <String index="290">
    <ToTranslate>Rinrin$&quot;I won&apos;t fall behind Sui! Let&apos;s go wild too!&quot;@</ToTranslate>
  </String>
  <String index="291">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="292">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="293">
    <ToTranslate>Soldier$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="294">
    <ToTranslate>tcg_syo21da</ToTranslate>
  </String>
  <String index="295">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="296">
    <ToTranslate>syur0500</ToTranslate>
  </String>
  <String index="297">
    <ToTranslate>Shuri$&quot;Then organize your formation while you intercept! Everyone, keep your ear out for the gong signal from our main force!&quot;@</ToTranslate>
  </String>
  <String index="298">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="299">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="300">
    <ToTranslate>shio0111</ToTranslate>
  </String>
  <String index="301">
    <ToTranslate>Shion$&quot;Understood. Then let&apos;s begin to intercept. Everyone, let&apos;s all do our best.&quot;@</ToTranslate>
  </String>
  <String index="302">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
