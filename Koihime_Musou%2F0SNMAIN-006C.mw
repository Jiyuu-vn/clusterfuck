[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>tcg_kan11fa</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>aisy0543</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Aisha$&quot;Where is Enshou? Unless we capture Enshou, this battle won&apos;t end!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>rinr0333</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Rinrin$&quot;It&apos;s no good, Aisha! We can&apos;t find her!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>aisy0544</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>Aisha$&quot;What?&quot;@</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>sei_0048</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>Sei$&quot;It&apos;s impossible to find her in this chaos.&quot;@</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>shio0001</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>Shion$&quot;Oh my, my. It lacks the finishing touch, doesn&apos;t it?&quot;@</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>sui_0018</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>Sui$&quot;What do we do? Should we split up and search for her?&quot;@</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>bgm03</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>syur0310</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>Shuri$&quot;No. We don&apos;t have to worry about Enshou-san.&quot;@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>sei_0049</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Sei$&quot;What do you mean?&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>syur0311</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>Shuri$&quot;Enshou&apos;s army has already suffered a devastating blow. Even if Enshou-san entered some fortress, it would be almost impossible for her to recover.&quot;@</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>sui_0019</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>Sui$&quot;But we can&apos;t just let her run free, can we?&quot;@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>tcg_syo41ba</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>syur0312</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>Shuri$&quot;Yes we can. But meanwhile, we&apos;ll continue on and enter the palace in Nanpi.&quot;@</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>shio0002</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>Shion$&quot;So we&apos;re going to occupy her base before she returns to it?&quot;@</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_syo11ba</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>syur0313</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>Shuri$&quot;That&apos;s right. If Enshou-san is the flower, then she can&apos;t bloom without her roots, Nanpi.&quot;@</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>aisy0545</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>Aisha$&quot;I see. If we take Nanpi, then the rest of her branches will wither. Is that your plan, Shuri?&quot;@</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>syur0314</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>Shuri$&quot;Yes. Nanpi is nearly empty right now, so let&apos;s take it.&quot;@</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>Kazuto$&quot;If you think so, it must be right. ......But what about our soldiers? If they are exhausted, I think we should let them rest.&quot;@</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>sei_0050</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>Sei$&quot;No, My Lord. That would be a big misunderstanding.&quot;@</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>shio0003</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>Shion$&quot;If we work hard for half a day more, then we&apos;ll be able to harvest the great fruit, Nanpi. I think that should be our priority.&quot;@</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>rinr0334</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>Rinrin$&quot;The soldiers&apos;ll just have to hang in there for a little while longer.&quot;@</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>Kazuto$&quot;Are you sure? They&apos;ve been continuously fighting and haven&apos;t rested.&quot;@</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>sui_0020</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>Sui$&quot;Don&apos;t you remember what Shuri said? When you&apos;re winning, you don&apos;t feel fatigue.&quot;@</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>Kazuto$&quot;......Okay. If you all say so, then you must be right. Let&apos;s head to Nanpi.&quot;@</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>aisy0546</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>Aisha$&quot;As you wish. I&apos;ll take the vanguard......&quot;@</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>bgm26</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>rinr0335</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>Rinrin$&quot;Wait a minute, Aisha.&quot;@</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>tcg_cun31ia</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>sei_0051</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>Sei$&quot;You said it very casually, but we won&apos;t let you take the honorable position of the vanguard that easily.&quot;@</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>tcg_bac12ga</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>sui_0021</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>Sui$&quot;That&apos;s so sly, Aisha.&quot;@</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>aisy0547</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>Aisha$&quot;How can you call me sly of all things? I have a plan for entering the enemy&apos;s territory, and...&quot;@</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>rinr0336</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>Rinrin$&quot;Yeah, yeah, excuses, excuses.&quot;@</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>tcg_bac12ga</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>sui_0022</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>Sui$&quot;If you have a plan, why don&apos;t you tell us now?&quot;@</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>sei_0052</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>Sei$&quot;Before that, do we really even need a plan to storm the palace?&quot;@</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>$The four warriors began arguing loudly over who would be the vanguard.@</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>Kazuto$&quot;Good grief......&quot;@</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>$I can only sigh......@</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>tcg_bac12ga</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>shio0004</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>Shion$&quot;U-hu-hu, they&apos;re all quite young, aren&apos;t they?&quot;@</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>syur0315</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>Shuri$&quot;Isn&apos;t that great? They&apos;re so full of energy!&quot;@</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>Kazuto$&quot;I think it&apos;s a little different from being full of energy, though......&quot;@</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>$After some more light conversation with Shuri and Shion, we rallied the troops and headed to Nanpi.@</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>bgm11</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>tcg_ens12fa</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>tcg_ens12m</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>ensy0125</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>Enshou$&quot;Hah, hah, hah, hah......&quot;@</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>ensy0126</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>Enshou$&quot;Kkh...... I can&apos;t believe I lost to those lowlifes......!&quot;@</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>ensy0127</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>Enshou$&quot;That&apos;s as impossible as the world being turned upside-down. Grrrrr! Ahh, they make me sick!&quot;@</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>ganr0096</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>Ganryou$&quot;Hey, Bun-chan. I&apos;m telling you, we&apos;re heading North!&quot;@</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>tcg_ens12ja</ToTranslate>
  </String>
  <String index="212">
