  <String index="382">
    <ToTranslate>tcg_ens22m</ToTranslate>
  </String>
  <String index="383">
    <ToTranslate>ensy0144</ToTranslate>
  </String>
  <String index="384">
    <ToTranslate>Enshou$&quot;Don&apos;t you think that&apos;s generous of me, befitting a member of a prestigious family of the three kingdoms? Oh-ho-ho-ho-ho-ho!&quot;@</ToTranslate>
