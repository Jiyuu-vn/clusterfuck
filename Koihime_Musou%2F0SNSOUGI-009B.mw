    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>kann0049</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>Kannei$&quot;Renfa-sama. I have returned.&quot;@</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
