<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>kann0029</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Kannei$&quot;Ggh, with a response like this, we&apos;re the ones in trouble!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>kann0030</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Kannei$&quot;We&apos;re going to merge with the main force! Pull back!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_bac41aa</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_bac41m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>sui_0210</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>Sui$&quot;Wait! Don&apos;t think you can withdraw so easily! I, Bachou the Splendid, never let go of my prey!&quot;@</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>kann0031</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>Kannei$&quot;Tsk......Just who I didn&apos;t want to see right when I didn&apos;t need this!&quot;@</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_bac12ba</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>sui_0211</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>Sui$&quot;Heh. I&apos;ve been irritated because I didn&apos;t get to be the vanguard. I&apos;m going to make sure you fight against me!&quot;@</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_kne11da</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>kann0032</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>Kannei$&quot;......Fine, I&apos;m going to take you down quickly so I can merge with the main force.&quot;@</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>sui_0212</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>Sui$&quot;Don&apos;t think you can go that easily.&quot;@</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>ecg_kne01a</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>kann0033</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>kannei$&quot;We won&apos;t know until we fight......!&quot;@</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>battle_kiriage</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>sui_0213</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Sui$&quot;Wow!&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>s_yure_r</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_bac22da</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_bac22m</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>sui_0214</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>Sui$&quot;Hey, that was close! What are you doing?!&quot;@</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>kann0034</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>Kannei$&quot;Tsk...... I missed. Here comes another one!&quot;@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>ecg_kne01a</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>kann0035</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Kannei$&quot;Yahhh!&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>battle_kiriage2</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>sui_0215</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>Sui$&quot;Wow, wow! W-wait!&quot;@</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>s_yure_t</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>s_yure_r</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>kann0036</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>Kannei$&quot;Ggh, you block well. I guess they don&apos;t call you Bachou the Splendid for nothing.&quot;@</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>sui_0216</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>Sui$&quot;Dammit! Quit hopping around like some rabbit! Can&apos;t you fight normally?!&quot;@</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>tcg_kne11da</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>kann0037</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>Kannei$&quot;No thanks. I&apos;m not masochistic enough to take on a stupid woman&apos;s animalistic strength head on.&quot;@</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_bac22ha</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>tcg_bac22m</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>sui_0217</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>Sui$&quot;A-Animalistic strength...... I&apos;m an animal......&quot;@</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>tcg_kne11ea</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>kann0038</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>Kannei$&quot;What? Didn&apos;t you know that?&quot;@</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_bac32fa</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>tcg_bac32m</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>sui_0218</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>Sui$&quot;......Sh-shut up!&quot;@</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>SE014</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>tcg_bac41aa</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>tcg_bac41m</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>sui_0219</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>Sui$&quot;How dare you call a beautiful maiden an animal?!&quot;@</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_bac91aa</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>tcg_bac91m</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>sui_0220</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>Sui$&quot;Yahhhhhh!&quot;@</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>battle_kesa</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>tcg_kne31aa</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_kne31m</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>kann0039</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>Kannei$&quot;Hmph......&quot;@</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>SE081</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>yure_t</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>tcg_kne11ca</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>kann0040</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>Kannei$&quot;You made a deep crater in the ground with just one blow. ...You do have an animal&apos;s strength.&quot;@</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>sui_0221</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>Sui$&quot;Y-you said it again! You said I have an animal&apos;s strength again! You...... you&apos;re going to pay for that!&quot;@</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_kne11ea</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>kann0041</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>Kannei$&quot;Heh...... what are you going to do?&quot;@</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>tcg_bac41aa</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>tcg_bac41m</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>sui_0222</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>Sui$&quot;There&apos;s only one thing to do! I&apos;m going to blow you away!&quot;@</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>tcg_bac91aa</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>tcg_bac91m</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>sui_0223</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>Sui$&quot;Take this, you bitch!&quot;@</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>battle_kesa</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>SE081</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>s_yure_t</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_bac91aa</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>tcg_bac91m</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>sui_0224</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>Sui$&quot;Here comes more!&quot;@</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>battle_kabuto</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>SE081</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>s_yure_t</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>tcg_bac91aa</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>tcg_bac91m</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>sui_0225</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>Sui$&quot;One more!&quot;@</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>battle_kiriage</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>tcg_kne51aa</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>tcg_kne51m</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>kann0042</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>Kannei$&quot;Guh......!&quot;@</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>SE081</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>yure_t</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>kann0043</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>Kannei$&quot;Tsk...... pretty good.&quot;@</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>sui_0226</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>Sui$&quot;Dammit. Quit moving around and let me blow you away!&quot;@</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>tcg_kne11ea</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>kann0044</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>Kannei$&quot;Heh...... If you want to blow me away that badly, then why don&apos;t you try hitting me first?&quot;@</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>sui_0227</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>Sui$&quot;I can&apos;t because you move too much!&quot;@</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>kann0045</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>Kannei$&quot;This is how I fight. But...... this is taking a bit too long. It&apos;s about time to settle this. ......The next attack will end this.&quot;@</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>tcg_bac32fa</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>tcg_bac32m</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>sui_0228</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>Sui$&quot;Okay, come on! I accept the challenge!&quot;@</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>kann0046</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>Kannei$&quot;All right, Bachou the Splendid! Your head is mine!&quot;@</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>SE013</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>ecg_kne01a</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>kann0047</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>Kannei$&quot;Yahhhh!&quot;@</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>sui_0229</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>Sui$&quot;Kkh......!&quot;@</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>SE079</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>tcg_bac22da</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>tcg_bac22m</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>sui_0230</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>Sui$&quot;Huh? Hey? What about the attack......?&quot;@</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>tcg_kne11ea</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>kann0048</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>Kannei$&quot;......Heh. Bachou the Splendid, we&apos;ll meet again.&quot;@</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>sui_0231</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>Sui$&quot;Oooo...... she pretended to attack, and took that chance to run......&quot;@</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>tcg_bac22na</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>tcg_bac22m</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>sui_0232</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>Sui$&quot;......Oooo, she made a fool out of me! Whahhhhhh!&quot;@</ToTranslate>
  </String>
