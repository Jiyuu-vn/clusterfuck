  <String index="661">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="662">
    <ToTranslate>aisy3221</ToTranslate>
  </String>
  <String index="663">
    <ToTranslate>Aisha$&quot;Rinrin! Sui! Can you lend us a hand?!&quot;@</ToTranslate>
  </String>
  <String index="664">
    <ToTranslate>$Um, maybe not. I don&apos;t want to get scolded by Aisha anymore today.@</ToTranslate>
  </String>
  <String index="665">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="666">
    <ToTranslate>tcg_cho21ia</ToTranslate>
  </String>
  <String index="667">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="668">
    <ToTranslate>rinr2043</ToTranslate>
  </String>
  <String index="669">
    <ToTranslate>Rinrin$&quot;Looks like you&apos;ve got enough hands already. Here!&quot;@</ToTranslate>
  </String>
  <String index="670">
    <ToTranslate>SE007</ToTranslate>
  </String>
  <String index="671">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="672">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="673">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="674">
    <ToTranslate>aisy3222</ToTranslate>
  </String>
  <String index="675">
    <ToTranslate>Aisha$&quot;You&apos;re always like that. You never try to help...&quot;@</ToTranslate>
  </String>
  <String index="676">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="677">
    <ToTranslate>tcg_cho31ga</ToTranslate>
  </String>
  <String index="678">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="679">
    <ToTranslate>rinr2044</ToTranslate>
  </String>
  <String index="680">
    <ToTranslate>Rinrin$&quot;Nah, eight times.&quot;@</ToTranslate>
  </String>
  <String index="681">
    <ToTranslate>tcg_bac11ba</ToTranslate>
  </String>
  <String index="682">
    <ToTranslate>tcg_bac11m</ToTranslate>
  </String>
  <String index="683">
    <ToTranslate>sui_2456</ToTranslate>
  </String>
  <String index="684">
    <ToTranslate>Sui$&quot;Ha ha ha! That was pathetic.&quot;@</ToTranslate>
  </String>
  <String index="685">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="686">
    <ToTranslate>tcg_kan11qa</ToTranslate>
