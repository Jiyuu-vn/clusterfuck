  <String index="439">
    <ToTranslate>bgm26</ToTranslate>
  </String>
  <String index="440">
    <ToTranslate>$I left a bit late. I heard many voices when I made it out of the palace.@</ToTranslate>
  </String>
  <String index="441">
    <ToTranslate>Kazuto$&quot;Sorry I&apos;m late.&quot;@</ToTranslate>
  </String>
  <String index="442">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="443">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="444">
    <ToTranslate>rinr2036</ToTranslate>
  </String>
  <String index="445">
    <ToTranslate>Rinrin$&quot;Oh, we were just saying we might have to go get you.&quot;@</ToTranslate>
  </String>
  <String index="446">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="447">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="448">
    <ToTranslate>sei_2384</ToTranslate>
  </String>
  <String index="449">
    <ToTranslate>Sei$&quot;You must&apos;ve dozed off.&quot;@</ToTranslate>
  </String>
  <String index="450">
    <ToTranslate>$Sei sounded as harsh as ever. Rinrin was next to her and Sui, Shion, and Shuri were in the back...@</ToTranslate>
  </String>
  <String index="451">
    <ToTranslate>kyara_kesi</ToTranslate>
