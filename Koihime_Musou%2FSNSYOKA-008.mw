  <String index="524">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="525">
    <ToTranslate>rinr2038</ToTranslate>
  </String>
  <String index="526">
    <ToTranslate>Rinrin $&quot;We made it! The river!&quot;@</ToTranslate>
  </String>
  <String index="527">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="528">
