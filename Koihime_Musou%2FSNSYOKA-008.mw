  <String index="688">
    <ToTranslate>muka</ToTranslate>
  </String>
  <String index="689">
    <ToTranslate>aisy3223</ToTranslate>
  </String>
  <String index="690">
    <ToTranslate>Aisha$&quot;Listeeen!&quot;@</ToTranslate>
  </String>
  <String index="691">
    <ToTranslate>Kazuto$&quot;Whoa, whoa, calm down, Aisha.&quot;@</ToTranslate>
  </String>
  <String index="692">
    <ToTranslate>$She almost spilled the water she just scooped. I rushed over to her and tapped her back.@</ToTranslate>
  </String>
  <String index="693">
    <ToTranslate>Kazuto$&quot;Don&apos;t be so upset. Things will go smoother if they don&apos;t help.&quot;@</ToTranslate>
  </String>
