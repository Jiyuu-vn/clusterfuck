    <ToTranslate>tcg_cho21ia</ToTranslate>
  </String>
  <String index="787">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="788">
    <ToTranslate>rinr2048</ToTranslate>
  </String>
  <String index="789">
    <ToTranslate>Rinrin$&quot;*Munch.* Nyahaha, delicious. I&apos;ll have all this boiled meat. *Munch, munch.*&quot;@</ToTranslate>
  </String>
  <String index="790">
    <ToTranslate>tcg_syo21ma</ToTranslate>
  </String>
  <String index="791">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="792">
    <ToTranslate>syur1340</ToTranslate>
  </String>
  <String index="793">
    <ToTranslate>Shuri$&quot;No, th-that&apos;s for Our Lord...&quot;@</ToTranslate>
  </String>
  <String index="794">
    <ToTranslate>Kazuto$&quot;I&apos;m having it. This pork is great.&quot; @</ToTranslate>
  </String>
  <String index="795">
    <ToTranslate>tcg_syo21fa</ToTranslate>
  </String>
  <String index="796">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="797">
    <ToTranslate>syur1341</ToTranslate>
  </String>
  <String index="798">
    <ToTranslate>Shuri$&quot;Hau. Thank you!&quot;@</ToTranslate>
  </String>
  <String index="799">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="800">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="801">
    <ToTranslate>sei_2390</ToTranslate>
  </String>
  <String index="802">
    <ToTranslate>Sei$&quot;Don&apos;t be so greedy, people. I just made a fire. Let&apos;s boil water, make tea, and relax...&quot;@</ToTranslate>
  </String>
  <String index="803">
    <ToTranslate>tcg_bac11aa</ToTranslate>
  </String>
  <String index="804">
    <ToTranslate>tcg_bac11m</ToTranslate>
  </String>
  <String index="805">
    <ToTranslate>sui_2461</ToTranslate>
  </String>
  <String index="806">
    <ToTranslate>Sui$&quot;*Munch, munch!*&quot;@</ToTranslate>
  </String>
