  <String index="751">
    <ToTranslate>tcg_cho31ma</ToTranslate>
  </String>
  <String index="752">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="753">
    <ToTranslate>rinr2046</ToTranslate>
  </String>
  <String index="754">
    <ToTranslate>Rinrin$&quot;Okay! Lunch, lunch, lunch!&quot;@</ToTranslate>
  </String>
  <String index="755">
    <ToTranslate>tcg_bac11aa</ToTranslate>
  </String>
  <String index="756">
    <ToTranslate>tcg_bac11m</ToTranslate>
  </String>
  <String index="757">
    <ToTranslate>sui_2458</ToTranslate>
  </String>
  <String index="758">
    <ToTranslate>Sui$&quot;All right, we&apos;ll finish this later. Where&apos;s my seat?! There&apos;s nowhere for me. Prepare one!&quot;@</ToTranslate>
  </String>
  <String index="759">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="760">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="761">
    <ToTranslate>aisy3227</ToTranslate>
  </String>
  <String index="762">
    <ToTranslate>Aisha$&quot;...You demand that even though you didn&apos;t help us?&quot;@</ToTranslate>
  </String>
  <String index="763">
    <ToTranslate>$Rinrin and Sui didn&apos;t hear Aisha&apos;s gripes. I didn&apos;t quite have the nerve to force a smile.@</ToTranslate>
  </String>
  <String index="764">
    <ToTranslate>tcg_koc11aa</ToTranslate>
  </String>
  <String index="765">
    <ToTranslate>tcg_koc11m</ToTranslate>
  </String>
  <String index="766">
    <ToTranslate>shio1971</ToTranslate>
  </String>
  <String index="767">
    <ToTranslate>Shion$&quot;Sui-chan, sit here.&quot;@</ToTranslate>
  </String>
  <String index="768">
    <ToTranslate>tcg_hg21da</ToTranslate>
  </String>
  <String index="769">
    <ToTranslate>tcg_hg21m</ToTranslate>
  </String>
  <String index="770">
    <ToTranslate>chos0394</ToTranslate>
  </String>
  <String index="771">
    <ToTranslate>Chousen$&quot;Here? I want to sit next to My Lord.&quot;@</ToTranslate>
  </String>
  <String index="772">
    <ToTranslate>tcg_bac21da</ToTranslate>
  </String>
  <String index="773">
    <ToTranslate>tcg_bac21m</ToTranslate>
  </String>
  <String index="774">
    <ToTranslate>sui_2459</ToTranslate>
  </String>
  <String index="775">
    <ToTranslate>Sui$&quot;What?! I-It&apos;ll ruin the taste...&quot;@</ToTranslate>
  </String>
  <String index="776">
    <ToTranslate>Kazuto$&quot;Okay, let&apos;s eat.&quot;@</ToTranslate>
  </String>
  <String index="777">
    <ToTranslate>$I won&apos;t let him sit by me. Aisha and Shuri protected my spot.@</ToTranslate>
  </String>
  <String index="778">
    <ToTranslate>Kazuto$&quot;Everybody, I would like to thank you for your constant support. Let&apos;s relax and have fun today.&quot;@</ToTranslate>
  </String>
  <String index="779">
    <ToTranslate>$I looked at each of them. Then I put my palms together.@</ToTranslate>
  </String>
  <String index="780">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="781">
    <ToTranslate>Kazuto$&quot;All right. For what we&apos;re about to eat...&quot;@</ToTranslate>
  </String>
  <String index="782">
    <ToTranslate>gousei0038</ToTranslate>
  </String>
  <String index="783">
    <ToTranslate>gousei0038</ToTranslate>
  </String>
  <String index="784">
    <ToTranslate>All$&quot;I show my gratitude.&quot;@</ToTranslate>
  </String>
  <String index="785">
    <ToTranslate>$Everybody echoed me and... a war-like lunch begins.@</ToTranslate>
  </String>
