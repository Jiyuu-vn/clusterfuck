  </String>
  <String index="650">
    <ToTranslate>rinr2042</ToTranslate>
  </String>
  <String index="651">
    <ToTranslate>Rinrin$&quot;Seventeen times! That was awesome!&quot;@</ToTranslate>
  </String>
  <String index="652">
    <ToTranslate>tcg_bac11ea</ToTranslate>
  </String>
  <String index="653">
    <ToTranslate>tcg_bac11m</ToTranslate>
  </String>
  <String index="654">
    <ToTranslate>sui_2455</ToTranslate>
  </String>
  <String index="655">
    <ToTranslate>Sui$&quot;Shoot. Now look at this... here!&quot;@</ToTranslate>
  </String>
  <String index="656">
    <ToTranslate>SE007</ToTranslate>
  </String>
  <String index="657">
    <ToTranslate>$They&apos;re heating up.@</ToTranslate>
  </String>
  <String index="658">
    <ToTranslate>$Should I mingle with them now...?@</ToTranslate>
  </String>
  <String index="659">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="660">
    <ToTranslate>tcg_kan11ea</ToTranslate>
