  <String index="630">
    <ToTranslate>aisy3220</ToTranslate>
  </String>
  <String index="631">
    <ToTranslate>Aisha $&quot;Wow, who&apos;s lifting the rock?! Are you going to kill us?&quot;@</ToTranslate>
  </String>
  <String index="632">
    <ToTranslate>$I don&apos;t see him. I don&apos;t see him.@</ToTranslate>
  </String>
  <String index="633">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="634">
    <ToTranslate>$I look away... hmm?@</ToTranslate>
  </String>
  <String index="635">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="636">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="637">
    <ToTranslate>rinr2040</ToTranslate>
  </String>
  <String index="638">
    <ToTranslate>Rinrin$&quot;Uh, this one looks good because it&apos;s flat. It&apos;ll bounce well. Okay, ten rounds. The loser will buy the winner thirty steamed beef buns, got it?&quot;@</ToTranslate>
  </String>
  <String index="639">
    <ToTranslate>tcg_bac11ba</ToTranslate>
