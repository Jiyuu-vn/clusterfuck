  <String index="623">
    <ToTranslate>tcg_hg51m</ToTranslate>
  </String>
  <String index="624">
    <ToTranslate>chos0392</ToTranslate>
  </String>
  <String index="625">
    <ToTranslate>Chousen$&quot;Hrrnnnnn!&quot;@</ToTranslate>
  </String>
  <String index="626">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="627">
