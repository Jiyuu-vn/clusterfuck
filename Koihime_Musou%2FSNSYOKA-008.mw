  <String index="604">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="605">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="606">
    <ToTranslate>syur1336</ToTranslate>
  </String>
  <String index="607">
    <ToTranslate>Shuri$&quot;Okay. I&apos;ll hurry up and get it ready. Shion-san!&quot;@</ToTranslate>
  </String>
  <String index="608">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="609">
    <ToTranslate>tcg_koc11ba</ToTranslate>
  </String>
  <String index="610">
    <ToTranslate>tcg_koc11m</ToTranslate>
  </String>
  <String index="611">
    <ToTranslate>shio1968</ToTranslate>
  </String>
  <String index="612">
    <ToTranslate>Shion$&quot;Okay. Lunch is here.&quot;@</ToTranslate>
  </String>
  <String index="613">
    <ToTranslate>$Shion was carrying a large package. Shuri trotted up to her, and Aisha and Sei fetched some water.@</ToTranslate>
  </String>
