  <String index="519">
    <ToTranslate>SE053</ToTranslate>
  </String>
  <String index="520">
    <ToTranslate>$These people are all marvelous fighters.@</ToTranslate>
  </String>
  <String index="521">
    <ToTranslate>$I&apos;m almost oblivious to the fact myself.@</ToTranslate>
  </String>
  <String index="522">
    <ToTranslate>tcg_cho31ma</ToTranslate>
  </String>
  <String index="523">
    <ToTranslate>tcg_cho31m</ToTranslate>
