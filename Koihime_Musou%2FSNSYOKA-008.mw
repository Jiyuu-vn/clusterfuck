    <ToTranslate>tcg_koc31ia</ToTranslate>
  </String>
  <String index="615">
    <ToTranslate>tcg_koc31m</ToTranslate>
  </String>
  <String index="616">
    <ToTranslate>shio1969</ToTranslate>
  </String>
  <String index="617">
    <ToTranslate>Shion$&quot;This boulder&apos;s in the way, isn&apos;t it? Chousen? Could you?&quot;@</ToTranslate>
  </String>
