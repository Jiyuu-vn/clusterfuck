    <ToTranslate>tcg_bac11m</ToTranslate>
  </String>
  <String index="641">
    <ToTranslate>sui_2454</ToTranslate>
  </String>
  <String index="642">
    <ToTranslate>Sui$&quot;Bring it on. I&apos;ll drain all of your allowance!&quot;@</ToTranslate>
  </String>
  <String index="643">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="644">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="645">
    <ToTranslate>rinr2041</ToTranslate>
  </String>
  <String index="646">
    <ToTranslate>Rinrin$&quot;That&apos;s my line! Here I go, nyaaaa!&quot; @</ToTranslate>
  </String>
  <String index="647">
    <ToTranslate>SE007</ToTranslate>
  </String>
  <String index="648">
    <ToTranslate>tcg_cho21ia</ToTranslate>
