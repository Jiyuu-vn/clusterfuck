  <Line StartPos="3931" Length="7" Skip="false">
    <Translated>Servant</Translated>
  </Line>
  <Line StartPos="3951" Length="13" Skip="false">
    <Translated>&quot;P-Please...&quot;</Translated>
  </Line>
  <Line StartPos="3979" Length="5" Skip="false">
    <Translated>Kitty</Translated>
  </Line>
  <Line StartPos="3997" Length="28" Skip="false">
    <Translated>&quot;Wow, this place is pretty.&quot;</Translated>
