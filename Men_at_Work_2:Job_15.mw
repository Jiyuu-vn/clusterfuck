  <Line StartPos="4118" Length="7" Skip="false">
    <Translated>Servant</Translated>
  </Line>
  <Line StartPos="4138" Length="19" Skip="false">
    <Translated>&quot;W-What was that!?&quot;</Translated>
  </Line>
  <Line StartPos="4172" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="4189" Length="27" Skip="false">
