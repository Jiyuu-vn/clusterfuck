    <ToTranslate>tcg_gan12ga</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>ganr0066</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>Ganryou$&quot;Here I am too!&quot;@</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>aisy0473</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>Aisha$&quot;You too... Huh, Ganryou. Good, I&apos;ll fight both of you at the same time.&quot;@</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>tcg_bun12ea</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_gan12ga</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>buns0058</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>Bunshuu$&quot;Don&apos;t underestimate me! I don&apos;t need Toshi to fight you. I can take you down in a snap on my own!&quot;@</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
