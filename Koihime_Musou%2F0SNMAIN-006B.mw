  <String index="1664">
    <ToTranslate>riri0009</ToTranslate>
  </String>
  <String index="1665">
    <ToTranslate>Riri$&quot;Mommy!&quot;@</ToTranslate>
  </String>
  <String index="1666">
    <ToTranslate>$It was an innocent voice that didn&apos;t belong on a battlefield. The little girl ran up to Kouchuu.@</ToTranslate>
  </String>
