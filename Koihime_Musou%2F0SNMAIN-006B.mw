  <String index="795">
    <ToTranslate>sui_0011</ToTranslate>
  </String>
  <String index="796">
    <ToTranslate>Sui$&quot;A daughter? Do you mean Kouchuu&apos;s daughter?&quot;@</ToTranslate>
