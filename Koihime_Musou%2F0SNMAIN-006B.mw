  <String index="1551">
    <ToTranslate>rinr0317</ToTranslate>
  </String>
  <String index="1552">
    <ToTranslate>Rinrin$&quot;Okay. Riri, are you okay? Are you scared?&quot;@</ToTranslate>
  </String>
  <String index="1553">
    <ToTranslate>riri0006</ToTranslate>
  </String>
  <String index="1554">
    <ToTranslate>Riri$&quot;......(Shake)&quot;@</ToTranslate>
  </String>
  <String index="1555">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="1556">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="1557">
    <ToTranslate>rinr0318</ToTranslate>
  </String>
  <String index="1558">
    <ToTranslate>Rinrin$&quot;I see. You&apos;re strong, Riri.&quot;@</ToTranslate>
  </String>
  <String index="1559">
    <ToTranslate>riri0007</ToTranslate>
  </String>
  <String index="1560">
    <ToTranslate>Riri$&quot;That&apos;s because I have to rescue Mommy. I can&apos;t be scared.&quot;@</ToTranslate>
  </String>
  <String index="1561">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="1562">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="1563">
    <ToTranslate>sei_0028</ToTranslate>
  </String>
  <String index="1564">
    <ToTranslate>Sei$&quot;That&apos;s the spirit. I respect you for that. We&apos;ll protect you with our lives, Riri-dono.&quot;@</ToTranslate>
  </String>
  <String index="1565">
    <ToTranslate>tcg_rr11fa</ToTranslate>
