  <String index="221">
    <ToTranslate>aisy0479</ToTranslate>
  </String>
  <String index="222">
    <ToTranslate>Aisha$&quot;Ggh......I won&apos;t let you get away!&quot;@</ToTranslate>
  </String>
  <String index="223">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="224">
    <ToTranslate>tcg_zen11aa</ToTranslate>
