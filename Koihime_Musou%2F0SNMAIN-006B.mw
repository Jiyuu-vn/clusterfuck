  <String index="888">
    <ToTranslate>aisy0506</ToTranslate>
  </String>
  <String index="889">
    <ToTranslate>Aisha$&quot;As you wish!&quot;@</ToTranslate>
  </String>
  <String index="890">
    <ToTranslate>Kazuto$&quot;It&apos;s pretty dangerous...... but I know you can do it.&quot;@</ToTranslate>
