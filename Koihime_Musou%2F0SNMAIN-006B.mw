  <String index="661">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="662">
    <ToTranslate>Kazuto$&quot;Well...... the city is surrounded, but what do we do now?&quot;@</ToTranslate>
  </String>
