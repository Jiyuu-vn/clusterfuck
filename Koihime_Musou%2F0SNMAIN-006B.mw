  <String index="1468">
    <ToTranslate>sei_0023</ToTranslate>
  </String>
  <String index="1469">
    <ToTranslate>Sei$&quot;My name is Chou Shiryuu. I&apos;ve been looking for you so I can save Kouchuu-dono.&quot;@</ToTranslate>
  </String>
  <String index="1470">
    <ToTranslate>riri0002</ToTranslate>
  </String>
  <String index="1471">
    <ToTranslate>Girl$&quot;Are you going to save mommy?&quot;@</ToTranslate>
  </String>
  <String index="1472">
    <ToTranslate>rinr0310</ToTranslate>
  </String>
  <String index="1473">
    <ToTranslate>Rinrin$&quot;Yes! So, can you come with us?&quot;@</ToTranslate>
  </String>
  <String index="1474">
    <ToTranslate>riri0003</ToTranslate>
