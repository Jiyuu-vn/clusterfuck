  </String>
  <String index="95">
    <ToTranslate>aisy0474</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>Aisha$&quot;How brave. Come! Bunshuu!&quot;@</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
