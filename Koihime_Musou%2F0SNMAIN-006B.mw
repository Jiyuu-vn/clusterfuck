  <String index="783">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="784">
    <ToTranslate>rinr0277</ToTranslate>
  </String>
  <String index="785">
    <ToTranslate>Rinrin$&quot;What kind of a message is it?&quot;@</ToTranslate>
