  <String index="261">
    <ToTranslate>bgm23</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>tcg_bun22fa</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>tcg_gan12ga</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>buns0066</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>Bunshuu$&quot;Hah, hah, hah......&quot;@</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>ganr0075</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>Ganryou$&quot;Hah, hah, hah...... do you think they&apos;re okay?&quot;@</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>tcg_bun12da</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>buns0067</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>Bunshuu$&quot;I don&apos;t know, but they should be okay. I noticed Kan&apos;u was holding back a little.&quot;@</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>tcg_gan22ea</ToTranslate>
  </String>
  <String index="275">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="276">
    <ToTranslate>ganr0076</ToTranslate>
  </String>
  <String index="277">
    <ToTranslate>Ganryou$&quot;She was using the back of her blade...... Hahhh...... I thought I was dead.&quot;@</ToTranslate>
  </String>
  <String index="278">
    <ToTranslate>tcg_bun22ha</ToTranslate>
  </String>
  <String index="279">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="280">
    <ToTranslate>buns0068</ToTranslate>
  </String>
  <String index="281">
    <ToTranslate>Bunshuu$&quot;She isn&apos;t just strong. As you said, she is a monster!&quot;@</ToTranslate>
  </String>
  <String index="282">
    <ToTranslate>tcg_gan12ca</ToTranslate>
  </String>
  <String index="283">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="284">
    <ToTranslate>ganr0077</ToTranslate>
  </String>
  <String index="285">
    <ToTranslate>Ganryou$&quot;I can&apos;t believe we couldn&apos;t even touch her...... Oooo...... I&apos;m mortified......&quot;@</ToTranslate>
  </String>
  <String index="286">
    <ToTranslate>tcg_bun22fa</ToTranslate>
  </String>
  <String index="287">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="288">
    <ToTranslate>buns0069</ToTranslate>
  </String>
  <String index="289">
    <ToTranslate>Bunshuu$&quot;Really? That was enough for me. She&apos;s out of my league, plain and simple.&quot;@</ToTranslate>
  </String>
  <String index="290">
    <ToTranslate>tcg_gan12ka</ToTranslate>
  </String>
  <String index="291">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="292">
    <ToTranslate>ganr0078</ToTranslate>
  </String>
  <String index="293">
    <ToTranslate>Ganryou$&quot;I know, but still...... By the way, what should we do, Bun-chan?&quot;@</ToTranslate>
  </String>
  <String index="294">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="295">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="296">
    <ToTranslate>buns0070</ToTranslate>
  </String>
  <String index="297">
    <ToTranslate>Bunshuu$&quot;Don&apos;t you know? There&apos;s only one thing to do.&quot;@</ToTranslate>
  </String>
  <String index="298">
    <ToTranslate>tcg_gan12aa</ToTranslate>
  </String>
  <String index="299">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="300">
    <ToTranslate>ganr0079</ToTranslate>
  </String>
  <String index="301">
    <ToTranslate>Ganryou$&quot;Only one thing? Ah, return to Nanpi, huh?&quot;@</ToTranslate>
  </String>
  <String index="302">
    <ToTranslate>tcg_bun22fa</ToTranslate>
  </String>
  <String index="303">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="304">
    <ToTranslate>buns0071</ToTranslate>
  </String>
  <String index="305">
    <ToTranslate>Bunshuu$&quot;Huh? What are you talking about? Can&apos;t you imagine how furious the Princess will be if we come back like this?&quot;@</ToTranslate>
  </String>
  <String index="306">
    <ToTranslate>tcg_gan12ca</ToTranslate>
  </String>
  <String index="307">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="308">
    <ToTranslate>ganr0080</ToTranslate>
  </String>
  <String index="309">
    <ToTranslate>Ganryou$&quot;I know, but......&quot;@</ToTranslate>
  </String>
  <String index="310">
    <ToTranslate>tcg_bun12ea</ToTranslate>
  </String>
  <String index="311">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="312">
    <ToTranslate>bgm28</ToTranslate>
  </String>
  <String index="313">
    <ToTranslate>buns0072</ToTranslate>
  </String>
  <String index="314">
    <ToTranslate>Bunshuu$&quot;We don&apos;t go back. We run!&quot;@</ToTranslate>
  </String>
  <String index="315">
    <ToTranslate>tcg_gan22da</ToTranslate>
  </String>
  <String index="316">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="317">
    <ToTranslate>ganr0081</ToTranslate>
  </String>
  <String index="318">
    <ToTranslate>Ganryou$&quot;What!?&quot;@</ToTranslate>
  </String>
  <String index="319">
    <ToTranslate>tcg_bun22ha</ToTranslate>
  </String>
  <String index="320">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="321">
    <ToTranslate>buns0073</ToTranslate>
  </String>
  <String index="322">
    <ToTranslate>Bunshuu$&quot;Well, if we don&apos;t, then we&apos;ll be severely punished by the Princess, right? We can only run.&quot;@</ToTranslate>
  </String>
  <String index="323">
    <ToTranslate>tcg_gan12ca</ToTranslate>
  </String>
  <String index="324">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="325">
    <ToTranslate>ganr0082</ToTranslate>
  </String>
  <String index="326">
    <ToTranslate>Ganryou$&quot;Oooo......&quot;@</ToTranslate>
  </String>
  <String index="327">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="328">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="329">
    <ToTranslate>buns0074</ToTranslate>
  </String>
  <String index="330">
    <ToTranslate>Bunshuu$&quot;Hey, do you want to be punished?&quot;@</ToTranslate>
  </String>
  <String index="331">
    <ToTranslate>tcg_gan12ha</ToTranslate>
  </String>
  <String index="332">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="333">
    <ToTranslate>ganr0083</ToTranslate>
  </String>
  <String index="334">
    <ToTranslate>Ganryou$&quot;No.&quot;@</ToTranslate>
  </String>
  <String index="335">
    <ToTranslate>tcg_bun22ba</ToTranslate>
  </String>
  <String index="336">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="337">
    <ToTranslate>buns0075</ToTranslate>
  </String>
  <String index="338">
    <ToTranslate>Bunshuu$&quot;Okay, then it&apos;s decided.&quot;@</ToTranslate>
  </String>
  <String index="339">
    <ToTranslate>tcg_gan22ia</ToTranslate>
  </String>
  <String index="340">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="341">
    <ToTranslate>ganr0084</ToTranslate>
  </String>
  <String index="342">
    <ToTranslate>Ganryou$&quot;Ooo...... But... but... Even if we run, what do we do after that, Iishe?&quot;@</ToTranslate>
  </String>
  <String index="343">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="344">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="345">
    <ToTranslate>buns0076</ToTranslate>
  </String>
  <String index="346">
    <ToTranslate>Bunshuu$&quot;Let&apos;s see...... We&apos;ll be free, at least for now. We don&apos;t need to worry about protecting the Princess, and we can do whatever we want to do.&quot;@</ToTranslate>
  </String>
  <String index="347">
    <ToTranslate>tcg_gan12aa</ToTranslate>
  </String>
  <String index="348">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="349">
    <ToTranslate>ganr0085</ToTranslate>
  </String>
  <String index="350">
    <ToTranslate>Ganryou$&quot;Yeah.&quot;@</ToTranslate>
  </String>
  <String index="351">
    <ToTranslate>tcg_bun22ca</ToTranslate>
  </String>
  <String index="352">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="353">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="354">
    <ToTranslate>buns0077</ToTranslate>
  </String>
  <String index="355">
    <ToTranslate>Bunshuu$&quot;Then there is only one thing to do!&quot;@</ToTranslate>
  </String>
  <String index="356">
    <ToTranslate>tcg_gan22ia</ToTranslate>
  </String>
  <String index="357">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="358">
    <ToTranslate>ganr0086</ToTranslate>
  </String>
  <String index="359">
    <ToTranslate>Ganryou$&quot;Eh? What is it?&quot;@</ToTranslate>
  </String>
  <String index="360">
    <ToTranslate>tcg_bun22ba</ToTranslate>
  </String>
  <String index="361">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="362">
    <ToTranslate>buns0078</ToTranslate>
  </String>
  <String index="363">
    <ToTranslate>Bunshuu$&quot;We&apos;ll set off on a journey to the legendary paradise in the South! It&apos;ll be our grand adventure!&quot;@</ToTranslate>
  </String>
  <String index="364">
    <ToTranslate>tcg_gan22da</ToTranslate>
  </String>
  <String index="365">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="366">
    <ToTranslate>ase</ToTranslate>
  </String>
  <String index="367">
    <ToTranslate>ganr0087</ToTranslate>
  </String>
  <String index="368">
    <ToTranslate>Ganryou$&quot;An adventure?&quot;@</ToTranslate>
  </String>
  <String index="369">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="370">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="371">
    <ToTranslate>buns0079</ToTranslate>
  </String>
  <String index="372">
    <ToTranslate>Bunshuu$&quot;Yep. An adventure!&quot;@</ToTranslate>
  </String>
  <String index="373">
    <ToTranslate>ganr0088</ToTranslate>
  </String>
  <String index="374">
    <ToTranslate>Ganryou$&quot;......Are you serious, Bun-chan?&quot;@</ToTranslate>
  </String>
  <String index="375">
    <ToTranslate>tcg_bun22ba</ToTranslate>
  </String>
  <String index="376">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="377">
    <ToTranslate>buns0080</ToTranslate>
  </String>
  <String index="378">
    <ToTranslate>Bunshuu$&quot;Very serious! If we can find the legendary paradise, don&apos;t you think the rest of our lives will be perfect?&quot;@</ToTranslate>
  </String>
  <String index="379">
    <ToTranslate>buns0081</ToTranslate>
  </String>
  <String index="380">
    <ToTranslate>Bunshuu$&quot;According to the rumors, the houses and everything else there are all made of gold, and there is plenty of food. Wow! We&apos;d be living like nobles, don&apos;t you think?&quot;@</ToTranslate>
  </String>
  <String index="381">
    <ToTranslate>tcg_gan12ca</ToTranslate>
  </String>
  <String index="382">
    <ToTranslate>tcg_gan12m</ToTranslate>
  </String>
  <String index="383">
    <ToTranslate>ganr0089</ToTranslate>
  </String>
  <String index="384">
    <ToTranslate>Ganryou$&quot;Don&apos;t present it as a question.&quot;@</ToTranslate>
  </String>
  <String index="385">
    <ToTranslate>tcg_bun22ca</ToTranslate>
  </String>
  <String index="386">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="387">
    <ToTranslate>buns0082</ToTranslate>
  </String>
  <String index="388">
    <ToTranslate>Bunshuu$&quot;No, no, seriously, that&apos;d be perfect! Don&apos;t you think, Toshi?&quot;@</ToTranslate>
  </String>
  <String index="389">
    <ToTranslate>tcg_gan22ea</ToTranslate>
  </String>
  <String index="390">
    <ToTranslate>tcg_gan22m</ToTranslate>
  </String>
  <String index="391">
    <ToTranslate>ganr0090</ToTranslate>
  </String>
  <String index="392">
    <ToTranslate>Ganryou$&quot;......You know, Bun-chan.&quot;@</ToTranslate>
  </String>
  <String index="393">
    <ToTranslate>tcg_bun12aa</ToTranslate>
  </String>
  <String index="394">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="395">
    <ToTranslate>buns0083</ToTranslate>
  </String>
  <String index="396">
    <ToTranslate>Bunshuu$&quot;Hmm? What is it?&quot;@</ToTranslate>
  </String>
  <String index="397">
    <ToTranslate>ganr0091</ToTranslate>
  </String>
  <String index="398">
    <ToTranslate>Ganryou$&quot;Don&apos;t they call it legendary because nobody can find it? You do understand that, right?&quot;@</ToTranslate>
  </String>
  <String index="399">
    <ToTranslate>buns0084</ToTranslate>
  </String>
  <String index="400">
    <ToTranslate>Bunshuu$&quot;That means that if we find it, it&apos;ll all be ours, OK?&quot;@</ToTranslate>
  </String>
  <String index="401">
    <ToTranslate>tenten</ToTranslate>
  </String>
  <String index="402">
    <ToTranslate>ganr0092</ToTranslate>
  </String>
  <String index="403">
    <ToTranslate>Ganryou$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="404">
    <ToTranslate>tcg_bun22ca</ToTranslate>
  </String>
  <String index="405">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="406">
    <ToTranslate>buns0085</ToTranslate>
  </String>
  <String index="407">
    <ToTranslate>Bunshuu$&quot;Oh my gosh! If that happens, we&apos;ll never have to live like this anymore! Good bye, my old life!&quot;@</ToTranslate>
  </String>
  <String index="408">
    <ToTranslate>buns0086</ToTranslate>
  </String>
  <String index="409">
    <ToTranslate>Bunshuu$&quot;We&apos;re talking gold! And plenty of it! We just have to find it!!&quot;@</ToTranslate>
  </String>
  <String index="410">
    <ToTranslate>ganr0093</ToTranslate>
  </String>
  <String index="411">
    <ToTranslate>Ganryou$&quot;......You&apos;re serious, aren&apos;t you?&quot;@</ToTranslate>
  </String>
  <String index="412">
    <ToTranslate>tcg_bun22ba</ToTranslate>
  </String>
  <String index="413">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="414">
    <ToTranslate>buns0087</ToTranslate>
  </String>
  <String index="415">
    <ToTranslate>Bunshuu$&quot;&apos;Course!&quot;@</ToTranslate>
  </String>
  <String index="416">
    <ToTranslate>ganr0094</ToTranslate>
  </String>
  <String index="417">
    <ToTranslate>Ganryou$&quot;Hahh......&quot;@</ToTranslate>
  </String>
  <String index="418">
    <ToTranslate>tcg_bun12ia</ToTranslate>
  </String>
  <String index="419">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="420">
    <ToTranslate>buns0088</ToTranslate>
  </String>
  <String index="421">
    <ToTranslate>Bunshuu$&quot;Hey, why do you keep sighing? Just trust me, okay?&quot;@</ToTranslate>
  </String>
  <String index="422">
    <ToTranslate>tcg_bun22ca</ToTranslate>
  </String>
  <String index="423">
    <ToTranslate>tcg_bun22m</ToTranslate>
  </String>
  <String index="424">
    <ToTranslate>buns0089</ToTranslate>
  </String>
  <String index="425">
    <ToTranslate>Bunshuu$&quot;Come on, let&apos;s go find the legendary paradise! Our adventure is about to begin!&quot;@</ToTranslate>
  </String>
  <String index="426">
    <ToTranslate>ganr0095</ToTranslate>
  </String>
  <String index="427">
    <ToTranslate>Ganryou$&quot;*sigh*......&quot;@</ToTranslate>
  </String>
