    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="776">
    <ToTranslate>Soldier$&quot;They asked me to deliver a message...&quot;@</ToTranslate>
  </String>
  <String index="777">
    <ToTranslate>Kazuto$&quot;A message for whom? Is it for me?&quot;@</ToTranslate>
  </String>
  <String index="778">
    <ToTranslate>Soldier$&quot;Yes, sir.&quot;@</ToTranslate>
