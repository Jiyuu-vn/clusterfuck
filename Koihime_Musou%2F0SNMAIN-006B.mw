  <String index="1657">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="1658">
    <ToTranslate>Kazuto$&quot;......! That&apos;s Chouun&apos;s voice! Aisha, everything went well!&quot;@</ToTranslate>
  </String>
  <String index="1659">
    <ToTranslate>$I realized our operation was a success, so I quickly called out to the fighting women.@</ToTranslate>
  </String>
  <String index="1660">
    <ToTranslate>$Another voice joined mine.@</ToTranslate>
  </String>
  <String index="1661">
    <ToTranslate>tcg_rr21ea</ToTranslate>
  </String>
  <String index="1662">
    <ToTranslate>tcg_rr21m</ToTranslate>
  </String>
  <String index="1663">
