  <String index="1529">
    <ToTranslate>rinr0314</ToTranslate>
  </String>
  <String index="1530">
    <ToTranslate>Rinrin$&quot;Hmm? Why is that?&quot;@</ToTranslate>
  </String>
  <String index="1531">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="1532">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="1533">
    <ToTranslate>sei_0025</ToTranslate>
  </String>
  <String index="1534">
    <ToTranslate>Sei$&quot;That&apos;s because I&apos;m sure Enshou has someone observing Kouchuu-dono to make sure she fights seriously.&quot;@</ToTranslate>
  </String>
  <String index="1535">
    <ToTranslate>tcg_cho31da</ToTranslate>
