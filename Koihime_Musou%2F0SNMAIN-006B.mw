  <String index="1571">
    <ToTranslate>sei_0029</ToTranslate>
  </String>
  <String index="1572">
    <ToTranslate>Sei$&quot;Mhmm ......What a well-mannered girl.&quot;@</ToTranslate>
  </String>
  <String index="1573">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="1574">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="1575">
    <ToTranslate>rinr0319</ToTranslate>
  </String>
  <String index="1576">
    <ToTranslate>Rinrin$&quot;We must finish the battle quickly for her sake, too.&quot;@</ToTranslate>
  </String>
  <String index="1577">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="1578">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="1579">
    <ToTranslate>sei_0030</ToTranslate>
  </String>
  <String index="1580">
    <ToTranslate>Sei$&quot;Yeah...... Let&apos;s go.&quot;@</ToTranslate>
