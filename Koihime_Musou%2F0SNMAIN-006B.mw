    <ToTranslate>rinr0282</ToTranslate>
  </String>
  <String index="993">
    <ToTranslate>Rinrin$&quot;Nyanyah?! What are you doing?&quot;@</ToTranslate>
  </String>
  <String index="994">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
