  </String>
  <String index="226">
    <ToTranslate>tcg_zen11aa</ToTranslate>
  </String>
  <String index="227">
    <ToTranslate>tcg_zen11m</ToTranslate>
  </String>
  <String index="228">
    <ToTranslate>Soldier$&quot;Oh no you don&apos;t!&quot;@</ToTranslate>
  </String>
  <String index="229">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="230">
    <ToTranslate>tcg_kan11fa</ToTranslate>
  </String>
  <String index="231">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="232">
    <ToTranslate>aisy0480</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>Aisha$&quot;Arrgh, move away! Do you want to die?&quot;@</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>tcg_zen11aa</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>tcg_zen11m</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>tcg_zen11aa</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>tcg_zen11m</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>Soldier$&quot;I&apos;ll be more than happy to die if it means they can escape!&quot;@</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>aisy0481</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>Aisha$&quot;What......? I see. They have loyal soldiers too, huh?&quot;@</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>aisy0482</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>Aisha$&quot;Fine, then in honor of your loyalty, I, Kan Unchou, will face you with everything I&apos;ve got.&quot;@</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>SE014</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>aisy0483</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>Aisha$&quot;Here I come!&quot;@</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>tcg_kan91aa</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>tcg_kan91m</ToTranslate>
  </String>
  <String index="256">
