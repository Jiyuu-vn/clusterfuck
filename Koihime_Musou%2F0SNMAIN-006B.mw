  <String index="1669">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="1670">
    <ToTranslate>tcg_rr21ea</ToTranslate>
  </String>
  <String index="1671">
    <ToTranslate>tcg_rr21m</ToTranslate>
  </String>
  <String index="1672">
    <ToTranslate>koch0014</ToTranslate>
  </String>
  <String index="1673">
    <ToTranslate>Kouchuu$&quot;Riri!&quot;@</ToTranslate>
  </String>
  <String index="1674">
    <ToTranslate>tcg_rr11ea</ToTranslate>
  </String>
  <String index="1675">
    <ToTranslate>tcg_rr11m</ToTranslate>
  </String>
  <String index="1676">
    <ToTranslate>riri0010</ToTranslate>
  </String>
  <String index="1677">
    <ToTranslate>Riri$&quot;Mommy!&quot;@</ToTranslate>
  </String>
  <String index="1678">
    <ToTranslate>tcg_koc22qa</ToTranslate>
  </String>
  <String index="1679">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1680">
    <ToTranslate>koch0015</ToTranslate>
  </String>
  <String index="1681">
    <ToTranslate>Kouchuu$&quot;Ahh...... you&apos;re safe...... I&apos;m so glad you&apos;re safe......&quot;@</ToTranslate>
  </String>
  <String index="1682">
    <ToTranslate>tcg_rr11da</ToTranslate>
  </String>
  <String index="1683">
    <ToTranslate>tcg_rr11m</ToTranslate>
  </String>
  <String index="1684">
    <ToTranslate>riri0011</ToTranslate>
  </String>
  <String index="1685">
    <ToTranslate>Riri$&quot;I was strong...... I was scared, but I didn&apos;t cry...... sniffle...... sniffle......&quot;@</ToTranslate>
  </String>
  <String index="1686">
    <ToTranslate>tcg_koc32ia</ToTranslate>
  </String>
  <String index="1687">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="1688">
    <ToTranslate>koch0016</ToTranslate>
  </String>
  <String index="1689">
    <ToTranslate>Kouchuu$&quot;I&apos;m sorry...... I&apos;m sorry to put you through that...... I&apos;m so sorry.&quot;@</ToTranslate>
  </String>
  <String index="1690">
    <ToTranslate>tcg_rr11ea</ToTranslate>
  </String>
  <String index="1691">
    <ToTranslate>tcg_rr11m</ToTranslate>
  </String>
  <String index="1692">
    <ToTranslate>riri0012</ToTranslate>
  </String>
  <String index="1693">
    <ToTranslate>Riri$&quot;Sniffle...... sniffle, I-I&apos;m okay. I wasn&apos;t scared...... wasn&apos;t scared at all...... whahhhhh......&quot;@</ToTranslate>
  </String>
  <String index="1694">
    <ToTranslate>tcg_koc22qa</ToTranslate>
  </String>
  <String index="1695">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1696">
    <ToTranslate>koch0017</ToTranslate>
  </String>
  <String index="1697">
    <ToTranslate>Kouchuu$&quot;You&apos;re okay...... you&apos;re okay now......&quot;@</ToTranslate>
  </String>
  <String index="1698">
    <ToTranslate>$Finally relieved, the little girl began to cry, and Kouchuu soothed her with a gentle voice.@</ToTranslate>
  </String>
  <String index="1699">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1700">
    <ToTranslate>tcg_cho31ma</ToTranslate>
  </String>
  <String index="1701">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="1702">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="1703">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="1704">
    <ToTranslate>rinr0324</ToTranslate>
  </String>
  <String index="1705">
    <ToTranslate>Rinrin$&quot;Heh-heh. What an emotional reunion!&quot;@</ToTranslate>
  </String>
  <String index="1706">
    <ToTranslate>sei_0036</ToTranslate>
  </String>
  <String index="1707">
    <ToTranslate>Sei$&quot;Hmm, mother and child are beautiful when they are together.&quot;@</ToTranslate>
  </String>
  <String index="1708">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1709">
    <ToTranslate>tcg_koc22da</ToTranslate>
  </String>
  <String index="1710">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1711">
    <ToTranslate>tcg_rr11da</ToTranslate>
  </String>
  <String index="1712">
    <ToTranslate>tcg_rr11m</ToTranslate>
  </String>
  <String index="1713">
    <ToTranslate>koch0018</ToTranslate>
  </String>
  <String index="1714">
    <ToTranslate>Kouchuu$&quot;......Who are you?&quot;@</ToTranslate>
  </String>
  <String index="1715">
    <ToTranslate>riri0013</ToTranslate>
  </String>
  <String index="1716">
    <ToTranslate>Riri$&quot;Sniffle...... Hey, Mommy, these are Rinrin-oneechan and Chouun-oneechan. They rescued me.&quot;@</ToTranslate>
  </String>
  <String index="1717">
    <ToTranslate>koch0019</ToTranslate>
  </String>
  <String index="1718">
    <ToTranslate>Kouchuu$&quot;Eh......? They rescued you?&quot;@</ToTranslate>
  </String>
  <String index="1719">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1720">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="1721">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="1722">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="1723">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="1724">
    <ToTranslate>sei_0037</ToTranslate>
  </String>
  <String index="1725">
    <ToTranslate>Sei$&quot;Yes. My name is Chouun. My common name is Shiryuu. I rescued Riri-dono for certain reasons. And this is......&quot;@</ToTranslate>
  </String>
  <String index="1726">
    <ToTranslate>rinr0325</ToTranslate>
  </String>
  <String index="1727">
    <ToTranslate>Rinrin$&quot;I&apos;m Chouhi.&quot;@</ToTranslate>
  </String>
  <String index="1728">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1729">
    <ToTranslate>tcg_koc22da</ToTranslate>
  </String>
  <String index="1730">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1731">
    <ToTranslate>tcg_rr11da</ToTranslate>
  </String>
  <String index="1732">
    <ToTranslate>tcg_rr11m</ToTranslate>
  </String>
  <String index="1733">
    <ToTranslate>koch0020</ToTranslate>
  </String>
  <String index="1734">
    <ToTranslate>Kouchuu$&quot;Eh? Chouhi is a general of Youzhou......&quot;@</ToTranslate>
  </String>
  <String index="1735">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1736">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="1737">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="1738">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="1739">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="1740">
    <ToTranslate>rinr0326</ToTranslate>
  </String>
  <String index="1741">
    <ToTranslate>Rinrin$&quot;Yep. Onii-chan told me to sneak into the castle and rescue Riri.&quot;@</ToTranslate>
  </String>
  <String index="1742">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1743">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="1744">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="1745">
    <ToTranslate>tcg_rr11da</ToTranslate>
  </String>
  <String index="1746">
    <ToTranslate>tcg_rr11m</ToTranslate>
  </String>
  <String index="1747">
    <ToTranslate>koch0021</ToTranslate>
  </String>
  <String index="1748">
    <ToTranslate>Kouchuu$&quot;What&apos;s going on......?&quot;@</ToTranslate>
  </String>
  <String index="1749">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="1750">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="1751">
    <ToTranslate>aisy0532</ToTranslate>
  </String>
  <String index="1752">
    <ToTranslate>Aisha$&quot;I&apos;ll explain.&quot;@</ToTranslate>
  </String>
  <String index="1753">
    <ToTranslate>tcg_koc22da</ToTranslate>
  </String>
  <String index="1754">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1755">
    <ToTranslate>koch0022</ToTranslate>
  </String>
  <String index="1756">
    <ToTranslate>Kouchuu$&quot;Kan&apos;u......&quot;@</ToTranslate>
  </String>
  <String index="1757">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="1758">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="1759">
    <ToTranslate>aisy0533</ToTranslate>
  </String>
  <String index="1760">
    <ToTranslate>Aisha$&quot;I&apos;m glad your daughter is unharmed......&quot;@</ToTranslate>
  </String>
  <String index="1761">
    <ToTranslate>tcg_koc22ja</ToTranslate>
  </String>
  <String index="1762">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1763">
    <ToTranslate>koch0023</ToTranslate>
  </String>
  <String index="1764">
    <ToTranslate>Kouchuu$&quot;Thank you...... But what is going on?&quot;@</ToTranslate>
  </String>
  <String index="1765">
    <ToTranslate>aisy0534</ToTranslate>
  </String>
  <String index="1766">
    <ToTranslate>Aisha$&quot;......When we were surrounding Lecheng in pursuit of Enshou, Chouun happened to be inside, and she sent a message telling us about the circumstances.&quot;@</ToTranslate>
  </String>
  <String index="1767">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1768">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="1769">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1770">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="1771">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="1772">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="1773">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="1774">
    <ToTranslate>sei_0038</ToTranslate>
  </String>
  <String index="1775">
    <ToTranslate>Sei$&quot;That&apos;s right. I told them that the city lord&apos;s daughter was abducted, and that she was fighting for that reason.&quot;@</ToTranslate>
  </String>
  <String index="1776">
    <ToTranslate>aisy0535</ToTranslate>
  </String>
  <String index="1777">
    <ToTranslate>Aisha$&quot;We are only after Enshou. Therefore, to avoid unnecessary battle, Our Lord suggested rescuing your daughter.&quot;@</ToTranslate>
  </String>
  <String index="1778">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="1779">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="1780">
    <ToTranslate>rinr0327</ToTranslate>
  </String>
  <String index="1781">
    <ToTranslate>Rinrin$&quot;So Rinrin snuck into the castle, met up with Sei, and rescued Riri.&quot;@</ToTranslate>
  </String>
  <String index="1782">
    <ToTranslate>tcg_koc22da</ToTranslate>
  </String>
  <String index="1783">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1784">
    <ToTranslate>koch0024</ToTranslate>
  </String>
  <String index="1785">
    <ToTranslate>Kouchuu$&quot;Then you requested a duel because......&quot;@</ToTranslate>
  </String>
  <String index="1786">
    <ToTranslate>Kazuto$&quot;In order to buy time, Kouchuu-san.&quot;@</ToTranslate>
  </String>
  <String index="1787">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="1788">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="1789">
    <ToTranslate>aisy0536</ToTranslate>
  </String>
  <String index="1790">
    <ToTranslate>Aisha$&quot;My Lord?! Enshou&apos;s people may still be lurking around. It&apos;s too dangerous to stay here!&quot;@</ToTranslate>
  </String>
  <String index="1791">
    <ToTranslate>Kazuto$&quot;But I wanted to thank everyone...... Chouun, thank you for letting us know what was going on. Thanks to you, we didn&apos;t have to do battle.&quot;@</ToTranslate>
  </String>
  <String index="1792">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="1793">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="1794">
    <ToTranslate>sei_0039</ToTranslate>
  </String>
  <String index="1795">
    <ToTranslate>Sei$&quot;Hmph. That was nothing to be thanked for.&quot;@</ToTranslate>
  </String>
  <String index="1796">
    <ToTranslate>Kazuto$&quot;But still, thank you...... Rinrin, good work. You did a great job.&quot;@</ToTranslate>
  </String>
  <String index="1797">
    <ToTranslate>tcg_cho31ia</ToTranslate>
  </String>
  <String index="1798">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="1799">
    <ToTranslate>rinr0328</ToTranslate>
  </String>
  <String index="1800">
    <ToTranslate>Rinrin$&quot;Hee, hee! Rinrin was good, huh?&quot;@</ToTranslate>
  </String>
  <String index="1801">
    <ToTranslate>Kazuto$&quot;Yep. I&apos;m glad I sent you in.&quot;@</ToTranslate>
  </String>
  <String index="1802">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="1803">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="1804">
    <ToTranslate>rinr0329</ToTranslate>
  </String>
  <String index="1805">
    <ToTranslate>Rinrin$&quot;Ahem!&quot;@</ToTranslate>
  </String>
  <String index="1806">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="1807">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="1808">
    <ToTranslate>aisy0537</ToTranslate>
  </String>
  <String index="1809">
    <ToTranslate>Aisha$&quot;......Ah, umm.&quot;@</ToTranslate>
  </String>
  <String index="1810">
    <ToTranslate>Kazuto$&quot;You too, Aisha, good work. ......You did well.&quot;@</ToTranslate>
  </String>
  <String index="1811">
    <ToTranslate>tcg_kan21oa</ToTranslate>
  </String>
  <String index="1812">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="1813">
    <ToTranslate>aisy0538</ToTranslate>
  </String>
  <String index="1814">
    <ToTranslate>Aisha$&quot;......Thank you.&quot;@</ToTranslate>
  </String>
  <String index="1815">
    <ToTranslate>tcg_koc22na</ToTranslate>
  </String>
  <String index="1816">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1817">
    <ToTranslate>koch0025</ToTranslate>
  </String>
  <String index="1818">
    <ToTranslate>Kouchuu$&quot;......Are you Hongou Kazuto-sama?&quot;@</ToTranslate>
  </String>
  <String index="1819">
    <ToTranslate>Kazuto$&quot;Yes. Kouchuu-san, are you hurt?&quot;@</ToTranslate>
  </String>
  <String index="1820">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="1821">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1822">
    <ToTranslate>koch0026</ToTranslate>
  </String>
  <String index="1823">
    <ToTranslate>Kouchuu$&quot;No. But why did you rescue Riri......?&quot;@</ToTranslate>
  </String>
  <String index="1824">
    <ToTranslate>Kazuto$&quot;Just as Aisha said. Our target is only Enshou. Unfortunately, we almost had to fight with your army as well.&quot;@</ToTranslate>
  </String>
  <String index="1825">
    <ToTranslate>Kazuto$&quot;Thanks to Aisha, Rinrin, Sei...... and everyone else, we were able to avoid a battle.&quot;@</ToTranslate>
  </String>
  <String index="1826">
    <ToTranslate>tcg_koc32ia</ToTranslate>
  </String>
  <String index="1827">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="1828">
    <ToTranslate>koch0027</ToTranslate>
  </String>
  <String index="1829">
    <ToTranslate>Kouchuu$&quot;That&apos;s true, but......&quot;@</ToTranslate>
  </String>
  <String index="1830">
    <ToTranslate>Kazuto$&quot;Anyway, now that your daughter is free, there is no reason for us to fight, correct?&quot;@</ToTranslate>
  </String>
  <String index="1831">
    <ToTranslate>tcg_koc22da</ToTranslate>
  </String>
  <String index="1832">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1833">
    <ToTranslate>koch0028</ToTranslate>
  </String>
  <String index="1834">
    <ToTranslate>Kouchuu$&quot;......Indeed.&quot;@</ToTranslate>
  </String>
  <String index="1835">
    <ToTranslate>Kazuto$&quot;Good. Well, we have to go after Enshou now, so could you please let us through Lecheng City?&quot;@</ToTranslate>
  </String>
  <String index="1836">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="1837">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1838">
    <ToTranslate>koch0029</ToTranslate>
  </String>
  <String index="1839">
    <ToTranslate>Kouchuu$&quot;Are you still going to fight against Enshou......?&quot;@</ToTranslate>
  </String>
  <String index="1840">
    <ToTranslate>Kazuto$&quot;Yes, we are...... To protect our comrades.&quot;@</ToTranslate>
  </String>
  <String index="1841">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="1842">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1843">
    <ToTranslate>koch0030</ToTranslate>
  </String>
  <String index="1844">
    <ToTranslate>Kouchuu$&quot;......I understand. I will let you into the city.&quot;@</ToTranslate>
  </String>
  <String index="1845">
    <ToTranslate>Kazuto$&quot;Thank you. Shuri, please make arrangements. We must catch up with Enshou.&quot;@</ToTranslate>
  </String>
  <String index="1846">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="1847">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="1848">
    <ToTranslate>syur0307</ToTranslate>
  </String>
  <String index="1849">
    <ToTranslate>Shuri$&quot;Got it!&quot;@</ToTranslate>
  </String>
  <String index="1850">
    <ToTranslate>Kazuto$&quot;......Okay, Kouchuu-san, sorry for the disturbance. I&apos;m truly sorry Riri-chan had to go through something so scary because of us.&quot;@</ToTranslate>
  </String>
  <String index="1851">
    <ToTranslate>$I bowed my head and turned around to leave so we could pursue Enshou.@</ToTranslate>
  </String>
  <String index="1852">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1853">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="1854">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="1855">
    <ToTranslate>koch0031</ToTranslate>
  </String>
  <String index="1856">
    <ToTranslate>Kouchuu$&quot;Please wait.&quot;@</ToTranslate>
  </String>
  <String index="1857">
    <ToTranslate>Kazuto$&quot;Huh? What is it?&quot;@</ToTranslate>
  </String>
  <String index="1858">
    <ToTranslate>koch0032</ToTranslate>
  </String>
  <String index="1859">
    <ToTranslate>Kouchuu$&quot;I owe you for rescuing Riri. Are you planning to leave before I can repay you for that?&quot;@</ToTranslate>
  </String>
  <String index="1860">
    <ToTranslate>Kazuto$&quot;I didn&apos;t do much, so you don&apos;t owe me anything. Besides, you two had to go through this danger because of me......&quot;@</ToTranslate>
  </String>
  <String index="1861">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="1862">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1863">
    <ToTranslate>koch0033</ToTranslate>
  </String>
  <String index="1864">
    <ToTranslate>Kouchuu$&quot;However, I am still in your debt. In fact, allow me to take this opportunity to return the favor.&quot;@</ToTranslate>
  </String>
  <String index="1865">
    <ToTranslate>Kazuto$&quot;Huh?&quot;@</ToTranslate>
  </String>
  <String index="1866">
    <ToTranslate>koch0034</ToTranslate>
  </String>
  <String index="1867">
    <ToTranslate>Kouchuu$&quot;I entrust you with this city.&quot;@</ToTranslate>
  </String>
  <String index="1868">
    <ToTranslate>Kazuto$&quot;Wh...Huh!? What are you saying? This is your city, Kouchuu-san. You should continue to......&quot;@</ToTranslate>
  </String>
  <String index="1869">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="1870">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1871">
    <ToTranslate>koch0035</ToTranslate>
  </String>
  <String index="1872">
    <ToTranslate>Kouchuu$&quot;No, I can&apos;t.&quot;@</ToTranslate>
  </String>
  <String index="1873">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1874">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="1875">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1876">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="1877">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="1878">
    <ToTranslate>aisy0539</ToTranslate>
  </String>
  <String index="1879">
    <ToTranslate>Aisha$&quot;What do you mean you can&apos;t?&quot;@</ToTranslate>
  </String>
  <String index="1880">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="1881">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1882">
    <ToTranslate>koch0036</ToTranslate>
  </String>
  <String index="1883">
    <ToTranslate>Kouchuu$&quot;To return the favor, I will align myself with you. ......My life is yours, Kazuto-sama.&quot;@</ToTranslate>
  </String>
  <String index="1884">
    <ToTranslate>tcg_cun11da</ToTranslate>
  </String>
  <String index="1885">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="1886">
    <ToTranslate>sei_0040</ToTranslate>
  </String>
  <String index="1887">
    <ToTranslate>Sei$&quot;Wow...... That&apos;s rather drastic, Kouchuu-dono.&quot;@</ToTranslate>
  </String>
  <String index="1888">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="1889">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="1890">
    <ToTranslate>koch0037</ToTranslate>
  </String>
  <String index="1891">
    <ToTranslate>Kouchuu$&quot;This is a turbulent era. ......Even if this incident with Enshou didn&apos;t happen, I wouldn&apos;t have been able to protect this city or its people any longer.&quot;@</ToTranslate>
  </String>
  <String index="1892">
    <ToTranslate>koch0038</ToTranslate>
  </String>
  <String index="1893">
    <ToTranslate>Kouchuu$&quot;Since that&apos;s the case, I want to give this city and myself to someone who&apos;s faithful and trustworthy......&quot;@</ToTranslate>
  </String>
  <String index="1894">
    <ToTranslate>Kazuto$&quot;That&apos;s why you want us to...?&quot;@</ToTranslate>
  </String>
  <String index="1895">
    <ToTranslate>koch0039</ToTranslate>
  </String>
  <String index="1896">
    <ToTranslate>Kouchuu$&quot;Yes. I want to return the favor. Also, I do this to protect the city of Lecheng, as its lord.&quot;@</ToTranslate>
  </String>
  <String index="1897">
    <ToTranslate>Kazuto$&quot;......I understand. I accept your offer. Is that okay, everyone?&quot;@</ToTranslate>
  </String>
  <String index="1898">
    <ToTranslate>tcg_kan21ba</ToTranslate>
  </String>
  <String index="1899">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="1900">
    <ToTranslate>aisy0540</ToTranslate>
  </String>
  <String index="1901">
    <ToTranslate>Aisha$&quot;Of course.&quot;@</ToTranslate>
  </String>
  <String index="1902">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="1903">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="1904">
    <ToTranslate>rinr0330</ToTranslate>
  </String>
  <String index="1905">
    <ToTranslate>Rinrin$&quot;Comrades, comrades, I love getting more comrades!&quot;@</ToTranslate>
  </String>
  <String index="1906">
    <ToTranslate>Kazuto$&quot;That&apos;s right. Then Kouchuu-san, please continue to govern Lecheng as you&apos;ve been doing...... is that okay?&quot;@</ToTranslate>
  </String>
  <String index="1907">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="1908">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1909">
    <ToTranslate>koch0040</ToTranslate>
  </String>
  <String index="1910">
    <ToTranslate>Kouchuu$&quot;I decline.&quot;@</ToTranslate>
  </String>
  <String index="1911">
    <ToTranslate>Kazuto$&quot;Huh?&quot;@</ToTranslate>
  </String>
  <String index="1912">
    <ToTranslate>koch0041</ToTranslate>
  </String>
  <String index="1913">
    <ToTranslate>Kouchuu$&quot;Please let someone else do that. I want to go after Enshou with you, My Lord.&quot;@</ToTranslate>
  </String>
  <String index="1914">
    <ToTranslate>Kazuto$&quot;Wait, did you just call me &apos;My Lord&apos;......?&quot;@</ToTranslate>
  </String>
  <String index="1915">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="1916">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1917">
    <ToTranslate>koch0042</ToTranslate>
  </String>
  <String index="1918">
    <ToTranslate>Kouchuu$&quot;Oh? But I&apos;m yours now. Isn&apos;t it only natural that I call my master &apos;My Lord&apos;?&quot;@</ToTranslate>
  </String>
  <String index="1919">
    <ToTranslate>$Kouchuu giggled teasingly and looked at me with bewitching eyes.@</ToTranslate>
  </String>
  <String index="1920">
    <ToTranslate>tcg_koc12ha</ToTranslate>
  </String>
  <String index="1921">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1922">
    <ToTranslate>koch0043</ToTranslate>
  </String>
  <String index="1923">
    <ToTranslate>Kouchuu$&quot;Isn&apos;t that right, My Lord?&quot;@</ToTranslate>
  </String>
  <String index="1924">
    <ToTranslate>Kazuto$&quot;Ahh...... so this is kind of person Kouchuu-san was.&quot;@</ToTranslate>
  </String>
  <String index="1925">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="1926">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1927">
    <ToTranslate>koch0044</ToTranslate>
  </String>
  <String index="1928">
    <ToTranslate>Kouchuu$&quot;Now that I&apos;m your vassal, there is no need to use a name of honor. My true name is Shion. Please call me Shion from now on.&quot;@</ToTranslate>
  </String>
  <String index="1929">
    <ToTranslate>Kazuto$&quot;Yeah, okay......&quot;@</ToTranslate>
  </String>
  <String index="1930">
    <ToTranslate>$Now she had complete control of our conversation, and I smiled bitterly from a sense of defeat.@</ToTranslate>
  </String>
  <String index="1931">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="1932">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="1933">
    <ToTranslate>shio0000</ToTranslate>
  </String>
  <String index="1934">
    <ToTranslate>Shion$&quot;U-hu-hu, we&apos;ll have fun, My Lord!&quot;@</ToTranslate>
  </String>
  <String index="1935">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="1936">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="1937">
    <ToTranslate>bgm06</ToTranslate>
  </String>
  <String index="1938">
    <ToTranslate>sei_0041</ToTranslate>
  </String>
  <String index="1939">
    <ToTranslate>Sei$&quot;Mhmm...... then I&apos;ll call you My Lord as well.&quot;@</ToTranslate>
  </String>
  <String index="1940">
    <ToTranslate>Kazuto$&quot;Huh?! Wh-what are you saying all of a sudden?&quot;@</ToTranslate>
  </String>
  <String index="1941">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="1942">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="1943">
    <ToTranslate>sei_0042</ToTranslate>
  </String>
  <String index="1944">
    <ToTranslate>Sei$&quot;Why are you so surprised? You&apos;re the one who said you&apos;d be waiting for me forever.&quot;@</ToTranslate>
  </String>
  <String index="1945">
    <ToTranslate>Kazuto$&quot;Ah, well, yeah... I did, but...... What happened to traveling around the continent?&quot;@</ToTranslate>
  </String>
  <String index="1946">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="1947">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="1948">
    <ToTranslate>sei_0043</ToTranslate>
  </String>
  <String index="1949">
    <ToTranslate>Sei$&quot;I don&apos;t need to do that anymore. It&apos;s plain to see the world will be divided into three camps.&quot;@</ToTranslate>
  </String>
  <String index="1950">
    <ToTranslate>Kazuto$&quot;Three?&quot;@</ToTranslate>
  </String>
  <String index="1951">
    <ToTranslate>sei_0044</ToTranslate>
  </String>
  <String index="1952">
    <ToTranslate>Sei$&quot;You, Sousou, and Sonken...... Those three camps will battle for hegemony over the land.&quot;@</ToTranslate>
  </String>
  <String index="1953">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="1954">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="1955">
    <ToTranslate>sei_0045</ToTranslate>
  </String>
  <String index="1956">
    <ToTranslate>Sei$&quot;Therefore, I want to be in one of those three camps. But...... I like you better than Sousou or Sonken, so I&apos;ll become your vassal.&quot;@</ToTranslate>
  </String>
  <String index="1957">
    <ToTranslate>Kazuto$&quot;Are you sure about that......?&quot;@</ToTranslate>
  </String>
  <String index="1958">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="1959">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="1960">
    <ToTranslate>sei_0046</ToTranslate>
  </String>
  <String index="1961">
    <ToTranslate>Sei$&quot;Of course. I will swear my spear to your service.&quot;@</ToTranslate>
  </String>
  <String index="1962">
    <ToTranslate>Kazuto$&quot;......Thank you.&quot;@</ToTranslate>
  </String>
  <String index="1963">
    <ToTranslate>tcg_cun31ia</ToTranslate>
  </String>
  <String index="1964">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="1965">
    <ToTranslate>sei_0047</ToTranslate>
  </String>
  <String index="1966">
    <ToTranslate>Sei$&quot;......Now, My Lord, why don&apos;t we defeat Enshou first.&quot;@</ToTranslate>
  </String>
  <String index="1967">
    <ToTranslate>Kazuto$&quot;That&apos;s right. Okay, after we merge with the main force, we&apos;ll head to Enshou&apos;s base, Nanpi!&quot;@</ToTranslate>
  </String>
  <String index="1968">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="1969">
    <ToTranslate>tcg_syo41ea</ToTranslate>
  </String>
  <String index="1970">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="1971">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="1972">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="1973">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="1974">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="1975">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="1976">
    <ToTranslate>aisy0541</ToTranslate>
  </String>
  <String index="1977">
    <ToTranslate>Aisha$&quot;As you wish!&quot;@</ToTranslate>
  </String>
  <String index="1978">
    <ToTranslate>rinr0331</ToTranslate>
  </String>
  <String index="1979">
    <ToTranslate>Rinrin$&quot;All units, fall in! Our army will resume pursuit of Enshou!&quot;@</ToTranslate>
  </String>
  <String index="1980">
    <ToTranslate>syur0308</ToTranslate>
  </String>
  <String index="1981">
    <ToTranslate>Shuri$&quot;Let&apos;s move out!&quot;@</ToTranslate>
  </String>
  <String index="1982">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="1983">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="1984">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="1985">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="1986">
    <ToTranslate>$Now we have more new comrades.@</ToTranslate>
  </String>
  <String index="1987">
    <ToTranslate>$One&apos;s family name is Kou. Her first name is Chuu, her common name is Kanshou, and her true name is Shion.@</ToTranslate>
  </String>
  <String index="1988">
    <ToTranslate>$She&apos;s a widow, who is a master with the bow, and excels in government.@</ToTranslate>
  </String>
  <String index="1989">
    <ToTranslate>$The other&apos;s family name is Chou. Her first name is Un, her common name is Shiryuu, and her true name is Sei.@</ToTranslate>
  </String>
  <String index="1990">
    <ToTranslate>$She is a master spearman, and as brave as Aisha.@</ToTranslate>
  </String>
  <String index="1991">
    <ToTranslate>$With our wonderful new comrades filling my heart with excitement, we resumed our pursuit of Enshou.@</ToTranslate>
  </String>
  <String index="1992">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="1993">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1994">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="1995">
    <ToTranslate>Soldier$&quot;I see the rear guard of Enshou&apos;s army!&quot;@</ToTranslate>
  </String>
  <String index="1996">
    <ToTranslate>Kazuto$&quot;Okay, Shuri!&quot;@</ToTranslate>
  </String>
  <String index="1997">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="1998">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="1999">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="2000">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="2001">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="2002">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="2003">
    <ToTranslate>syur0309</ToTranslate>
  </String>
  <String index="2004">
    <ToTranslate>Shuri$&quot;Yes! We&apos;ll charge right in!&quot;@</ToTranslate>
  </String>
  <String index="2005">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="2006">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="2007">
    <ToTranslate>aisy0542</ToTranslate>
  </String>
  <String index="2008">
    <ToTranslate>Aisha$&quot;Okay! All units, prepare for battle!&quot;@</ToTranslate>
  </String>
  <String index="2009">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="2010">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="2011">
    <ToTranslate>rinr0332</ToTranslate>
  </String>
  <String index="2012">
    <ToTranslate>Rinrin$&quot;Charge!&quot;@</ToTranslate>
  </String>
  <String index="2013">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="2014">
    <ToTranslate>SE124</ToTranslate>
  </String>
  <String index="2015">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
