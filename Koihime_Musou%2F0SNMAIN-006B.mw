  <String index="1648">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="1649">
    <ToTranslate>sei_0035</ToTranslate>
  </String>
  <String index="1650">
    <ToTranslate>Sei$&quot;My name is Chou Shiryuu! I have rescued Riri-sama from the evil clutches of Enshou, who cowardly abducted her from the lord of Lecheng, Kouchuu-dono.&quot;@</ToTranslate>
  </String>
  <String index="1651">
    <ToTranslate>Sei$&quot;There is no need to fight anymore. Both armies, fall back!&quot;@</ToTranslate>
  </String>
