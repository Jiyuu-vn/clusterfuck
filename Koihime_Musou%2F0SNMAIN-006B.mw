  <String index="861">
    <ToTranslate>aisy0504</ToTranslate>
  </String>
  <String index="862">
    <ToTranslate>Aisha$&quot;Yes! My Lord!&quot;@</ToTranslate>
  </String>
  <String index="863">
    <ToTranslate>Kazuto$&quot;Yeah, it&apos;s probably Chouun.&quot;@</ToTranslate>
