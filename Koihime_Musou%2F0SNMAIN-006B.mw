  <String index="1536">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="1537">
    <ToTranslate>rinr0315</ToTranslate>
  </String>
  <String index="1538">
    <ToTranslate>Rinrin$&quot;Hmmm...... what can I do, then?&quot;@</ToTranslate>
  </String>
  <String index="1539">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="1540">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="1541">
    <ToTranslate>sei_0026</ToTranslate>
  </String>
  <String index="1542">
    <ToTranslate>Sei$&quot;That&apos;s easy. We already have Riri-dono with us. We just leave through the city gates.&quot;@</ToTranslate>
  </String>
  <String index="1543">
    <ToTranslate>tcg_cho31ja</ToTranslate>
  </String>
  <String index="1544">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="1545">
    <ToTranslate>rinr0316</ToTranslate>
  </String>
  <String index="1546">
    <ToTranslate>Rinrin$&quot;Are you sure about that?&quot;@</ToTranslate>
  </String>
  <String index="1547">
    <ToTranslate>sei_0027</ToTranslate>
  </String>
  <String index="1548">
    <ToTranslate>Sei$&quot;Of course. Justice is on our side, so we can hold our heads up proud. ......Come, let&apos;s go, Rinrin.&quot;@</ToTranslate>
  </String>
  <String index="1549">
    <ToTranslate>tcg_cho11aa</ToTranslate>
