  </String>
  <String index="59">
    <ToTranslate>aisy0470</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>Aisha$&quot;My name is Kan Unchou, the first vassal of Hongou, and I am the incarnation of the Green Dragon Crescent Blade!&quot;@</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>aisy0471</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>Aisha$&quot;I have no quarrel with nameless soldiers! The opponents I seek are Bunshuu and Ganryou alone! Bunshuu! Ganryou! If you wish to protect the pride of the En family, then come and face me!&quot;@</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>tcg_bun12ia</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>buns0057</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>Bunshuu$&quot;Sure! Here I am!&quot;@</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>aisy0472</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>Aisha$&quot;......There you are, Bunshuu!&quot;@</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>tcg_bun12ia</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>tcg_bun12m</ToTranslate>
  </String>
