  </String>
  <String index="371">
    <ToTranslate>yure_r</ToTranslate>
  </String>
  <String index="372">
    <ToTranslate>tcg_zo211aa</ToTranslate>
  </String>
  <String index="373">
    <ToTranslate>tcg_zo211m</ToTranslate>
  </String>
  <String index="374">
    <ToTranslate>Fatso$&quot;Shit...&quot;@</ToTranslate>
  </String>
  <String index="375">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="376">
    <ToTranslate>SE081</ToTranslate>
  </String>
  <String index="377">
    <ToTranslate>yure_t</ToTranslate>
  </String>
  <String index="378">
    <ToTranslate>tcg_zo311ba</ToTranslate>
  </String>
  <String index="379">
    <ToTranslate>tcg_zo311m</ToTranslate>
  </String>
  <String index="380">
    <ToTranslate>Midget$&quot;F-Fatso...?&quot;@</ToTranslate>
  </String>
  <String index="381">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="382">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="383">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="384">
    <ToTranslate>kanu0008</ToTranslate>
  </String>
  <String index="385">
    <ToTranslate>???$&quot;One down! Who&apos;s next?! Will it be thou?!&quot;@</ToTranslate>
  </String>
  <String index="386">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="387">
    <ToTranslate>tcg_zo111ba</ToTranslate>
  </String>
  <String index="388">
    <ToTranslate>tcg_zo111m</ToTranslate>
  </String>
  <String index="389">
    <ToTranslate>Aniki$&quot;Damn... hey, Midget!&quot;@</ToTranslate>
  </String>
  <String index="390">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
