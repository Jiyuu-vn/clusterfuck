  <String index="408">
    <ToTranslate>kanu0010</ToTranslate>
  </String>
  <String index="409">
    <ToTranslate>???$&quot;A super woman...?&quot;@</ToTranslate>
  </String>
  <String index="410">
    <ToTranslate>SE072</ToTranslate>
  </String>
  <String index="411">
    <ToTranslate>$Gurrrr...@</ToTranslate>
  </String>
  <String index="412">
    <ToTranslate>tcg_kan11wa</ToTranslate>
  </String>
  <String index="413">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="414">
    <ToTranslate>kanu0011</ToTranslate>
  </String>
  <String index="415">
    <ToTranslate>???$&quot;Thou darest call me a super woman?!&quot;@</ToTranslate>
  </String>
  <String index="416">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="417">
    <ToTranslate>tcg_zo311ba</ToTranslate>
  </String>
  <String index="418">
    <ToTranslate>tcg_zo311m</ToTranslate>
  </String>
  <String index="419">
    <ToTranslate>Midget$&quot;Eh? Ah, ahhh, no, I was just kidding! Really, I didn&apos;t mean that!&quot;@</ToTranslate>
  </String>
  <String index="420">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="421">
    <ToTranslate>tcg_kan91aa</ToTranslate>
  </String>
  <String index="422">
    <ToTranslate>tcg_kan91m</ToTranslate>
  </String>
  <String index="423">
    <ToTranslate>kanu0012</ToTranslate>
  </String>
  <String index="424">
    <ToTranslate>???$&quot;No excuses!&quot;@</ToTranslate>
  </String>
  <String index="425">
    <ToTranslate>battle_tuki</ToTranslate>
  </String>
  <String index="426">
    <ToTranslate>SE002</ToTranslate>
  </String>
  <String index="427">
    <ToTranslate>$The girl silenced his plead with a shout, and moving faster than I could see, she struck in him in the solar plexus with the butt of her polearm.@</ToTranslate>
  </String>
  <String index="428">
    <ToTranslate>tcg_zo311da</ToTranslate>
  </String>
  <String index="429">
    <ToTranslate>tcg_zo311m</ToTranslate>
  </String>
  <String index="430">
    <ToTranslate>Midget$&quot;Gheh...&quot;@</ToTranslate>
  </String>
  <String index="431">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="432">
    <ToTranslate>SE038</ToTranslate>
  </String>
  <String index="433">
    <ToTranslate>yure_t</ToTranslate>
  </String>
  <String index="434">
    <ToTranslate>tcg_zo111ba</ToTranslate>
  </String>
  <String index="435">
    <ToTranslate>tcg_zo111m</ToTranslate>
  </String>
  <String index="436">
    <ToTranslate>Aniki$&quot;Midget?&quot;@</ToTranslate>
  </String>
  <String index="437">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="438">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="439">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="440">
    <ToTranslate>kanu0013</ToTranslate>
  </String>
  <String index="441">
    <ToTranslate>???$&quot;Now then, thou art the last. ...What next? Wilt thou fight or wilt thou escape with thine companions? It is thy choice.&quot;@</ToTranslate>
  </String>
  <String index="442">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="443">
    <ToTranslate>tcg_zo111da</ToTranslate>
  </String>
  <String index="444">
    <ToTranslate>tcg_zo111m</ToTranslate>
  </String>
  <String index="445">
    <ToTranslate>Aniki$&quot;Shit... fine. I&apos;ll let both of you go this time.&quot;@</ToTranslate>
  </String>
  <String index="446">
    <ToTranslate>$The man glared at her with detest, unwilling to admit defeat.@</ToTranslate>
  </String>
  <String index="447">
    <ToTranslate>tcg_zo111ca</ToTranslate>
  </String>
  <String index="448">
    <ToTranslate>tcg_zo111m</ToTranslate>
  </String>
  <String index="449">
    <ToTranslate>Aniki$&quot;Hey, get up!@</ToTranslate>
  </String>
  <String index="450">
    <ToTranslate>SE003</ToTranslate>
  </String>
  <String index="451">
    <ToTranslate>tcg_zo311da</ToTranslate>
  </String>
  <String index="452">
    <ToTranslate>tcg_zo311m</ToTranslate>
  </String>
  <String index="453">
    <ToTranslate>Midget$&quot;Ahh!&quot;@</ToTranslate>
  </String>
  <String index="454">
    <ToTranslate>SE003</ToTranslate>
  </String>
  <String index="455">
    <ToTranslate>tcg_zo211aa</ToTranslate>
  </String>
  <String index="456">
    <ToTranslate>tcg_zo211m</ToTranslate>
  </String>
  <String index="457">
    <ToTranslate>Fatso$&quot;Ahoo.&quot;@</ToTranslate>
  </String>
  <String index="458">
    <ToTranslate>$He kicked his two friends who were lying spread out on the ground, causing them to wake up.@</ToTranslate>
  </String>
  <String index="459">
    <ToTranslate>Bro$&quot;Come on, we&apos;re going!&quot;@</ToTranslate>
  </String>
  <String index="460">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="461">
    <ToTranslate>$After one more glance at the girl, they hastily made off.@</ToTranslate>
  </String>
  <String index="462">
    <ToTranslate>Kazuto$&quot;...&quot;@</ToTranslate>
  </String>
  <String index="463">
    <ToTranslate>bgm03</ToTranslate>
  </String>
  <String index="464">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="465">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="466">
    <ToTranslate>kanu0014</ToTranslate>
  </String>
  <String index="467">
    <ToTranslate>???$&quot;Whew. ...Are you hurt anywhere?&quot;@</ToTranslate>
  </String>
  <String index="468">
    <ToTranslate>Kazuto$&quot;Eh? Ah, yeah... I&apos;m okay.&quot;@</ToTranslate>
  </String>
  <String index="469">
    <ToTranslate>tcg_kan21ba</ToTranslate>
  </String>
  <String index="470">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="471">
    <ToTranslate>kanu0015</ToTranslate>
  </String>
  <String index="472">
    <ToTranslate>???$&quot;Good. I&apos;m glad to hear it.&quot;@</ToTranslate>
  </String>
  <String index="473">
    <ToTranslate>$The girl gave me a warm smile which belied her earlier gallantry.@</ToTranslate>
  </String>
  <String index="474">
    <ToTranslate>Kazuto$&quot;...&quot;@</ToTranslate>
  </String>
  <String index="475">
    <ToTranslate>$Her eyes were so dark and fascinating.@</ToTranslate>
  </String>
  <String index="476">
    <ToTranslate>$Likewise, her silky black hair sparkled like obsidian.@</ToTranslate>
  </String>
  <String index="477">
    <ToTranslate>$She was dignified, yet her comforting gaze invited me to finally speak.@</ToTranslate>
  </String>
  <String index="478">
    <ToTranslate>Kazuto$&quot;Who are you...?&quot;@</ToTranslate>
  </String>
  <String index="479">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="480">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="481">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="482">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="483">
    <ToTranslate>kanu0016</ToTranslate>
  </String>
  <String index="484">
    <ToTranslate>???$&quot;Oh, pardon me. I haven&apos;t introduced myself yet. My family name is Kan, my first name is U, and my common name is Unchou. I have travelled from Yuushuu to welcome you.&quot;@</ToTranslate>
  </String>
  <String index="485">
    <ToTranslate>Kazuto$&quot;...Huh?&quot;@</ToTranslate>
  </String>
  <String index="486">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="487">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="488">
    <ToTranslate>hatena</ToTranslate>
  </String>
  <String index="489">
    <ToTranslate>kanu0017</ToTranslate>
  </String>
  <String index="490">
    <ToTranslate>???$&quot;What is the matter?&quot;@</ToTranslate>
  </String>
  <String index="491">
    <ToTranslate>Kazuto$&quot;No, umm...your name...Could you repeat it again? I didn&apos;t hear it well over the wind.&quot;@</ToTranslate>
  </String>
  <String index="492">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="493">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="494">
    <ToTranslate>kanu0018</ToTranslate>
  </String>
  <String index="495">
    <ToTranslate>???$&quot;I see. Allow me to name myself once again. My name is Kan&apos;U, and my common name is Unchou. I have traveled from...&quot;@</ToTranslate>
  </String>
  <String index="496">
    <ToTranslate>Kazuto$&quot;W, w, wait! Wait a minute! What do you mean your name is Kan&apos;u?&quot;@</ToTranslate>
  </String>
  <String index="497">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="498">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="499">
    <ToTranslate>kanu0019</ToTranslate>
  </String>
  <String index="500">
    <ToTranslate>Kan&apos;u $&quot;What do you mean?&quot;@</ToTranslate>
  </String>
  <String index="501">
    <ToTranslate>Kazuto$&quot;Well... I know a little bit about a guy named Kan&apos;U, but he&apos;s from long ago. Oh, did you adopt his name out of admiration?&quot;@</ToTranslate>
  </String>
  <String index="502">
    <ToTranslate>Kazuto$&quot;By the way, Kan&apos;u doesn&apos;t sound like a Japanese name... This is Japan, right? Why is that your name?&quot;@</ToTranslate>
  </String>
  <String index="503">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="504">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="505">
    <ToTranslate>kanu0020</ToTranslate>
  </String>
  <String index="506">
    <ToTranslate>Kan&apos;u$&quot;Please hold on a moment.&quot;@</ToTranslate>
  </String>
  <String index="507">
    <ToTranslate>Kazuto$&quot;Eh?&quot;@</ToTranslate>
  </String>
  <String index="508">
    <ToTranslate>kanu0021</ToTranslate>
  </String>
  <String index="509">
    <ToTranslate>Kan&apos;u$&quot;What is this &apos;Japan&apos;?&quot;@</ToTranslate>
  </String>
  <String index="510">
    <ToTranslate>Kazuto$&quot;.........Eh?&quot;@</ToTranslate>
  </String>
  <String index="511">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="512">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="513">
    <ToTranslate>kanu0022</ToTranslate>
  </String>
  <String index="514">
    <ToTranslate>Kan&apos;u$&quot;Do you call the heavens from which you descended &apos;Japan&apos;?&quot;@</ToTranslate>
  </String>
  <String index="515">
    <ToTranslate>Kazuto$&quot;Ehhh... wait a minute. Am I not in Japan?&quot;@</ToTranslate>
  </String>
  <String index="516">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="517">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="518">
    <ToTranslate>kanu0023</ToTranslate>
  </String>
  <String index="519">
    <ToTranslate>Kan&apos;u$&quot;We are in the Zhuo District of Youzhou, as you can tell by the mountain ridge alongside Mount Wutai in the distance. A village lies a little further west from here...Are you all right?&quot;@</ToTranslate>
  </String>
  <String index="520">
    <ToTranslate>Kazuto$&quot;Well... I just don&apos;t understand what you&apos;re saying...&quot;@</ToTranslate>
  </String>
  <String index="521">
    <ToTranslate>$Where am I?@</ToTranslate>
  </String>
  <String index="522">
    <ToTranslate>$If this isn&apos;t Japan...then where the hell am I?@</ToTranslate>
  </String>
  <String index="523">
    <ToTranslate>Kazuto$&quot;What&apos;s going on...?&quot;@</ToTranslate>
  </String>
  <String index="524">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="525">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="526">
    <ToTranslate>kanu0024</ToTranslate>
  </String>
  <String index="527">
    <ToTranslate>Kan&apos;u$&quot;Excuse me...&quot;@</ToTranslate>
  </String>
  <String index="528">
    <ToTranslate>Kazuto$&quot;Eh?&quot;@</ToTranslate>
  </String>
  <String index="529">
    <ToTranslate>kanu0025</ToTranslate>
  </String>
  <String index="530">
    <ToTranslate>Kan&apos;u$&quot;Might you grace me with your name?&quot;@</ToTranslate>
  </String>
  <String index="531">
    <ToTranslate>Kazuto$&quot;Oh, sorry. You&apos;re right. How rude of me, not to name myself after hearing yours.&quot;@</ToTranslate>
  </String>
  <String index="532">
    <ToTranslate>Kazuto$&quot;I&apos;m Hongou Kazuto. Thank you for saving me.&quot;@</ToTranslate>
  </String>
  <String index="533">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="534">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="535">
    <ToTranslate>kanu0026</ToTranslate>
  </String>
  <String index="536">
    <ToTranslate>Kan&apos;u$&quot;No, there is no need to thank me. It is my duty to protect you, the Messenger from Heaven.&quot;@</ToTranslate>
  </String>
  <String index="537">
    <ToTranslate>Kazuto$&quot;...Messenger from Heaven? What&apos;s that?&quot;@</ToTranslate>
  </String>
  <String index="538">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="539">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="540">
    <ToTranslate>kanu0027</ToTranslate>
  </String>
  <String index="541">
    <ToTranslate>Kan&apos;u$&quot;The other day an oracle known as Kanro foretold that a Messenger from Heaven would descend to put an end to the wars.&quot;@</ToTranslate>
  </String>
  <String index="542">
    <ToTranslate>tcg_kan21ba</ToTranslate>
  </String>
  <String index="543">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="544">
    <ToTranslate>kanu0028</ToTranslate>
  </String>
  <String index="545">
    <ToTranslate>Kan&apos;u$&quot;This is the exact place of which the oracle spoke, and here I have met you... Who else could be the Messenger from Heaven but you?&quot;@</ToTranslate>
  </String>
  <String index="546">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="547">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="548">
    <ToTranslate>kanu0029</ToTranslate>
  </String>
  <String index="549">
    <ToTranslate>Kan&apos;u$&quot;Furthermore, your very clothes shine with the light of Heaven. That alone clearly signifies that you are a Messenger from Heaven. Am I mistaken?&quot;@</ToTranslate>
  </String>
  <String index="550">
    <ToTranslate>Kazuto$&quot;Ah, well... these are made of polyester and that&apos;s why they reflect light.&quot;@</ToTranslate>
  </String>
  <String index="551">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="552">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="553">
    <ToTranslate>kanu0030</ToTranslate>
  </String>
  <String index="554">
    <ToTranslate>Kan&apos;u$&quot; &apos;Po-ly-es-ter&apos;? Hmm, the language of Heaven is indeed filled with strange words.&quot;@</ToTranslate>
  </String>
  <String index="555">
    <ToTranslate>Kazuto$&quot;Anyway, I&apos;m not really a messenger of Heaven...&quot;@</ToTranslate>
  </String>
  <String index="556">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="557">
    <ToTranslate>choh0000</ToTranslate>
  </String>
  <String index="558">
    <ToTranslate>???$&quot;Anejaaaa!&quot;@</ToTranslate>
  </String>
  <String index="559">
    <ToTranslate>SE024</ToTranslate>
  </String>
  <String index="560">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="561">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="562">
    <ToTranslate>bgm04</ToTranslate>
  </String>
  <String index="563">
    <ToTranslate>kanu0031</ToTranslate>
  </String>
  <String index="564">
    <ToTranslate>Kan&apos;u$&quot;Oh, Rinrin. You finally caught up.&quot;@</ToTranslate>
  </String>
  <String index="565">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="566">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="567">
    <ToTranslate>choh0001</ToTranslate>
  </String>
  <String index="568">
    <ToTranslate>???$&quot;That was mean! How dare you leave Rinrin behind!&quot;@</ToTranslate>
  </String>
  <String index="569">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="570">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="571">
    <ToTranslate>kanu0032</ToTranslate>
  </String>
  <String index="572">
    <ToTranslate>Kan&apos;u$&quot;What are you talking about? Is it not your fault for playing with that puppy?&quot;@</ToTranslate>
  </String>
  <String index="573">
    <ToTranslate>tcg_cho31ga</ToTranslate>
  </String>
  <String index="574">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="575">
    <ToTranslate>choh0002</ToTranslate>
  </String>
  <String index="576">
    <ToTranslate>???$&quot;Urrrgh, I guess you&apos;re right... By the way, who&apos;s this guy?&quot;@</ToTranslate>
  </String>
  <String index="577">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="578">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="579">
    <ToTranslate>kanu0033</ToTranslate>
  </String>
  <String index="580">
    <ToTranslate>Kan&apos;u$&quot;Hey, do not be rude. This man is the Messenger from Heaven we&apos;ve been seeking.&quot;@</ToTranslate>
  </String>
  <String index="581">
    <ToTranslate>tcg_cho31ja</ToTranslate>
  </String>
  <String index="582">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="583">
    <ToTranslate>choh0003</ToTranslate>
  </String>
  <String index="584">
    <ToTranslate>???$&quot;Wow! Onii-chan, you&apos;re a Messenger from Heaven?&quot;@</ToTranslate>
  </String>
  <String index="585">
    <ToTranslate>Kazuto$&quot;No, not really...&quot;@</ToTranslate>
  </String>
  <String index="586">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="587">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="588">
    <ToTranslate>choh0004</ToTranslate>
  </String>
  <String index="589">
    <ToTranslate>???$&quot;Okay, then I better introduce myself!&quot;@</ToTranslate>
  </String>
  <String index="590">
    <ToTranslate>Kazuto$&quot;(She isn&apos;t listening to me, is she...?)&quot;@</ToTranslate>
  </String>
  <String index="591">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="592">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="593">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="594">
    <ToTranslate>choh0005</ToTranslate>
  </String>
  <String index="595">
    <ToTranslate>???$&quot;Rinrin&apos;s family name is Chou! My first name is Hi! My common name is Yokutoku! And my true name is Rinrin!&quot;@</ToTranslate>
  </String>
  <String index="596">
    <ToTranslate>Kazuto$&quot;Chouhi... do you mean THE Chouhi?&quot;@</ToTranslate>
  </String>
  <String index="597">
    <ToTranslate>tcg_cho31ia</ToTranslate>
  </String>
  <String index="598">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="599">
    <ToTranslate>choh0006</ToTranslate>
  </String>
  <String index="600">
    <ToTranslate>Chouhi$&quot;I don&apos;t know, but I guess I am!&quot;@</ToTranslate>
  </String>
  <String index="601">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="602">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="603">
    <ToTranslate>kanu0034</ToTranslate>
  </String>
  <String index="604">
    <ToTranslate>Kan&apos;u$&quot;Hey, Rinrin, do not tell fibs. Who do you think you are?&quot;@</ToTranslate>
  </String>
  <String index="605">
    <ToTranslate>tcg_cho31ma</ToTranslate>
  </String>
  <String index="606">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="607">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="608">
    <ToTranslate>choh0007</ToTranslate>
  </String>
  <String index="609">
    <ToTranslate>Chouhi$&quot;Nyahaha! Maybe Rinrin&apos;s become really famous without knowing it!&quot;@</ToTranslate>
  </String>
  <String index="610">
    <ToTranslate>$As I watch Chouhi laugh, I try to think about the current situation.@</ToTranslate>
  </String>
  <String index="611">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="612">
    <ToTranslate>Kazuto$&quot;(...Kan&apos;u and Chouhi...Does that mean I&apos;m in the world of the Romance of the Three Kingdoms?)&quot;@</ToTranslate>
  </String>
  <String index="613">
    <ToTranslate>$Did I travel through time?@</ToTranslate>
  </String>
  <String index="614">
    <ToTranslate>Kazuto$&quot;(This isn&apos;t some anime or manga. That couldn&apos;t happen.)&quot;@</ToTranslate>
  </String>
  <String index="615">
    <ToTranslate>Kazuto$&quot;(But... given the situation I&apos;m in right now, I can&apos;t deny that possibility...)&quot;@</ToTranslate>
  </String>
  <String index="616">
    <ToTranslate>$There were guys in ancient garments who attacked with me real weapons.@</ToTranslate>
  </String>
  <String index="617">
    <ToTranslate>$There was also a girl who called herself Kan&apos;u and another girl who called herself Chouhi.@</ToTranslate>
  </String>
  <String index="618">
    <ToTranslate>$Every one of those is telling me I&apos;m not in Japan anymore.@</ToTranslate>
  </String>
  <String index="619">
    <ToTranslate>$Not only that, I&apos;m in a different era too.@</ToTranslate>
  </String>
  <String index="620">
    <ToTranslate>Kazuto$&quot;(Hahh...)&quot;@</ToTranslate>
  </String>
  <String index="621">
    <ToTranslate>$How did this happen...? But at least I&apos;m still alive.@</ToTranslate>
  </String>
  <String index="622">
    <ToTranslate>$As long as you&apos;re alive, things may still work out. That&apos;s the Hongou family precept.@</ToTranslate>
  </String>
  <String index="623">
    <ToTranslate>Kazuto$&quot;I guess I just have to accept this...&quot;@</ToTranslate>
  </String>
  <String index="624">
    <ToTranslate>tcg_kan21ba</ToTranslate>
  </String>
  <String index="625">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="626">
    <ToTranslate>kanu0035</ToTranslate>
  </String>
  <String index="627">
    <ToTranslate>Kan&apos;u$&quot;Oh! So you will accept it?!&quot;@</ToTranslate>
  </String>
  <String index="628">
    <ToTranslate>tcg_cho31ma</ToTranslate>
  </String>
  <String index="629">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="630">
    <ToTranslate>choh0008</ToTranslate>
  </String>
  <String index="631">
    <ToTranslate>Chouhi$&quot;Thanks, Onii-chan!&quot;@</ToTranslate>
  </String>
  <String index="632">
    <ToTranslate>Kazuto$&quot;Huh...?&quot;@</ToTranslate>
  </String>
  <String index="633">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="634">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="635">
    <ToTranslate>kanu0036</ToTranslate>
  </String>
  <String index="636">
    <ToTranslate>Kan&apos;u$&quot;You will become our leader and fight to bring order to this world enveloped in war!&quot;@</ToTranslate>
  </String>
  <String index="637">
    <ToTranslate>Kazuto$&quot;To fight...? Huh?&quot;@</ToTranslate>
  </String>
  <String index="638">
    <ToTranslate>choh0009</ToTranslate>
  </String>
  <String index="639">
    <ToTranslate>Chouhi$&quot;That&apos;s right, Onii-chan! You&apos;re going to be our master and help us save the weak!&quot;@</ToTranslate>
  </String>
  <String index="640">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="641">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="642">
    <ToTranslate>kanu0037</ToTranslate>
  </String>
  <String index="643">
    <ToTranslate>Kan&apos;u$&quot;That&apos;s right! Master! Let us rise against these wars, and fight for the sake of the people!&quot;@</ToTranslate>
  </String>
  <String index="644">
    <ToTranslate>choh0010</ToTranslate>
  </String>
  <String index="645">
    <ToTranslate>Chouhi$&quot;Okay! I&apos;m getting excited! Why don&apos;t we take down the Yellow Turban party to begin with?&quot;@</ToTranslate>
  </String>
  <String index="646">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="647">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="648">
    <ToTranslate>kanu0038</ToTranslate>
  </String>
  <String index="649">
    <ToTranslate>Kan&apos;u$&quot;Yes. I hear they hide in a valley near the border. Let us gather volunteers from the nearby village and raise an army!&quot;@</ToTranslate>
  </String>
  <String index="650">
    <ToTranslate>choh0011</ToTranslate>
  </String>
  <String index="651">
    <ToTranslate>Chouhi$&quot;That sounds great! Let&apos;s get going! Hurry, let&apos;s run!&quot;@</ToTranslate>
  </String>
  <String index="652">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="653">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="654">
    <ToTranslate>kanu0039</ToTranslate>
  </String>
  <String index="655">
    <ToTranslate>Kan&apos;u$&quot;Very well. Then why don&apos;t you go first, Rinrin, and gather the villagers for us. I will proceed together with our master.&quot;@</ToTranslate>
  </String>
  <String index="656">
    <ToTranslate>choh0012</ToTranslate>
  </String>
  <String index="657">
    <ToTranslate>Chouhi$&quot;Got it! I&apos;ll see you later, Onii-chan!&quot;@</ToTranslate>
  </String>
  <String index="658">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="659">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="660">
    <ToTranslate>$Before I could respond, Chouhi (or is it Rinrin?) ran off at a tremendous speed.@</ToTranslate>
  </String>
  <String index="661">
    <ToTranslate>tcg_kan21ba</ToTranslate>
  </String>
  <String index="662">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="663">
    <ToTranslate>bgm03</ToTranslate>
  </String>
  <String index="664">
    <ToTranslate>kanu0040</ToTranslate>
  </String>
  <String index="665">
    <ToTranslate>Kan&apos;u$&quot;Now, Master, let us make haste as well.&quot;@</ToTranslate>
  </String>
  <String index="666">
    <ToTranslate>Kazuto$&quot;...Wait a minute!&quot;@</ToTranslate>
  </String>
  <String index="667">
    <ToTranslate>$Just as Kan&apos;u reached her hand out to me with a broad smile, I had her stop.@</ToTranslate>
  </String>
  <String index="668">
    <ToTranslate>Kazuto$&quot;I still haven&apos;t fully understood what&apos;s going on. Moving the discussion along without me will cause problems.&quot;@</ToTranslate>
  </String>
  <String index="669">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="670">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="671">
    <ToTranslate>kanu0041</ToTranslate>
  </String>
  <String index="672">
    <ToTranslate>Kan&apos;u$&quot;It will...cause you problems?&quot;@</ToTranslate>
  </String>
  <String index="673">
    <ToTranslate>Kazuto$&quot;To be precise, I don&apos;t even know if it will or not.&quot;@</ToTranslate>
  </String>
  <String index="674">
    <ToTranslate>tcg_kan31ka</ToTranslate>
  </String>
  <String index="675">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="676">
    <ToTranslate>kanu0042</ToTranslate>
  </String>
  <String index="677">
    <ToTranslate>Kan&apos;u$&quot;Why...is that? Are you not a Messenger from Heaven?&quot;@</ToTranslate>
  </String>
  <String index="678">
    <ToTranslate>Kazuto$&quot;I still don&apos;t understand what this Messenger from Heaven thing means. All I know is that I&apos;m just a student.&quot;@</ToTranslate>
  </String>
  <String index="679">
    <ToTranslate>Kazuto$&quot;You want a student to quell your wars? To save the weak? Do you honestly believe I can do that when I don&apos;t even understand where I am?&quot;@</ToTranslate>
  </String>
  <String index="680">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="681">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="682">
    <ToTranslate>kanu0043</ToTranslate>
  </String>
  <String index="683">
    <ToTranslate>Kan&apos;u$&quot;Are you trying to say that you do not yet understand this world because you just descended from Heaven?&quot;@</ToTranslate>
  </String>
  <String index="684">
    <ToTranslate>Kazuto$&quot;Hmm... not exactly, but yeah, something like that. In other words, I don&apos;t know where I am or why I&apos;m here yet.&quot;@</ToTranslate>
  </String>
  <String index="685">
    <ToTranslate>Kazuto$&quot;Furthermore, you... Kan&apos;u and Chouhi are popular characters of a story in my country. They lived a long time ago, over 1800 years ago in fact.&quot;@</ToTranslate>
  </String>
  <String index="686">
    <ToTranslate>Kazuto$&quot;Is this the world from 1800 years ago? I just can&apos;t get my head around that.&quot;@</ToTranslate>
  </String>
  <String index="687">
    <ToTranslate>tcg_kan31ka</ToTranslate>
  </String>
  <String index="688">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="689">
    <ToTranslate>kanu0044</ToTranslate>
  </String>
  <String index="690">
    <ToTranslate>Kan&apos;u$&quot;Umm...I do not understand your meaning?&quot;@</ToTranslate>
  </String>
  <String index="691">
    <ToTranslate>Kazuto$&quot;Well... I guess it&apos;s not easy. I can&apos;t believe half of what I&apos;m saying myself. But... the reality is that I am here...&quot;@</ToTranslate>
  </String>
  <String index="692">
    <ToTranslate>$That&apos;s why I can understand this Kan&apos;u girl&apos;s reaction.@</ToTranslate>
  </String>
  <String index="693">
    <ToTranslate>Kazuto$&quot;Hahh...&quot;@</ToTranslate>
  </String>
  <String index="694">
    <ToTranslate>$What am I going to do now...?@</ToTranslate>
  </String>
  <String index="695">
    <ToTranslate>$The girl watched me sigh deeply, and then she spoke in a sad tone.@</ToTranslate>
  </String>
  <String index="696">
    <ToTranslate>tcg_kan31ja</ToTranslate>
  </String>
  <String index="697">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="698">
    <ToTranslate>tenten</ToTranslate>
  </String>
  <String index="699">
    <ToTranslate>kanu0045</ToTranslate>
  </String>
  <String index="700">
    <ToTranslate>Kan&apos;u$&quot;So...... Then you are not the Messenger from Heaven?&quot;@</ToTranslate>
  </String>
  <String index="701">
    <ToTranslate>Kazuto$&quot;Probably not. .....I&apos;m just a student who lived in the nation of Japan and attended St. Francessca Academy.&quot;@</ToTranslate>
  </String>
  <String index="702">
    <ToTranslate>tcg_kan31ka</ToTranslate>
  </String>
  <String index="703">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="704">
    <ToTranslate>kanu0046</ToTranslate>
  </String>
  <String index="705">
    <ToTranslate>Kan&apos;u$&quot;I... see...&quot;@</ToTranslate>
  </String>
  <String index="706">
    <ToTranslate>Kazuto$&quot;What&apos;s wrong?&quot;@</ToTranslate>
  </String>
  <String index="707">
    <ToTranslate>tcg_kan31ja</ToTranslate>
  </String>
  <String index="708">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="709">
    <ToTranslate>kanu0047</ToTranslate>
  </String>
  <String index="710">
    <ToTranslate>Kan&apos;u$&quot;I left my hometown with Rinrin to save the people suffering from these wars, searching for a master whom I could depend on and whom held the power to quell these conflicts.&quot;@</ToTranslate>
  </String>
  <String index="711">
    <ToTranslate>tcg_kan31ka</ToTranslate>
  </String>
  <String index="712">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="713">
    <ToTranslate>kanu0048</ToTranslate>
  </String>
  <String index="714">
    <ToTranslate>Kan&apos;u$&quot;However, the flames of war continued to spread, and those without the power to fight fell one after another. I was mortified. I was heartbroken...&quot;@</ToTranslate>
  </String>
  <String index="715">
    <ToTranslate>kanu0049</ToTranslate>
  </String>
  <String index="716">
    <ToTranslate>Kan&apos;u$&quot;Then I met Kanro and when I heard his prophecy I thought, &apos;Finally, I can help save people.&apos;&quot;@</ToTranslate>
  </String>
  <String index="717">
    <ToTranslate>kanu0050</ToTranslate>
  </String>
  <String index="718">
    <ToTranslate>Kan&apos;u$&quot;However... if you aren&apos;t a messenger of Heaven, what am I going to do? What will happen to this world? What will happen to those weak people? When I think about that...&quot;@</ToTranslate>
  </String>
  <String index="719">
    <ToTranslate>$Her obsidian eyes began to glisten with tears and her shoulders began to shake.@</ToTranslate>
  </String>
  <String index="720">
    <ToTranslate>tcg_kan21ta</ToTranslate>
  </String>
  <String index="721">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="722">
    <ToTranslate>kanu0051</ToTranslate>
  </String>
  <String index="723">
    <ToTranslate>Kan&apos;u$&quot;Pardon me. There is no use in telling you such things.&quot;@</ToTranslate>
  </String>
  <String index="724">
    <ToTranslate>Kazuto$&quot;...&quot;@</ToTranslate>
  </String>
  <String index="725">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="726">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="727">
    <ToTranslate>kanu0052</ToTranslate>
  </String>
  <String index="728">
    <ToTranslate>Kan&apos;u$&quot;I shall rejoin with Rinrin and we will fight against the Yellow Turbans lurking in the nearby village.&quot;@</ToTranslate>
  </String>
  <String index="729">
    <ToTranslate>kanu0053</ToTranslate>
  </String>
  <String index="730">
    <ToTranslate>Kan&apos;u$&quot;We will probably gather less than a hundred volunteer soldiers to aid us, but this battle is one for justice.&quot;@</ToTranslate>
  </String>
  <String index="731">
    <ToTranslate>Kan&apos;u$&quot;Surely...... Surely we shall win. ...You shall surely hear our names on your travels. When you do, please pray for our victory.&quot;@</ToTranslate>
  </String>
  <String index="732">
    <ToTranslate>Kazuto$&quot;...&quot;@</ToTranslate>
  </String>
  <String index="733">
    <ToTranslate>tcg_kan11ta</ToTranslate>
  </String>
  <String index="734">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="735">
    <ToTranslate>kanu0054</ToTranslate>
  </String>
  <String index="736">
    <ToTranslate>Kan&apos;u$&quot;Now then, if you&apos;ll excuse me.&quot;@</ToTranslate>
  </String>
  <String index="737">
    <ToTranslate>$Bow.@</ToTranslate>
  </String>
  <String index="738">
    <ToTranslate>$She forced a painful smile and began to take her leave after bowing.@</ToTranslate>
  </String>
  <String index="739">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="740">
    <ToTranslate>Kazuto$&quot;...&quot;@</ToTranslate>
  </String>
  <String index="741">
    <ToTranslate>$I couldn&apos;t bring myself to keep griping after I saw that heart-broken smile.@</ToTranslate>
  </String>
  <String index="742">
    <ToTranslate>Kazuto$&quot;Wait...&quot;@</ToTranslate>
  </String>
  <String index="743">
    <ToTranslate>$I called out to her before she left.@</ToTranslate>
  </String>
  <String index="744">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="745">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="746">
    <ToTranslate>kanu0055</ToTranslate>
  </String>
  <String index="747">
    <ToTranslate>Kan&apos;u$&quot;What is it?&quot;@</ToTranslate>
  </String>
  <String index="748">
    <ToTranslate>Kazuto$&quot;This Messenger from Heaven... It&apos;s something people will unite under, right?&quot;@</ToTranslate>
  </String>
  <String index="749">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="750">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="751">
    <ToTranslate>kanu0056</ToTranslate>
  </String>
  <String index="752">
    <ToTranslate>Kan&apos;u$&quot;Why of course. All the people suffering from these wars are waiting for a hero who will save them. They would surely welcome a hero descended from Heaven with open arms.&quot;@</ToTranslate>
  </String>
  <String index="753">
    <ToTranslate>Kazuto$&quot;Then you could gather a lot of volunteer soldiers?&quot;@</ToTranslate>
  </String>
  <String index="754">
    <ToTranslate>kanu0057</ToTranslate>
  </String>
  <String index="755">
    <ToTranslate>Kan&apos;u$&quot;Yes.&quot;@</ToTranslate>
  </String>
  <String index="756">
    <ToTranslate>Kazuto$&quot;...Alright. I don&apos;t consider myself a Messenger from Heaven, but I&apos;ll do what I can to help you. ...That way you&apos;ll gather more volunteers, right?&quot;@</ToTranslate>
  </String>
  <String index="757">
    <ToTranslate>tcg_kan31ka</ToTranslate>
  </String>
  <String index="758">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="759">
    <ToTranslate>kanu0058</ToTranslate>
  </String>
  <String index="760">
    <ToTranslate>Kan&apos;u$&quot;That is true, but... You are not the Messenger from Heaven, are you?&quot;@</ToTranslate>
  </String>
  <String index="761">
    <ToTranslate>Kazuto$&quot;I&apos;m probably not, but I still think I can help. ......I also have a fair deal of knowledge.&quot;@</ToTranslate>
  </String>
  <String index="762">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="763">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="764">
    <ToTranslate>kanu0059</ToTranslate>
  </String>
  <String index="765">
    <ToTranslate>Kan&apos;u$&quot;Knowledge, you say?&quot;@</ToTranslate>
  </String>
  <String index="766">
    <ToTranslate>Kazuto$&quot;Yeah. I read books on tactics at grandpa&apos;s house. I think that will help.&quot;@</ToTranslate>
  </String>
  <String index="767">
    <ToTranslate>tcg_kan31ja</ToTranslate>
  </String>
  <String index="768">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="769">
    <ToTranslate>kanu0060</ToTranslate>
  </String>
  <String index="770">
    <ToTranslate>Kan&apos;u$&quot;If you know tactics, then that certainly will be a great aid... but why?&quot;@</ToTranslate>
  </String>
  <String index="771">
    <ToTranslate>Kazuto$&quot;......I&apos;m not cold-hearted enough to leave a girl alone when she&apos;s in trouble.&quot;@</ToTranslate>
  </String>
  <String index="772">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="773">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="774">
    <ToTranslate>kanu0061</ToTranslate>
  </String>
  <String index="775">
    <ToTranslate>Kan&apos;u$&quot;Why...A girl you say?! I am nothing of the sort! I ask that you please do not insult me!&quot;@</ToTranslate>
  </String>
  <String index="776">
    <ToTranslate>Kazuto$&quot;Oh...I get it. So that&apos;s the type of character Kan&apos;u was, huh?&quot;@</ToTranslate>
  </String>
  <String index="777">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="778">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="779">
    <ToTranslate>kanu0062</ToTranslate>
  </String>
  <String index="780">
    <ToTranslate>Kan&apos;u$&quot;What is this &apos;character&apos;?! I cannot understand you when you use such words from Heaven! Either way, I ask that you do not disgrace me by treating me like a girl ever again!&quot;@</ToTranslate>
  </String>
  <String index="781">
    <ToTranslate>Kazuto$&quot;I had no intention of disgracing you whatsoever. But why not? Is it wrong to tell a pretty girl that she&apos;s pretty?&quot;@</ToTranslate>
  </String>
  <String index="782">
    <ToTranslate>$I shrugged my shoulders and continued to forestall any further argument.@</ToTranslate>
  </String>
  <String index="783">
    <ToTranslate>Kazuto$&quot;Anyway, I don&apos;t think I&apos;m the one who should decide whether or not I&apos;m a Messenger from Heaven.&quot;@</ToTranslate>
  </String>
  <String index="784">
    <ToTranslate>Kazuto$&quot;......You and Chouhi can decide that yourselves, though perhaps this world will be the one to decide that.&quot;@</ToTranslate>
  </String>
  <String index="785">
    <ToTranslate>$I calmly explained my plan for hereafter.@</ToTranslate>
  </String>
  <String index="786">
    <ToTranslate>Kazuto$&quot;So, as an expedient means, you can praise me as the Messenger from Heaven for now. But in return, if you ever deem me unworthy of the title...&quot;@</ToTranslate>
  </String>
  <String index="787">
    <ToTranslate>Kazuto$&quot;Abandon me then and there. I don&apos;t want anyone to suffer because of a lie.&quot;@</ToTranslate>
  </String>
  <String index="788">
    <ToTranslate>tcg_kan31ja</ToTranslate>
  </String>
  <String index="789">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="790">
    <ToTranslate>kanu0063</ToTranslate>
  </String>
  <String index="791">
    <ToTranslate>Kan&apos;u$&quot;But......Is that what you truly wish for?&quot;@</ToTranslate>
  </String>
  <String index="792">
    <ToTranslate>Kazuto$&quot;Yes. I&apos;ve already ran the calculations in my head for the merits and demerits to both you two and to me. I have made my decision with all of that in mind. ...Besides.&quot;@</ToTranslate>
  </String>
  <String index="793">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="794">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="795">
    <ToTranslate>hatena</ToTranslate>
  </String>
  <String index="796">
    <ToTranslate>kanu0064</ToTranslate>
  </String>
  <String index="797">
    <ToTranslate>Kan&apos;u$&quot;...?&quot;@</ToTranslate>
  </String>
  <String index="798">
    <ToTranslate>Kazuto$&quot;Whether she considers it a disgrace or not, I just don&apos;t want to see a pretty girl suffer.&quot;@</ToTranslate>
  </String>
  <String index="799">
    <ToTranslate>Kazuto$&quot;......What do you think? Will you accept it?&quot;@</ToTranslate>
  </String>
  <String index="800">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="801">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="802">
    <ToTranslate>kanu0065</ToTranslate>
  </String>
  <String index="803">
    <ToTranslate>Kan&apos;u$&quot;W-why, of course... Yes...&quot;@</ToTranslate>
  </String>
  <String index="804">
    <ToTranslate>Kazuto$&quot;Okay, then we&apos;re a team now.&quot;@</ToTranslate>
  </String>
  <String index="805">
    <ToTranslate>$I held out my hand towards Kan&apos;u.@</ToTranslate>
  </String>
  <String index="806">
    <ToTranslate>tcg_kan31ja</ToTranslate>
  </String>
  <String index="807">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="808">
    <ToTranslate>kanu0066</ToTranslate>
  </String>
  <String index="809">
    <ToTranslate>Kan&apos;u$&quot;Umm... Uhh...&quot;@</ToTranslate>
  </String>
  <String index="810">
    <ToTranslate>Kazuto$&quot;A handshake. To be on the same team.&quot;@</ToTranslate>
  </String>
  <String index="811">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="812">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="813">
    <ToTranslate>kanu0067</ToTranslate>
  </String>
  <String index="814">
    <ToTranslate>Kan&apos;u$&quot;......No, I cannot do that.&quot;@</ToTranslate>
  </String>
  <String index="815">
    <ToTranslate>Kazuto$&quot;Huh?&quot;@</ToTranslate>
  </String>
  <String index="816">
    <ToTranslate>kanu0068</ToTranslate>
  </String>
  <String index="817">
    <ToTranslate>Kan&apos;u$&quot;After hearing your words, I am convinced that you are indeed the Messenger from Heaven. Therefore...&quot;@</ToTranslate>
  </String>
  <String index="818">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="819">
    <ToTranslate>$Kan&apos;u bowed deeply as she spoke.@</ToTranslate>
  </String>
  <String index="820">
    <ToTranslate>Kazuto$&quot;Wait, what are you...?&quot;@</ToTranslate>
  </String>
  <String index="821">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="822">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="823">
    <ToTranslate>kanu0069</ToTranslate>
  </String>
  <String index="824">
    <ToTranslate>Kan&apos;u$&quot;My Lord, Messenger from Heaven, may we fight together and quell these wars.&quot;@</ToTranslate>
  </String>
  <String index="825">
    <ToTranslate>Kazuto$&quot;Lord...?&quot;@</ToTranslate>
  </String>
  <String index="826">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="827">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="828">
    <ToTranslate>kanu0070</ToTranslate>
  </String>
  <String index="829">
    <ToTranslate>Kan&apos;u$&quot;Yes. I have hereby acknowledge you as my Lord. Rinrin - I mean Chouhi - must feel the same. Therefore, please call us by our true names and treat us as your retainers.&quot;@</ToTranslate>
  </String>
  <String index="830">
    <ToTranslate>Kazuto$&quot;Huh?! R-retainers?!&quot;@</ToTranslate>
  </String>
  <String index="831">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="832">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="833">
    <ToTranslate>kanu0071</ToTranslate>
  </String>
  <String index="834">
    <ToTranslate>Kan&apos;u$&quot;Yes, my name is Kan&apos;u, my common name is Unchou, and my true name is Aisha. ...Please call me Aisha from now on. Henceforth, I shall you call you My Lord.&quot;@</ToTranslate>
  </String>
  <String index="835">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="836">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="837">
    <ToTranslate>kanu0072</ToTranslate>
  </String>
  <String index="838">
    <ToTranslate>Kan&apos;u$&quot;Now, My Lord, we shall make for Rinrin and drive the Yellow Turbans away. Let our battle hereby begin!&quot;@</ToTranslate>
  </String>
  <String index="839">
    <ToTranslate>Kazuto$&quot;W, wait, wait, wait! Listen to me...&quot;@</ToTranslate>
  </String>
  <String index="840">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="841">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="842">
    <ToTranslate>kanu0073</ToTranslate>
  </String>
  <String index="843">
    <ToTranslate>Kan&apos;u$&quot;Come, let us make haste! We shall fight for our people!&quot;@</ToTranslate>
  </String>
  <String index="844">
    <ToTranslate>Kazuto$&quot;Listen to me, please!&quot;@</ToTranslate>
  </String>
  <String index="845">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="846">
    <ToTranslate>bg_black</ToTranslate>
  </String>
</DBS>

</poem>
