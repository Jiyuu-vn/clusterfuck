[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_gi_outcast_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>aisy0751</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Aisha$&quot;Good! The enemy&apos;s front has fallen!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>syur0365</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Shuri$&quot;Chouhi squad, Bachou squad, please charge! Kan&apos;u squad, surround the enemy&apos;s right flank! Chouun squad, flank the enemy&apos;s left flank!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>shio0036</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Shion$&quot;Kouchuu squad will continue our cover fire from the rear. Three volleys to the front, then two the right flank. After that, advance on their left flank.&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>sui_0078</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>Sui$&quot;Come on, Rinrin, let&apos;s go!&quot;@</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>rinr0396</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Rinrin$&quot;Got it!&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>aisy0752</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>Aisha$&quot;Kan&apos;u squad move out! Follow my flag!&quot;@</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>Soldier$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>sei_0150</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Sei$&quot;Don&apos;t fall behind Kan&apos;u&apos;s squad! Let&apos;s show everyone how powerful our squad is!&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>Soldier$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>tcg_koc32ga</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>shio0037</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>Shion$&quot;Oh, my, my, they are all so excited.&quot;@</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>Kazuto$&quot;That&apos;s great.&quot;@</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>shio0038</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>Shion$&quot;It sure is. Well My Lord, I will lead my troops and help deliver the final blow.&quot;@</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>Kazuto$&quot;Yeah, I&apos;m counting on you.&quot;@</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>shio0039</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>Shion$&quot;Now then......&quot;@</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>SE028</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>Kazuto$&quot;Please...... come back safely, everyone.&quot;@</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_kyo12ca</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>kii_0030</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>Kyocho$&quot;Shuuran-sama! The front line is about to collapse! Our battle line will soon be destroyed!&quot;@</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>kaen0085</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>Kakouen$&quot;Hmm...... just as you said, Shuuran, Hongou&apos;s army has gotten powerful......&quot;@</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_ton14aa</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>kato0262</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>Kakouton$&quot;Yeah. Although they don&apos;t have that many soldiers, they are led by warriors worth a thousand men.&quot;@</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>tcg_en12ha</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>kaen0086</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>Kakouen$&quot;We are at a disadvantage......&quot;@</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_kyo12ca</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>kii_0031</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>Kyocho$&quot;What should we do?&quot;@</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>tcg_en12aa</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>kaen0087</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>Kakouen$&quot;......We&apos;re going to retreat.&quot;@</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>kato0263</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>Kakouton$&quot;We don&apos;t have any other choice, do we? Shit. If only I was more powerful......!&quot;@</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>tcg_en12da</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>kaen0088</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>Kakouen$&quot;You&apos;re not the only one, Aneja. We weren&apos;t powerful enough, either...... but this isn&apos;t the end quite yet.&quot;@</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_kyo12ea</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>kii_0032</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>Kyochho$&quot;Of course not! I&apos;m absolutely, positively going to destroy that Chouhi for sure!&quot;@</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_en12ba</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>kaen0089</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>Kakouen$&quot;Hmph, that&apos;s the spirit. Aneja, let&apos;s retreat.&quot;@</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_ton14ea</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>kato0264</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>Kakouton$&quot;......Ugh.&quot;@</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>tcg_en22ga</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>tcg_en22m</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>kaen0090</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>Kakouen$&quot;Don&apos;t cry.&quot;@</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>kato0265</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>Kakouton$&quot;I&apos;m not crying!&quot;@</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_en12aa</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>kaen0091</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>Kakouen$&quot;Then keep your head up. We&apos;ll gather the remaining soldiers and retreat. I&apos;ll bring up the rear, so Aneja, I&apos;m counting on you and Kii to lead the retreat.&quot;@</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>kato0266</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>Kakouton$&quot;......All right.&quot;@</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>kaen0092</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>Kakouen$&quot;I&apos;m counting on you. Sound the gong! Give the entire army the signal to retreat! Regrettably, our army is going to abandon the castle and withdraw!&quot;@</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>bcg_gi_outcast_a</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>bgm05</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>syur0366</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>Shuri$&quot;My Lord, the enemy has begun their retreat!&quot;@</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>Kazuto$&quot;Yeah. I can see their flags retreating.&quot;@</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>shio0040</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>Shion$&quot;That&apos;s another victory for now.&quot;@</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>Kazuto$&quot;......Yeah. What do you think? Should we pursue them?&quot;@</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>syur0367</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>Shuri$&quot;No. This is different from what happened with Enshou-san. We have suffered great losses during this battle. We should occupy the fortress and recuperate.&quot;@</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>Kazuto$&quot;Alright. Then we&apos;ll leave the enemy be as they retreat, and once they&apos;re gone, check the fortress.&quot;@</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>Kazuto$&quot;Shuri, Shion, you take care of the details.&quot;@</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>tcg_syo11ba</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>syur0368</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>Shuri$&quot;As you wish.&quot;@</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>Kazuto$&quot;......Whew.&quot;@</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>shio0041</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>Shion$&quot;Hu-hu...... you look exhausted.&quot;@</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>Kazuto$&quot;Yeah...... I&apos;m still not used to being on a battlefield......&quot;@</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>syur0369</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>Shuri$&quot;Shall we fall back to the rear?&quot;@</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>Kazuto$&quot;No. I don&apos;t want to be the only one in a safe place while everyone else is fighting. That&apos;s why I&apos;m hanging in here.&quot;@</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>tcg_koc32ga</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>shio0042</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>Shion$&quot;......I knew you&apos;d say that.&quot;@</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>Kazuto$&quot;I don&apos;t want to go against what I said. Though I know full well it&apos;s just self-satisfaction.&quot;@</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>tcg_syo11ba</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>syur0370</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>Shuri$&quot;No. It&apos;s because you try hard to stand on the front that everyone follows you.&quot;@</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>shio0043</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>Shion$&quot;As long as other people support your attitude, it&apos;s not self-satisfaction.&quot;@</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>Kazuto$&quot;......Is that how it is?&quot;@</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>syur0371</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>Shuri$&quot;That&apos;s how it is. But...... if you think you are tired, My Lord, please let us know immediately.&quot;@</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>shio0044</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>Shion$&quot;Supporting Our Lord......is one of the reasons we&apos;re here.&quot;@</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>Kazuto$&quot;......Thank you. I&apos;m counting on you.&quot;@</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>tcg_syo11ba</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>syur0372</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>Shuri$&quot;Hee, hee, hee...... please count on us to your heart&apos;s content!&quot;@</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>$After I smiled at Shuri, who tapped her chest, we headed into the abandoned fortress.@</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>bcg_go_impseat_a</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>tcg_rik11ca</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>bgm10</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>riku0023</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>Rikuson$&quot;Extra, extra. I have the results of the second match between Hongou and Sou Gi!&quot;@</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>tcg_son12aa</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>sonk0079</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>Sonken$&quot;Okay...... And?&quot;@</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>tcg_rik11ba</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>riku0024</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>Rikuson$&quot;Believe it or not, Hongou&apos;s army won again! Aren&apos;t they great? They really are powerful.&quot;@</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>kann0018</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>Kannei$&quot;Being powerful is good, but... if they are too powerful, it won&apos;t be good for us.&quot;@</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>tcg_son12ia</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>sonk0080</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>Sonken$&quot;Kouha is right. But on the other hand, this is a once-in-a-lifetime chance to crush Sougi.&quot;@</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="219">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="220">
    <ToTranslate>syuy0047</ToTranslate>
  </String>
  <String index="221">
    <ToTranslate>Shuuyu$&quot;Then as per the original plan, shall we join hands with Hongou?&quot;@</ToTranslate>
  </String>
  <String index="222">
    <ToTranslate>tcg_son12aa</ToTranslate>
  </String>
  <String index="223">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="224">
    <ToTranslate>sonk0081</ToTranslate>
  </String>
  <String index="225">
    <ToTranslate>Sonken$&quot;Yes, let&apos;s do that. I&apos;ll let Koukin choose our messenger.&quot;@</ToTranslate>
  </String>
  <String index="226">
    <ToTranslate>syuy0048</ToTranslate>
  </String>
  <String index="227">
    <ToTranslate>Shuuyu$&quot;As you wish.&quot;@</ToTranslate>
  </String>
  <String index="228">
    <ToTranslate>sonk0082</ToTranslate>
  </String>
  <String index="229">
    <ToTranslate>Sonken$&quot;Kouha, Hakugen, prepare to move out.&quot;@</ToTranslate>
  </String>
  <String index="230">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="231">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="232">
    <ToTranslate>kann0019</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>Kannei$&quot;Yes ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>tcg_rik11ba</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>riku0025</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>Rikuson$&quot;How big do you want the army to be?&quot;@</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>sonk0083</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>Sonken$&quot;We&apos;ll take as many soldiers as are ready. ......We must demonstrate our nation&apos;s power. No objections, right, Koukin?&quot;@</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>tcg_syu11ca</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>syuy0049</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>Shuuyu$&quot;......As you wish.&quot;@</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>tcg_rik11aa</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>riku0026</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>Rikuson$&quot;Well...... th-then I&apos;ll go give the command to the entire army, just as Renfa-sama said.&quot;@</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>sonk0084</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>Sonken$&quot;I&apos;ll leave it to you. This battle will be a cornerstone in the history of Son-Go. I hope you will all do your best.&quot;@</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>kann0020</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>Kannei$&quot;Yes ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>sonk0085</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>Sonken$&quot;You&apos;re excused!&quot;@</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>syuy0050</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>Shuuyu$&quot;.........&quot;@</ToTranslate>
  </String>
