  <String index="375">
    <ToTranslate>muka</ToTranslate>
  </String>
  <String index="376">
    <ToTranslate>kii_0027</ToTranslate>
  </String>
  <String index="377">
    <ToTranslate>Kyocho$&quot;She didn&apos;t get away! I let her go!&quot;@</ToTranslate>
  </String>
  <String index="378">
    <ToTranslate>kato0259</ToTranslate>
  </String>
  <String index="379">
    <ToTranslate>Kakouton$&quot;......That&apos;s her version.&quot;@</ToTranslate>
  </String>
  <String index="380">
    <ToTranslate>tcg_en12ba</ToTranslate>
  </String>
  <String index="381">
