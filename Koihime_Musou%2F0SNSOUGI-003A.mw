  <String index="340">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="341">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="342">
    <ToTranslate>Soldier$&quot;Yes sir!&quot;@</ToTranslate>
  </String>
  <String index="343">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="344">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="345">
    <ToTranslate>syur0363</ToTranslate>
  </String>
  <String index="346">
    <ToTranslate>Shuri$&quot;As soon as everyone returns, we&apos;ll reform and attack!&quot;@</ToTranslate>
  </String>
  <String index="347">
    <ToTranslate>Kazuto$&quot;Alright. Shuri, please issue the orders!&quot;@</ToTranslate>
  </String>
  <String index="348">
    <ToTranslate>syur0364</ToTranslate>
  </String>
  <String index="349">
