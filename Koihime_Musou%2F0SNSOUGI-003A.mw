<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_ton14ga</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>kato0229</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Kakouton$&quot;Shit...... I didn&apos;t know Hongou&apos;s army was this strong!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_kyo12ca</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>kii_0011</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Kyocho$&quot;What do we do, Shunran-sama?!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>kato0230</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Kakouton$&quot;Shuuran should be here soon with reinforcements! Until then we&apos;ll just have to dig our heels in!&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>kii_0012</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>Kyocho$&quot;I&apos;m doing that, but I don&apos;t think we can hold out that long?!&quot;@</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>kato0231</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>Kakouton$&quot;Don&apos;t act weak! If they pass through here, they&apos;ll have a direct path to Xuchang!&quot;@</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_kyo22ga</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>kii_0013</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Kyocho$&quot;Ooo, I&apos;ll do my best so don&apos;t yell at me!&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>SE018</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>kato0232</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>Kakouton$&quot;Kii?!&quot;@</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>battle_kabuto</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>m_yure_y</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_kyo22da</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>kii_0014</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Kyocho$&quot;Wahh! W-what was that?&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_ton14ga</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>kato0233</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Kakouton$&quot;Tsk! I can&apos;t believe we let you get so close, Chouhi!&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>rinr0383</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>Rinrin$&quot;Hee, hee, you found me. But I&apos;m impressed that you managed to knock down Shion&apos;s arrow.&quot;@</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>kato0234</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>Kakouton$&quot;This belongs to Kouchuu, huh......?&quot;@</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>tcg_cho31ia</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>rinr0384</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>Rinrin$&quot;She gave me cover fire.&quot;@</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>tcg_kyo12ea</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>kii_0015</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>Kyocho$&quot;Oooo, how dare you shoot at me!&quot;@</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>rinr0385</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Rinrin$&quot;No, actually, we were aiming for Kakouton-oneechan. You just happened to be in the way, that&apos;s all. No point bothering with small fry like you.&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>kii_0016</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>Kyocho$&quot;Small fry? Am I small fry? Argh, Shunran-sama! I&apos;m going to take her down!&quot;@</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>tcg_ton14aa</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>kato0235</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>Kakouton$&quot;Hmm...... Okay, I&apos;ll let you take care of her, Kii. I&apos;m going to urge on the soldiers in the right flank.&quot;@</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>kii_0017</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>Kyocho$&quot;Okay!&quot;@</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>kato0236</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>Kakouton$&quot;......Good luck.&quot;@</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>rinr0386</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>Rinrin$&quot;Ahh! I won&apos;t let you get away!&quot;@</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>tcg_kyo31aa</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>tcg_kyo31m</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>kii_0018</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>Kyocho$&quot;I&apos;m you&apos;re opponent!&quot;@</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>battle_kyo</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>SE017</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>m_yure_r</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_cho71aa</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>tcg_cho71m</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>rinr0387</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>Rinrin$&quot;Whoah!?&quot;@</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>rinr0388</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>Rinrin$&quot;Hey, that&apos;s dangerous.&quot;@</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>tcg_kyo22ba</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>kii_0019</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>Kyocho$&quot;Hmph! This is what you get for calling me small fry!&quot;@</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>rinr0389</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>Rinrin$&quot;Hmmm...... but you are small fry, no two ways about it!&quot;@</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_kyo31aa</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_kyo31m</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>kii_0020</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>Kyocho$&quot;Hey, you called me small fry again! You&apos;ll pay for that!&quot;@</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>battle_kyo</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_cho51aa</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_cho51m</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>rinr0390</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>Rinrin$&quot;Wow.&quot;@</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_kyo22ha</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>kii_0021</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>Kyocho$&quot;Oooo! Don&apos;t dodge, coward!&quot;@</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>tcg_cho21da</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>rinr0391</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>Rinrin$&quot;Don&apos;t be ridiculous. If I don&apos;t dodge, I&apos;ll die.&quot;@</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>tcg_kyo12ea</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>kii_0022</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>Kyocho$&quot;Die, midget!&quot;@</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>rinr0392</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>Rinrin$&quot;Don&apos;t call me midget, flat chest.&quot;@</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>tcg_kyo22ha</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>kii_0023</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>Kyocho$&quot;Look at yourself, you&apos;re flat, too!&quot;@</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>tcg_cho21ia</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>rinr0393</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>Rinrin$&quot;Rinrin still has plenty of room to grow. But you&apos;re......heh-heh, all out of luck.&quot;@</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>tcg_kyo12ea</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>kii_0024</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>Kyocho$&quot;Ooo...... I&apos;m going to blow you away!&quot;@</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>rinr0394</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>Rinrin$&quot;Go ahead, do it if you can!&quot;@</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>tcg_kyo31aa</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>tcg_kyo31m</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>kii_0025</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>Kyocho$&quot;Sure thing! Here I come!&quot;@</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>tcg_kyo91aa</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>tcg_kyo91m</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>BSE09</ToTranslate>
  </String>
