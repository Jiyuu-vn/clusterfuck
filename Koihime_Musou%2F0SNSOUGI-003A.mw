  <String index="308">
    <ToTranslate>kato0254</ToTranslate>
  </String>
  <String index="309">
    <ToTranslate>Kakouton$&quot;Running away? Hmph...... don&apos;t get the wrong idea. I&apos;m going to fight you as a general, not as a soldier.&quot;@</ToTranslate>
  </String>
  <String index="310">
    <ToTranslate>tcg_ton14aa</ToTranslate>
