    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>bcg_go_impseat_a</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>tcg_syu11ca</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>syuy0051</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>Shuuyu$&quot;Hmph, do you want me to take the bait......that badly, My Lord?&quot;@</ToTranslate>
  </String>
