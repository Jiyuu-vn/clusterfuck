    <ToTranslate>tcg_ton32m</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>kato0243</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>Kakouton$&quot;......&quot;@</ToTranslate>
  </String>
