  </String>
  <String index="149">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>tcg_ton14ea</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>kato0237</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>Kakouton$&quot;Kii......I hope she&apos;ll be okay......&quot;@</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>aisy0736</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>Aisha$&quot;Do you have time to be worrying about someone else?&quot;@</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>tcg_ton14ga</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>kato0238</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>Kakouton$&quot;......Kan&apos;u, huh?&quot;@</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>aisy0737</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>Aisha$&quot;Long time no see, Kakouton.&quot;@</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>kato0239</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>Kakouton$&quot;Hmph...... I think we last saw each other at the Anti-Toutaku Alliance&apos;s camp, am I right?&quot;@</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>tcg_kan11fa</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>aisy0738</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>Aisha$&quot;That&apos;s right. I&apos;ve wanted to give you a taste of my blade ever since that day......&quot;@</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>SE014</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>aisy0739</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>Aisha$&quot;Now, face me!&quot;@</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>tcg_ton32aa</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>tcg_ton32m</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>kato0240</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>Kakouton$&quot;Very well......&quot;@</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>SE013</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>kato0241</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>Kakouton$&quot;Kneel before my blade, Kan&apos;u!&quot;@</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>aisy0740</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>Aisha$&quot;I&apos;ll say the same to you!&quot;@</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>tcg_ton32aa</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>tcg_ton32m</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>kato0242</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>Kakouton$&quot;Hmph. En garde.&quot;@</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>aisy0741</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>Aisha$&quot;......&quot;@</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>tcg_ton32aa</ToTranslate>
  </String>
