  <String index="333">
    <ToTranslate>Shuri$&quot;My Lord! Our enemy has received reinforcements! We must call back everyone on the front!&quot;@</ToTranslate>
  </String>
  <String index="334">
    <ToTranslate>Kazuto$&quot;Call them back?&quot;@</ToTranslate>
  </String>
  <String index="335">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="336">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="337">
    <ToTranslate>shio0035</ToTranslate>
  </String>
  <String index="338">
    <ToTranslate>Shion$&quot;Yes! We must call everyone back and reassemble our formation immediately or it&apos;ll be too dangerous.&quot;@</ToTranslate>
  </String>
  <String index="339">
    <ToTranslate>Kazuto$&quot;I see...... Okay, sound the gong and signal them to retreat!&quot;@</ToTranslate>
