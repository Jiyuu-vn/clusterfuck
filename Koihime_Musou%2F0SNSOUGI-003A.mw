  <String index="396">
    <ToTranslate>kato0261</ToTranslate>
  </String>
  <String index="397">
    <ToTranslate>Kakouton$&quot;Alright.&quot;@</ToTranslate>
  </String>
  <String index="398">
    <ToTranslate>tcg_kyo22ba</ToTranslate>
  </String>
  <String index="399">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="400">
    <ToTranslate>kii_0029</ToTranslate>
  </String>
  <String index="401">
    <ToTranslate>Kyocho$&quot;Hurry! Let&apos;s hurry up and go!&quot;@</ToTranslate>
  </String>
  <String index="402">
    <ToTranslate>kaen0084</ToTranslate>
  </String>
  <String index="403">
