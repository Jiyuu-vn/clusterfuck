[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_castcorrid_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>bgm02</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>$I was roaming the palace, looking for Aisha, because I had to consult her on a few orders regarding military affairs.@</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>Kazuto$&quot;Where did she go?&quot;@</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>$I couldn&apos;t find her in her room or in the war ministry.@</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Kazuto$&quot;Did she go on patrol?&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>$Then I heard voices from the gardens.@</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>Kazuto$&quot;Could that be Aisha?&quot;@</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>$I walked through the gardens to check.@</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>bcg_castgurden_a</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>bgm26</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>aisy1134</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>Aisha$&quot;You suddenly show up and demand we spar...? What happened?&quot;@</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>tcg_cry11ca</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>chor0502</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>Chouryou$&quot;Oh, it&apos;s nothing. I just thought I should ask you because I haven&apos;t much chance to practice lately.&quot;@</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>aisy1135</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Aisha$&quot;Is that what you want to spar for? Hmm... no wonder people call you the god of speed, Chouryou.&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>chor0503</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>Chouryou$&quot;I&apos;m flattered that you would compliment me, Aisha.&quot;@</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>aisy1136</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Aisha$&quot;Of course I&apos;d compliment you. I was just in need of some training myself. I&apos;ll gladly spar with you.&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>chor0504</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>Chouryou$&quot;Thanks.&quot;@</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_cry41aa</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_cry41m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>chor0505</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Chouryou$&quot;All right... Shall we?&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>aisy1137</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>Aisha$&quot;Yes.&quot;@</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>aisy1138</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>Aisha$&quot;Come at me any time.&quot;@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>$They both held their weapons, looking for a chance to strike.@</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>SE071</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_cry11aa</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>aisy1139</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>Aisha$&quot;What&apos;s keeping you, Shia? Won&apos;t you come?&quot; @</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_cry21ga</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>chor0506</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>Chouryou$&quot;If I see a chance.&quot;@</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>aisy1140</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>Aisha$&quot;I will not give you a chance to attack... If you&apos;re not coming, then I shall strike first.&quot;@</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>chor0507</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>Chouryou$&quot;Come on, try me.&quot;@</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>aisy1141</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>Aisha$&quot;Huh... you&apos;re a braggart, aren&apos;t you?&quot;@</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>chor0508</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>Chouryou$&quot;Heh heh, so are you.&quot;@</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>$They circled around in order to close the distance, looking for a chance to attack.@</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>aisy1142</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>Aisha$&quot;I heard... that you imitated my weapon.&quot;@</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_cry11ca</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>chor0509</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>Chouryou$&quot;That&apos;s right. When I saw you fighting at Hulao Pass, I fell in love.&quot;@</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>aisy1143</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>Aisha$&quot;......&quot;@</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_cry21da</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>chor0510</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>Chouryou$&quot;I changed my weapon because I wanted to get as close to you as possible.&quot;@</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>chor0511</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>Chouryou$&quot;I&apos;m happy because you&apos;re looking me in the eyes, even if it&apos;s just for this moment... I feel like you are mine and mine alone.&quot;@</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>aisy1144</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>Aisha$&quot;Stop joking around...&quot;@</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>chor0512</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>Chouryou$&quot;It&apos;s not a joke. I love you, Aisha.&quot;@</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_kan31ja</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>aisy1145</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>Aisha$&quot;Uh...&quot;@</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_cry11aa</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>chor0513</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>Chouryou$&quot;Gotcha!&quot;@</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>chor0514</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>Chouryou$&quot;Yaaaarrrgh!&quot;@</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>ecg_cry01a</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>$Chouryou stepped forward strongly, swinging her Green Dragon Crescent Blade.@</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>bcg_castgurden_a</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_kan11fa</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>aisy1146</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>Aisha$&quot;Tssk...!&quot;@</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>battle_harai</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>s_yure_y</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>tcg_kan61aa</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_kan61m</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>aisy1147</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>Aisha$&quot;Ghh... you agitated me for a chance to attack? Impressive, Chouryou.&quot;@</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>tcg_cry21da</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>chor0515</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>Chouryou$&quot;Heh-heh. Are you complimenting me? Thanks.&quot;@</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>aisy1148</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>Aisha$&quot;Now it&apos;s my turn. Take this, Shia!&quot;@</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>tcg_kan91aa</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>tcg_kan91m</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>aisy1149</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>Aisha$&quot;Yaaaaahhh!&quot;@</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>battle_kesa</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>s_yure_r</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>tcg_cry61aa</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>tcg_cry61m</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>chor0516</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>Chouryou$&quot;Ghh...!&quot;@</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>tcg_cry11aa</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>chor0517</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>Chouryou$&quot;Tsk... your strike is powerful, Aisha. No wonder why people call you Kan Unchou, the god of war.&quot;@</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>aisy1150</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>Aisha$&quot;I feel the same way about you. I thought the god of speed referred to the speed of your army&apos;s march, but...&quot; @</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>tcg_cry11ca</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>chor0518</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>Chouryou$&quot;It also refers to my spear.&quot;@</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>aisy1151</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>Aisha$&quot;You&apos;re right. Your last attack almost got me.&quot;@</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>tcg_cry31la</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>tcg_cry31ml</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>bgm26</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>chor0519</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>Chouryou$&quot;Tee-hee... Ah, I can&apos;t stop smirking...&quot;@</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>aisy1152</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>Aisha$&quot;You&apos;re twisted...&quot;@</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>chor0520</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>Chouryou$&quot;It&apos;s natural to smile when somebody I like compliments me.&quot;@</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>aisy1153</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>Aisha$&quot;Stop saying those bizarre things... I mean... I get embarrassed.&quot;@</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>tcg_cry11oa</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>chor0521</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>Chouryou$&quot;Ooh... I can&apos;t. I can&apos;t, I can&apos;t. I can&apos;t concentrate anymore.&quot;@</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>SE021</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>aisy1154</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>Aisha$&quot;Oh? Are we finished with just one round?&quot;@</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>tcg_cry31la</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>tcg_cry31ml</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>chor0522</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>Chouryou$&quot;Aisha, I love you.&quot;@</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>tcg_kan31ga</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>aisy1155</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>Aisha$&quot;Whoa!? Wh-what? Hey... stay away from me, Shia...!&quot;@</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>ecg_cry02a</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>bgm20</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>chor0523</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>Chouryou$&quot;Hee-hee... your hair is so beautiful, Aisha...&quot;@</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>aisy1156</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>Aisha$&quot;Sh-Shia, stop joking. Just get away from me...&quot;@</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>chor0524</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>Chouryou$&quot;No. Let me cuddle you like this for a bit...&quot;@</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>aisy1157</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>Aisha$&quot;Ooh... j-just a bit, okay...?&quot;@</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>chor0525</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>Chouryou$&quot;Okay, okay, just a bit... Hee-hee... *sniff, sniff...*&quot;@</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>aisy1158</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>Aisha$&quot;Hey... don&apos;t smell my hair!&quot;@</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>chor0526</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>Chouryou$&quot;Ah. It smells so good, though...&quot;@</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>aisy1159</ToTranslate>
