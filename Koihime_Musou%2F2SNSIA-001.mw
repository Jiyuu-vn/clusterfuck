  <String index="204">
    <ToTranslate>chor0244</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>Chouryou$&quot;Isn&apos;t that okay every now and then? Let&apos;s help them.&quot;@</ToTranslate>
