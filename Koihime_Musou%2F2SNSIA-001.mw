  <String index="243">
    <ToTranslate>aisy1112</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>Aisha$&quot;Uh... I, umm... I feel a bit glad, as a woman...&quot;@</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>chor0248</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>Chouryou$&quot;Oh!? It sounds like I might have a chance!&quot;@</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>aisy1113</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>Aisha$&quot;Wh-what chance!? Whatever it is, you don&apos;t have it!&quot;@</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>chor0249</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>Chouryou$&quot;What...? I&apos;m disappointed that you deny it so flatly...&quot;@</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>sei_0396</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>Sei$&quot;Chouryou, when girls say no, they often mean the opposite. Pay her no mind.&quot;@</ToTranslate>
