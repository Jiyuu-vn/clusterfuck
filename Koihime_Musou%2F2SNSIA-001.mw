  <String index="319">
    <ToTranslate>Aisha$&quot;Shia.&quot;@</ToTranslate>
  </String>
  <String index="320">
    <ToTranslate>chor0257</ToTranslate>
  </String>
  <String index="321">
    <ToTranslate>Chouryou$&quot;Hahhh! Awesome! I didn&apos;t know I&apos;d feel this happy when somebody I love called me by my true name.&quot; @</ToTranslate>
