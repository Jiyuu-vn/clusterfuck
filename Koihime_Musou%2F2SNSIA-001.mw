  <String index="304">
    <ToTranslate>chor0254</ToTranslate>
  </String>
  <String index="305">
    <ToTranslate>Chouryou$&quot;Kan&apos;u... don&apos;t call me by that name. It&apos;s so sad when you sound like you&apos;re keeping a distance from me...&quot; @</ToTranslate>
  </String>
  <String index="306">
    <ToTranslate>aisy1122</ToTranslate>
  </String>
  <String index="307">
    <ToTranslate>Aisha$&quot;B-but I haven&apos;t quite befriended you yet...&quot;@</ToTranslate>
  </String>
  <String index="308">
    <ToTranslate>sei_0402</ToTranslate>
  </String>
  <String index="309">
    <ToTranslate>Sei$&quot;Why don&apos;t we befriend each other then? My full name is Chouun, my common name is Shiryuu, and true name is Sei.&quot;@</ToTranslate>
  </String>
  <String index="310">
    <ToTranslate>chor0255</ToTranslate>
  </String>
  <String index="311">
    <ToTranslate>Chouryou$&quot;Thanks. My true name is Shia. Call me Shia.&quot;@</ToTranslate>
  </String>
  <String index="312">
    <ToTranslate>sei_0403</ToTranslate>
  </String>
  <String index="313">
    <ToTranslate>Sei$&quot;Yes, I will... Come on, Aisha. Tell her your true name.&quot;@</ToTranslate>
  </String>
  <String index="314">
    <ToTranslate>aisy1123</ToTranslate>
  </String>
  <String index="315">
    <ToTranslate>Aisha$&quot;My true name is Aisha, Shia.&quot;@</ToTranslate>
  </String>
  <String index="316">
    <ToTranslate>chor0256</ToTranslate>
  </String>
  <String index="317">
    <ToTranslate>Chouryou$&quot;Ahhh. Aisha, will you say my name again?&quot;@</ToTranslate>
