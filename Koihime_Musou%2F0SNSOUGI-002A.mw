[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm0000</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>sei_0127</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Sei$&quot;Hmph. No wonder she prattles on about being the might of Gi... She&apos;s pretty tough.&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>shio0019</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Shion$&quot;Yes, she is. We&apos;re going to lose a lot of men, but we have to hang in there somehow.&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>sei_0128</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>Sei$&quot;Yeah. The messenger we sent should have arrived at the main outpost by now. If we hold out, we can win.&quot;@</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>shio0020</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>Shion$&quot;No, Sei-chan, that&apos;s not right.&quot;@</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>tcg_cun11da</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>sei_0129</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>Sei$&quot;No? Oh...Am I misunderstanding something?&quot;@</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>shio0021</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Shion$&quot;Yes. We must win this battle by ourselves. That&apos;s how we&apos;ll create an opening to attack Gi.&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>sei_0130</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Sei$&quot;...I see. Once we dispose of Kakouton, we&apos;ll unite with our reinforcements and surge into Gi&apos;s territory.&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>shio0022</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>Shion$&quot;Yes. It&apos;s going to be a tough battle, but let&apos;s do our best for Our Lord.&quot;@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>sei_0131</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>Sei$&quot;Of course. Shion, watch my back.&quot;@</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>shio0023</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>Shion$&quot;Of course.&quot;@</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>bcg_impseat_a</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>Soldier$&quot;Reporting!&quot;@</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>sui_0064</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>Sui$&quot;Yes! What is it?&quot;@</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>Soldier$&quot;I bear a message from General Chouun on the front! Gi&apos;s army has crossed the border and invaded our land! Requesting reinforcements immediately!&quot;@</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>tcg_syo21ca</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>syur0345</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>Shuri$&quot;Hauah?! H-How can that... Did they manage to find out about our plan?&quot;@</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>sui_0065</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>Sui$&quot;They must have. Dammit! They&apos;re one step ahead of us.&quot;@</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>tcg_syo21ca</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>syur0346</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>Shuri$&quot;Hawawa, what do we do, what do we do, what do we do, what do we do......?!&quot;@</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>aisy0724</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>Aisha$&quot;Calm down, Shuri. Even if Gi&apos;s army is attacking, Sei and Shion are defending. The outpost won&apos;t fall that easily.&quot;@</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>rinr0370</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>Rinrin$&quot;But we must send reinforcements quickly, or Sei and Shion will be in danger!&quot;@</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>Kazuto$&quot;That&apos;s true. Let&apos;s send them immediately. Shuri.&quot;@</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>tcg_syo21ca</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>syur0347</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>Shuri$&quot;Y-yes!&quot;@</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>Kazuto$&quot;What kind of reinforcements should we send?&quot;@</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>syur0348</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>Shuri$&quot;Hauah?! U-umm, uhh......&quot;@</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_syo21da</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>syur0349</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>Shuri$&quot;Oh, I know! Let&apos;s see, why don&apos;t we all go as reinforcements?&quot;@</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>rinr0371</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>Rinrin$&quot;Agreed!&quot;@</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>aisy0725</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>Aisha$&quot;Wait, Rinrin. Don&apos;t just agree without knowing why.&quot;@</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>rinr0372</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>Rinrin$&quot;Boo......&quot;@</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>sui_0066</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>Sui$&quot;Why do you think we should all go? Can you explain?&quot;@</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>syur0350</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>Shuri$&quot;Yes. You see, our strategy to send troops to Gi&apos;s borders in small units and surprise them has been uncovered, and thus failed.&quot;@</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>aisy0726</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>Aisha$&quot;That&apos;s right. I don&apos;t know how...... but judging by the way Gi&apos;s army is moving, they must have found out.&quot;@</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>syur0351</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>Shuri$&quot;That&apos;s right. But let&apos;s not worry about how they found out right now.&quot;@</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>Kazuto$&quot;Hmmm...... and?&quot;@</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_syo21da</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>syur0352</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>Shuri$&quot;So, since they found out, there&apos;s no point to playing petty tricks at this point.&quot;@</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>syur0353</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>Shuri$&quot;Therefore, I think it&apos;s best for us to lead a huge army to rescue Sei-san and the others, crush the enemy there, and use that momentum to continue our assault on Gi.&quot;@</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>tcg_bac12ba</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>sui_0067</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>Sui$&quot;I see. We are going to knock out Gi with a frontal attack, huh?&quot;@</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>syur0354</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>Shuri$&quot;That&apos;s right. If Sei-san has already defeated the enemy, then we can invade Gi without further losses,&quot;@</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>Shuri$&quot;but even if she hasn&apos;t, our huge army will be able to smash the enemy and rescue Sei-san and Shion-san.&quot;@</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>rinr0373</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>Rinrin$&quot;We&apos;ll kill two birds with one stone!&quot;@</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>Kazuto$&quot;......Okay. Let&apos;s get on with it.&quot;@</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>tcg_bac12ba</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>aisy0727</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>Aisha$&quot;Yes. I&apos;ll inform the troops we&apos;re deploying! Our departure will be shortly hereafter, correct?&quot;@</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>Kazuto$&quot;The sooner the better. I&apos;m counting on you, Aisha!&quot;@</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>aisy0728</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>Aisha$&quot;As you wish! Okay everyone, go back to your stations and get ready! Don&apos;t waste even a second!&quot;@</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>sui_0068</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>Sui$&quot;Okay! I can hardly wait!&quot;@</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>rinr0374</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>Rinrin$&quot;Charge, smash, victory!&quot;@</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>tcg_cun11da</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>bgm0000</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>sei_0132</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>Sei$&quot;Mmm......?&quot;@</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>tcg_koc22da</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>shio0024</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>Shion$&quot;What is it?&quot;@</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>sei_0133</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>Sei$&quot;Nothing...... I thought I just heard Rinrin and Sui&apos;s voices.&quot;@</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>shio0025</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>Shion$&quot;Oh my. I bet they are ready to depart at the main fortress.&quot;@</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>sei_0134</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>Sei$&quot;What makes you say that? Is it your intuition?&quot;@</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>shio0026</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>Shion$&quot;Yeah, it&apos;s my intuition.... But I really mean it.&quot;@</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>sei_0135</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>Sei$&quot;...I see. But under the circumstances, maybe it is possible for their voices to reach us here.&quot;@</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>tcg_koc12aa</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>shio0027</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>Shion$&quot;Right? Rinrin-chan and the others are so energetic. Now that we know that, we can&apos;t lose this battle.&quot;@</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>sei_0136</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>Sei$&quot;I have no intention of losing.&quot;@</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>shio0028</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>Shion$&quot;Hu-hu-hu, how encouraging! Well then, General Chouun, please give us your command.&quot;@</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>sei_0137</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>Sei$&quot;Brave soldiers of Youzhou! Don&apos;t forget that our future depends on this battle! Swing your spears, shoot your arrows! Seize victory with your own hands!&quot;@</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>sei_0138</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>Sei$&quot;All units, charge!&quot;@</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>SE124</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>bg_black</ToTranslate>
  </String>
