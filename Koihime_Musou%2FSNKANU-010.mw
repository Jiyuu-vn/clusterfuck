  </String>
  <String index="417">
    <ToTranslate>tcg_zo211m</ToTranslate>
  </String>
  <String index="418">
    <ToTranslate>Midget$&quot;This bitch... I-Is she......?&quot;@</ToTranslate>
  </String>
  <String index="419">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="420">
    <ToTranslate>SE010</ToTranslate>
  </String>
  <String index="421">
    <ToTranslate>tcg_kan51ac</ToTranslate>
  </String>
  <String index="422">
    <ToTranslate>tcg_kan51m</ToTranslate>
  </String>
  <String index="423">
    <ToTranslate>aisy2381</ToTranslate>
  </String>
  <String index="424">
    <ToTranslate>Aisha$&quot;Repent what thou hast done!&quot;@</ToTranslate>
  </String>
  <String index="425">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="426">
