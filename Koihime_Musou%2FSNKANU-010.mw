  <String index="403">
    <ToTranslate>tcg_zo211m</ToTranslate>
  </String>
  <String index="404">
    <ToTranslate>Bro$&quot;Wait a damn minute! Having legs doesn&apos;t mean we aren&apos;t ghosts! Think about it!&quot;@</ToTranslate>
  </String>
  <String index="405">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="406">
    <ToTranslate>tcg_kan41ac</ToTranslate>
  </String>
  <String index="407">
