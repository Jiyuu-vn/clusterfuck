    <ToTranslate>tcg_kyo91aa</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_kyo91m</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>kii_0074</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>Kyocho$&quot;Yahhhh!!&quot;@</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>battle_naguri</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>SE004</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>yure_r</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_zak11aa</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_zak11m</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>Soldier$&quot;Guahh!&quot;@</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_kyo12ea</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>kii_0075</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>Kyocho$&quot;Shuuran-sama! Now!&quot;@</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>kaen0188</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>Kakouen$&quot;I know! Keifa!&quot;@</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>tcg_jun21ha</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>jyun0167</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>Jun&apos;iku$&quot;Got it! Karin-sama!&quot;@</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>tcg_sou32oa</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>tcg_sou32m</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>sous0246</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>Sousou$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>tcg_sou32oa</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>tcg_sou32m</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_jun11da</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_jun11m</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>jyun0168</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>Jun&apos;iku$&quot;Ahh...... this is heart wrenching...... to see you powerless like this......&quot;@</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>sous0247</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>Sousou$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>kaen0189</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>Kakouen$&quot;What are you doing, Keifa? Hurry up and take Karin-sama away from here!&quot;@</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_jun21ha</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>jyun0169</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>Jun&apos;iku$&quot;Y-yeah......!&quot;@</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>kaen0190</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>Kakouen$&quot;Aneja!&quot;@</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>tcg_ton14aa</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>kato0348</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>Kakouton$&quot;Yeah?!&quot;@</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>kaen0191</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>Kakouen$&quot;Karin-sama and Keifa have fallen back!&quot;@</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_ton14ga</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>kato0349</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>Kakouton$&quot;Alright! Kii, we&apos;re withdrawing too!&quot;@</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>tcg_ton14ga</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_kyo12ca</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>kii_0076</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>Kyocho$&quot;Ahaha...... Shunran-sama...... that may not be possible.&quot;@</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>kaen0192</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>Kakouen$&quot;What&apos;s the matter?&quot;@</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>tcg_kyo22ga</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>kii_0077</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>Kyocho$&quot;We&apos;re...... surrounded......&quot;@</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>ecg_ton03a</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>Soldier$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>Soldier$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>kato0350</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>Kakouton$&quot;Kkh......&quot;@</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>Soldier$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>Soldier$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>kaen0193</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>Kakouen$&quot;......One after another, they just keep pouring out.&quot;@</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>kato0351</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>Kakouton$&quot;Yeah. But if they&apos;re trying to stop us, we won&apos;t hold back. ......Even if they are former Gi soldiers.&quot;@</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>kaen0194</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>Kakouen$&quot;Of course. I do pity them, but all those who oppose us will be slaughtered. ......Now that we&apos;ve rescued Karin-sama, we must return to her side.&quot;@</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>kii_0078</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>Kyocho$&quot;Heh, heh......It&apos;s been a while since the three of us went wild together!&quot;@</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>kaen0195</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>Kakouen$&quot;The losses have been building up lately. ......Maybe I&apos;ll relieve my stress here.&quot;@</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>kato0352</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>Kakouton$&quot;That&apos;s right. My worries about the future are gone now. I&apos;m going to enjoy swinging my greatsword!&quot;@</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>SE013</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>ecg_ton04b</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>kato0353</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>Kakouton$&quot;Are you ready, Shuuran, Kii?&quot;@</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>ecg_ton04c</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>kii_0079</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>Kyocho$&quot;Yep! I&apos;m all ready to go!&quot;@</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>ecg_ton04a</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>kaen0196</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>Kakouen$&quot;Hmph, of course I am. I&apos;ll watch your back.&quot;@</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>kato0354</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>Kakouton$&quot;Then that means we&apos;re invincible. Hee-hee.&quot;@</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>kaen0197</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>Kakouen$&quot;What are you laughing for?&quot;@</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>kato0355</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>Kakouton$&quot;Well...... I just remembered the day when Karin-sama, Shuuran, Kii and I rose to lead the war.&quot;@</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>kaen0198</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>Kakouen$&quot;I see. Heh, heh......It&apos;s been a long time since I remembered the way my blood boiled then.&quot;@</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>kato0356</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>Kakouton$&quot;Yeah... We bathed in the blood of our slain enemies, basking in their screams of anguish as we pressed forward that day......I remember it all.&quot;@</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>kaen0199</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>Kakouen$&quot;Heh...... you look brutal, Aneja.&quot;@</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>kato0357</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>Kakouton$&quot;I&apos;m the sword of Gi. I&apos;m proud to be brutal.&quot;@</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>kii_0080</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>Kyocho$&quot;Oh, my. You&apos;re so cool, Shunran-sama! I&apos;m falling in love with you all over again!&quot;@</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>kaen0200</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>Kakouen$&quot;Me too...... I&apos;m proud of you, Aneja. I&apos;m counting on you.&quot;@</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>kato0358</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>Kakouton$&quot;I&apos;ll accept those words after we break though this encirclement...... Let&apos;s go, Shuuran, Kii!&quot;@</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>kaen0201</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>Kakouen$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>kii_0081</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>Kyocho$&quot;Yes!&quot;@</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>kato0359</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>Kakouton$&quot;Listen well, soldiers mesmerized by this apparition! My name is Kakou Genjyou! I am the sword of Gi!&quot;@</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>SE013</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>tcg_ton32aa</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>tcg_ton32m</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>kato0360</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>Kakouton$&quot;You fools who have forgotten the pride of Gi and fell to mere hounds, I shall become a rakshasa and devour your lives! Prepare to die!&quot;@</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>tcg_ton92aa</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>tcg_ton92m</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>kato0361</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>Kakouton$&quot;Here...... I come!&quot;@</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="219">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="220">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="221">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="222">
    <ToTranslate>tcg_ukt11ca</ToTranslate>
  </String>
  <String index="223">
    <ToTranslate>tcg_ukt11m</ToTranslate>
  </String>
  <String index="224">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="225">
    <ToTranslate>ukit0054</ToTranslate>
  </String>
  <String index="226">
    <ToTranslate>Ukitsu$&quot;......We lost a pawn? I suppose puppets are nothing more than puppets......they&apos;re useless.&quot;@</ToTranslate>
  </String>
  <String index="227">
    <ToTranslate>tcg_ukt11da</ToTranslate>
  </String>
  <String index="228">
    <ToTranslate>tcg_ukt11m</ToTranslate>
  </String>
  <String index="229">
    <ToTranslate>ukit0055</ToTranslate>
  </String>
  <String index="230">
    <ToTranslate>Ukitsu$&quot;Oh well, then. All that means is that I&apos;ll call in reinforcements and build a wall of flesh...&quot;@</ToTranslate>
  </String>
  <String index="231">
    <ToTranslate>ukit0056</ToTranslate>
  </String>
  <String index="232">
    <ToTranslate>Ukitsu$&quot;Hee-hee...... we&apos;ll make sure they waste plenty of time......&quot;@</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>tcg_syo21da</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>syur0440</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>Shuri$&quot;I see a signal fire ahead of us! Kakouton-san has successfully rescued Sousou-san!&quot;@</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>sui_0174</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>Sui$&quot;Good!&quot;@</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>sei_0233</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>Sei$&quot;Hmm, then all we have to do is to destroy the enemy in front of us.&quot;@</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>shio0081</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>Shion$&quot;But there are many more of them than us. ......This will still be a tough battle.&quot;@</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>aisy0883</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>Aisha$&quot;We&apos;ve fought nothing but tough battles before. ...We can overcome this easily.&quot;@</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>sui_0175</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>Sui$&quot;Aisha is right! Now that Sousou is safe, we can fight with our full might!&quot;@</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>rinr0488</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>Rinrin$&quot;Rinrin&apos;s going to use her full might too! Heh, heh, it&apos;s been a while since I got serious!&quot;@</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>sei_0234</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>Sei$&quot;Hmph, then let me see how Enhito Chouhi fights when she&apos;s serious.&quot;@</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>rinr0489</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>Rinrin$&quot;Sure thing!&quot;@</ToTranslate>
  </String>
  <String index="275">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="276">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="277">
    <ToTranslate>aisy0884</ToTranslate>
  </String>
  <String index="278">
    <ToTranslate>Aisha$&quot;All right. All units, counterattack with full force!&quot;@</ToTranslate>
  </String>
