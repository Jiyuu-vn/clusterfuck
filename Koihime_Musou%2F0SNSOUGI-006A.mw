  <String index="5">
    <ToTranslate>kato0344</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Kakouton$&quot;The battle has begun.&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_jun21ha</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>jyun0166</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Jun&apos;iku$&quot;Yes. We must act while the enemy is paying attention to Hongou&apos;s army.&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_en12aa</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>kaen0185</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Kakouen$&quot;We only have this one chance. Let&apos;s go over the procedure one more time.&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>kato0345</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>Kakouton$&quot;Kii and I will disrupt the enemy&apos;s lines. Meanwhile, Keifa and Shuuran rescue Karin-sama. Good luck, everyone.&quot;@</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_kyo12aa</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>kii_0072</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Kyocho$&quot;Please count on me. I&apos;ll wager even my life on rescuing Karin-sama.&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>kaen0186</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>Kakouen$&quot;Don&apos;t wager it in the wrong place, though. ......I won&apos;t allow you to die like that.&quot;@</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>tcg_kyo22fa</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>kii_0073</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>Kyocho$&quot;I know!&quot;@</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_ton14ga</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>kato0346</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Kakouton$&quot;......Okay, let&apos;s go take back Our Lord!&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>kaen0187</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>Kakouen$&quot;Yeah......!&quot;@</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_ukt11aa</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_ukt11m</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>ukit0051</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>Ukitsu$&quot;Hmm...... Hongou&apos;s army is pretty good.&quot;@</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>ukit0052</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>Ukitsu$&quot;Just what you&apos;d expect with so many heroes together.  ......It&apos;s troublesome.&quot;@</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_ukt11fa</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>tcg_ukt11m</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>ukit0053</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>Ukitsu$&quot;But there&apos;s no entertainment in just being shoved around like this. ......Perhaps it&apos;s time to use Gi&apos;s warriors.&quot;@</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_ton92aa</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_ton92m</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>kato0347</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>Kakouton$&quot;Woooohhhh!!&quot;@</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>battle_kabuto2</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>SE033</ToTranslate>
  </String>
