  <Line StartPos="13683" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="13700" Length="39" Skip="false">
    <Translated>&quot;Um... I have an announcement to make.&quot;</Translated>
