  <Line StartPos="15257" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="15274" Length="26" Skip="false">
    <Translated>&quot;I&apos;m sure! I&apos;m not lying!&quot;</Translated>
  </Line>
  <Line StartPos="15315" Length="4" Skip="false">
    <Translated>Lucy</Translated>
  </Line>
  <Line StartPos="15332" Length="76" Skip="false">
    <Translated>&quot;I&apos;m not saying you&apos;re lying, but it\0might be just your misunderstanding...&quot;</Translated>
