  </Line>
  <Line StartPos="13796" Length="4" Skip="false">
    <Translated>Lucy</Translated>
  </Line>
  <Line StartPos="13813" Length="13" Skip="false">
    <Translated>&quot;What is it?&quot;</Translated>
  </Line>
  <Line StartPos="13840" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="13857" Length="26" Skip="false">
