  <Line StartPos="15422" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="15439" Length="16" Skip="false">
    <Translated>&quot;That can&apos;t be!&quot;</Translated>
  </Line>
  <Line StartPos="15470" Length="3" Skip="false">
    <Translated>Aoi</Translated>
  </Line>
  <Line StartPos="15486" Length="89" Skip="false">
    <Translated>&quot;Now, now... It will be my job if it is\0a ghost. Please call for me if it\0appears again.&quot;</Translated>
  </Line>
  <Line StartPos="15590" Length="3" Skip="false">
    <Translated>Aoi</Translated>
  </Line>
  <Line StartPos="15606" Length="44" Skip="false">
    <Translated>&quot;I shall send it back to heaven right\0away.&quot;</Translated>
  </Line>
  <Line StartPos="15664" Length="4" Skip="false">
    <Translated>Mena</Translated>
  </Line>
  <Line StartPos="15681" Length="20" Skip="false">
    <Translated>&quot;Oh... thank you...&quot;</Translated>
  </Line>
  <Line StartPos="15712" Length="54" Skip="false">
