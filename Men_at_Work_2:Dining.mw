  <Line StartPos="2244" Length="10" Skip="false">
    <Translated>Chuchu\0Top</Translated>
  </Line>
  <Line StartPos="2330" Length="6" Skip="false">
    <Translated>Dinner</Translated>
  </Line>
  <Line StartPos="2365" Length="8" Skip="false">
    <Translated>Benjamin</Translated>
  </Line>
  <Line StartPos="2403" Length="6" Skip="false">
