  <String index="263">
    <ToTranslate>sonk0191</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>Sonken$&quot;Ugh...... how could this be? I didn&apos;t even land one strike......&quot;@</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>aisy0954</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>Aisha$&quot;......Your sword has never seen blood, has it? ......It&apos;s not a rakshasa&apos;s sword.&quot;@</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>aisy0955</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>Aisha$&quot;And that does you credit as a ruler. However, it&apos;s a serious crime that you were unable to see what was behind this battle.&quot;@</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>sonk0192</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>Sonken$&quot;Behind this battle......?&quot;@</ToTranslate>
  </String>
  <String index="275">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="276">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="277">
    <ToTranslate>aisy0956</ToTranslate>
  </String>
  <String index="278">
    <ToTranslate>Aisha$&quot;If you want to know more about it, drop your weapon and surrender. My lord will tell you in detail.&quot;@</ToTranslate>
  </String>
  <String index="279">
    <ToTranslate>aisy0957</ToTranslate>
  </String>
  <String index="280">
    <ToTranslate>Aisha$&quot;Although you&apos;ve already lost your soldiers, you&apos;ve fought bravely with just a few generals. Even if you surrender, it should no longer defile your pride.&quot;@</ToTranslate>
  </String>
  <String index="281">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="282">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="283">
    <ToTranslate>sonk0193</ToTranslate>
  </String>
  <String index="284">
    <ToTranslate>Sonken$&quot;Are you saying I&apos;ve saved face?&quot;@</ToTranslate>
  </String>
  <String index="285">
    <ToTranslate>aisy0958</ToTranslate>
  </String>
  <String index="286">
    <ToTranslate>Aisha$&quot;......A ruler should not waste her life. That&apos;s what I want to say.&quot;@</ToTranslate>
  </String>
  <String index="287">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="288">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="289">
    <ToTranslate>sonk0194</ToTranslate>
  </String>
  <String index="290">
    <ToTranslate>Sonken$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="291">
    <ToTranslate>tcg_kan11aa</ToTranslate>
  </String>
  <String index="292">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="293">
    <ToTranslate>aisy0959</ToTranslate>
  </String>
  <String index="294">
    <ToTranslate>Aisha$&quot;Would you surrender?&quot;@</ToTranslate>
  </String>
  <String index="295">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="296">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="297">
    <ToTranslate>sonk0195</ToTranslate>
  </String>
  <String index="298">
    <ToTranslate>Sonken$&quot;......Okay. My soldiers lost the battle, and I have lost at the sword. I won&apos;t resist anymore. Do what you want with me.&quot;@</ToTranslate>
  </String>
  <String index="299">
    <ToTranslate>tcg_kan21aa</ToTranslate>
  </String>
  <String index="300">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="301">
    <ToTranslate>aisy0960</ToTranslate>
  </String>
  <String index="302">
    <ToTranslate>Aisha$&quot;Then I will take you on your word as a ruler, and I won&apos;t restrain you. Follow me on your feet.&quot;@</ToTranslate>
  </String>
  <String index="303">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="304">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="305">
    <ToTranslate>sonk0196</ToTranslate>
  </String>
  <String index="306">
    <ToTranslate>Sonken$&quot;All right......&quot;@</ToTranslate>
  </String>
