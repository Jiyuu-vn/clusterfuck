  <String index="352">
    <ToTranslate>sonk0200</ToTranslate>
  </String>
  <String index="353">
    <ToTranslate>Sonken$&quot;Shaoren! You&apos;re safe!?&quot;@</ToTranslate>
  </String>
  <String index="354">
    <ToTranslate>tcg_syk11aa</ToTranslate>
  </String>
  <String index="355">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="356">
    <ToTranslate>sons0077</ToTranslate>
  </String>
  <String index="357">
    <ToTranslate>Sonshoukou$&quot;Sure I am. I&apos;m doing great!&quot;@</ToTranslate>
  </String>
  <String index="358">
    <ToTranslate>tcg_son12ca</ToTranslate>
  </String>
  <String index="359">
