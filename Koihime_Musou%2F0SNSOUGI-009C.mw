  </String>
  <String index="312">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="313">
    <ToTranslate>sonk0197</ToTranslate>
  </String>
  <String index="314">
    <ToTranslate>Sonken$&quot;Shishun. My life is in Hongou&apos;s hands...... I&apos;m sorry.&quot;@</ToTranslate>
  </String>
  <String index="315">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="316">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="317">
    <ToTranslate>kann0065</ToTranslate>
  </String>
  <String index="318">
    <ToTranslate>Kannei$&quot;Then I, Kan Kouha, will come with you as well!&quot;@</ToTranslate>
  </String>
  <String index="319">
    <ToTranslate>tcg_rik11da</ToTranslate>
  </String>
