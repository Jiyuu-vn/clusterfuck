[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>Soldier$&quot;The enemy front has collapsed! We&apos;ve routed their entire army!&quot;@</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>aisy0937</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>Aisha$&quot;Good! Now&apos;s our chance! Let&apos;s charge forward and take down their supreme commander!&quot;@</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Kazuto$&quot;Aisha, don&apos;t hurt Sonken! Make sure you capture her!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>aisy0938</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Aisha$&quot;Understood! Let&apos;s go!&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>rinr0512</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>Rinrin$&quot;Charge!&quot;@</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>sonk0173</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>Sonken$&quot;Ggh...... did we lose?&quot;@</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>kann0055</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Kannei$&quot;Renfa-sama! I&apos;ll stop the enemy&apos;s pursuit! Please fall back with Non!&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>sonk0174</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>Sonken$&quot;No! I am the ruler of Go! I can not run before my soldiers!&quot;@</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_rik11da</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>riku0052</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Rikuson$&quot;That&apos;s not the case! You have to run before the soldiers, or Go will perish!&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>sonk0175</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>Sonken$&quot;No! I won&apos;t do such a cowardly thing! I don&apos;t want to disgrace my mother and sister&apos;s names!&quot;@</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_kne11ca</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>kann0056</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>Kannei$&quot;But if you die here, Renfa-sama, Go will perish!&quot;@</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>kann0057</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>kannei$&quot;I know it&apos;s a difficult decision to make, but as long as you&apos;re alive, it will be possible for Go to be revived! So please retreat for now!&quot;@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>SE047a</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>aisy0939</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Aisha$&quot;I don&apos;t think so!&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>kann0058</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>Kannei$&quot;Ugh...... Kan&apos;u!&quot;@</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>rinr0513</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>Rinrin$&quot;Rinrin&apos;s here too!&quot;@</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>tcg_rik21ha</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>tcg_rik21m</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>riku0053</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>Rikuson$&quot;Whoa, Ch-Chouhi is here too!&quot;@</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>aisy0940</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>Aisha$&quot;Sonken, Ruler of Go, your soldiers have scattered, and I&apos;m sure you don&apos;t have the strength to resist. I would like you to quietly come with me to My Lord.&quot;@</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>sonk0176</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>Sonken$&quot;Even without my soldiers, I am the ruler of Go! I won&apos;t be captured that easily!&quot;@</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>SE013</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_son41aa</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>tcg_son41m</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>sonk0177</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>Sonken$&quot;If you want me, come and get me by force!&quot;@</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>kann0059</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>Kannei$&quot;R-Renfa-sama! That&apos;s the courageous general, Kan&apos;u! Please let me deal with her, and withdraw!&quot;@</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_son41aa</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>tcg_son41m</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>sonk0178</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>Sonken$&quot;I will not withdraw! I am the daughter of the Tiger of Jiangdong! How can I turn my back to an enemy who proudly named herself?!&quot;@</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>aisy0941</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>Aisha$&quot;Spoken like a true ruler of Go. In deference to your pride, I, Kan Unchou will fight you with all my strength.&quot;@</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>kann0060</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>Kannei$&quot;Ugh...... I will not let you do that!&quot;@</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_kne91aa</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_kne91m</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>kann0061</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>Kannei$&quot;Yahhhhh!&quot;@</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>battle_kiriage</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>s_yure_r</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_kne51aa</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>tcg_kne51m</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>kann0062</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>Kannei$&quot;Argh......!&quot;@</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>tcg_cho31ia</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>rinr0514</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>Rinrin$&quot;Hee-hee. No, no, you&apos;re mine, Onee-chan.&quot;@</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>kann0063</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>Kannei$&quot;Don&apos;t interfere, Chouhi!&quot;@</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>rinr0515</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>Rinrin$&quot;I can&apos;t help it. We&apos;ve already decided that Aisha will take on Sonken-onee-chan, and that I&apos;ll take you on.&quot;@</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>aisy0942</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>Aisha$&quot;Rinrin, you deal with Kannei.&quot;@</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>rinr0516</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>Rinrin$&quot;Sure!&quot;@</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>aisy0943</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>Aisha$&quot;I&apos;m going to concentrate on Sonken. ......Okay, I&apos;m ready whenever you are.&quot;@</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>aisy0944</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>Aisha$&quot;Let me see what the pride of Go&apos;s ruler is like!&quot;@</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>tcg_son41aa</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>tcg_son41m</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>sonk0179</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>Sonken$&quot;Impudent woman! How dare a mere officer mock me, the ruler!&quot;@</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>aisy0945</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>Aisha$&quot;If you don&apos;t want me to mock you, then try to knock me down by force!&quot;@</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>tcg_son91aa</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>tcg_son91m</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>sonk0181</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>Sonken$&quot;Take this!&quot;@</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>battle_kabuto2</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>aisy0946</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>Aisha$&quot;Hmph, that&apos;s nothing, Sonken!&quot;@</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>battle_kan</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>s_yure_r</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>SE031</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>tcg_son61aa</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>tcg_son61m</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>sonk0182</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>Sonken$&quot;What......?&quot;@</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>aisy0947</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>Aisha$&quot;Did you really think such a weak attack would work on me, Kan Unchou?&quot;@</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>sonk0183</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>Sonken$&quot;You repelled all of my attacks with just one stroke of your spear......?&quot;@</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>tcg_kan31ha</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>aisy0948</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>Aisha$&quot;Hmph. It&apos;s nothing but an aristocrat&apos;s swordplay, after all. What you have in your sword is simply self-satisfying pride.&quot;@</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>aisy0949</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>Aisha$&quot;However, such pride is no match to my Green Dragon Blade, which has fought against numerous brave generals!&quot;@</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>tcg_son41aa</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>tcg_son41m</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>sonk0184</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>Sonken$&quot;How dare you......!&quot;@</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>sonk0185</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>Sonken$&quot;My pride doesn&apos;t just belong to me! It belongs to the Tiger of Jiangdong, the Little Conqueror of Jiangdong...... and to the Son-Go soldiers who died in battle!&quot;@</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>sonk0186</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>Sonken$&quot;Every one of their lives forms my pride! Traitors like you all have no right to talk to me that way!&quot;@</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>aisy0950</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>Aisha$&quot;Then why don&apos;t you try to take my life with all of that pride of yours?!&quot;@</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>tcg_son41aa</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>tcg_son41m</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>sonk0187</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>Sunken$&quot;You don&apos;t need to tell me!&quot;@</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>tcg_son91aa</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>tcg_son91m</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>sonk0188</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>Sunken$&quot;Yahhhh!&quot;@</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>battle_kabuto</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>yure_y</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>tcg_kan41aa</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>tcg_kan41m</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>aisy0951</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>Aisha$&quot;Is that all your pride amounts to?!&quot;@</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>tcg_son91aa</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>tcg_son91m</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>sonk0189</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>Sonken$&quot;No way! Here!&quot;@</ToTranslate>
