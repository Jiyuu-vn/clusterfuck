  <String index="360">
    <ToTranslate>sonk0201</ToTranslate>
  </String>
  <String index="361">
    <ToTranslate>Sonken$&quot;I see. I&apos;m glad......&quot;@</ToTranslate>
  </String>
  <String index="362">
    <ToTranslate>tcg_syk11ia</ToTranslate>
  </String>
  <String index="363">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="364">
    <ToTranslate>sons0078</ToTranslate>
  </String>
  <String index="365">
    <ToTranslate>Sonshoukou$&quot;Are you okay, Onee-chan? You aren&apos;t hurt, are you?&quot;@</ToTranslate>
  </String>
  <String index="366">
    <ToTranslate>sonk0202</ToTranslate>
  </String>
  <String index="367">
    <ToTranslate>Sonken$&quot;No...... thanks to Shishun and Non.&quot;@</ToTranslate>
  </String>
  <String index="368">
    <ToTranslate>tcg_syk11ca</ToTranslate>
  </String>
  <String index="369">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="370">
    <ToTranslate>sons0079</ToTranslate>
  </String>
  <String index="371">
    <ToTranslate>Sonshoukou$&quot;I see. I&apos;m glad. I was pretty worried that you might have become reckless.&quot;@</ToTranslate>
  </String>
  <String index="372">
    <ToTranslate>tcg_son22da</ToTranslate>
  </String>
  <String index="373">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="374">
    <ToTranslate>sonk0203</ToTranslate>
  </String>
  <String index="375">
    <ToTranslate>Sonken$&quot;Reckless? What do you mean?&quot;@</ToTranslate>
  </String>
  <String index="376">
    <ToTranslate>tcg_syk11aa</ToTranslate>
  </String>
  <String index="377">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="378">
    <ToTranslate>sons0080</ToTranslate>
  </String>
  <String index="379">
    <ToTranslate>Sonshoukou$&quot;Knowing how you are, I thought maybe you&apos;d kill yourself rather than be captured as a hostage.&quot;@</ToTranslate>
  </String>
  <String index="380">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="381">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="382">
    <ToTranslate>sonk0204</ToTranslate>
  </String>
  <String index="383">
    <ToTranslate>Sonken$&quot;......I certainly would have preferred that, but she said something that bothered me. So I decided it wouldn&apos;t be too late to do so after hearing them out.&quot;@</ToTranslate>
  </String>
  <String index="384">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="385">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="386">
    <ToTranslate>sons0081</ToTranslate>
  </String>
  <String index="387">
    <ToTranslate>Sonshoukou$&quot;No! I won&apos;t allow you to kill yourself!&quot;@</ToTranslate>
  </String>
  <String index="388">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="389">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="390">
    <ToTranslate>sonk0205</ToTranslate>
  </String>
  <String index="391">
    <ToTranslate>Sonken$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="392">
    <ToTranslate>Kazuto$&quot;I don&apos;t think you should, either.&quot;@</ToTranslate>
  </String>
  <String index="393">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="394">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="395">
    <ToTranslate>bgm10</ToTranslate>
  </String>
  <String index="396">
    <ToTranslate>sonk0206</ToTranslate>
  </String>
  <String index="397">
    <ToTranslate>Sonken$&quot;Hongou! You...... how dare you say such a thing, you traitor?&quot;@</ToTranslate>
  </String>
  <String index="398">
    <ToTranslate>Kazuto$&quot;Wait. I don&apos;t want to argue with you. After you listen closely to what I have to tell you, if you still think I&apos;m a traitor, you can yell at me as much as you want.&quot;@</ToTranslate>
  </String>
  <String index="399">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="400">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="401">
    <ToTranslate>sonk0207</ToTranslate>
  </String>
  <String index="402">
    <ToTranslate>Sonken$&quot;......Alright. Then let me hear what you want to say.&quot;@</ToTranslate>
  </String>
  <String index="403">
    <ToTranslate>Kazuto$&quot;Actually, I&apos;ll let Shuri explain it to you. Shuri, please.&quot;@</ToTranslate>
  </String>
  <String index="404">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="405">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="406">
    <ToTranslate>syur0481</ToTranslate>
  </String>
  <String index="407">
    <ToTranslate>Shuri$&quot;Okay. First of all, Sonken-san, we&apos;ve been set up.&quot;@</ToTranslate>
  </String>
  <String index="408">
    <ToTranslate>tcg_son22da</ToTranslate>
  </String>
  <String index="409">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="410">
    <ToTranslate>sonk0208</ToTranslate>
  </String>
  <String index="411">
    <ToTranslate>Sonken$&quot;......We&apos;ve been set up?&quot;@</ToTranslate>
  </String>
  <String index="412">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="413">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="414">
    <ToTranslate>kann0066</ToTranslate>
  </String>
  <String index="415">
    <ToTranslate>Kannei$&quot;What do you mean by that?&quot;@</ToTranslate>
  </String>
  <String index="416">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="417">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="418">
    <ToTranslate>sons0082</ToTranslate>
  </String>
  <String index="419">
    <ToTranslate>Sonshoukou$&quot;Well, before we departed to retaliate against Hongou&apos;s army&apos;s......&quot;@</ToTranslate>
  </String>
  <String index="420">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="421">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="422">
    <ToTranslate>syur0482</ToTranslate>
  </String>
  <String index="423">
    <ToTranslate>Shuri$&quot;We received a report that said Son-Go had attacked a city on the border, so we departed.&quot;@</ToTranslate>
  </String>
  <String index="424">
    <ToTranslate>tcg_son22da</ToTranslate>
  </String>
  <String index="425">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="426">
    <ToTranslate>sonk0209</ToTranslate>
  </String>
  <String index="427">
    <ToTranslate>Sonken$&quot;What!? Our soldiers attacked a city?&quot;@</ToTranslate>
  </String>
  <String index="428">
    <ToTranslate>syur0483</ToTranslate>
  </String>
  <String index="429">
    <ToTranslate>Shuri$&quot;That&apos;s right. When we arrived in the city, Son-Go&apos;s army had already left. We saw Son-Go&apos;s flags and armor everywhere.&quot;@</ToTranslate>
  </String>
  <String index="430">
    <ToTranslate>syur0484</ToTranslate>
  </String>
  <String index="431">
    <ToTranslate>Shuri$&quot;When we saw that, we assumed Go had waged war. But meanwhile......&quot;@</ToTranslate>
  </String>
  <String index="432">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="433">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="434">
    <ToTranslate>sons0083</ToTranslate>
  </String>
  <String index="435">
    <ToTranslate>Sonshoukou$&quot;After we received report of their attack and departed, we discovered Kazuto and his army deployed near the city.&quot;@</ToTranslate>
  </String>
  <String index="436">
    <ToTranslate>tcg_rik11da</ToTranslate>
  </String>
  <String index="437">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="438">
    <ToTranslate>riku0055</ToTranslate>
  </String>
  <String index="439">
    <ToTranslate>Rikuson$&quot;So naturally, we tried to start a battle to defend Go......&quot;@</ToTranslate>
  </String>
  <String index="440">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="441">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="442">
    <ToTranslate>aisy0962</ToTranslate>
  </String>
  <String index="443">
    <ToTranslate>Aisha$&quot;And we thought Son-Go&apos;s army had come back to attack the city again, so we prepared to counterattack......&quot;@</ToTranslate>
  </String>
  <String index="444">
    <ToTranslate>Kazuto$&quot;And both armies clashed......&quot;@</ToTranslate>
  </String>
  <String index="445">
    <ToTranslate>tcg_son22da</ToTranslate>
  </String>
  <String index="446">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="447">
    <ToTranslate>sonk0210</ToTranslate>
  </String>
  <String index="448">
    <ToTranslate>Sonken$&quot;B-but you all don&apos;t have any proof that all that is true.&quot;@</ToTranslate>
  </String>
  <String index="449">
    <ToTranslate>Kazuto$&quot;Yes, I do. ......Right now, you&apos;re talking to me without being restrained. Isn&apos;t that more proof than anything?&quot;@</ToTranslate>
  </String>
  <String index="450">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="451">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="452">
    <ToTranslate>sonk0211</ToTranslate>
  </String>
  <String index="453">
    <ToTranslate>Sonken$&quot;Mmm...... that&apos;s true...... If you were plotting to annex Go, killing me would make future battles easier. But is it possible for anyone to come up with a scheme to deceive both of us?&quot;@</ToTranslate>
  </String>
  <String index="454">
    <ToTranslate>tcg_syo41ea</ToTranslate>
  </String>
  <String index="455">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="456">
    <ToTranslate>syur0485</ToTranslate>
  </String>
  <String index="457">
    <ToTranslate>Shuri$&quot;Actually, the steps are rather simple. All they had to do was to attack the city, pretending to be Go&apos;s army, and leave Go flags at the site.&quot;@</ToTranslate>
  </String>
  <String index="458">
    <ToTranslate>syur0486</ToTranslate>
  </String>
  <String index="459">
    <ToTranslate>Shuri$&quot;Then they just needed to send false information to lure Go&apos;s army here at about the same time we arrived.&quot;@</ToTranslate>
  </String>
  <String index="460">
    <ToTranslate>syur0487</ToTranslate>
  </String>
  <String index="461">
    <ToTranslate>Shuri$&quot;Both armies were already deeply suspicious of each other, so it was only a matter of time before they engaged.&quot;@</ToTranslate>
  </String>
  <String index="462">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="463">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="464">
    <ToTranslate>syur0488</ToTranslate>
  </String>
  <String index="465">
    <ToTranslate>Shuri$&quot;......It&apos;s also my fault for not being able to see through it, though......&quot;@</ToTranslate>
  </String>
  <String index="466">
    <ToTranslate>Kazuto$&quot;It wasn&apos;t your fault alone. It was our fault for making assumptions.&quot;@</ToTranslate>
  </String>
  <String index="467">
    <ToTranslate>tcg_syo21fa</ToTranslate>
  </String>
  <String index="468">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="469">
    <ToTranslate>syur0489</ToTranslate>
  </String>
  <String index="470">
    <ToTranslate>Shuri$&quot;My Lord......&quot;@</ToTranslate>
  </String>
  <String index="471">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="472">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="473">
    <ToTranslate>kann0067</ToTranslate>
  </String>
  <String index="474">
    <ToTranslate>Kannei$&quot;But! If what you&apos;re saying is true, who would do something this well thought out?&quot;@</ToTranslate>
  </String>
  <String index="475">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="476">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="477">
    <ToTranslate>sei_0306</ToTranslate>
  </String>
  <String index="478">
    <ToTranslate>Sei$&quot;Some force who&apos;s trying to benefit from making our two camps sever ties and become enemies...&quot;@</ToTranslate>
  </String>
  <String index="479">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="480">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="481">
    <ToTranslate>shio0101</ToTranslate>
  </String>
  <String index="482">
    <ToTranslate>Shion$&quot;Or someone who benefits if you&apos;re gone, Sonken-san......&quot;@</ToTranslate>
  </String>
  <String index="483">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="484">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="485">
    <ToTranslate>aisy0963</ToTranslate>
  </String>
  <String index="486">
    <ToTranslate>Aisha$&quot;There&apos;s always someone who benefits behind the scenes. It&apos;s just a matter of who it is.&quot;@</ToTranslate>
  </String>
  <String index="487">
    <ToTranslate>Kazuto$&quot;I&apos;m sure the White Clothes have something to do with it.&quot;@</ToTranslate>
  </String>
  <String index="488">
    <ToTranslate>tcg_son22da</ToTranslate>
  </String>
  <String index="489">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="490">
    <ToTranslate>sonk0212</ToTranslate>
  </String>
  <String index="491">
    <ToTranslate>Sonken$&quot;Who are the White Clothes?&quot;@</ToTranslate>
  </String>
  <String index="492">
    <ToTranslate>Kazuto$&quot;Hmm...... we aren&apos;t too sure about them, but they are after my life.&quot;@</ToTranslate>
  </String>
  <String index="493">
    <ToTranslate>Kazuto$&quot;That&apos;s why they wanted to use Son-Go&apos;s power to smash us.&quot;@</ToTranslate>
  </String>
  <String index="494">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="495">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="496">
    <ToTranslate>sonk0213</ToTranslate>
  </String>
  <String index="497">
    <ToTranslate>Sonken$&quot;Are you saying those White Clothes deceived us......&quot;@</ToTranslate>
  </String>
  <String index="498">
    <ToTranslate>Kazuto$&quot;Yes, this battle was based on a misunderstanding. Therefore, if you believe my words, I would like both of us to withdraw our soldiers and be friendly again...... what do you think?&quot;@</ToTranslate>
  </String>
  <String index="499">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="500">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="501">
    <ToTranslate>sonk0214</ToTranslate>
  </String>
  <String index="502">
    <ToTranslate>Sunken$&quot;If what you&apos;re saying is true, that&apos;s fine. However, I still can&apos;t trust what you&apos;re saying......&quot;@</ToTranslate>
  </String>
  <String index="503">
    <ToTranslate>tcg_son12ga</ToTranslate>
  </String>
  <String index="504">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="505">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="506">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="507">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="508">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="509">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="510">
    <ToTranslate>Soldier$&quot;Reporting!&quot;@</ToTranslate>
  </String>
  <String index="511">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="512">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="513">
    <ToTranslate>aisy0964</ToTranslate>
  </String>
  <String index="514">
    <ToTranslate>Aisha$&quot;What is it? We&apos;re in a middle of an important war council!&quot;@</ToTranslate>
  </String>
  <String index="515">
    <ToTranslate>Soldier$&quot;Yes, but this is an emergency! At Son-Go&apos;s main camp, the Son family flag has been taken down, and replaced by a Shuu flag! We believe it&apos;s a rebellion staged by general Shuuyu!&quot;@</ToTranslate>
  </String>
  <String index="516">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="517">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="518">
    <ToTranslate>kann0068</ToTranslate>
  </String>
  <String index="519">
    <ToTranslate>Kannei$&quot;What!? Is that true?&quot;@</ToTranslate>
  </String>
  <String index="520">
    <ToTranslate>Soldier$&quot;Huh, uhh......&quot;@</ToTranslate>
  </String>
  <String index="521">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="522">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="523">
    <ToTranslate>sei_0307</ToTranslate>
  </String>
  <String index="524">
    <ToTranslate>Sei$&quot;That&apos;s okay, tell her.&quot;@</ToTranslate>
  </String>
  <String index="525">
    <ToTranslate>Soldier$&quot;Yes! Out of all our scouts, six units are reporting the same information! It is highly reliable information!&quot;@</ToTranslate>
  </String>
  <String index="526">
    <ToTranslate>Soldier$&quot;Also, Shuu flags are rising everywhere, not just at their main camp. Supposedly more than half the states that belonged to Go are now with Shuuyu!&quot;@</ToTranslate>
  </String>
  <String index="527">
    <ToTranslate>tcg_kne11ca</ToTranslate>
  </String>
  <String index="528">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="529">
    <ToTranslate>kann0069</ToTranslate>
  </String>
  <String index="530">
    <ToTranslate>Kannei$&quot;Wh-what&apos;s going on......? I knew she had greedy ambitions of her own!&quot;@</ToTranslate>
  </String>
  <String index="531">
    <ToTranslate>tcg_rik21ha</ToTranslate>
  </String>
  <String index="532">
    <ToTranslate>tcg_rik21m</ToTranslate>
  </String>
  <String index="533">
    <ToTranslate>riku0056</ToTranslate>
  </String>
  <String index="534">
    <ToTranslate>Rikuson$&quot;Meirin-sama staged a rebellion...... how can that be true......?&quot;@</ToTranslate>
  </String>
  <String index="535">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="536">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="537">
    <ToTranslate>sonk0215</ToTranslate>
  </String>
  <String index="538">
    <ToTranslate>Sonken$&quot;Meirin...... I knew it......&quot;@</ToTranslate>
  </String>
  <String index="539">
    <ToTranslate>tcg_kne11ga</ToTranslate>
  </String>
  <String index="540">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="541">
    <ToTranslate>kann0070</ToTranslate>
  </String>
  <String index="542">
    <ToTranslate>Kannei$&quot;Renfa-sama?! Did you know what she was planning?!&quot;@</ToTranslate>
  </String>
  <String index="543">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="544">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="545">
    <ToTranslate>sonk0216</ToTranslate>
  </String>
  <String index="546">
    <ToTranslate>Sonken$&quot;......I had a feeling that someday, something like this would happen......&quot;@</ToTranslate>
  </String>
  <String index="547">
    <ToTranslate>Kazuto$&quot;What do you mean?&quot;@</ToTranslate>
  </String>
  <String index="548">
    <ToTranslate>tcg_son12aa</ToTranslate>
  </String>
  <String index="549">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="550">
    <ToTranslate>sonk0217</ToTranslate>
  </String>
  <String index="551">
    <ToTranslate>Sonken$&quot;......Originally, Shuu Koukin and Sonsaku, my sister, were extremely close friends.&quot;@</ToTranslate>
  </String>
  <String index="552">
    <ToTranslate>sonk0218</ToTranslate>
  </String>
  <String index="553">
    <ToTranslate>Sonken$&quot;My sister, Sonsaku, was called the Little Conqueror of Jiangdong and she dreamed of unifying the world, just like her mother Sonken, a great hero known as the Tiger of Jiangdong.&quot;@</ToTranslate>
  </String>
  <String index="554">
    <ToTranslate>sonk0219</ToTranslate>
  </String>
  <String index="555">
    <ToTranslate>Sonken$&quot;Shuuyu believed in my sister&apos;s dream, and walked with her in pursuit of it.&quot;@</ToTranslate>
  </String>
  <String index="556">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="557">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="558">
    <ToTranslate>sonk0220</ToTranslate>
  </String>
  <String index="559">
    <ToTranslate>Sonken$&quot;However, my sister Sonsaku unfortunately died in battle on her path to conquest. And I succeeded her.&quot;@</ToTranslate>
  </String>
  <String index="560">
    <ToTranslate>sonk0221</ToTranslate>
  </String>
  <String index="561">
    <ToTranslate>Sonken$&quot;Shuuyu first thought I would walk the road to supremacy just like Sonsaku...... but I wasn&apos;t interested in such a path of carnage.&quot;@</ToTranslate>
  </String>
  <String index="562">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="563">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="564">
    <ToTranslate>sonk0222</ToTranslate>
  </String>
  <String index="565">
    <ToTranslate>Sonken$&quot;As long as the people of Go are at peace and live happily, I am satisfied. Or so I thought.&quot;@</ToTranslate>
  </String>
  <String index="566">
    <ToTranslate>sonk0223</ToTranslate>
  </String>
  <String index="567">
    <ToTranslate>Sonken$&quot;However, the way I acted was simply betrayal to Shuuyu, who still dreamed of conquering the world with my sister.&quot;@</ToTranslate>
  </String>
  <String index="568">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="569">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="570">
    <ToTranslate>sonk0224</ToTranslate>
  </String>
  <String index="571">
    <ToTranslate>Sonken$&quot;Soon after, we started to oppose each other, and clashed often......&quot;@</ToTranslate>
  </String>
  <String index="572">
    <ToTranslate>Kazuto$&quot;So that&apos;s why Shuuyu rebelled......&quot;@</ToTranslate>
  </String>
  <String index="573">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="574">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="575">
    <ToTranslate>sonk0225</ToTranslate>
  </String>
  <String index="576">
    <ToTranslate>Sonken$&quot;......It&apos;s not just because of that.&quot;@</ToTranslate>
  </String>
  <String index="577">
    <ToTranslate>sonk0226</ToTranslate>
  </String>
  <String index="578">
    <ToTranslate>Sonken$&quot;I love Go and Sonsaku, and that&apos;s why I like the pureness of Shuuyu, who still wants to follow the road to supremacy.&quot;@</ToTranslate>
  </String>
  <String index="579">
    <ToTranslate>sonk0227</ToTranslate>
  </String>
  <String index="580">
    <ToTranslate>Sonken$&quot;That&apos;s why......I tested her, and her feelings. ......I tested her to see what she would do......bearing her feelings towards Sonsaku and her love of Go.&quot;@</ToTranslate>
  </String>
  <String index="581">
    <ToTranslate>sonk0228</ToTranslate>
  </String>
  <String index="582">
    <ToTranslate>Sonken$&quot;And Shuuyu has chosen to rebel and to follow the path of conquest. She has declared that she wants to succeed Sonsaku&apos;s dream.&quot;@</ToTranslate>
  </String>
  <String index="583">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="584">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="585">
    <ToTranslate>sonk0229</ToTranslate>
  </String>
  <String index="586">
    <ToTranslate>Sonken$&quot;Now that Koukin has declared this, I think I&apos;ll step down......&quot;@</ToTranslate>
  </String>
  <String index="587">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="588">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="589">
    <ToTranslate>kann0071</ToTranslate>
  </String>
  <String index="590">
    <ToTranslate>Kannei$&quot;Are you saying you&apos;ll acknowledge Shuuyu&apos;s rebellion and hand over your position?&quot;@</ToTranslate>
  </String>
  <String index="591">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="592">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="593">
    <ToTranslate>sonk0230</ToTranslate>
  </String>
  <String index="594">
    <ToTranslate>Sonken$&quot;......Yes. If civil war takes place due to this rebellion, the people will suffer. That&apos;s not what I want. I don&apos;t care about being a ruler.&quot;@</ToTranslate>
  </String>
  <String index="595">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="596">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="597">
    <ToTranslate>kann0072</ToTranslate>
  </String>
  <String index="598">
    <ToTranslate>Kannei$&quot;B-but......&quot;@</ToTranslate>
  </String>
  <String index="599">
    <ToTranslate>tcg_rik11aa</ToTranslate>
  </String>
  <String index="600">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="601">
    <ToTranslate>riku0057</ToTranslate>
  </String>
  <String index="602">
    <ToTranslate>Rikuson$&quot;Don&apos;t bother, Shishun-chan. You know how stubborn Renfa-sama is, right?&quot;@</ToTranslate>
  </String>
  <String index="603">
    <ToTranslate>kann0073</ToTranslate>
  </String>
  <String index="604">
    <ToTranslate>Kannei$&quot;But this isn&apos;t an issue that can be ended by stubbornness!&quot;@</ToTranslate>
  </String>
  <String index="605">
    <ToTranslate>tcg_rik11ia</ToTranslate>
  </String>
  <String index="606">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="607">
    <ToTranslate>riku0058</ToTranslate>
  </String>
  <String index="608">
    <ToTranslate>Rikuson$&quot;I think it can. Unfortunately, Meirin-sama can&apos;t steer Go.&quot;@</ToTranslate>
  </String>
  <String index="609">
    <ToTranslate>tcg_kne11ba</ToTranslate>
  </String>
  <String index="610">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="611">
    <ToTranslate>kann0074</ToTranslate>
  </String>
  <String index="612">
    <ToTranslate>Kannei$&quot;A prediction like that isn&apos;t useful!&quot;@</ToTranslate>
  </String>
  <String index="613">
    <ToTranslate>tcg_rik11da</ToTranslate>
  </String>
  <String index="614">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="615">
    <ToTranslate>riku0059</ToTranslate>
  </String>
  <String index="616">
    <ToTranslate>Rikuson$&quot;But what can we do under the circumstances?&quot;@</ToTranslate>
  </String>
  <String index="617">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="618">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="619">
    <ToTranslate>sonk0231</ToTranslate>
  </String>
  <String index="620">
    <ToTranslate>Sonken$&quot;We are Hongou&apos;s hostages now...... There&apos;s nothing we can do.&quot;@</ToTranslate>
  </String>
  <String index="621">
    <ToTranslate>tcg_kne11ha</ToTranslate>
  </String>
  <String index="622">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="623">
    <ToTranslate>kann0075</ToTranslate>
  </String>
  <String index="624">
    <ToTranslate>Kannei$&quot;That&apos;s true...... but......&quot;@</ToTranslate>
  </String>
  <String index="625">
    <ToTranslate>tcg_son12aa</ToTranslate>
  </String>
  <String index="626">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="627">
    <ToTranslate>sonk0232</ToTranslate>
  </String>
  <String index="628">
    <ToTranslate>Sonken$&quot;But Hongou Kazuto, what are you going to do?&quot;@</ToTranslate>
  </String>
  <String index="629">
    <ToTranslate>Kazuto$&quot;......So long as Shuuyu aims to conquer the world, she will wage war against us. ......We&apos;ll just have to brush away the sparks that fall.&quot;@</ToTranslate>
  </String>
  <String index="630">
    <ToTranslate>Kazuto$&quot;We can&apos;t do anything right away, but once we&apos;re prepared, we&apos;ll challenge Shuuyu and conquer Go.&quot;@</ToTranslate>
  </String>
  <String index="631">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="632">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="633">
    <ToTranslate>sonk0233</ToTranslate>
  </String>
  <String index="634">
    <ToTranslate>Sonken$&quot;I see. ......I am the loser. Voicing an objection would only amount to a whipped dog&apos;s howls.&quot;@</ToTranslate>
  </String>
  <String index="635">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="636">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="637">
    <ToTranslate>sonk0234</ToTranslate>
  </String>
  <String index="638">
    <ToTranslate>Sonken$&quot;But...... if you have mercy, please don&apos;t make the people of Son-Go suffer......&quot;@</ToTranslate>
  </String>
  <String index="639">
    <ToTranslate>Kazuto$&quot;I know. I won&apos;t allow any plundering or acts of violence. I&apos;ll do my best not to bother the people.&quot;@</ToTranslate>
  </String>
  <String index="640">
    <ToTranslate>tcg_son12ca</ToTranslate>
  </String>
  <String index="641">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="642">
    <ToTranslate>sonk0235</ToTranslate>
  </String>
  <String index="643">
    <ToTranslate>Sonken$&quot;I&apos;m relieved to hear that. Then I have nothing else to say. But......&quot;@</ToTranslate>
  </String>
  <String index="644">
    <ToTranslate>Kazuto$&quot;Hmm?&quot;@</ToTranslate>
  </String>
  <String index="645">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="646">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="647">
    <ToTranslate>sonk0236</ToTranslate>
  </String>
  <String index="648">
    <ToTranslate>Sonken$&quot;Although I was chased from my throne, I am still the ruler of Go. If I simply surrender, I can&apos;t explain it to my people......&quot;@</ToTranslate>
  </String>
  <String index="649">
    <ToTranslate>sonk0237</ToTranslate>
  </String>
  <String index="650">
    <ToTranslate>Sonken$&quot;Hongou Kazuto, humiliate me as the loser. That&apos;s what I can do as compensation to my people..&quot;@</ToTranslate>
  </String>
  <String index="651">
    <ToTranslate>Kazuto$&quot;Do you want me to punish you as a loser?&quot;@</ToTranslate>
  </String>
  <String index="652">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="653">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="654">
    <ToTranslate>sonk0238</ToTranslate>
  </String>
  <String index="655">
    <ToTranslate>Sonken$&quot;Yes......&quot;@</ToTranslate>
  </String>
  <String index="656">
    <ToTranslate>Kazuto$&quot;......Okay. Then why don&apos;t you come to my room after we get back to the city?&quot;@</ToTranslate>
  </String>
  <String index="657">
    <ToTranslate>tcg_kan21pa</ToTranslate>
  </String>
  <String index="658">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="659">
    <ToTranslate>aisy0965</ToTranslate>
  </String>
  <String index="660">
    <ToTranslate>Aisha$&quot;...!? My Lord?! You aren&apos;t thinking about......!&quot;@</ToTranslate>
  </String>
  <String index="661">
    <ToTranslate>Kazuto$&quot;Yes, I am. ......If that makes her feel better, then I think that&apos;s what I should do for her.&quot;@</ToTranslate>
  </String>
  <String index="662">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="663">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="664">
    <ToTranslate>aisy0966</ToTranslate>
  </String>
  <String index="665">
    <ToTranslate>Aisha$&quot;But......!&quot;@</ToTranslate>
  </String>
  <String index="666">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="667">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="668">
    <ToTranslate>sei_0308</ToTranslate>
  </String>
  <String index="669">
    <ToTranslate>Sei$&quot;Wait, Aisha. Try to understand Sonken&apos;s pain.&quot;@</ToTranslate>
  </String>
  <String index="670">
    <ToTranslate>tcg_koc32ia</ToTranslate>
  </String>
  <String index="671">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="672">
    <ToTranslate>shio0102</ToTranslate>
  </String>
  <String index="673">
    <ToTranslate>Shion$&quot;She wants to atone for the suffering of her people and for losing, by giving up herself.&quot;@</ToTranslate>
  </String>
  <String index="674">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="675">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="676">
    <ToTranslate>sei_0309</ToTranslate>
  </String>
  <String index="677">
    <ToTranslate>Sei$&quot;That&apos;s right. Our lord understood her feelings and that&apos;s why he agreed. Try to understand.&quot;@</ToTranslate>
  </String>
  <String index="678">
    <ToTranslate>tcg_kan31la</ToTranslate>
  </String>
  <String index="679">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="680">
    <ToTranslate>aisy0967</ToTranslate>
  </String>
  <String index="681">
    <ToTranslate>Aisha$&quot;Ugh...... okay......&quot;@</ToTranslate>
  </String>
  <String index="682">
    <ToTranslate>Kazuto$&quot;Thank you. Then let&apos;s go back to the city for now.&quot;@</ToTranslate>
  </String>
  <String index="683">
    <ToTranslate>Kazuto$&quot;We are the only ones who can fight against Shuuyu...... If we lose against her, this continent will suffer from another war.&quot;@</ToTranslate>
  </String>
  <String index="684">
    <ToTranslate>Kazuto$&quot;We have to attack, and we must win. That&apos;s the only thing we can do.&quot;@</ToTranslate>
  </String>
  <String index="685">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="686">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="687">
    <ToTranslate>syur0490</ToTranslate>
  </String>
  <String index="688">
    <ToTranslate>Shuri$&quot;Yes. When we arrive in the city, Shion-san and I will devise the plan for our supply train.&quot;@</ToTranslate>
  </String>
  <String index="689">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="690">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="691">
    <ToTranslate>shio0103</ToTranslate>
  </String>
  <String index="692">
    <ToTranslate>Shion$&quot;We have to reinforce our soldiers and secure provisions. There&apos;s a mountain of work which must be done.&quot;@</ToTranslate>
  </String>
  <String index="693">
    <ToTranslate>Kazuto$&quot;Aisha, can you reorganize the army? We don&apos;t know when our enemy will attack. Have everyone ready to depart at a moment&apos;s notice, okay?&quot;@</ToTranslate>
  </String>
  <String index="694">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="695">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="696">
    <ToTranslate>aisy0968</ToTranslate>
  </String>
  <String index="697">
    <ToTranslate>Aisha$&quot;As you wish!&quot;@</ToTranslate>
  </String>
  <String index="698">
    <ToTranslate>Kazuto$&quot;Thanks. Let&apos;s go back to the city for now so we can recuperate, at least, for a little while.&quot;@</ToTranslate>
  </String>
  <String index="699">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="700">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="701">
    <ToTranslate>sei_0310</ToTranslate>
  </String>
  <String index="702">
    <ToTranslate>Sei$&quot;Understood...... All troops, about face! We are going back to the city to prepare for the next battle!&quot;@</ToTranslate>
  </String>
  <String index="703">
    <ToTranslate>$Under Sei&apos;s command, our army began to leave the battlefield.@</ToTranslate>
  </String>
  <String index="704">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="705">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="706">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="707">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="708">
    <ToTranslate>$That night......@</ToTranslate>
  </String>
  <String index="709">
    <ToTranslate>SE062</ToTranslate>
  </String>
  <String index="710">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="711">
    <ToTranslate>bcg_heroroom_c</ToTranslate>
  </String>
  <String index="712">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="713">
    <ToTranslate>bgm12</ToTranslate>
  </String>
  <String index="714">
    <ToTranslate>Kazuto$&quot;Come in......&quot;@</ToTranslate>
  </String>
  <String index="715">
    <ToTranslate>SE057</ToTranslate>
  </String>
  <String index="716">
    <ToTranslate>tcg_son22ka</ToTranslate>
  </String>
  <String index="717">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="718">
    <ToTranslate>sonk0239</ToTranslate>
  </String>
  <String index="719">
    <ToTranslate>Sonken$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="720">
    <ToTranslate>Kazuto$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="721">
    <ToTranslate>sonk0240</ToTranslate>
  </String>
  <String index="722">
    <ToTranslate>Sonken$&quot;H-here I am, Hongou.&quot;@</ToTranslate>
  </String>
  <String index="723">
    <ToTranslate>Kazuto$&quot;Yeah...... welcome.&quot;@</ToTranslate>
  </String>
  <String index="724">
    <ToTranslate>$I invited Sonken in as she stood there, her body and expression stiff with tension.@</ToTranslate>
  </String>
  <String index="725">
    <ToTranslate>sonk0241</ToTranslate>
  </String>
  <String index="726">
    <ToTranslate>Sonken$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="727">
    <ToTranslate>Kazuto$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="728">
    <ToTranslate>$A strange silence filled the room.@</ToTranslate>
  </String>
  <String index="729">
    <ToTranslate>$The silence made it clear we both knew what was about to take place between us.@</ToTranslate>
  </String>
  <String index="730">
    <ToTranslate>Kazuto$&quot;Uhh, why don&apos;t you have a seat?&quot;@</ToTranslate>
  </String>
  <String index="731">
    <ToTranslate>tcg_son12ma</ToTranslate>
  </String>
  <String index="732">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="733">
    <ToTranslate>sonk0242</ToTranslate>
  </String>
  <String index="734">
    <ToTranslate>Sonken$&quot;No...... I&apos;m fine like this......&quot;@</ToTranslate>
  </String>
  <String index="735">
    <ToTranslate>Kazuto$&quot;A-are you sure? Then I&apos;ll stand too, haha.&quot;@</ToTranslate>
  </String>
  <String index="736">
    <ToTranslate>$I tried to lighten up this heavy atmosphere with chit-chat, but...@</ToTranslate>
  </String>
  <String index="737">
    <ToTranslate>tcg_son22ka</ToTranslate>
  </String>
  <String index="738">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="739">
    <ToTranslate>sonk0243</ToTranslate>
  </String>
  <String index="740">
    <ToTranslate>Sonken$&quot;I-I&apos;m, umm...... ready whenever you are. I already made up my mind.&quot;@</ToTranslate>
  </String>
  <String index="741">
    <ToTranslate>$Sonken didn&apos;t beat around the bush.@</ToTranslate>
  </String>
  <String index="742">
    <ToTranslate>Kazuto$&quot;......Are you sure?&quot;@</ToTranslate>
  </String>
  <String index="743">
    <ToTranslate>tcg_son12ma</ToTranslate>
  </String>
  <String index="744">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="745">
    <ToTranslate>sonk0244</ToTranslate>
  </String>
  <String index="746">
    <ToTranslate>Sonken$&quot;......I already told you I made up my mind. I accept this punishment as the loser.&quot;@</ToTranslate>
  </String>
  <String index="747">
    <ToTranslate>Kazuto$&quot;But I don&apos;t think this is that kind of punishment, though......&quot;@</ToTranslate>
  </String>
  <String index="748">
    <ToTranslate>sonk0245</ToTranslate>
  </String>
  <String index="749">
    <ToTranslate>Sonken$&quot;Who told you to be a smart mouth?! If you&apos;re a man, then, umm......  d-don&apos;t make a girl embarrassed!&quot;@</ToTranslate>
  </String>
  <String index="750">
    <ToTranslate>Kazuto$&quot;......Sorry.&quot;@</ToTranslate>
  </String>
  <String index="751">
    <ToTranslate>$Sonken turned beet red and became angry. I saw a cute girl in her. I apologized and bowed.@</ToTranslate>
  </String>
  <String index="752">
    <ToTranslate>Kazuto$&quot;Well, uhh...... can I ask you... one thing......?&quot;@</ToTranslate>
  </String>
  <String index="753">
    <ToTranslate>tcg_son22ka</ToTranslate>
  </String>
  <String index="754">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="755">
    <ToTranslate>sonk0246</ToTranslate>
  </String>
  <String index="756">
    <ToTranslate>Sonken$&quot;W-what?&quot;@</ToTranslate>
  </String>
  <String index="757">
    <ToTranslate>Kazuto$&quot;Is this...... your first time, Sonken?&quot;@</ToTranslate>
  </String>
  <String index="758">
    <ToTranslate>tcg_son12ma</ToTranslate>
  </String>
  <String index="759">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="760">
    <ToTranslate>sonk0247</ToTranslate>
  </String>
  <String index="761">
    <ToTranslate>Sonken$&quot;W-why does that matter?!&quot;@</ToTranslate>
  </String>
  <String index="762">
    <ToTranslate>Kazuto$&quot;Because it&apos;s important. Could you answer?&quot;@</ToTranslate>
  </String>
  <String index="763">
    <ToTranslate>tcg_son22ka</ToTranslate>
  </String>
  <String index="764">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="765">
    <ToTranslate>sonk0248</ToTranslate>
  </String>
  <String index="766">
    <ToTranslate>Sonken$&quot;......This is my first time.&quot;@</ToTranslate>
  </String>
  <String index="767">
    <ToTranslate>Kazuto$&quot;Okay, then I&apos;ll do my best to be gentle with you. Come over here.&quot;@</ToTranslate>
  </String>
  <String index="768">
    <ToTranslate>$Sonken stood as still as a statue until I pulled her hand.@</ToTranslate>
  </String>
  <String index="769">
    <ToTranslate>$As soon as I did, her body shook.@</ToTranslate>
  </String>
  <String index="770">
    <ToTranslate>Kazuto$&quot;I know you&apos;re nervous, but try to relax as much as you can......&quot;@</ToTranslate>
  </String>
  <String index="771">
    <ToTranslate>sonk0249</ToTranslate>
  </String>
  <String index="772">
    <ToTranslate>Sonken$&quot;Th-that&apos;s just impossible.&quot;@</ToTranslate>
  </String>
  <String index="773">
    <ToTranslate>Kazuto$&quot;......Yeah, you&apos;re right. But don&apos;t worry...... don&apos;t be scared.&quot;@</ToTranslate>
  </String>
  <String index="774">
    <ToTranslate>$I embraced her shoulders and led her to the bed.@</ToTranslate>
  </String>
  <String index="775">
    <ToTranslate>sonk0250</ToTranslate>
  </String>
  <String index="776">
    <ToTranslate>Sonken$&quot;......You must be used to doing this.&quot;@</ToTranslate>
  </String>
  <String index="777">
    <ToTranslate>Kazuto$&quot;Ah...... well, I have done this a few times before.&quot;@</ToTranslate>
  </String>
  <String index="778">
    <ToTranslate>sonk0251</ToTranslate>
  </String>
  <String index="779">
    <ToTranslate>Sonken$&quot;......What a depraved guy you are.&quot;@</ToTranslate>
  </String>
  <String index="780">
    <ToTranslate>Kazuto$&quot;Sorry......&quot;@</ToTranslate>
  </String>
  <String index="781">
    <ToTranslate>tcg_son12ja</ToTranslate>
  </String>
  <String index="782">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="783">
    <ToTranslate>sonk0252</ToTranslate>
  </String>
  <String index="784">
    <ToTranslate>Sonken$&quot;......Hmph. You apologize quite often, don&apos;t you?&quot;@</ToTranslate>
  </String>
  <String index="785">
    <ToTranslate>Kazuto$&quot;Well...... that&apos;s because I realize girls do deserve to call me a depraved guy.&quot;@</ToTranslate>
  </String>
  <String index="786">
    <ToTranslate>tcg_son32la</ToTranslate>
  </String>
  <String index="787">
    <ToTranslate>tcg_son32m</ToTranslate>
  </String>
  <String index="788">
    <ToTranslate>sonk0253</ToTranslate>
  </String>
  <String index="789">
    <ToTranslate>Sonken$&quot;Hee-hee...... don&apos;t worry. There is a saying: heroes prefer colorful lives.&quot;@</ToTranslate>
  </String>
  <String index="790">
    <ToTranslate>Kazuto$&quot;All right, then I&apos;ll thankfully let myself be called a hero for today.&quot;@</ToTranslate>
  </String>
  <String index="791">
    <ToTranslate>$Sonken spoke more calmly and started smiling again, finally feeling relaxed. I took her hand and laid her down on the bed.@</ToTranslate>
  </String>
  <String index="792">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="793">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="794">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="795">
    <ToTranslate>bgm20</ToTranslate>
  </String>
  <String index="796">
    <ToTranslate>sonk0254</ToTranslate>
  </String>
  <String index="797">
    <ToTranslate>Sonken$&quot;Y-you know...... this is nerve-racking.&quot;@</ToTranslate>
  </String>
  <String index="798">
    <ToTranslate>Kazuto$&quot;Are you scared?&quot;@</ToTranslate>
  </String>
  <String index="799">
    <ToTranslate>sonk0255</ToTranslate>
  </String>
  <String index="800">
    <ToTranslate>Sonken$&quot;......Yeah, I am. But this is the fate of a loser...... I will resignedly accept it.&quot;@</ToTranslate>
  </String>
  <String index="801">
    <ToTranslate>Kazuto$&quot;Hmmm...... I&apos;m sure that makes you feel even more afraid.&quot;@</ToTranslate>
  </String>
  <String index="802">
    <ToTranslate>sonk0256</ToTranslate>
  </String>
  <String index="803">
    <ToTranslate>Sonken$&quot;What do you mean?&quot;@</ToTranslate>
  </String>
  <String index="804">
    <ToTranslate>Kazuto$&quot;I understand stuff like atonement...... But I want you to just be a girl now that you are with me.&quot;@</ToTranslate>
  </String>
  <String index="805">
    <ToTranslate>sonk0257</ToTranslate>
  </String>
  <String index="806">
    <ToTranslate>Sonken$&quot;Just a girl......?&quot;@</ToTranslate>
  </String>
  <String index="807">
    <ToTranslate>Kazuto$&quot;Yep. You&apos;re a girl first, ruler second.&quot;@</ToTranslate>
  </String>
  <String index="808">
    <ToTranslate>sonk0258</ToTranslate>
  </String>
  <String index="809">
    <ToTranslate>Sonken$&quot;......Th-then.&quot;@</ToTranslate>
  </String>
  <String index="810">
    <ToTranslate>Kazuto$&quot;......Hmm?&quot;@</ToTranslate>
  </String>
  <String index="811">
    <ToTranslate>sonk0259</ToTranslate>
  </String>
  <String index="812">
    <ToTranslate>Sonken$&quot;......If you acknowledge me as a girl, I want you to call me by my true name......&quot;@</ToTranslate>
