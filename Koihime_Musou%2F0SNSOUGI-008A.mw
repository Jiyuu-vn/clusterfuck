[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_rik21ha</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_rik21m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>riku0041</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Rikuson$&quot;Sh-Shaoren-sama! It&apos;s too dangerous to stay here! Please escape immediately!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>sons0016</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Sonshoukou$&quot;Oooo...... no, I wont! We haven&apos;t lost yet!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>riku0042</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>Rikuson$&quot;Yes, we have!&quot;@</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>sons0017</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>Sonshoukou$&quot;No, we haven&apos;t! If you want to run, Non, go ahead by yourself.&quot;@</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>tcg_rik21ga</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>tcg_rik21m</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>riku0043</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>Rikuson$&quot;If I run and leave you here, princess, then Renfa-sama and Shishun-chan will kill me for sure!&quot;@</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>tcg_syk11ia</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>sons0018</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>Sonshoukou$&quot;No, they won&apos;t. You&apos;re far more useful to Renfa-onee-chan than me.&quot;@</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>sons0019</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Sonshoukou$&quot;Therefore, you must escape alone. This is a command from a member of the Son family.&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>tcg_rik21ha</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_rik21m</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>riku0044</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>Rinkuson$&quot;B-but then you should come with me, Shaoren-sama......!&quot;@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>tcg_cun41aa</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_cun41m</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>sei_0255</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Sei$&quot;I don&apos;t think so.&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_rik21ha</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_rik21m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>riku0045</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Rikuson$&quot;Eeeek, a monster!&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>tcg_cun11da</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>sei_0256</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>Sei$&quot;Hey, that&apos;s a rude welcome. It&apos;s normal for a warrior to attack an enemy general.&quot;@</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>sons0020</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>Sonshoukou$&quot;......Who are you?&quot;@</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>sei_0257</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>Sei$&quot;My name is Chou Shiryuu. You&apos;re Shoukou, the youngest daughter of the Son family, I take it.&quot;@</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>sons0021</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Sonshoukou$&quot;That&apos;s correct. Are you here to chop my head off?&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>sei_0258</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>Sei$&quot;Indeed, yes. Fight me.&quot;@</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>SE014</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_cun41aa</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_cun41m</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>sei_0259</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>Sei$&quot;I want a princess of the Son family to enjoy my divine spear.&quot;@</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>tcg_syk11ia</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>sons0022</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>Sonshoukou$&quot;......Non, hurry up and go.&quot;@</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>tcg_rik21ha</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>tcg_rik21m</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>riku0046</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>Rikuson$&quot;B-but!&quot;@</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>sons0023</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>Sonshoukou$&quot;This is an order! Hurry up and go!&quot;@</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>tcg_rik11da</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>riku0047</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>Rikuson$&quot;O-okay! But promise me that you will come back too! Please promise me that!&quot;@</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_syk11ia</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>sons0024</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>Sonshoukou$&quot;I wish I could......&quot;@</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>sei_0260</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>Sei$&quot;Unfortunately, I don&apos;t think that&apos;s possible. I won&apos;t let you escape.&quot;@</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>sons0025</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>Sonshoukou$&quot;You let Non go, though.&quot;@</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>sei_0261</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>Sei$&quot;I couldn&apos;t care less about a strategist or two. However, the Son family is far more valuable as a prize or a hostage. That&apos;s why.&quot;@</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>sons0026</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>Sonshoukou$&quot;That&apos;s interesting. I&apos;m a woman of the Son family, too! Don&apos;t think you can take me that easily!&quot;@</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>sei_0262</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>Sei$&quot;Oh, are you saying you&apos;re going to resist, princess? I know you&apos;re pretty strong, but...... we&apos;ll see if you can win against me.&quot;@</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>sons0027</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>Sonshoukou$&quot;I didn&apos;t get the name Bow-waist Beauty for nothing! If you underestimate me, you&apos;ll be sorry!&quot;@</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>sei_0263</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>Sei$&quot;Oh, that sounds like fun. ......So, shall we exchange blows?&quot;@</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>sons0028</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>Sonshoukou$&quot;Bring it! I&apos;ll crush you and send you back home!&quot;@</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_syk41aa</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>tcg_syk41m</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>sons0029</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>Sonshoukou$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>battle_syk</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>m_yure_y</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_cun41aa</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_cun41m</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>sei_0264</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>Sei$&quot;Hmph. Too soft! If that&apos;s the best you can do, then &apos;Bow-waist Beauty&apos; is just a joke.&quot;@</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>sons0030</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>Sonshoukou$&quot;What!?&quot;@</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>sei_0265</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>Sei$&quot;It appears the Son family are content with playing games.&quot;@</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>sons0031</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>Sonshoukou$&quot;Th-that&apos;s because I wasn&apos;t being serious!&quot;@</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>tcg_cun31ia</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>sei_0266</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>Sei$&quot;Oh, then why don&apos;t you show me what you&apos;ve really got? A weak opponent is nothing but boring.&quot;@</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>sons0032</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>Sonshoukou$&quot;H-how dare you say that?&quot;@</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>sei_0267</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>Sei$&quot;If you don&apos;t like what you hear, then come fight against me!&quot;@</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>sons0033</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>Sonshoukou$&quot;You don&apos;t need to tell me that!&quot;@</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>tcg_syk91aa</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>tcg_syk91m</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>sons0034</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>Sonshoukou$&quot;Dhaaaaaaaaa!&quot;@</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>battle_harai</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>yure_r</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>tcg_cun61aa</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>tcg_cun61m</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>sei_0268</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>Sei$&quot;That&apos;s still weak.&quot;@</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>tcg_syk91aa</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>tcg_syk91m</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>sons0035</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>Sonshoukou$&quot;Here, here, here!&quot;@</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>ef_harai01b</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>ef_harai01b_m</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>BSE14</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>ef_harai01a_m</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>SE010</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>ef_harai01a</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>bg_white</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>bg_white</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>SE016</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>m_yure_t</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>SE015</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>m_yure_y</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>tcg_cun41aa</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>tcg_cun41m</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>sei_0269</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>Sei$&quot;......This is a waste of my time.&quot;@</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>sons0036</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>Sonshoukou$&quot;Ooo, ooo, ooo......!&quot;@</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>sei_0270</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>Sei$&quot;This is simply a waste of time. Let me just finish this up.&quot;@</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>sons0037</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>Sonshoukou$&quot;I can&apos;t let you do that!&quot;@</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>tcg_cun21ga</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>sei_0271</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>Sei$&quot;Scream while you can. Enjoy the dance of Chou Shiryuu&apos;s divine spear!&quot;@</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>tcg_cun91aa</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>tcg_cun91m</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>sei_0272</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>Sei$&quot;Yahhhhhhhhh!&quot;@</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>battle_tuki2</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>tcg_syk61aa</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>tcg_syk61m</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>sons0038</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>Sonshoukou$&quot;Kyah!&quot;@</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>SE002</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>s_yure_r</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>SE004</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>s_yure_t</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>tcg_syk21oa</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>sons0039</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>Sonshoukou$&quot;Ngh...... O-ouch......&quot;@</ToTranslate>
  </String>
  <String index="219">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="220">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="221">
    <ToTranslate>sei_0273</ToTranslate>
  </String>
  <String index="222">
    <ToTranslate>Sei$&quot;Hmph...... I win.&quot;@</ToTranslate>
  </String>
  <String index="223">
    <ToTranslate>tcg_syk21la</ToTranslate>
  </String>
  <String index="224">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="225">
    <ToTranslate>sons0040</ToTranslate>
  </String>
  <String index="226">
    <ToTranslate>Sonshoukou$&quot;No, not yet! I haven&apos;t lost yet!&quot;@</ToTranslate>
  </String>
  <String index="227">
    <ToTranslate>tcg_cun11da</ToTranslate>
  </String>
  <String index="228">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="229">
    <ToTranslate>sei_0274</ToTranslate>
  </String>
  <String index="230">
    <ToTranslate>Sei$&quot;Hmm...... I see you won&apos;t give up.&quot;@</ToTranslate>
  </String>
  <String index="231">
    <ToTranslate>tcg_syk21oa</ToTranslate>
  </String>
  <String index="232">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>sons0041</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>Sonshoukou$&quot;Of course not!&quot;@</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>tcg_cun31ia</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>sei_0275</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>Sei$&quot;Fine, then I&apos;ll react accordingly. Hee-hee......&quot;@</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>sons0042</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>Sonshoukou$&quot;Hey, what are you planning to do with that rope?&quot;@</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>sei_0276</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>Sei$&quot;I thought I&apos;d tie you up. I&apos;ll make it good, so you should feel grateful.&quot;@</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>bgm28</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>tcg_syk61aa</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>tcg_syk61m</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>sons0043</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>Sonshoukou$&quot;H-hey, no, the way you&apos;re touching me is dirty! Stay away from me, idiot! Molester! Pervert!&quot;@</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>tcg_cun31na</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>sei_0277</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>Sei$&quot;I&apos;ll take that as a compliment.&quot;@</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>tcg_syk61aa</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>tcg_syk61m</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>sons0044</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>Sonshoukou$&quot;I&apos;m not complimenting you...... Ahh!&quot;@</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>bcg_camp_a</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>bgm03</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>syur0449</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>Shuri$&quot;The enemy has evacuated. For now, it&apos;s our victory.&quot;@</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>Kazuto$&quot;Yeah, I think so. I was surprised at how small the number of soldiers was, especially since they were the ones who started this conflict.&quot;@</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>tcg_koc32ca</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>tcg_koc32m</ToTranslate>
  </String>
  <String index="275">
    <ToTranslate>shio0087</ToTranslate>
  </String>
  <String index="276">
    <ToTranslate>Shion$&quot;Do you think they were advance troops?&quot;@</ToTranslate>
  </String>
  <String index="277">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="278">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="279">
    <ToTranslate>sui_0191</ToTranslate>
  </String>
  <String index="280">
    <ToTranslate>Sui$&quot;Does that mean their main force is on the way? If that&apos;s the case, I don&apos;t think we have enough soldiers to fight against them.&quot;@</ToTranslate>
  </String>
  <String index="281">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="282">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="283">
    <ToTranslate>rinr0495</ToTranslate>
  </String>
  <String index="284">
    <ToTranslate>Rinrin$&quot;The number of soldiers decides a battle. The side with more troops has a definite advantage, which means we&apos;re at an extreme disadvantage.&quot;@</ToTranslate>
  </String>
  <String index="285">
    <ToTranslate>tcg_syo41ea</ToTranslate>
  </String>
  <String index="286">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="287">
    <ToTranslate>syur0450</ToTranslate>
  </String>
  <String index="288">
    <ToTranslate>Shuri$&quot;Rinrin-chan is right. What should we do......?&quot;@</ToTranslate>
  </String>
  <String index="289">
    <ToTranslate>$As Shuri closed her eyes to think of a plan, someone came up from behind her.@</ToTranslate>
  </String>
  <String index="290">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="291">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="292">
    <ToTranslate>bgm06</ToTranslate>
  </String>
  <String index="293">
    <ToTranslate>sei_0278</ToTranslate>
  </String>
  <String index="294">
    <ToTranslate>Sei$&quot;My Lord, I want you to praise me for a great job.&quot;@</ToTranslate>
  </String>
  <String index="295">
    <ToTranslate>$She approached, dragging someone with her as spoke with confidence.@</ToTranslate>
  </String>
  <String index="296">
    <ToTranslate>Kazuto$&quot;Welcome back, Sei...... What am I praising you for, anyway?&quot;@</ToTranslate>
  </String>
  <String index="297">
    <ToTranslate>tcg_kan21da</ToTranslate>
  </String>
  <String index="298">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="299">
    <ToTranslate>aisy0911</ToTranslate>
  </String>
  <String index="300">
    <ToTranslate>Aisha$&quot;Hmmm? Who is this? Did you capture Son-Go&apos;s general as a hostage?&quot;@</ToTranslate>
  </String>
  <String index="301">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="302">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="303">
    <ToTranslate>sei_0279</ToTranslate>
  </String>
  <String index="304">
    <ToTranslate>Sei$&quot;That&apos;s right.&quot;@</ToTranslate>
  </String>
  <String index="305">
    <ToTranslate>$Sei answered proudly. The girl she brought back knelt down on the ground.@</ToTranslate>
  </String>
  <String index="306">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="307">
    <ToTranslate>ecg_syk01a</ToTranslate>
  </String>
  <String index="308">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="309">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="310">
    <ToTranslate>sons0045</ToTranslate>
  </String>
  <String index="311">
    <ToTranslate>Sonshoukou$&quot;Mmm......&quot;@</ToTranslate>
  </String>
  <String index="312">
    <ToTranslate>sei_0280</ToTranslate>
  </String>
  <String index="313">
    <ToTranslate>Sei$&quot;This is Sonshoukou-dono, the youngest daughter of the Son family, and Sonken&apos;s sister.&quot;@</ToTranslate>
  </String>
  <String index="314">
    <ToTranslate>aisy0912</ToTranslate>
  </String>
  <String index="315">
    <ToTranslate>Aisha$&quot;What!? A princess of the Son family!? That means she is the supreme commander of the troops from earlier, right?&quot;@</ToTranslate>
  </String>
  <String index="316">
    <ToTranslate>sei_0281</ToTranslate>
  </String>
  <String index="317">
    <ToTranslate>Sei$&quot;That&apos;s why I said you better praise me.&quot;@</ToTranslate>
  </String>
  <String index="318">
    <ToTranslate>sui_0192</ToTranslate>
  </String>
  <String index="319">
    <ToTranslate>Sui$&quot;Damn. You stole the lead, Sei!&quot;@</ToTranslate>
  </String>
  <String index="320">
    <ToTranslate>rinr0496</ToTranslate>
  </String>
  <String index="321">
    <ToTranslate>Rinrin$&quot;That&apos;s not fair!&quot;@</ToTranslate>
  </String>
  <String index="322">
    <ToTranslate>sei_0282</ToTranslate>
  </String>
  <String index="323">
    <ToTranslate>Sei$&quot;I didn&apos;t steal a lead. I was simply alert enough not to miss a chance.&quot;@</ToTranslate>
  </String>
  <String index="324">
    <ToTranslate>sui_0193</ToTranslate>
  </String>
  <String index="325">
    <ToTranslate>Sui$&quot;That&apos;s the same thing. Ahh, shit...... I wish I was there to steal the lead.&quot;@</ToTranslate>
  </String>
  <String index="326">
    <ToTranslate>Kazuto$&quot;Ha, ha, that&apos;s okay. By the way, Sonshoukou-chan, right? I want to ask you a few things...... I&apos;d appreciate it if you answered me honestly.&quot;@</ToTranslate>
  </String>
  <String index="327">
    <ToTranslate>sons0046</ToTranslate>
  </String>
  <String index="328">
    <ToTranslate>Sonshoukou$&quot;Mmmm......&quot;@</ToTranslate>
  </String>
  <String index="329">
    <ToTranslate>Kazuto$&quot;To begin with... why did Sonken betray us and attack this city?&quot;@</ToTranslate>
  </String>
  <String index="330">
    <ToTranslate>sons0047</ToTranslate>
  </String>
  <String index="331">
    <ToTranslate>Sonshoukou$&quot;What are you saying?! You&apos;re the one who betrayed us!&quot;@</ToTranslate>
  </String>
  <String index="332">
    <ToTranslate>aisy0913</ToTranslate>
  </String>
  <String index="333">
    <ToTranslate>Aisha$&quot;You liar! You breached the Anti-Toutaku Alliance without notice and attacked this city! We discovered Son-Go flags and armor in the ruins, so you can&apos;t play dumb!&quot;@</ToTranslate>
  </String>
  <String index="334">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="335">
    <ToTranslate>ecg_syk01b</ToTranslate>
  </String>
  <String index="336">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="337">
    <ToTranslate>sons0048</ToTranslate>
  </String>
  <String index="338">
    <ToTranslate>Sonshoukou$&quot;We didn&apos;t do it! We came here only because we received a report that you had attacked us!&quot;@</ToTranslate>
  </String>
  <String index="339">
    <ToTranslate>syur0451</ToTranslate>
  </String>
  <String index="340">
    <ToTranslate>Shuri$&quot;W-wait. Are you saying you received a report saying our army crossed the borders invaded Go?&quot;@</ToTranslate>
  </String>
  <String index="341">
    <ToTranslate>sons0049</ToTranslate>
  </String>
  <String index="342">
    <ToTranslate>Sonshoukou$&quot;Yes! So you&apos;re the ones who betrayed us!&quot;@</ToTranslate>
  </String>
  <String index="343">
    <ToTranslate>shio0088</ToTranslate>
  </String>
  <String index="344">
    <ToTranslate>Shion$&quot;That may be what you think......but what is going on?&quot;@</ToTranslate>
  </String>
  <String index="345">
    <ToTranslate>sei_0283</ToTranslate>
  </String>
  <String index="346">
    <ToTranslate>Sei$&quot;We heard the city was attacked by Son-Go, and that&apos;s why we advanced to the border......&quot;@</ToTranslate>
  </String>
  <String index="347">
    <ToTranslate>sui_0194</ToTranslate>
  </String>
  <String index="348">
    <ToTranslate>Sui$&quot;But Son-Go moved their army to ambush us because they heard we attacked.....&quot;@</ToTranslate>
  </String>
  <String index="349">
    <ToTranslate>shio0089</ToTranslate>
  </String>
  <String index="350">
    <ToTranslate>Shion$&quot;And both armies clashed on the border. Oh my, we&apos;ve been set up well.&quot;@</ToTranslate>
  </String>
  <String index="351">
    <ToTranslate>syur0452</ToTranslate>
  </String>
  <String index="352">
    <ToTranslate>Shuri$&quot;Yes, we have. It&apos;s almost too contrived. It seems as if it was a coincidence, but we really were caught in a cunning plan.&quot;@</ToTranslate>
  </String>
  <String index="353">
    <ToTranslate>sons0050</ToTranslate>
  </String>
  <String index="354">
    <ToTranslate>Sonshoukou$&quot;What cunning plan? What, what&apos;s going on?&quot;@</ToTranslate>
  </String>
  <String index="355">
    <ToTranslate>Kazuto$&quot;We received a report that Go had attacked the city, and that&apos;s why we came.&quot;@</ToTranslate>
  </String>
  <String index="356">
    <ToTranslate>Kazuto$&quot;There were Son-Go flags and armor in the burnt city. Obviously, it appeared to have been attacked by Son-Go&apos;s soldiers.&quot;@</ToTranslate>
  </String>
  <String index="357">
    <ToTranslate>syur0453</ToTranslate>
  </String>
  <String index="358">
    <ToTranslate>Shuri$&quot;If, at the same time, Son-Go received a report of an invasion by us, Son-Go would send either their army or a scout, and the scout would see us on the border.&quot;@</ToTranslate>
  </String>
  <String index="359">
    <ToTranslate>sei_0284</ToTranslate>
  </String>
  <String index="360">
    <ToTranslate>Sei$&quot;Then, Son-Go would think Hongou really did invade, and if we saw Son-Go, we would think they were back to attack the city again.&quot;@</ToTranslate>
  </String>
  <String index="361">
    <ToTranslate>rinr0497</ToTranslate>
  </String>
  <String index="362">
    <ToTranslate>Rinrin$&quot;Therefore, both armies would clash.&quot;@</ToTranslate>
  </String>
  <String index="363">
    <ToTranslate>sui_0195</ToTranslate>
  </String>
  <String index="364">
    <ToTranslate>Sui$&quot;Hahhh. Whoever thought of this plan must be really evil.&quot;@</ToTranslate>
  </String>
  <String index="365">
    <ToTranslate>syur0454</ToTranslate>
  </String>
  <String index="366">
    <ToTranslate>Shuri$&quot;Divide and conquer...... It&apos;s an extremely effective plan, but I never thought we would fall for it......&quot;@</ToTranslate>
  </String>
  <String index="367">
    <ToTranslate>syur0455</ToTranslate>
  </String>
  <String index="368">
    <ToTranslate>Shuri$&quot;I&apos;m sorry. I should have noticed it as a strategist...... it&apos;s my fault.&quot;@</ToTranslate>
  </String>
  <String index="369">
    <ToTranslate>Kazuto$&quot;It&apos;s not your fault, Shuri. Of course, it&apos;s not Sonshoukou-chan&apos;s fault either.&quot;@</ToTranslate>
  </String>
  <String index="370">
    <ToTranslate>sons0051</ToTranslate>
  </String>
  <String index="371">
    <ToTranslate>Sonshoukou$&quot;......What does that mean? Who would think of such a complicated plan?&quot;@</ToTranslate>
  </String>
  <String index="372">
    <ToTranslate>sei_0285</ToTranslate>
  </String>
  <String index="373">
    <ToTranslate>Sei$&quot;I&apos;m sure it&apos;s someone who wants our ties cut, but since Sougi is out of the picture now, I can only think of the surrounding small countries.&quot;@</ToTranslate>
  </String>
  <String index="374">
    <ToTranslate>syur0456</ToTranslate>
  </String>
  <String index="375">
    <ToTranslate>Shuri$&quot;No...... there is one. It&apos;s probably the strongest enemy we&apos;ve ever faced.&quot;@</ToTranslate>
  </String>
  <String index="376">
    <ToTranslate>Kazuto$&quot;Yeah...... It&apos;s them.&quot;@</ToTranslate>
  </String>
  <String index="377">
    <ToTranslate>aisy0914</ToTranslate>
  </String>
  <String index="378">
    <ToTranslate>Aisha$&quot;Them...... Huh?! Are you talking about the White Clothes?&quot;@</ToTranslate>
  </String>
  <String index="379">
    <ToTranslate>shio0090</ToTranslate>
  </String>
  <String index="380">
    <ToTranslate>Shion$&quot;It&apos;s probably them. But...... Son-Go has a superb strategist, Shuuyu. I can&apos;t believe even she was deceived......&quot;@</ToTranslate>
  </String>
  <String index="381">
    <ToTranslate>rinr0498</ToTranslate>
  </String>
  <String index="382">
    <ToTranslate>Rinrin$&quot;I just don&apos;t get it.&quot;@</ToTranslate>
  </String>
  <String index="383">
    <ToTranslate>sons0052</ToTranslate>
  </String>
  <String index="384">
    <ToTranslate>Sonshoukou$&quot;......I get it, I think. I think I understand the trick behind this.&quot;@</ToTranslate>
  </String>
  <String index="385">
    <ToTranslate>sei_0286</ToTranslate>
  </String>
  <String index="386">
    <ToTranslate>Sei$&quot;Oh, then do tell us.&quot;@</ToTranslate>
  </String>
  <String index="387">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="388">
    <ToTranslate>ecg_syk01a</ToTranslate>
  </String>
  <String index="389">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="390">
    <ToTranslate>sons0053</ToTranslate>
  </String>
  <String index="391">
    <ToTranslate>Sonshoukou$&quot;But before that, can you untie me? Otherwise, I won&apos;t tell you.&quot;@</ToTranslate>
  </String>
  <String index="392">
    <ToTranslate>sei_0287</ToTranslate>
  </String>
  <String index="393">
    <ToTranslate>Sei$&quot;I can&apos;t do that. There&apos;s no guarantee you won&apos;t run.&quot;@</ToTranslate>
  </String>
  <String index="394">
    <ToTranslate>sons0054</ToTranslate>
  </String>
  <String index="395">
    <ToTranslate>Sonshoukou$&quot;My chest and hands hurt! I promise you I won&apos;t run, so untie me!&quot;@</ToTranslate>
  </String>
  <String index="396">
    <ToTranslate>sei_0288</ToTranslate>
  </String>
  <String index="397">
    <ToTranslate>Sei$&quot;Mmm...... What should I do, My Lord?&quot;@</ToTranslate>
  </String>
  <String index="398">
    <ToTranslate>Kazuto$&quot;It should be okay. By the way, did you tie her up yourself, Sei?&quot;@</ToTranslate>
  </String>
  <String index="399">
    <ToTranslate>sei_0289</ToTranslate>
  </String>
  <String index="400">
    <ToTranslate>Sei$&quot;Of course. Is something wrong?&quot;@</ToTranslate>
  </String>
  <String index="401">
    <ToTranslate>Kazuto$&quot;The way you tied her......&quot;@</ToTranslate>
  </String>
  <String index="402">
    <ToTranslate>sei_0290</ToTranslate>
  </String>
  <String index="403">
    <ToTranslate>Sei$&quot;Yeah, that&apos;s my hobby. I&apos;m rather proud that I did a beautiful job......&quot;@</ToTranslate>
  </String>
  <String index="404">
    <ToTranslate>Kazuto$&quot;Your hobby, huh? Hahh......&quot;@</ToTranslate>
  </String>
  <String index="405">
    <ToTranslate>$I guess it&apos;s interesting to see a general whose hobby is tortoise shell bondage.@</ToTranslate>
  </String>
  <String index="406">
    <ToTranslate>Kazuto$&quot;Anyway, now we know there&apos;s been a misunderstanding, it should be okay. Why don&apos;t you untie her?&quot;@</ToTranslate>
  </String>
  <String index="407">
    <ToTranslate>sei_0291</ToTranslate>
  </String>
  <String index="408">
    <ToTranslate>Sei$&quot;As you wish.&quot;@</ToTranslate>
  </String>
  <String index="409">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="410">
    <ToTranslate>bcg_camp_a</ToTranslate>
  </String>
  <String index="411">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="412">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="413">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="414">
    <ToTranslate>sons0055</ToTranslate>
  </String>
  <String index="415">
    <ToTranslate>Sonshoukou$&quot;Whew...... finally I&apos;m free......&quot;@</ToTranslate>
  </String>
  <String index="416">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="417">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="418">
    <ToTranslate>sei_0292</ToTranslate>
  </String>
  <String index="419">
    <ToTranslate>Sei$&quot;Hmm? I guess you didn&apos;t like the way you were tied.&quot;@</ToTranslate>
  </String>
  <String index="420">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="421">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="422">
    <ToTranslate>sons0056</ToTranslate>
  </String>
  <String index="423">
    <ToTranslate>Sonshoukou$&quot;Of course not!&quot;@</ToTranslate>
  </String>
  <String index="424">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="425">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="426">
    <ToTranslate>aisy0915</ToTranslate>
  </String>
  <String index="427">
    <ToTranslate>Aisha$&quot;Who cares about how you were tied? Princess of Son-Go, you just said you figured out the trick, so can you explain it to us?&quot;@</ToTranslate>
  </String>
  <String index="428">
    <ToTranslate>sons0057</ToTranslate>
  </String>
  <String index="429">
    <ToTranslate>Sonshoukou$&quot;It&apos;s simple. Whoever thought of this cunning plot and Shuuyu are accomplices. Oh...... maybe Shuuyu herself plotted it.&quot;@</ToTranslate>
  </String>
  <String index="430">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="431">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="432">
    <ToTranslate>aisy0916</ToTranslate>
  </String>
  <String index="433">
    <ToTranslate>Aisha$&quot;Wha......Are you saying Shuuyu is trying to betray Son-Go?&quot;@</ToTranslate>
  </String>
  <String index="434">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="435">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="436">
    <ToTranslate>sons0058</ToTranslate>
  </String>
  <String index="437">
    <ToTranslate>Sonshoukou$&quot;......I&apos;m not sure about that. But I think Meirin has something to do with what happened.&quot;@</ToTranslate>
  </String>
  <String index="438">
    <ToTranslate>tcg_cho21ja</ToTranslate>
  </String>
  <String index="439">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="440">
    <ToTranslate>rinr0499</ToTranslate>
  </String>
  <String index="441">
    <ToTranslate>Rinrin$&quot;Then we should let Go know that they have a traitor immediately.&quot;@</ToTranslate>
  </String>
  <String index="442">
    <ToTranslate>sons0059</ToTranslate>
  </String>
  <String index="443">
    <ToTranslate>Sonshoukou$&quot;That&apos;s no use.&quot;@</ToTranslate>
  </String>
  <String index="444">
    <ToTranslate>tcg_koc22da</ToTranslate>
  </String>
  <String index="445">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="446">
    <ToTranslate>shio0091</ToTranslate>
  </String>
  <String index="447">
    <ToTranslate>Shion$&quot;Oh my, what do you mean?&quot;@</ToTranslate>
  </String>
  <String index="448">
    <ToTranslate>sons0060</ToTranslate>
  </String>
  <String index="449">
    <ToTranslate>Sonshoukou$&quot;......Although we&apos;ve been deceived, our soldiers were injured. My sister isn&apos;t heartless enough to forget about that......&quot;@</ToTranslate>
  </String>
  <String index="450">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="451">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="452">
    <ToTranslate>sei_0293</ToTranslate>
  </String>
  <String index="453">
    <ToTranslate>Sei$&quot;In other words, is she going to create a bigger battle to take revenge for the injured soldiers?&quot;@</ToTranslate>
  </String>
  <String index="454">
    <ToTranslate>sons0061</ToTranslate>
  </String>
  <String index="455">
    <ToTranslate>Sonshoukou$&quot;Yeah...... there&apos;s no reason, loss or gain. It&apos;s just the disposition and pride of Son-Go...... something emotional.&quot;@</ToTranslate>
  </String>
  <String index="456">
    <ToTranslate>sons0062</ToTranslate>
  </String>
  <String index="457">
    <ToTranslate>Sonshoukou$&quot;If comrades get hurt, we avenge them with heart and soul. That&apos;s just how it is in Son-Go.&quot;@</ToTranslate>
  </String>
  <String index="458">
    <ToTranslate>Kazuto$&quot;Then we&apos;ll just have to fight again, huh.......?&quot;@</ToTranslate>
  </String>
  <String index="459">
    <ToTranslate>tcg_syo11ea</ToTranslate>
  </String>
  <String index="460">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="461">
    <ToTranslate>syur0457</ToTranslate>
  </String>
  <String index="462">
    <ToTranslate>Shuri$&quot;......Unless we can convince them, we have to use force to win.&quot;@</ToTranslate>
  </String>
  <String index="463">
    <ToTranslate>Kazuto$&quot;I see...... Okay, then let&apos;s fight Sonken here and persuade her by force.&quot;@</ToTranslate>
  </String>
  <String index="464">
    <ToTranslate>Kazuto$&quot;I don&apos;t think that&apos;s the best way...... but otherwise, the battle will be too much of a mess.&quot;@</ToTranslate>
  </String>
  <String index="465">
    <ToTranslate>Kazuto$&quot;And only the White Clothes gain from that. Therefore, we must keep damage minimal and straighten things out with Son-Go.&quot;@</ToTranslate>
  </String>
  <String index="466">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="467">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="468">
    <ToTranslate>sui_0196</ToTranslate>
  </String>
  <String index="469">
    <ToTranslate>Sui$&quot;Once things are straightened out, we may be able to figure out who thought of such an evil plot. I agree with Our Lord.&quot;@</ToTranslate>
  </String>
  <String index="470">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="471">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="472">
    <ToTranslate>aisy0917</ToTranslate>
  </String>
  <String index="473">
    <ToTranslate>Aisha$&quot;Hmm, that&apos;s probably the only thing we can do.&quot;@</ToTranslate>
  </String>
  <String index="474">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="475">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="476">
    <ToTranslate>sei_0294</ToTranslate>
  </String>
  <String index="477">
    <ToTranslate>Sei$&quot;I agree.&quot;@</ToTranslate>
  </String>
  <String index="478">
    <ToTranslate>Kazuto$&quot;Good. Then inform the entire army. We&apos;ll wait for Sonken to arrive.&quot;@</ToTranslate>
  </String>
  <String index="479">
    <ToTranslate>tcg_cho31ja</ToTranslate>
  </String>
  <String index="480">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="481">
    <ToTranslate>rinr0500</ToTranslate>
  </String>
  <String index="482">
    <ToTranslate>Rinrin$&quot;What if she doesn&apos;t come?&quot;@</ToTranslate>
  </String>
  <String index="483">
    <ToTranslate>tcg_syk21ea</ToTranslate>
  </String>
  <String index="484">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="485">
    <ToTranslate>sons0063</ToTranslate>
  </String>
  <String index="486">
    <ToTranslate>Sonshoukou$&quot;That&apos;s impossible. Onee-chan will come for sure with her army.&quot;@</ToTranslate>
  </String>
  <String index="487">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="488">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="489">
    <ToTranslate>shio0092</ToTranslate>
  </String>
  <String index="490">
    <ToTranslate>Shion$&quot;Then let&apos;s use this time to replenish our soldiers and supplies.&quot;@</ToTranslate>
  </String>
  <String index="491">
    <ToTranslate>tcg_syo41ea</ToTranslate>
  </String>
  <String index="492">
    <ToTranslate>tcg_syo41m</ToTranslate>
  </String>
  <String index="493">
    <ToTranslate>syur0458</ToTranslate>
  </String>
  <String index="494">
    <ToTranslate>Shuri$&quot;Under the circumstances, I don&apos;t know how many soldiers we can gather......&quot;@</ToTranslate>
  </String>
  <String index="495">
    <ToTranslate>Kazuto$&quot;I understand, just do your best.&quot;@</ToTranslate>
  </String>
  <String index="496">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="497">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="498">
    <ToTranslate>syur0459</ToTranslate>
  </String>
  <String index="499">
    <ToTranslate>Shuri$&quot;As you wish!&quot;@</ToTranslate>
  </String>
  <String index="500">
    <ToTranslate>bgm02</ToTranslate>
  </String>
  <String index="501">
    <ToTranslate>Kazuto$&quot;Last is...... regarding Sonshoukou-chan.&quot;@</ToTranslate>
  </String>
  <String index="502">
    <ToTranslate>tcg_syk11ia</ToTranslate>
  </String>
  <String index="503">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="504">
    <ToTranslate>sons0064</ToTranslate>
  </String>
  <String index="505">
    <ToTranslate>Sonshoukou$&quot;Me? What about me?&quot;@</ToTranslate>
  </String>
  <String index="506">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="507">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="508">
    <ToTranslate>sei_0295</ToTranslate>
  </String>
  <String index="509">
    <ToTranslate>Sei$&quot;We can&apos;t just let you go home.&quot;@</ToTranslate>
  </String>
  <String index="510">
    <ToTranslate>Kazuto$&quot;That&apos;s right. ......Besides, you may have to persuade Sonken for us.&quot;@</ToTranslate>
  </String>
  <String index="511">
    <ToTranslate>tcg_syk21da</ToTranslate>
  </String>
  <String index="512">
    <ToTranslate>tcg_syk21m</ToTranslate>
  </String>
  <String index="513">
    <ToTranslate>sons0065</ToTranslate>
  </String>
  <String index="514">
    <ToTranslate>Sonshoukou$&quot;I don&apos;t think Onee-chan will listen to me, though......&quot;@</ToTranslate>
  </String>
  <String index="515">
    <ToTranslate>Kazuto$&quot;But just in case, I want you to stay with us as a hostage for a while. When the time comes, we&apos;ll be sure to let you go.&quot;@</ToTranslate>
  </String>
  <String index="516">
    <ToTranslate>sons0066</ToTranslate>
  </String>
  <String index="517">
    <ToTranslate>Sonshoukou$&quot;......Don&apos;t you think I will run immediately?&quot;@</ToTranslate>
  </String>
  <String index="518">
    <ToTranslate>Kazuto$&quot;I&apos;d appreciate it if you didn&apos;t.&quot;@</ToTranslate>
  </String>
  <String index="519">
    <ToTranslate>sons0067</ToTranslate>
  </String>
  <String index="520">
    <ToTranslate>Sonshoukou$&quot;That&apos;s it?&quot;@</ToTranslate>
  </String>
  <String index="521">
    <ToTranslate>Kazuto$&quot;That&apos;s it. Well, Aisha and Sei think I&apos;m too naive, but I don&apos;t believe men should treat girls roughly.&quot;@</ToTranslate>
  </String>
  <String index="522">
    <ToTranslate>tcg_kan31ha</ToTranslate>
  </String>
  <String index="523">
    <ToTranslate>tcg_kan31m</ToTranslate>
  </String>
  <String index="524">
    <ToTranslate>aisy0918</ToTranslate>
  </String>
  <String index="525">
    <ToTranslate>Aisha$&quot;I don&apos;t think it&apos;s a matter of whether she is a boy or a girl......&quot;@</ToTranslate>
  </String>
  <String index="526">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="527">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="528">
    <ToTranslate>sei_0296</ToTranslate>
  </String>
  <String index="529">
    <ToTranslate>Sei$&quot;Well, be easy on him, Aisha. That&apos;s his weak point and his strong point at the same time.&quot;@</ToTranslate>
  </String>
  <String index="530">
    <ToTranslate>Kazuto$&quot;That&apos;s harsh......&quot;@</ToTranslate>
  </String>
  <String index="531">
    <ToTranslate>$As I hung my head at Aisha&apos;s and Sei&apos;s complaints...@</ToTranslate>
  </String>
  <String index="532">
    <ToTranslate>tcg_syk31fa</ToTranslate>
  </String>
  <String index="533">
    <ToTranslate>tcg_syk31m</ToTranslate>
  </String>
  <String index="534">
    <ToTranslate>bgm26</ToTranslate>
  </String>
  <String index="535">
    <ToTranslate>sons0068</ToTranslate>
  </String>
  <String index="536">
    <ToTranslate>Sonshoukou$&quot;Hmm......&quot;@</ToTranslate>
  </String>
  <String index="537">
    <ToTranslate>$Sonshoukou peered into my face.@</ToTranslate>
  </String>
  <String index="538">
    <ToTranslate>Kazuto$&quot;W-what?&quot;@</ToTranslate>
  </String>
  <String index="539">
    <ToTranslate>tcg_syk11aa</ToTranslate>
  </String>
  <String index="540">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="541">
    <ToTranslate>sons0069</ToTranslate>
  </String>
  <String index="542">
    <ToTranslate>Sonshoukou$&quot;......You&apos;re a good guy.&quot;@</ToTranslate>
  </String>
  <String index="543">
    <ToTranslate>Kazuto$&quot;Me? Haha, thanks.&quot;@</ToTranslate>
  </String>
  <String index="544">
    <ToTranslate>tcg_syk11ca</ToTranslate>
  </String>
  <String index="545">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="546">
    <ToTranslate>sons0070</ToTranslate>
  </String>
  <String index="547">
    <ToTranslate>Sonshoukou$&quot;......I&apos;ve decided! I won&apos;t run. I&apos;m interested in you, too.&quot;@</ToTranslate>
  </String>
  <String index="548">
    <ToTranslate>tcg_syk11ca</ToTranslate>
  </String>
  <String index="549">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="550">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="551">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="552">
    <ToTranslate>aisy0919</ToTranslate>
  </String>
  <String index="553">
    <ToTranslate>Aisha$&quot;I won&apos;t let you even touch My Lord, though.&quot;@</ToTranslate>
  </String>
  <String index="554">
    <ToTranslate>tcg_syk11ba</ToTranslate>
  </String>
  <String index="555">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="556">
    <ToTranslate>sons0071</ToTranslate>
  </String>
  <String index="557">
    <ToTranslate>Sonshoukou$&quot;Oh, but I don&apos;t have to listen to a mere vassal. ......I&apos;m a princess of Son-Go, after all!&quot;@</ToTranslate>
  </String>
  <String index="558">
    <ToTranslate>tcg_syk31fa</ToTranslate>
  </String>
  <String index="559">
    <ToTranslate>tcg_syk31m</ToTranslate>
  </String>
  <String index="560">
    <ToTranslate>sons0072</ToTranslate>
  </String>
  <String index="561">
    <ToTranslate>Sonshoukou$&quot;Maybe there&apos;ll be a political marriage!&quot;@</ToTranslate>
  </String>
  <String index="562">
    <ToTranslate>aisy0920</ToTranslate>
  </String>
  <String index="563">
    <ToTranslate>Aisha$&quot;I-I will not allow that! If that is to happen, I swear that I, Kan Unchou will block it with my heart and soul!&quot;@</ToTranslate>
  </String>
  <String index="564">
    <ToTranslate>tcg_syk11ca</ToTranslate>
  </String>
  <String index="565">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="566">
    <ToTranslate>sons0073</ToTranslate>
  </String>
  <String index="567">
    <ToTranslate>Sonshoukou$&quot;Oh, I see Kan Unchou is dramatic in her love affairs, as well. How unsightly!&quot;@</ToTranslate>
  </String>
  <String index="568">
    <ToTranslate>tcg_kan11sa</ToTranslate>
  </String>
  <String index="569">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="570">
    <ToTranslate>aisy0921</ToTranslate>
  </String>
  <String index="571">
    <ToTranslate>Aisha$&quot;Ugh...... S-Sei! Say something to this girl!&quot;@</ToTranslate>
  </String>
  <String index="572">
    <ToTranslate>tcg_cun31fa</ToTranslate>
  </String>
  <String index="573">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="574">
    <ToTranslate>sei_0297</ToTranslate>
  </String>
  <String index="575">
    <ToTranslate>Sei$&quot;Don&apos;t drag me into this because you can&apos;t argue with her.&quot;@</ToTranslate>
  </String>
  <String index="576">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="577">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="578">
    <ToTranslate>aisy0922</ToTranslate>
  </String>
  <String index="579">
    <ToTranslate>Aisha$&quot;Sui! Rinrin!&quot;@</ToTranslate>
  </String>
  <String index="580">
    <ToTranslate>tcg_bac12ga</ToTranslate>
  </String>
  <String index="581">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="582">
    <ToTranslate>sui_0197</ToTranslate>
  </String>
  <String index="583">
    <ToTranslate>Sui$&quot;Ahahh...... I&apos;m not good at dealing with people who can argue well......&quot;@</ToTranslate>
  </String>
  <String index="584">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="585">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="586">
    <ToTranslate>rinr0501</ToTranslate>
  </String>
  <String index="587">
    <ToTranslate>Rinrin$&quot;Ehen, me neither!&quot;@</ToTranslate>
  </String>
  <String index="588">
    <ToTranslate>tcg_kan11sa</ToTranslate>
  </String>
  <String index="589">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="590">
    <ToTranslate>aisy0923</ToTranslate>
  </String>
  <String index="591">
    <ToTranslate>Aisha$&quot;Nngh, b-but even if I have to argue by myself, I won&apos;t let you have My Lord, never!&quot;@</ToTranslate>
  </String>
  <String index="592">
    <ToTranslate>tcg_syk11ca</ToTranslate>
  </String>
  <String index="593">
    <ToTranslate>tcg_syk11m</ToTranslate>
  </String>
  <String index="594">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="595">
    <ToTranslate>sons0074</ToTranslate>
  </String>
  <String index="596">
    <ToTranslate>Sonshoukou$&quot;That&apos;s up to Kazuto!&quot;@</ToTranslate>
  </String>
  <String index="597">
    <ToTranslate>tcg_kan21pa</ToTranslate>
  </String>
  <String index="598">
    <ToTranslate>tcg_kan21m</ToTranslate>
  </String>
  <String index="599">
    <ToTranslate>aisy0924</ToTranslate>
  </String>
  <String index="600">
    <ToTranslate>Aisha$&quot;H-h-how dare you call My Lord by his name! How I envy...... I mean, how rude of you! I will not allow it!&quot;@</ToTranslate>
  </String>
  <String index="601">
    <ToTranslate>tcg_syk31fa</ToTranslate>
  </String>
  <String index="602">
    <ToTranslate>tcg_syk31m</ToTranslate>
  </String>
  <String index="603">
    <ToTranslate>sons0075</ToTranslate>
  </String>
  <String index="604">
    <ToTranslate>Sonshoukou$&quot;Who cares? You can call me Shaoren, okay, Kazuto!&quot;@</ToTranslate>
  </String>
  <String index="605">
    <ToTranslate>Kazuto$&quot;Haha, okay.&quot;@</ToTranslate>
  </String>
  <String index="606">
    <ToTranslate>tcg_kan11qa</ToTranslate>
  </String>
  <String index="607">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="608">
    <ToTranslate>aisy0925</ToTranslate>
  </String>
  <String index="609">
    <ToTranslate>Aisha$&quot;My Lord! How can you allow yourself to be seduced so easily?!&quot;@</ToTranslate>
  </String>
  <String index="610">
    <ToTranslate>aisy0926</ToTranslate>
  </String>
  <String index="611">
    <ToTranslate>Aisha$&quot;Be more resolute! More gallant! More noble! You can&apos;t be so loose with women!&quot;@</ToTranslate>
  </String>
  <String index="612">
    <ToTranslate>bcg_sky_a</ToTranslate>
  </String>
  <String index="613">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="614">
    <ToTranslate>$Aisha&apos;s voice echoed through the camp.@</ToTranslate>
  </String>
  <String index="615">
    <ToTranslate>$As I listened to it mix with everyone&apos;s laughter, I turned my thoughts towards our next battle...@</ToTranslate>
  </String>
  <String index="616">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="617">
    <ToTranslate>bg_black</ToTranslate>
  </String>
