  <Line StartPos="3237" Length="5" Skip="false">
    <Translated>Cliff</Translated>
  </Line>
  <Line StartPos="3264" Length="15" Skip="false">
    <Translated>&quot;Yeah, please.&quot;</Translated>
  </Line>
  <Line StartPos="3293" Length="3" Skip="false">
    <Translated>Aoi</Translated>
  </Line>
  <Line StartPos="3309" Length="31" Skip="false">
    <Translated>&quot;And... do you need something?&quot;</Translated>
  </Line>
  <Line StartPos="3358" Length="3" Skip="false">
    <Translated>Aoi</Translated>
  </Line>
  <Line StartPos="3374" Length="34" Skip="false">
    <Translated>&quot;Oh... Thanks for your hard work.&quot;</Translated>
  </Line>
  <Line StartPos="3413" Length="5" Skip="false">
    <Translated>Cliff</Translated>
  </Line>
  <Line StartPos="3440" Length="75" Skip="false">
    <Translated>&quot;You too. How was it? Did\0you hear anything about\0incidents with monsters?&quot;</Translated>
  </Line>
  <Line StartPos="3529" Length="3" Skip="false">
    <Translated>Aoi</Translated>
  </Line>
  <Line StartPos="3545" Length="24" Skip="false">
