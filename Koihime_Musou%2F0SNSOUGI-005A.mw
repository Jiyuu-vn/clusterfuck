[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>kato0285</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Kakouton$&quot;Damn...... All troops, withdraw!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_kyo22da</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>kii_0052</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Kyocho$&quot;What? We can go on!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>kato0286</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>Kakouton$&quot;Fool! We&apos;re going to fake a retreat and let Shia flank them! If you&apos;re a general, then figure that out!&quot;@</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>tcg_kyo22ba</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>kii_0053</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>Kyocho$&quot;Oh, I see. Understood!&quot;@</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>kato0287</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>Kakouton$&quot;Then gather your unit! We are retreating!&quot;@</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>tcg_kyo22fa</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>kii_0054</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>Kyocho$&quot;Okay!&quot;@</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>bgm0000</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>sui_0140</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Sui$&quot;Hey! The enemy is withdrawing!&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>rinr0456</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Rinrin$&quot;After them!&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>tcg_kan11ea</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>aisy0811</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>Aisha$&quot;Ah, wait, Rinrin!&quot;@</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_syo21ca</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>syur0413</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>Shuri$&quot;Aisha-san! Please go after Rinrin! It&apos;s too dangerous to charge in with just one unit!&quot;@</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>aisy0812</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>Aisha$&quot;O-okay!&quot;@</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>sui_0141</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Sui$&quot;We&apos;re going after them, too!&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>sei_0200</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>Sei$&quot;Don&apos;t fall behind the Kan&apos;u squad or Bachou squad! Chouun squad, advance!&quot;@</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>syur0414</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>Shuri$&quot;Ah, wait! It&apos;s too dangerous for everyone to give chase together, too!!&quot;@</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>tcg_cho31da</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>rinr0457</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>Rinrin$&quot;Nyah? ......They all ran away. This isn&apos;t any fun!&quot;@</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>tcg_cho21da</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>rinr0458</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>Rinrin$&quot;Isn&apos;t there anyone who will fight against Chouhi?&quot;@</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>SE047a</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_cho21da</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_cry21ga</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>bgm17</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>chor0167</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>Chouryou$&quot;Here I am!&quot;@</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_cho41ha</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_cho41m</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>rinr0459</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>Rinrin$&quot;Nyanyah!? Chouryou-onee-chan?&quot;@</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_cry21da</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>chor0168</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>Chouryou$&quot;Yo. Long time no see, midget.&quot;@</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>rinr0460</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>Rinrin$&quot;Yo! Good to see you, Chouryou-onee-chan. But why are you here, anyway?&quot;@</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>tcg_cry11ba</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_cry11m</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>chor0169</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>Chouryou$&quot;As a member of Gi, I laid an ambush with my troops to fight you all!&quot;@</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>tcg_cho21ja</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_cho21m</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>rinr0461</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>Rinrin$&quot;I see. So Rinrin fell hook, line, and sinker?&quot;@</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>chor0170</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>Chouryou$&quot;You got it.&quot;@</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>rinr0462</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>Rinrin$&quot;Heh-heh. I made a little mistake, didn&apos;t I?&quot;@</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>tcg_cry21da</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>chor0171</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>Chouryou$&quot;Hahaha, it&apos;s not a little mistake, it&apos;s a big mistake, careless girl!&quot;@</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>tcg_cho31ia</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>rinr0463</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>Rinrin$&quot;Careless, careless!&quot;@</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>tcg_cry21ga</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>chor0172</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>Chouryou$&quot;......Okay, that&apos;s enough for the jokes. It looks like Kan&apos;u and the others are coming up from the rear, so I&apos;ll be giving you my all.&quot;@</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>rinr0464</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>Rinrin$&quot;I&apos;ll take you whenever you&apos;re ready!&quot;@</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>chor0173</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>Chouryou$&quot;Yeah. Let&apos;s fight fair and square.&quot;@</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>rinr0465</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>Rinrin$&quot;Sure! See you, then!&quot;@</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>SE026</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>tcg_cry21ga</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>chor0174</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>Chouryou$&quot;Sure. Alright, let&apos;s go men! Show them the strength of Chou Bunen and the swiftness of Sougi!&quot;@</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>tcg_cry21ha</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>tcg_cry21m</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>chor0175</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>Chouryou$&quot;All units, charge!&quot;@</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>SE124</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>bg_black</ToTranslate>
  </String>
