[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_outcast_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>kato0222</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Kakouton$&quot;Kkh......! I can&apos;t believe I&apos;m losing......!&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_kyo12ca</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>kii_0002</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Kyocho$&quot;Shunran-sama! That&apos;s enough! We must retreat!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>kato0223</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Kakouton$&quot;...I will not retreat! If you want, go ahead and retreat alone, Kii!&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_kyo22da</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>kii_0003</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>Kyocho$&quot;No, no, no, no, don&apos;t say that. There&apos;s no way we can maintain this front!&quot;@</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>kato0224</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Kakouton$&quot;But if I retreat now, how can I face Karin-sama......?&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_kyo12ca</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>kii_0004</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Kyocho$&quot;But Karin-sama would rather you came back safely too, Shunran-sama!&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>kato0225</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>Kakouton$&quot;But......&quot;@</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_kyo12ea</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>kii_0005</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Kyocho$&quot;Jeez! When did you become someone who moans and groans?! Please go back to being impressive, Shunran-sama!&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>kii_0006</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>Kyocho$&quot;Otherwise, I won&apos;t like you anymore!&quot;@</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>tcg_ton14da</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>kato0226</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>Kakouton$&quot;Kii......&quot;@</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>kato0227</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>Kakouton$&quot;......You&apos;re right. I don&apos;t want Karin-sama to dislike me, but I don&apos;t want you to dislike me, either.&quot;@</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_kyo12aa</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>kii_0007</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>Kyocho$&quot;Then let&apos;s retreat. Okay?&quot;@</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>tcg_ton14aa</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>kato0228</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>Kakouton$&quot;......Okay.&quot;@</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>tcg_kyo22fa</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>kii_0008</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>Kyocho$&quot;Whew...... Okay everyone, I&apos;ll retreat with Shunran-sama! Please tell Choko-tan that she is in charge now!&quot;@</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>tcg_zgi11aa</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_zgi11m</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>Soldier$&quot;Yes ma&apos;am! You mean General Choukou, right?&quot;@</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>tcg_kyo12aa</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>kii_0009</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>Kyocho$&quot;Yep! Tell Choko-tan that I&apos;m counting on her. Also...... sorry, everyone.&quot;@</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>Soldier$&quot;Don&apos;t mention it. We don&apos;t mind giving up our lives for general Kakouton. Please take her to safety.&quot;@</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>kii_0010</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>Kyocho$&quot;Thank you...... See ya!&quot;@</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>Soldier$&quot;Yes ma&apos;am.&quot;@</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>bgm08</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>Soldier$&quot;Look! The enemy is retreating!&quot;@</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>Soldier$&quot;We won! We won!&quot;@</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>sei_0139</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>Sei$&quot;That&apos;s right! This is our victory! Everyone, let us raise our voices in honor of it!&quot;@</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>Soldier$&quot;Whoooo!&quot;@</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>tcg_koc22aa</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>shio0029</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>Shion$&quot;Whew...... we did it.&quot;@</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>tcg_cun11aa</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>sei_0140</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>Sei$&quot;Yeah. We&apos;ve suffered many losses...... but once the main force gets here, we should be okay.&quot;@</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>shio0030</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>Shion$&quot;I suppose this means we cleared our first hurdle.&quot;@</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>sei_0141</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>Sei$&quot;But we still have a long way to go. Until we take Gi&apos;s capital, we can&apos;t let our guards down.&quot;@</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_koc12ba</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>tcg_koc12m</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>shio0031</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>Shion$&quot;That&apos;s true. But, I say we rest up for now.&quot;@</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>tcg_cun11ba</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>sei_0142</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>Sei$&quot;You&apos;re right. All units, back to the outpost! I&apos;m sure you&apos;re all tired! Get some rest!&quot;@</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>Soldier$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>bcg_gi_impseat_a</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>tcg_jun21ha</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>bgm09</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>jyun0118</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>Jun&apos;iku$&quot;Karin-sama. It seems Shunran was defeated.&quot;@</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>tcg_sou12ha</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>sous0198</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>Sousou$&quot;......Is Shunran safe?&quot;@</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>jyun0119</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>Jun&apos;iku$&quot;Yes......&quot;@</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>tcg_sou12aa</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>sous0199</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>Sousou$&quot;I see...... that&apos;s good.&quot;@</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>jyun0120</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>Jun&apos;iku$&quot;...But we can&apos;t leave it at that. Hongou&apos;s army has defeated our advance party, and their morale is high. Once their main army arrives, they will attack our territory.&quot;@</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>jyun0121</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>Jun&apos;iku$&quot;I believe our best plan now would be to command Shunran to defend the base.&quot;@</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>tcg_sou12ha</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>sous0200</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>Sousou$&quot;So what you&apos;re suggesting is that... we seclude ourselves in a fortress and repel Hongou&apos;s army&apos;s attack?&quot;@</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>jyun0122</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>Jun&apos;iku$&quot;Yes.&quot;@</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>tcg_sou12ca</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>sous0201</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>Sousou$&quot;......Alright. Send a messenger to Shunran and Kii...... Send Shuuran and her men to reinforce them.&quot;@</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>tcg_jun21ea</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>jyun0123</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>Jun&apos;iku$&quot;Yes ma&apos;am...... but that won&apos;t leave anyone to protect this palace......&quot;@</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>tcg_sou12aa</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>sous0202</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>Sousou$&quot;If they are able to attack this palace in the first place, we&apos;ve lost. Finish them off in the next battle. If that doesn&apos;t happen, I&apos;ll fight myself.&quot;@</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>tcg_jun21ha</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>jyun0124</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>Jun&apos;iku$&quot;......Understood. We&apos;ll do our best to defeat Hongou&apos;s army.&quot;@</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>tcg_sou12la</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>sous0203</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>Sousou$&quot;I&apos;m counting on you, Keifa. ......&quot;@</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>bcg_go_impseat_a</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>tcg_rik11aa</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>bgm10</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>onpu</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>riku0019</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>Rikuson$&quot;Renfa-sama. Now we know the result of the first battle between Hongou and Sousou!&quot;@</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>tcg_son12aa</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>sonk0072</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>Sonken$&quot;I see...... And who won?&quot;@</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>riku0020</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>Rikuson$&quot;Hongou, it seems!&quot;@</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>sonk0073</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>Sonken$&quot;Hongou won, huh? Hmm......&quot;@</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>tcg_syu11ba</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>syuy0045</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>Shuuyu$&quot;Their first battle went to Hongou...... but the next?&quot;@</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>sonk0074</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>Sonken$&quot;Yes. We&apos;ll decide how our county is going to act based on what happens in the next battle. What are they doing now?&quot;@</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>tcg_rik11aa</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>riku0021</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>Rikuson$&quot;Their main force has united with their task force and now they&apos;re taking a big break before commencing their invasion.&quot;@</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>sonk0075</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>Sonken$&quot;All right. ...Koukin, gather your soldiers so we&apos;ll be ready to act at a moment&apos;s notice.&quot;@</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>syuy0046</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>Shuuyu$&quot;As you wish.&quot;@</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>sonk0076</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>Sonken$&quot;Hakugen, get us information. Up to date information is going to be very important to us from now on. Make no mistake.&quot;@</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>tcg_rik11ba</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>tcg_rik11m</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>riku0022</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>Rikuson$&quot;Okay!&quot;@</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>sonk0077</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>Sonken$&quot;Kouha, gather my personal guards. Have them ready to move at my command.&quot;@</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>tcg_kne11aa</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>tcg_kne11m</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>kann0017</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>Kannei$&quot;As you wish!&quot;@</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>tcg_son12ia</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>sonk0078</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>Sonken$&quot;I&apos;m counting on you. Well, let&apos;s observe their battle for a while longer.&quot;@</ToTranslate>
  </String>
