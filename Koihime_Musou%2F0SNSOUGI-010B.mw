[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_go_outcast_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_syu21da</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm23</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>syuy0180</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Shuuyu$&quot;.........&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_zgo11aa</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_zgo11m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>Soldier$&quot;Sh-Shuuyu-sama! The front has collapsed and we can&apos;t organize our defense lines! Please, give us a command!&quot;@</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>syuy0181</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>Shuuyu$&quot;How many soldiers do we have left?&quot;@</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>Soldier$&quot;The only determined ones are the men of the Shuu clan. I estimate about two thousand of them......&quot;@</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>tcg_syu11ca</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>syuy0182</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>Shuuyu$&quot;I see. ......Our course of action must be chosen now. I suppose Heaven doesn&apos;t approve of my dream of supremacy.&quot;@</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>Soldier$&quot;Th-that&apos;s not so! As long as you survive, you can always strike back! Therefore, please retreat!&quot;@</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>syuy0183</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>Shuuyu$&quot;......Thank you for saying that. But I can&apos;t drag the people who served the Shuu family for so long down with me to Hell. ...This is the end.&quot;@</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Soldier$&quot;B-but!&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_syu21da</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>syuy0184</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Shuuyu$&quot;Don&apos;t say it. I&apos;m going back to the palace now. When you see a flame from the palace, surrender.&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>Soldier$&quot;......Will you be taking your own life?&quot;@</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_syu21ga</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>syuy0185</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>Shuuyu$&quot;I&apos;m going to be with Sonsaku...with Sheren. I hate to ask, but could you please hold them back a while longer?&quot;@</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>Soldier$&quot;......We understand. Please rest assured.&quot;@</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>Soldier$&quot;We&apos;ll make sure your final moment is honorable! That&apos;s the least we can do for you!&quot;@</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>tcg_syu21ia</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>syuy0186</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>Shuuyu$&quot;I&apos;ll remember your words as I walk the road to the afterworld. Thank you...everyone, live happily.&quot;@</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>Soldier$&quot;Koukin-sama.....Ggh...&quot;@</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>tcg_syu21ga</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>syuy0187</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>Shuuyu$&quot;......This is farewell. I leave the rest in your hands.&quot;@</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>Soldier$&quot;Ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>bcg_go_outcast_a</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>aisy0985</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>Aisha$&quot;Their organized resistance has ceased! All that remains is to seize the General! Everyone, find Shuuyu!&quot;@</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>tcg_cun21ga</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>sei_0325</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>Sei$&quot;Whoever captures General Shuuyu will receive a princely reward! Everyone, strive for it!&quot;@</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>Soldier$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>$With Aisha and Sei&apos;s words, the soldiers who were mopping up the remaining enemies desperately started to search for Shuuyu.@</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>tcg_son12ha</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>sonk0344</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>Sonken$&quot;This is it, Meirin......&quot;@</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>Kazuto$&quot;If we capture Shuuyu...... what are you going to do, Sonken?&quot;@</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>sonk0345</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>Sonken$&quot;I don&apos;t know...... I don&apos;t know whether I should condemn her, or forgive and accept......&quot;@</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>sonk0346</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>Sonken$&quot;Either way, I may need to have a talk with her one more time. ......So we can really understand each other.&quot;@</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>Kazuto$&quot;I hope you can do that.&quot;@</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>sonk0347</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>Sonken$&quot;Yeah......&quot;@</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>Soldier$&quot;Reporting!&quot;@</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>aisy0986</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>Aisha$&quot;What is it?!&quot;@</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>Soldier$&quot;Someone who appears to be Shuuyu has entered the palace! However, a group bearing the flag of Shuu is putting up firm resistance, and we can not confirm!&quot;@</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>tcg_son22ea</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_son22m</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>sonk0348</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>Sonken$&quot;I bet they&apos;re from the Shuu clan. ......Following their lord into death.&quot;@</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>Kazuto$&quot;Do you think you can persuade them?&quot;@</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>tcg_son12ba</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_son12m</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>sonk0349</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>Sonken$&quot;I don&apos;t think so. Everyone in the Shuu clan worships Meirin. ......I&apos;m sure they&apos;re resisting so we can&apos;t interfere with her final moment.&quot;@</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>aisy0987</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>Aisha$&quot;Then we just have to break through the palace gate and capture Shuuyu by force.&quot;@</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>sei_0326</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>Sei$&quot;Yeah, she caused this major war, so she has to take responsibility for it.&quot;@</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_bac12ea</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>sui_0242</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>Sui$&quot;Okay. I feel bad for those loyal soldiers, but we&apos;ll blow them away.&quot;@</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>tcg_cho11aa</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>rinr0528</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>Rinrin$&quot;And we&apos;ll charge into the palace and capture Shuuyu!&quot;@</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>syur0501</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>Shuri$&quot;Let&apos;s go, everyone!&quot;@</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>aisy0988</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>Aisha$&quot;Yeah!&quot;@</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>bb</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>bcg_castcorrid_a</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>tcg_syu21ia</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>SE029</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>bgm25</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>syuy0188</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>Shuuyu$&quot;Hmph...... there&apos;s nobody else here, huh......?&quot;@</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>syuy0189</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>Shuuyu$&quot;Bundai-sama founded it, and Hakufu expanded the power of Go, but this is its end.&quot;@</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>syuy0190</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>Shuuyu$&quot;However, I&apos;m sure Chuubou-sama will protect a different type of Go.&quot;@</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>tcg_syu11aa</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>tcg_syu11m</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>syuy0191</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>Shuuyu$&quot;Hmph...... I guess that&apos;s a selfish wish.&quot;@</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>cc</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>cc</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>bcg_castgurden_a</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>tcg_syu21ga</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>SE028</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>syuy0192</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>Shuuyu$&quot;Ahh...... now I finally remember. Sheren...... this is the garden where we met and spent time together.&quot;@</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>syuy0193</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>Shuuyu$&quot;We were fearless back then......&quot;@</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>dd</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>dd</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>ecg_syu03a</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>bgm24</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>sona0000</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>Sonsaku$&quot;Meirin!&quot;@</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>syuy0194</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>Shuuyu$&quot;Ah...... Sheren. What do you want?&quot;@</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>sona0001</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>Sonsaku$&quot;Well. That&apos;s not a nice response. Are you still angry that Daikyou-chan is mine?&quot;@</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>syuy0195</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>Shuuyu$&quot;N-no, I&apos;m not angry......&quot;@</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>sona0002</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>Sonsaku$&quot;That&apos;s okay. You have Shoukyou-chan. You must be so busy with two people to love.&quot;@</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>syuy0196</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>Shuuyu$&quot;Two people? I do love Shoukyou, but......&quot;@</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>sona0003</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>Sonsaku$&quot;And the other one is me, right?&quot;@</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>syuy0197</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>Shuuyu$&quot;......You&apos;re very confident about that, aren&apos;t you? If I say no, what face will you make then?&quot;@</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>sona0004</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>Sonsaku$&quot;My my, of course you love me, right? Even if you say no, I know you&apos;re lying, so that wouldn&apos;t upset me at all.&quot;@</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>syuy0198</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>Shuuyu$&quot;Hahh...... I don&apos;t know where you get such confidence.&quot;@</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>sona0005</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>Sonsaku$&quot;Hee, hee, I know everything about you, Meirin. Of course you know how much I love you, right?&quot;@</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>syuy0199</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>Shuuyu$&quot;Well...... yes...... I do, but.&quot;@</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>sona0006</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>Sonsaku$&quot;Good! Hee-hee, four people who swore their love for each other, inheriting Mother&apos;s will and setting out to change the world sounds just like a fairytale.&quot;@</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>syuy0200</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>Shuuyu$&quot;The world, huh?&quot;@</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>sona0007</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>Sonsaku$&quot;Yep, the world. My dream is to conquer the world with you, Meirin, and in that peaceful world, you, Daikyou-chan, Shoukyou-chan, and I will live happily ever after, together forever.&quot;@</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>syuy0201</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>Shuuyu$&quot;Hey, conquering the world isn&apos;t that easy, you know?&quot;@</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>sona0008</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>Sonsaku$&quot;What, it&apos;s not?&quot;@</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>syuy0202</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>Shuuyu$&quot;Of course not. My gosh, you still talk like a child.&quot;@</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>sona0009</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>Sonsaku$&quot;That&apos;s not true. Because with your resourcefulness, the world is easy to grasp, isn&apos;t it?&quot;@</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>syuy0203</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>Shuuyu$&quot;I&apos;m telling you, it&apos;s not that easy.&quot;@</ToTranslate>
  </String>
  <String index="200">
    <ToTranslate>sona0010</ToTranslate>
  </String>
  <String index="201">
    <ToTranslate>Sonsaku$&quot;Oh yes it is. You&apos;re an amazing girl, after all. So, you&apos;re going to help me, right?&quot;@</ToTranslate>
  </String>
  <String index="202">
    <ToTranslate>syuy0204</ToTranslate>
  </String>
  <String index="203">
    <ToTranslate>Shuuyu$&quot;Hahh...... what am I going to do with you? With you alone, it&apos;s absolutely impossible to even talk about the world. Okay, I&apos;ll help you.&quot;@</ToTranslate>
  </String>
  <String index="204">
    <ToTranslate>sona0011</ToTranslate>
  </String>
  <String index="205">
    <ToTranslate>Sonsaku$&quot;Good! Hee-hee-hee, let&apos;s do our best together!&quot;@</ToTranslate>
  </String>
  <String index="206">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="207">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="208">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="209">
    <ToTranslate>bcg_castgurden_a</ToTranslate>
  </String>
  <String index="210">
    <ToTranslate>bgm19</ToTranslate>
  </String>
  <String index="211">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="212">
    <ToTranslate>tcg_syu21ia</ToTranslate>
  </String>
  <String index="213">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="214">
    <ToTranslate>syuy0205</ToTranslate>
  </String>
  <String index="215">
    <ToTranslate>Shuuyu$&quot;......Heh, do our best together, huh?&quot;@</ToTranslate>
  </String>
  <String index="216">
    <ToTranslate>tcg_syu21ga</ToTranslate>
  </String>
  <String index="217">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="218">
    <ToTranslate>syuy0206</ToTranslate>
  </String>
  <String index="219">
    <ToTranslate>Shuuyu$&quot;But you left me behind and passed away...... Entrusting your dream with me.&quot;@</ToTranslate>
  </String>
  <String index="220">
    <ToTranslate>syuy0207</ToTranslate>
  </String>
  <String index="221">
    <ToTranslate>Shuuyu$&quot;You&apos;re a cruel woman......&quot;@</ToTranslate>
  </String>
  <String index="222">
    <ToTranslate>tcg_syu21ja</ToTranslate>
  </String>
  <String index="223">
    <ToTranslate>tcg_syu21m</ToTranslate>
  </String>
  <String index="224">
    <ToTranslate>syuy0208</ToTranslate>
  </String>
  <String index="225">
    <ToTranslate>Shuuyu$&quot;But...... I&apos;m sorry. I don&apos;t think...... I can make your dream come true.&quot;@</ToTranslate>
  </String>
  <String index="226">
    <ToTranslate>syuy0209</ToTranslate>
  </String>
  <String index="227">
    <ToTranslate>Shuuyu$&quot;I&apos;m sorry, Sheren......&quot;@</ToTranslate>
  </String>
  <String index="228">
    <ToTranslate>SE028</ToTranslate>
  </String>
  <String index="229">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="230">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="231">
    <ToTranslate>SE046a</ToTranslate>
  </String>
  <String index="232">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="233">
    <ToTranslate>ecg_syu01a</ToTranslate>
  </String>
  <String index="234">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="235">
    <ToTranslate>syuy0210</ToTranslate>
  </String>
  <String index="236">
    <ToTranslate>Shuuyu$&quot;Now everything is all set...... I can finally make my way to you......&quot;@</ToTranslate>
  </String>
  <String index="237">
    <ToTranslate>syuy0211</ToTranslate>
  </String>
  <String index="238">
    <ToTranslate>Shuuyu$&quot;It was such a short time that I spent with you, and it was such a long time that I had to spend alone...... but now it&apos;s finally over......&quot;@</ToTranslate>
  </String>
  <String index="239">
    <ToTranslate>syuy0212</ToTranslate>
  </String>
  <String index="240">
    <ToTranslate>Shuuyu$&quot;These flames will burn me, as well as the throne you once held...... Then, maybe, I can be with you through death......&quot;@</ToTranslate>
  </String>
  <String index="241">
    <ToTranslate>syuy0213</ToTranslate>
  </String>
  <String index="242">
    <ToTranslate>Shuuyu$&quot;Heh, heh...... I know that&apos;s not happening. But...... I want to be as near to you as I can.&quot;@</ToTranslate>
  </String>
  <String index="243">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="244">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="245">
    <ToTranslate>ecg_syu01b</ToTranslate>
  </String>
  <String index="246">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="247">
    <ToTranslate>syuy0214</ToTranslate>
  </String>
  <String index="248">
    <ToTranslate>Shuuyu$&quot;Hey, Sheren...... are you laughing at me......?&quot;@</ToTranslate>
  </String>
  <String index="249">
    <ToTranslate>syuy0215</ToTranslate>
  </String>
  <String index="250">
    <ToTranslate>Shuuyu$&quot;Or are you...... angry with me?&quot;@</ToTranslate>
  </String>
  <String index="251">
    <ToTranslate>syuy0216</ToTranslate>
  </String>
  <String index="252">
    <ToTranslate>Shuuyu$&quot;But...... I have no regrets. Even if it was for a short period of time, I followed in your footsteps towards your dream.&quot;@</ToTranslate>
  </String>
  <String index="253">
    <ToTranslate>syuy0217</ToTranslate>
  </String>
  <String index="254">
    <ToTranslate>Shuuyu$&quot;That makes me...... very happy. The wish you had for the world, and the feelings you had towards the world...... I think I felt them......&quot;@</ToTranslate>
  </String>
  <String index="255">
    <ToTranslate>syuy0218</ToTranslate>
  </String>
  <String index="256">
    <ToTranslate>Shuuyu$&quot;But this is the end......&quot;@</ToTranslate>
  </String>
  <String index="257">
    <ToTranslate>syuy0219</ToTranslate>
  </String>
  <String index="258">
    <ToTranslate>Shuuyu$&quot;Unless a dream comes true, it&apos;s just a dream. I now understand that very well......&quot;@</ToTranslate>
  </String>
  <String index="259">
    <ToTranslate>syuy0220</ToTranslate>
  </String>
  <String index="260">
    <ToTranslate>Shuuyu$&quot;I&apos;ll be coming to you soon. ......Now, I think I can finally be honest.&quot;@</ToTranslate>
  </String>
  <String index="261">
    <ToTranslate>syuy0221</ToTranslate>
  </String>
  <String index="262">
    <ToTranslate>Shuuyu$&quot;......I want to hear your voice. I want to see you...... I want to feel you.....&quot;@</ToTranslate>
  </String>
  <String index="263">
    <ToTranslate>syuy0222</ToTranslate>
  </String>
  <String index="264">
    <ToTranslate>Shuuyu$&quot;If I pass away, can I be near you......? Would you accept me like this?&quot;@</ToTranslate>
  </String>
  <String index="265">
    <ToTranslate>syuy0223</ToTranslate>
  </String>
  <String index="266">
    <ToTranslate>Shuuyu$&quot;Hey...... Sheren......&quot;@</ToTranslate>
  </String>
  <String index="267">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="268">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="269">
    <ToTranslate>ecg_syu01c</ToTranslate>
  </String>
  <String index="270">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="271">
    <ToTranslate>syuy0224</ToTranslate>
  </String>
  <String index="272">
    <ToTranslate>Shuuyu$&quot;Ahh...... I see you. Sheren...... I can see you......&quot;@</ToTranslate>
  </String>
  <String index="273">
    <ToTranslate>syuy0225</ToTranslate>
  </String>
  <String index="274">
    <ToTranslate>Shuuyu$&quot;Are you laughing at me......? Or are you angry with me......?&quot;@</ToTranslate>
  </String>
  <String index="275">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="276">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="277">
    <ToTranslate>ecg_syu01d</ToTranslate>
  </String>
  <String index="278">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="279">
    <ToTranslate>syuy0226</ToTranslate>
  </String>
  <String index="280">
    <ToTranslate>Shuuyu$&quot;No, I don&apos;t care which. I was able to see you in this final moment...... I&apos;m so happy.&quot;@</ToTranslate>
  </String>
  <String index="281">
    <ToTranslate>syuy0227</ToTranslate>
  </String>
  <String index="282">
    <ToTranslate>Shuuyu$&quot;Now I can come to you in peace......&quot;@</ToTranslate>
  </String>
  <String index="283">
    <ToTranslate>syuy0228</ToTranslate>
  </String>
  <String index="284">
    <ToTranslate>Shuuyu$&quot;I can finally see you again...... I...... want to spend time with you again......&quot;@</ToTranslate>
  </String>
  <String index="285">
    <ToTranslate>syuy0229</ToTranslate>
  </String>
  <String index="286">
    <ToTranslate>Shuuyu$&quot;This time, we&apos;ll be together forever...... Don&apos;t ever leave me again......&quot;@</ToTranslate>
  </String>
  <String index="287">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="288">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="289">
    <ToTranslate>ecg_syu01e</ToTranslate>
  </String>
  <String index="290">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="291">
    <ToTranslate>syuy0230</ToTranslate>
  </String>
  <String index="292">
    <ToTranslate>Shuuyu$&quot;I love you, Sheren......&quot;@</ToTranslate>
  </String>
  <String index="293">
    <ToTranslate>syuy0231</ToTranslate>
  </String>
  <String index="294">
    <ToTranslate>Shuuyu$&quot;So take me......&quot;@</ToTranslate>
  </String>
  <String index="295">
    <ToTranslate>SE063</ToTranslate>
  </String>
