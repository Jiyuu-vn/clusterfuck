[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_ukt11ca</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_ukt11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>ukit0112</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Ukitsu$&quot;Hmph. ......Although you were created, you certainly are heroes. ......However, you&apos;re still individuals. You can&apos;t win against violence en masse.&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_ukt11fa</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_ukt11m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>ukit0113</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Ukitsu$&quot;Increase.&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>SE094</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>tcg_bac22da</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>tcg_bac22m</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>sui_0256</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>Sui$&quot;Hey, wait! They&apos;re increasing in number!&quot;@</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>rinr0544</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>Rinrin$&quot;Ugahh! This is crazy!&quot;@</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>tcg_kan11ua</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>aisy1029</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>Aisha$&quot;Ngh...... we can&apos;t deal with this many......&quot;@</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_cun21ha</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>sei_0342</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>Sei$&quot;I know. The soldiers are gradually being cut down. We need to do something about it immediately.&quot;@</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>syur0524</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>Shuri$&quot;B-but we don&apos;t have any more allies......&quot;@</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Soldier$&quot;Whahh!&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_zsh11aa</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_zsh11m</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>tcg_koc22da</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>tcg_koc22m</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>shio0122</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>Shion$&quot;What is it?&quot;@</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>Soldier$&quot;The enemy has broken through a part of our front lines!&quot;@</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>tcg_bac12ca</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>sui_0257</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>Sui$&quot;Damn! We&apos;re just too outnumbered! At this rate, we&apos;ll be annihilated!&quot;@</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>tcg_cho11ca</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>rinr0545</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>Rinrin$&quot;Shall we withdraw for a time?&quot;@</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>tcg_cun21ha</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>sei_0343</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>Sei$&quot;Maybe we need to consider that.&quot;@</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>Kazuto$&quot;But there isn&apos;t much time left before they execute the ceremony!&quot;@</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>aisy1030</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>Aisha$&quot;But if we continue like this, you&apos;ll be in danger! My Lord, we should withdraw!&quot;@</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>sous0410</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>???$&quot;There is no need for that!&quot;@</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>aisy1031</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>Aisha$&quot;What?&quot;@</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>tcg_sou41aa</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>tcg_sou41m</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>sous0411</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>Sousou$&quot;You look like you&apos;re having a rough fight, Kazuto.&quot;@</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>Kazuto$&quot;Sousou?! Why are you here?!&quot;@</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>sous0412</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>Sousou$&quot;Hmph......isn&apos;t it obvious? I&apos;m here to send the ones who made a fool of me straight to hell.&quot;@</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>sous0413</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>Sousou$&quot;By the blade of my scythe and me, Sou Moutoku!&quot;@</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>battle_sou</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>SE034</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>tcg_zak11aa</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>tcg_zak11m</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>Soldier$&quot;Gyah!&quot;@</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>SE038</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>yure_t</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>Kazuto$&quot;Are you here to help me......?&quot;@</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>tcg_sou22ma</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>sous0414</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>Sousou$&quot;Hmph. I&apos;m not planning to help you at all. I&apos;m simply here to exact my revenge, that&apos;s all. ......Don&apos;t get the wrong idea.&quot;@</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>Kazuto$&quot;Sousou......&quot;@</ToTranslate>
  </String>
  <String index="98">
    <ToTranslate>tcg_sou22da</ToTranslate>
  </String>
  <String index="99">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="100">
    <ToTranslate>sous0415</ToTranslate>
  </String>
  <String index="101">
    <ToTranslate>Sousou$&quot;But since we happen to be here at the same time, then perhaps it would be wise to form a united front. ......Well? Do you want to borrow my help?&quot;@</ToTranslate>
  </String>
  <String index="102">
    <ToTranslate>Kazuto$&quot;Yeah! I want you to help us.&quot;@</ToTranslate>
  </String>
  <String index="103">
    <ToTranslate>tcg_sou22ea</ToTranslate>
  </String>
  <String index="104">
    <ToTranslate>tcg_sou22m</ToTranslate>
  </String>
  <String index="105">
    <ToTranslate>sous0416</ToTranslate>
  </String>
  <String index="106">
    <ToTranslate>Sousou$&quot;Heh heh. Very well. Then I shall lend you the power of Sou Moutoku. ......Shunran!&quot;@</ToTranslate>
  </String>
  <String index="107">
    <ToTranslate>tcg_ton14aa</ToTranslate>
  </String>
  <String index="108">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="109">
    <ToTranslate>kato0376</ToTranslate>
  </String>
  <String index="110">
    <ToTranslate>Kakouton$&quot;Yes ma&apos;am!&quot;@</ToTranslate>
  </String>
  <String index="111">
    <ToTranslate>sous0417</ToTranslate>
  </String>
  <String index="112">
    <ToTranslate>Sousou$&quot;This is our chance to regain the pride of Gi and slay those who mocked us!&quot;@</ToTranslate>
  </String>
  <String index="113">
    <ToTranslate>tcg_ton14ga</ToTranslate>
  </String>
  <String index="114">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="115">
    <ToTranslate>kato0377</ToTranslate>
  </String>
  <String index="116">
    <ToTranslate>Kakouton$&quot;As you wish! Kakou Genjou, to the vanguard! Hear my call! ......Everyone! Follow me!&quot;@</ToTranslate>
  </String>
  <String index="117">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="118">
    <ToTranslate>SE124</ToTranslate>
  </String>
  <String index="119">
    <ToTranslate>sous0418</ToTranslate>
  </String>
  <String index="120">
    <ToTranslate>Sousou$&quot;Shuuran!&quot;@</ToTranslate>
  </String>
  <String index="121">
    <ToTranslate>tcg_en12aa</ToTranslate>
  </String>
  <String index="122">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="123">
    <ToTranslate>kaen0214</ToTranslate>
  </String>
  <String index="124">
    <ToTranslate>Kakouen$&quot;Yes!&quot;@</ToTranslate>
  </String>
  <String index="125">
    <ToTranslate>sous0419</ToTranslate>
  </String>
  <String index="126">
    <ToTranslate>Sousou$&quot;What is your mission?&quot;@</ToTranslate>
  </String>
  <String index="127">
    <ToTranslate>kaen0215</ToTranslate>
  </String>
  <String index="128">
    <ToTranslate>Kakouen$&quot;My mission is to present victory to you, Sou Moutoku.&quot;@</ToTranslate>
  </String>
  <String index="129">
    <ToTranslate>sous0420</ToTranslate>
  </String>
  <String index="130">
    <ToTranslate>Sousou$&quot;Then grasp victory with your own hands and present it to me.&quot;@</ToTranslate>
  </String>
  <String index="131">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="132">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="133">
    <ToTranslate>kaen0216</ToTranslate>
  </String>
  <String index="134">
    <ToTranslate>Kakouen$&quot;As you wish. ......Kakouen Squad, gather under my flag! We will cover Kakouton Squad during their attack!&quot;@</ToTranslate>
  </String>
  <String index="135">
    <ToTranslate>kaen0217</ToTranslate>
  </String>
  <String index="136">
    <ToTranslate>Kakouen$&quot;Prepare! Draw!&quot;@</ToTranslate>
  </String>
  <String index="137">
    <ToTranslate>SE014</ToTranslate>
  </String>
  <String index="138">
    <ToTranslate>kaen0218</ToTranslate>
  </String>
  <String index="139">
    <ToTranslate>Kakouen$&quot;Fire!&quot;@</ToTranslate>
  </String>
  <String index="140">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="141">
    <ToTranslate>ecg_com01b</ToTranslate>
  </String>
  <String index="142">
    <ToTranslate>SE019</ToTranslate>
  </String>
  <String index="143">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="144">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="145">
    <ToTranslate>tcg_sou12aa</ToTranslate>
  </String>
  <String index="146">
    <ToTranslate>tcg_sou12m</ToTranslate>
  </String>
  <String index="147">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="148">
    <ToTranslate>sous0421</ToTranslate>
  </String>
  <String index="149">
    <ToTranslate>Sousou$&quot;Kii! Keifa!&quot;@</ToTranslate>
  </String>
  <String index="150">
    <ToTranslate>tcg_kyo12aa</ToTranslate>
  </String>
  <String index="151">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="152">
    <ToTranslate>kyoc0022</ToTranslate>
  </String>
  <String index="153">
    <ToTranslate>Kyocho$&quot;Yes!&quot;@</ToTranslate>
  </String>
  <String index="154">
    <ToTranslate>tcg_jun21ha</ToTranslate>
  </String>
  <String index="155">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="156">
    <ToTranslate>jyun0177</ToTranslate>
  </String>
  <String index="157">
    <ToTranslate>Jun&apos;iku$&quot;At your side......&quot;@</ToTranslate>
  </String>
  <String index="158">
    <ToTranslate>sous0422</ToTranslate>
  </String>
  <String index="159">
    <ToTranslate>Sousou$&quot;You two will command the main force. ......Make sure you kill every last one of those White Clothes churls.&quot;@</ToTranslate>
  </String>
  <String index="160">
    <ToTranslate>tcg_kyo22ba</ToTranslate>
  </String>
  <String index="161">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="162">
    <ToTranslate>kyoc0023</ToTranslate>
  </String>
  <String index="163">
    <ToTranslate>Kyocho$&quot;Understood!&quot;@</ToTranslate>
  </String>
  <String index="164">
    <ToTranslate>tcg_jun11aa</ToTranslate>
  </String>
  <String index="165">
    <ToTranslate>tcg_jun11m</ToTranslate>
  </String>
  <String index="166">
    <ToTranslate>jyun0178</ToTranslate>
  </String>
  <String index="167">
    <ToTranslate>Jun&apos;iku$&quot;As you wish. ......Preparations are already complete. Karin-sama, we await your command.&quot;@</ToTranslate>
  </String>
  <String index="168">
    <ToTranslate>tcg_sou51aa</ToTranslate>
  </String>
  <String index="169">
    <ToTranslate>tcg_sou51m</ToTranslate>
  </String>
  <String index="170">
    <ToTranslate>sous0423</ToTranslate>
  </String>
  <String index="171">
    <ToTranslate>Sousou$&quot;Listen up! Brave warriors of Gi! I am Sou Moutoku! The symbol of Gi&apos;s prowess! I have no intention of forgiving those who dared defile our pride!&quot;@</ToTranslate>
  </String>
  <String index="172">
    <ToTranslate>sous0424</ToTranslate>
  </String>
  <String index="173">
    <ToTranslate>Sousou$&quot;Everyone, consume the enemy with your ferocity and valiantly offer your lives unto me! For every Gi Soldier who falls, the pride of Gi will be protected!&quot;@</ToTranslate>
  </String>
  <String index="174">
    <ToTranslate>sous0425</ToTranslate>
  </String>
  <String index="175">
    <ToTranslate>Sousou$&quot;Fight! And die! Consume the fools who dared to oppose Gi&apos;s warriors, and lead them to the depths of hell!&quot;@</ToTranslate>
  </String>
  <String index="176">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="177">
    <ToTranslate>sous0426</ToTranslate>
  </String>
  <String index="178">
    <ToTranslate>Sousou$&quot;I hereby command all Gi warriors! Destroy every last enemy with your blazing wrath! All units, ready your arms!&quot;@</ToTranslate>
  </String>
  <String index="179">
    <ToTranslate>SE013</ToTranslate>
  </String>
  <String index="180">
    <ToTranslate>sous0427</ToTranslate>
  </String>
  <String index="181">
    <ToTranslate>Sousou$&quot;Raise your battle cry, and charge!&quot;@</ToTranslate>
  </String>
  <String index="182">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="183">
    <ToTranslate>SE124</ToTranslate>
  </String>
  <String index="184">
    <ToTranslate>tcg_cun11da</ToTranslate>
  </String>
  <String index="185">
    <ToTranslate>tcg_cun11m</ToTranslate>
  </String>
  <String index="186">
    <ToTranslate>sei_0344</ToTranslate>
  </String>
  <String index="187">
    <ToTranslate>Sei$&quot;......Well, what a violent way to encourage them.&quot;@</ToTranslate>
  </String>
  <String index="188">
    <ToTranslate>Kazuto$&quot;It&apos;s fitting of Sousou.&quot;@</ToTranslate>
  </String>
  <String index="189">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="190">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="191">
    <ToTranslate>aisy1032</ToTranslate>
  </String>
  <String index="192">
    <ToTranslate>Aisha$&quot;Indeed. ......But now we can not lose either. My Lord, light the signal fire for our counter attack!&quot;@</ToTranslate>
  </String>
  <String index="193">
    <ToTranslate>Kazuto$&quot;Yeah! Everyone, I know it&apos;s tough, but hang in there!&quot;@</ToTranslate>
  </String>
  <String index="194">
    <ToTranslate>tcg_syo21da</ToTranslate>
  </String>
  <String index="195">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="196">
    <ToTranslate>syur0525</ToTranslate>
  </String>
  <String index="197">
    <ToTranslate>Shuri$&quot;Yes! Everyone, commence the counterattack!&quot;@</ToTranslate>
  </String>
  <String index="198">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="199">
    <ToTranslate>bg_black</ToTranslate>
  </String>
</DBS>

