[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_gi_outcast_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm13</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>kato0295</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Kakouton$&quot;Kkh......! Am I...... Am I going to lose again?&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_en12aa</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>kaen0139</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Kakouen$&quot;Aneja, we are withdrawing.&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>tcg_ton14ja</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>kato0296</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>Kakouton$&quot;I&apos;m not!&quot;@</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_kyo12ca</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_kyo12m</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>kii_0058</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>Kyocho$&quot;What are you saying? At this rate, the enemy will capture you! That&apos;s not like you, Shunran-sama!&quot;@</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>kato0297</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Kakouton$&quot;But I&apos;ve already lost against them several times! I&apos;d rather rush into the enemy camp and die as a warrior than continue to live in shame!&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_en12ja</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>kaen0140</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Kakouen$&quot;I won&apos;t let you do that. Karin-sama is right behind us. We haven&apos;t necessarily lost yet.&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>tcg_jun21ha</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>jyun0148</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>Jun&apos;iku$&quot;Shuuran is right, Shunran.&quot;@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>tcg_ton14da</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>kato0298</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>Kakouton$&quot;Keifa......&quot;@</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>jyun0149</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>Jun&apos;iku$&quot;Our current mission is to merge with Karin-sama&apos;s unit.&quot;@</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_kyo22ha</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>tcg_kyo22m</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>kii_0059</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>Kyocho$&quot;Keifa-chan&apos;s right. The main unit needs more commanders. Besides, Sougi&apos;s army needs General Kakouton in order to look good.&quot;@</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>tcg_en12aa</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="43">
    <ToTranslate>kaen0141</ToTranslate>
  </String>
  <String index="44">
    <ToTranslate>Kakouen$&quot;That&apos;s right. I&apos;m telling you one more time. We are withdrawing, Aneja.&quot;@</ToTranslate>
  </String>
  <String index="45">
    <ToTranslate>tcg_ton14ea</ToTranslate>
  </String>
  <String index="46">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="47">
    <ToTranslate>kato0299</ToTranslate>
  </String>
  <String index="48">
    <ToTranslate>Kakouton$&quot;......Fine.&quot;@</ToTranslate>
  </String>
  <String index="49">
    <ToTranslate>tcg_en22ga</ToTranslate>
  </String>
  <String index="50">
    <ToTranslate>tcg_en22m</ToTranslate>
  </String>
  <String index="51">
    <ToTranslate>kaen0142</ToTranslate>
  </String>
  <String index="52">
    <ToTranslate>Kakouen$&quot;Are you going to cry again?&quot;@</ToTranslate>
  </String>
  <String index="53">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="54">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="55">
    <ToTranslate>kato0300</ToTranslate>
  </String>
  <String index="56">
    <ToTranslate>Kakouton$&quot;No, I&apos;m not!&quot;@</ToTranslate>
  </String>
  <String index="57">
    <ToTranslate>tcg_en12ba</ToTranslate>
  </String>
  <String index="58">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="59">
    <ToTranslate>kaen0143</ToTranslate>
  </String>
  <String index="60">
    <ToTranslate>Kakouen$&quot;Hmph, that&apos;s my sister. Let&apos;s withdraw and join Karin-sama.&quot;@</ToTranslate>
  </String>
  <String index="61">
    <ToTranslate>tcg_jun11da</ToTranslate>
  </String>
  <String index="62">
    <ToTranslate>tcg_jun11m</ToTranslate>
  </String>
  <String index="63">
    <ToTranslate>jyun0150</ToTranslate>
  </String>
  <String index="64">
    <ToTranslate>Jun&apos;iku$&quot;All units, retreat! We must abandon the fortress!&quot;@</ToTranslate>
  </String>
  <String index="65">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="66">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="67">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="68">
    <ToTranslate>aa</ToTranslate>
  </String>
  <String index="69">
    <ToTranslate>bcg_gi_outcast_a</ToTranslate>
  </String>
  <String index="70">
    <ToTranslate>tcg_bac12aa</ToTranslate>
  </String>
  <String index="71">
    <ToTranslate>tcg_bac12m</ToTranslate>
  </String>
  <String index="72">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="73">
    <ToTranslate>bgm08</ToTranslate>
  </String>
  <String index="74">
    <ToTranslate>sui_0147</ToTranslate>
  </String>
  <String index="75">
    <ToTranslate>Sui$&quot;The enemy army is retreating!&quot;@</ToTranslate>
  </String>
  <String index="76">
    <ToTranslate>tcg_cho31ba</ToTranslate>
  </String>
  <String index="77">
    <ToTranslate>tcg_cho31m</ToTranslate>
  </String>
  <String index="78">
    <ToTranslate>rinr0472</ToTranslate>
  </String>
  <String index="79">
    <ToTranslate>Rinrin$&quot;We won!&quot;@</ToTranslate>
  </String>
  <String index="80">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="81">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="82">
    <ToTranslate>aisy0853</ToTranslate>
  </String>
  <String index="83">
    <ToTranslate>Aisha$&quot;Good! Everyone, let&apos;s raise a cheer to victory!&quot;@</ToTranslate>
  </String>
  <String index="84">
    <ToTranslate>tcg_cun21ca</ToTranslate>
  </String>
  <String index="85">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="86">
    <ToTranslate>sei_0209</ToTranslate>
  </String>
  <String index="87">
    <ToTranslate>Sei$&quot;Our army has won!&quot;@</ToTranslate>
  </String>
  <String index="88">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="89">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="90">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="91">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="92">
    <ToTranslate>$......And so.@</ToTranslate>
  </String>
  <String index="93">
    <ToTranslate>$Although we fell into another pinch, we managed to conquer the fortress.@</ToTranslate>
  </String>
  <String index="94">
    <ToTranslate>$However, we took too many casualties to continue pursuing the enemy, and we quickly began arranging for reinforcements and supplies.@</ToTranslate>
  </String>
  <String index="95">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="96">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="97">
    <ToTranslate>_kyotenphase</ToTranslate>
  </String>
</DBS>

</poem>
