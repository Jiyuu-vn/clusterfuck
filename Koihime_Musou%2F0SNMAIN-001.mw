  </String>
  <String index="9">
    <Origin>Oikawa$&quot;You&apos;re right. Your head is steaming.&quot;@</Origin>
    <ToTranslate>Oikawa$&quot;You&apos;re right. Your head is steaming.&quot;@</ToTranslate>
  </String>
  <String index="10">
    <Origin>Kazuto$&quot;Whoa!&quot;@</Origin>
    <ToTranslate>Kazuto$&quot;Whoa!&quot;@</ToTranslate>
  </String>
  <String index="11">
    <Origin>m_yure_r</Origin>
    <ToTranslate>m_yure_r</ToTranslate>
  </String>
