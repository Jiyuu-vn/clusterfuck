[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_wasteland_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_kan11ma</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>bgm0000</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>aisy0132</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>Aisha$&quot;Shuri! Where is Kousonsan&apos;s army?&quot;@</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>tcg_syo21ja</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>tcg_syo21m</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>syur0038</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>Shuri$&quot;I don&apos;t know! They should show up any time now, but I don&apos;t know how that general will...&quot;@</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>$Just as Shuri was trying to weave a response for Aisha, a soldier interrupted.@</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>Soldier$&quot;A cloud of dust and cavalry on white horses have been spotted behind the enemy!&quot;@</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>tcg_cun21ga</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>tcg_cun21m</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>chou0033</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>Chouun$&quot;What flag do they fly?&quot;@</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>Soldier$&quot;Kouson! It&apos;s our reinforcements!&quot;@</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>bgm29</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>tcg_cun31ea</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>tcg_cun31m</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>chou0034</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Chouun$&quot;Great!&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>tcg_syo11aa</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>tcg_syo11m</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>syur0039</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>Shuri$&quot;We did it, Aisha-san!&quot;@</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>aisy0133</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>Aisha$&quot;Yes! Now it&apos;s time to go on the offensive! Let us join Kousonsan&apos;s army and catch them in a pincer attack!&quot;@</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>kyara_kesi</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>tcg_cho11ea</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>tcg_cho11m</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>choh0014</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Rinrin$&quot;Charge! Smash! Victory!&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>tcg_kan11ca</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>tcg_kan11m</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>aisy0134</ToTranslate>
  </String>
  <String index="39">
    <ToTranslate>Aisha$&quot;All units, charge!&quot;@</ToTranslate>
  </String>
  <String index="40">
    <ToTranslate>SE048</ToTranslate>
  </String>
  <String index="41">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="42">
    <ToTranslate>bg_black</ToTranslate>
  </String>
</DBS>

</poem>
