[[Koihime Musou/Прогресс перевода/Сюжет|[Назад]]]
<poem>

<?xml version="1.0" encoding="UTF-8"?>
<DBS>
  <String index="0">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="1">
    <ToTranslate>tcg_ton14ca</ToTranslate>
  </String>
  <String index="2">
    <ToTranslate>tcg_ton14m</ToTranslate>
  </String>
  <String index="3">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="4">
    <ToTranslate>kato0383</ToTranslate>
  </String>
  <String index="5">
    <ToTranslate>Kakouton$&quot;Don&apos;t let the enemy catch you in their momentum! Fight with the pride of a Gi warrior and die bravely!&quot;@</ToTranslate>
  </String>
  <String index="6">
    <ToTranslate>kato0384</ToTranslate>
  </String>
  <String index="7">
    <ToTranslate>Kakouton$&quot;All units, advance! Just keep facing forward and advance, swing your swords!&quot;@</ToTranslate>
  </String>
  <String index="8">
    <ToTranslate>kato0385</ToTranslate>
  </String>
  <String index="9">
    <ToTranslate>Kakouton$&quot;Fear not death! Fear for your honor! Let your bodies become swords, and reap the heads of all who oppose us!&quot;@</ToTranslate>
  </String>
  <String index="10">
    <ToTranslate>kato0386</ToTranslate>
  </String>
  <String index="11">
    <ToTranslate>Kakouton$&quot;Kakouton Squad, all units, charge!&quot;@</ToTranslate>
  </String>
  <String index="12">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="13">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="14">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="15">
    <ToTranslate>tcg_en12fa</ToTranslate>
  </String>
  <String index="16">
    <ToTranslate>tcg_en12m</ToTranslate>
  </String>
  <String index="17">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="18">
    <ToTranslate>kaen0219</ToTranslate>
  </String>
  <String index="19">
    <ToTranslate>Kakouen$&quot;Shoot the incoming enemy&apos;s vanguard. Prepare to fire.&quot;@</ToTranslate>
  </String>
  <String index="20">
    <ToTranslate>SE014</ToTranslate>
  </String>
  <String index="21">
    <ToTranslate>kaen0220</ToTranslate>
  </String>
  <String index="22">
    <ToTranslate>Kakouen$&quot;Show them the power of Kakouen squad! One arrow, one death! Waste no arrows!&quot;@</ToTranslate>
  </String>
  <String index="23">
    <ToTranslate>kaen0221</ToTranslate>
  </String>
  <String index="24">
    <ToTranslate>Kakouen$&quot;All units, fire!&quot;@</ToTranslate>
  </String>
  <String index="25">
    <ToTranslate>SE019</ToTranslate>
  </String>
  <String index="26">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
  <String index="27">
    <ToTranslate>bg_black</ToTranslate>
  </String>
  <String index="28">
    <ToTranslate>bcg_sisuikan_a</ToTranslate>
  </String>
  <String index="29">
    <ToTranslate>tcg_jun21ia</ToTranslate>
  </String>
  <String index="30">
    <ToTranslate>tcg_jun21m</ToTranslate>
  </String>
  <String index="31">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="32">
    <ToTranslate>jyun0179</ToTranslate>
  </String>
  <String index="33">
    <ToTranslate>Jun&apos;iku$&quot;Heh, heh, heh...... You&apos;ve all grouped together nicely.&quot;@</ToTranslate>
  </String>
  <String index="34">
    <ToTranslate>jyun0180</ToTranslate>
  </String>
  <String index="35">
    <ToTranslate>Jun&apos;iku$&quot;Behold my power! Burn in the flames of hell and let defeat be seared upon your body!&quot;@</ToTranslate>
  </String>
  <String index="36">
    <ToTranslate>SE052</ToTranslate>
  </String>
  <String index="37">
    <ToTranslate>SE045</ToTranslate>
  </String>
  <String index="38">
    <ToTranslate>_MsgBoxMain_Off</ToTranslate>
  </String>
